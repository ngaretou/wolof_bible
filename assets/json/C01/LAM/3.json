[{"style":"sp","verse":"","text":"Nit a ngi tawat"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Man maay nit ki gisu naqar,"},{"style":"q1","verse":"","text":"naqar wu ko yarub sànjum Aji Sax ji teg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Man la jañax, te lëndëm la ma doxloo,"},{"style":"q1","verse":"","text":"dug leer."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Man doŋŋ lay yen aka yenaat loxoom,"},{"style":"q1","verse":"","text":"bésoo bés."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Moo ràppal sama der ak samaw suux,"},{"style":"q1","verse":"","text":"tas samay yax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Moo tabax fi sama kaw, tanc ma,"},{"style":"q1","verse":"","text":"gawe ma tooke ak coono."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Béreb yu lëndëm la ma dëël,"},{"style":"q1","verse":"","text":"ni ña dee bu yàgg."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Moo ma ñag ba manumaa bëtt,"},{"style":"q1","verse":"","text":"te jéng yu diis la ma jénge."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Su ma yuuxoo, woote wall it,"},{"style":"q1","verse":"","text":"mu tanqamlu samag ñaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ay doji yatt la ñage samay yoon,"},{"style":"q1","verse":"","text":"te moo lajjal samay ñall."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Moo di rab wu aay wu may tëru,"},{"style":"q1","verse":"","text":"di gaynde gu waafe làquwaayam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Samaw yoon la ma wàcce, xotat ma,"},{"style":"q1","verse":"","text":"tumurànkeel ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Moo ma bankal xalaam,"},{"style":"q1","verse":"","text":"diire mag fittam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Sama xóotey xol la soqil ay fittam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Maa mujj di reetaanukaayu bokk yépp,"},{"style":"q1","verse":"","text":"di seen woyu bés bu nekk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Moo ma reggal wextan,"},{"style":"q1","verse":"","text":"màndal ma lu mel ni xay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Yéyloo na may doj,"},{"style":"q1","verse":"","text":"dëggaate ma ci pënd bi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Jàmmi xol a ma réer,"},{"style":"q1","verse":"","text":"ma fàtte sax luy mbégte."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ca laa ne sama muñukaay a jeex"},{"style":"q1","verse":"","text":"ak it sama yaakaar ci Aji Sax ji!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Bàyyil xel sama naqar ak sama tumurànke,"},{"style":"q1","verse":"","text":"mbete wextan akug tooke!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Dama ciy xalaat aka xalaataat,"},{"style":"q1","verse":"","text":"sama xol jeex tàkk!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Waaye li may dëkke bàyyi xel,"},{"style":"q1","verse":"","text":"ba dese yaakaar mooy lii:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Aji Sax ji, ngoram du dakk,"},{"style":"q1","verse":"","text":"te yërmandeem du jeex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Xanaa di yeeslu jant bu nekk."},{"style":"q1","verse":"","text":"Kee bare kóllëre!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"«Samab séddoo, Aji Sax jee,»"},{"style":"q1","verse":"","text":"la sama xol wax,"},{"style":"q1","verse":"","text":"te moo tax ma di ko xaar."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Aji Sax jee baax ci ñi ko yaakaar,"},{"style":"q1","verse":"","text":"baax ci ki ko wóolu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Li baax mooy muñ,"},{"style":"q1","verse":"","text":"xeremluy xaar wallub Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Li baax ci nit mooy"},{"style":"q1","verse":"","text":"nga nangoo muñ ba ngay ndaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Bu la Aji Sax ji tegee nattu,"},{"style":"q1","verse":"","text":"berul te muñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Dëppal sa jë ci suuf,"},{"style":"q1","verse":"","text":"jombul yaakaar am."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Dékkalub lex ku lay mbej,"},{"style":"q1","verse":"","text":"di la tifaare ay saaga, ba nga tooy."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Boroom bi déy"},{"style":"q1","verse":"","text":"du la wacc ba fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Bu la tegee naqar it, yërëm la,"},{"style":"q1","verse":"","text":"ngoram a ni baree."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Du bànneexam lay tege coono,"},{"style":"q1","verse":"","text":"aka naqaral doom aadama."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Bu ñuy dëggaate"},{"style":"q1","verse":"","text":"mboolem ñi ñu tëj cim réew,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"di jalgati yelleefu nit"},{"style":"q1","verse":"","text":"fi kanam Aji Kawe ji,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"36","text":"di been dëgg cib layoom,"},{"style":"q1","verse":"","text":"xanaa Boroom bi da ko dul gis?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"37","text":"Ana ku wax, mu am,"},{"style":"q1","verse":"","text":"te Boroom bi dogalu ko?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"38","text":"Muy lu baax, di lu aay,"},{"style":"q1","verse":"","text":"xanaa du Aji Kawe jeey àddu, amal?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"39","text":"Kuy dund, ana loo deeti jàmbat,"},{"style":"q1","verse":"","text":"bu ñu la añalee sa bàkkaar?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"40","text":"Nan càmbar, ba xoolaat jikko yi"},{"style":"q1","verse":"","text":"te dellu ci Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"41","text":"Nan tàllal sunu xol ak sunuy loxo"},{"style":"q1","verse":"","text":"Yàlla ja fa asamaan,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"42","text":"noo tooñ, noo fippu,"},{"style":"q1","verse":"","text":"ba tax yaw jéggalewoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"43","text":"Sam sànj nga làmboo, topp nu,"},{"style":"q1","verse":"","text":"di rey te ñéebluwoo,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"44","text":"aw niir nga sàngoo,"},{"style":"q1","verse":"","text":"ba ag ñaan du ko jàll."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"45","text":"Mbuubitum sën nga nu def"},{"style":"q1","verse":"","text":"fi biir xeet yi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"46","text":"Sunu noon yépp a nuy waxal,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"47","text":"tiitaange ak pax dikkal na nu,"},{"style":"q1","verse":"","text":"ak yàquteek sànkute."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"48","text":"Saay gët a ngi wal"},{"style":"q1","verse":"","text":"ndax sama askan wi rajaxoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"49","text":"Saay gët a ngi rogalaat,"},{"style":"q1","verse":"","text":"du fer, du ŋiis,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"50","text":"ba keroog Aji Sax ji séentoo asamaan, ba gis."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"51","text":"Bi ma gisee li dal jigéeni dëkk bi yépp,"},{"style":"q1","verse":"","text":"la sama xol jeex."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"52","text":"Ñi ma bañ ci dara"},{"style":"q1","verse":"","text":"ñoo ma këpptal a këpptal ni picc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"53","text":"Cib teen lañu ma tàbbal,"},{"style":"q1","verse":"","text":"ube kow doj, te may dund,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"54","text":"ndox ma mëdd ma,"},{"style":"q1","verse":"","text":"ma ne: «Sànku naa.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"55","text":"Aji Sax ji, fa xóotey pax ma"},{"style":"q1","verse":"","text":"laa la woowe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"56","text":"Sama baat nga dégg, ma ne la:"},{"style":"q1","verse":"","text":"«Bul fattal sab nopp"},{"style":"q1","verse":"","text":"samay onk ak samay yuux.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"57","text":"Bés ba ma la woowee nga dikk,"},{"style":"q1","verse":"","text":"ne ma: «Bul tiit.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"58","text":"Boroom bi, yaa sàmm sama àq,"},{"style":"q1","verse":"","text":"yaa jot sama bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"59","text":"Aji Sax ji, yaa gis ni ñu ma tooñe,"},{"style":"q1","verse":"","text":"jox ma sama àtte."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"60","text":"Yaa gis seen mboolem mébétu feyu,"},{"style":"q1","verse":"","text":"seen mboolem pexe yi ñu may lalal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"61","text":"Aji Sax ji, yaa dégg seeni kókkalee,"},{"style":"q1","verse":"","text":"ak mboolem pexe yi ñu may lalal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"62","text":"Ci man la noon yiy diisoo aka déeyoo,"},{"style":"q1","verse":"","text":"bés bu nekk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"63","text":"Xoolal, bu ñu toogee ak bu ñu jógee,"},{"style":"q1","verse":"","text":"man lañuy léeboo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"64","text":"Aji Sax ji, jox leen seenub añ,"},{"style":"q1","verse":"","text":"seen kemu liggéeyu loxo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"65","text":"Dërkiisalal seen xol,"},{"style":"q1","verse":"","text":"na leen sag alkànde ñeel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"66","text":"Toppe leen sa mer, te nga sànke leen"},{"style":"q1","verse":"","text":"mboolem fu asamaanu Aji Sax ji tiim."}]