[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Waaye man saxyaayu joor doŋŋ laa,"},{"style":"q1","verse":"","text":"nirook as tóor-tóor ciw xur."},{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Xanaa as tóor-tóor su ne ràññ ci biiri dég!"},{"style":"q1","verse":"","text":"Kookooy sama xarit ci biir janq ji."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Mbete garab gu gëna neex ci gott bi,"},{"style":"q1","verse":"","text":"kookooy sama nijaay ci biir xale yu góor yi."},{"style":"q1","verse":"","text":"May bége keppaaram, safoo doom ya."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Dugal na ma néegub sago-jeex-na,"},{"style":"q1","verse":"","text":"yiire ma mbëggeel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Gaaweleen ma nàkki reseñ, ma dëgër,"},{"style":"q1","verse":"","text":"leelleen may meññeef, leqalee ma."},{"style":"q1","verse":"","text":"Wopp laa def ndax mbëggeel!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Sama càmmoñu waay, ma gegenoo,"},{"style":"q1","verse":"","text":"ndijooram, ma laxasoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yeen janqi Yerusalem, waatalleen ma"},{"style":"q1","verse":"","text":"ci taaru kéwél ak koobay àll bi,"},{"style":"q1","verse":"","text":"ne dungeen dakkal te dungeen sooke tëraayu mbëggeel"},{"style":"q1","verse":"","text":"te jotul."},{"style":"s1","verse":"","text":"Wootante jib na"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Dégluleen, sama nijaay a,"},{"style":"q1","verse":"","text":"mu ngooguy ñëw,"},{"style":"q1","verse":"","text":"di xéluy jéggi tund yu ndaw yi,"},{"style":"q1","verse":"","text":"di tëb tund yu mag yiy dikk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Sama nijaay a ngi mel ni kéwél,"},{"style":"q1","verse":"","text":"mbaa kooba gu ndaw."},{"style":"q1","verse":"","text":"Mu ngi noonu taxaw ca gannaaw miir ba,"},{"style":"q1","verse":"","text":"di séentu ci palanteer bi"},{"style":"q1","verse":"","text":"aka yër ci caax bi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Sama nijaay ne ma:"},{"style":"q1","verse":"","text":"«Xarit, ayca,"},{"style":"q1","verse":"","text":"jongama sama, dikkal boog!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Xoolal, tawub sedd wees na,"},{"style":"q1","verse":"","text":"waame jàll, wéy\\f + \\fr 2.11 \\ft Ci Israayil, jamonoy sedd day taw. Bu taw bi jeexee, tàngoor wi dikk, gàncax gi jebbi, daldi naat.\\f*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Tóor-tóor yi lal na réew mi,"},{"style":"q1","verse":"","text":"jamonoy woy dikk,"},{"style":"q1","verse":"","text":"sabum péget\\f + \\fr 2.12 \\fk péget, \\ft xeetu picc la mu nirool pitax.\\f* jibe sunum réew."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Figg a ngi ñorsi,"},{"style":"q1","verse":"","text":"reseñ tóor, di gilli."},{"style":"q1","verse":"","text":"Xarit, ayca,"},{"style":"q1","verse":"","text":"jongama sama, dikkal boog!»"},{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Soppe, sore nga ni xati mu dëkke xar-xari doj,"},{"style":"q1","verse":"","text":"làqoo ruqi mbartal yi."},{"style":"q1","verse":"","text":"Won ma sa jëmm,"},{"style":"q1","verse":"","text":"dégtal ma sa baat."},{"style":"q1","verse":"","text":"Sa baat a neex,"},{"style":"q1","verse":"","text":"sa jëmm jekk!"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Jàppalleen ma till yi,"},{"style":"q1","verse":"","text":"till yu ndaw yiy ruur reseñ,"},{"style":"q1","verse":"","text":"te sunu reseñ jiy tóor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Sama nijaay, maa moom, man it, moo moom,"},{"style":"q1","verse":"","text":"di fore digg tóor-tóor yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Nijaay, balaa jant a fenk,"},{"style":"q1","verse":"","text":"ker toxu,"},{"style":"q1","verse":"","text":"dellul gaaw ni kéwél,"},{"style":"q1","verse":"","text":"mbaa kooba gu ndaw kaw tund yu xàjjalikoo ya."}]