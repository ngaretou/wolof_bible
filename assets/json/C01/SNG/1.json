[{"style":"mt1","text":"Ngën-gi-woy"},{"style":"s1","verse":"","text":"Cofeel door na"},{"style":"p","verse":"","text":""},{"style":"v","verse":"1","text":"Lii, di ngën-gi-woy, ñeel Suleymaan."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Fóon maa, ngalla fóon ma,"},{"style":"q1","verse":"","text":"sa cofeel a ma dàqal biiñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Sa diw yi ngay xeeñoo neex,"},{"style":"q1","verse":"","text":"saw tur jib, mel ni lu xeeñ a xelli."},{"style":"q1","verse":"","text":"Moo tax janq ji nob la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yóbbaale ma, nu daw, dem."},{"style":"q1","verse":"","text":"Sama buur, yóbbu ma sa néeg,"},{"style":"q1","verse":"","text":"nu bokk mbég ak bànneex,"},{"style":"q1","verse":"","text":"di tàqamtikoo sa cofeel gi dàq biiñ."},{"style":"q1","verse":"","text":"Ñoo yey nob la!"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Yeen janqi Yerusalem, damaa ñuul, rafet;"},{"style":"q1","verse":"","text":"ñuul ni xayma ya ca Kedar\\f + \\fr 1.5 \\fk Kedar, \\ft diiwaanu Arabi la woon bu genn giirug Ismayla dëkke woon bu yàgg. Dañu daan defare seeni xayma ay deri bëy yu ñuul.\\f*,"},{"style":"q1","verse":"","text":"mel ni ridoy Buur Suleymaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Buleen ma xoole sama ñuulaay bi,"},{"style":"q1","verse":"","text":"jant bee ma lakk."},{"style":"q1","verse":"","text":"Samay càmmiñ a ma mere,"},{"style":"q1","verse":"","text":"di ma wattuloo tóokëri reseñ ya,"},{"style":"q1","verse":"","text":"te wattuwuma sama tóokëri bopp\\f + \\fr 1.6 \\fk tóokër \\ft ak \\fk sàmm \\ft ay baat la yu ñuy méngalee ndaw si ci waxin. Seetal it ci aaya 8 (1.8).\\f*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Sama soppey xol, ngalla wax ma ana fooy foral,"},{"style":"q1","verse":"","text":"di fa gooral, digg bëccëg?"},{"style":"q1","verse":"","text":"Lu ko moy may muuru, di la wër"},{"style":"q1","verse":"","text":"fi say xarit ak seeni gétt."},{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Aa! Xamoo koo?"},{"style":"q1","verse":"","text":"Yaw mi dàq ci jigéen ñi!"},{"style":"q1","verse":"","text":"Génnal rekk, topp wewi jur gi,"},{"style":"q1","verse":"","text":"sàmmi say tef,"},{"style":"q1","verse":"","text":"fi dendeek mbaari sàmm si."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Xarit, xam nga lu ma lay xoole?"},{"style":"q1","verse":"","text":"Yànjaayu wajan wu takk watiiru Firawna."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Sa lex yaa ngi tàkk fi digg sàdd yi!"},{"style":"q1","verse":"","text":"Céy wii loos ak caqu peram!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Sàddi wurus lanu lay defaral,"},{"style":"q1","verse":"","text":"tapp ca xaalis."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Li sama buur tëdd, di xéewlu lépp,"},{"style":"q1","verse":"","text":"maa ngi gilli lu neex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Sama nijaay di mbuusum ndàbb"},{"style":"q1","verse":"","text":"may fanaan sama digg ween."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Saa nijaay, saa cabbu tóor-tóoru fuddën,"},{"style":"q1","verse":"","text":"fa digg tóokëri Engedi\\f + \\fr 1.14 \\fk Engedi, \\ft bëtu ndox la bu nekk ca sowub géeju Xorom ga. Engedi di tekki «bëtu ndoxu bëy».\\f*."},{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Xarit, yaaka rafet,"},{"style":"q1","verse":"","text":"yaaka taaru!"},{"style":"q1","verse":"","text":"Say gët niy pitax."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Nijaay, yaaka góorayiw,"},{"style":"q1","verse":"","text":"yaaka maa neex!"},{"style":"q1","verse":"","text":"Mbooy gu naat, nu laloo,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"garabi seedar di sunu xànqi néeg,"},{"style":"q1","verse":"","text":"garab gu dul ruus xadd ko\\f + \\fr 1.17 \\fk seedar \\ft ak \\fk garab gu dul ruus, \\ft ay garab lañu yu neex xet.\\f*."}]