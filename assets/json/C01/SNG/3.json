[{"style":"s1","verse":"","text":"Réeroo, gise"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Sama kaw lal, guddi,"},{"style":"q1","verse":"","text":"laa seet sama soppey xol."},{"style":"q1","verse":"","text":"Seet naa ko, gisuma ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Naa jóg, wër dëkk bi."},{"style":"q1","verse":"","text":"Mbedd yeek pénc yi"},{"style":"q1","verse":"","text":"laay seete sama soppey xol."},{"style":"q1","verse":"","text":"Seet naa ko, gisuma ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Wattukat yee ma gis,"},{"style":"q1","verse":"","text":"ba ñuy wër dëkk bi."},{"style":"q1","verse":"","text":"Ma ne leen: «Sama soppey xol, gisuleen ko?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ma romb leen tuuti rekk,"},{"style":"q1","verse":"","text":"gis sama soppey xol."},{"style":"q1","verse":"","text":"Ma ne ko taral, bàyyeetuma ko,"},{"style":"q1","verse":"","text":"ba yóbbu ko sama kër yaay,"},{"style":"q1","verse":"","text":"ba sama biir néegu ndey ji ma jur."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ngalla yeen, janqi Yerusalem, waatalleen ma"},{"style":"q1","verse":"","text":"ci taaru kéwél ak koobay àll bi,"},{"style":"q1","verse":"","text":"ne dungeen dakkal te dungeen sooke tëraayu mbëggeel"},{"style":"q1","verse":"","text":"te jotul."},{"style":"s1","verse":"","text":"Daagu, jëm toolu mbëggeel"},{"style":"sp","verse":"","text":"Ñeneen"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ana luy jóge màndiŋ ma nii,"},{"style":"q1","verse":"","text":"mel ni taxaaru saxaar,"},{"style":"q1","verse":"","text":"suuroo ndàbb ak xas mu xeeñ"},{"style":"q1","verse":"","text":"ak mboolem cuuraayi jula?"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Laltub Suleymaan a ngoog,"},{"style":"q1","verse":"","text":"juróom benn fukki jàmbaar dar ko,"},{"style":"q1","verse":"","text":"di jàmbaari Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ñépp mane saamar,"},{"style":"q1","verse":"","text":"ku ci nekk mokkal xare,"},{"style":"q1","verse":"","text":"saamar ci pooj,"},{"style":"q1","verse":"","text":"ngay fàggu mbettum guddi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ab toogu la Buur Suleymaan defarlu,"},{"style":"q1","verse":"","text":"ñu defare ko bantu Libaŋ,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"ponku ya, mu def ko xaalis,"},{"style":"q1","verse":"","text":"wéeruwaay ba wurus;"},{"style":"q1","verse":"","text":"tooguwaay ba di ndimo lu xonq curr,"},{"style":"q1","verse":"","text":"biir ba di liggéeyu ràbb"},{"style":"q1","verse":"","text":"bu janqi Yerusalem ràbbe cofeel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Yeen janqi \\w Siyoŋ|Siyoŋ:\\w*, génnleen,"},{"style":"q1","verse":"","text":"xool Buur Suleymaan ak mbaxanam céetal"},{"style":"q1","verse":"","text":"mi ko yaayam solal ci bésub céetalam,"},{"style":"q1","verse":"","text":"bésu bànneexam."}]