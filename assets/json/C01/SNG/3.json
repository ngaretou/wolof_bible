[{"style":"s1","text":"Réeroo, gise"},{"style":"sp","text":"Ndaw si"},{"style":"q1","text":""},{"style":"v","text":"Sama kaw lal, guddi,"},{"style":"q1","text":"laa seet sama soppey xol."},{"style":"q1","text":"Seet naa ko, gisuma ko."},{"style":"q1","text":""},{"style":"v","text":"Naa jóg, wër dëkk bi."},{"style":"q1","text":"Mbedd yeek pénc yi"},{"style":"q1","text":"laay seete sama soppey xol."},{"style":"q1","text":"Seet naa ko, gisuma ko."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Wattukat yee ma gis,"},{"style":"q1","text":"ba ñuy wër dëkk bi."},{"style":"q1","text":"Ma ne leen: «Sama soppey xol, gisuleen ko?»"},{"style":"q1","text":""},{"style":"v","text":"Ma romb leen tuuti rekk,"},{"style":"q1","text":"gis sama soppey xol."},{"style":"q1","text":"Ma ne ko taral, bàyyeetuma ko,"},{"style":"q1","text":"ba yóbbu ko sama kër yaay,"},{"style":"q1","text":"ba sama biir néegu ndey ji ma jur."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ngalla yeen, janqi Yerusalem, waatalleen ma"},{"style":"q1","text":"ci taaru kéwél ak koobay àll bi,"},{"style":"q1","text":"ne dungeen dakkal te dungeen sooke tëraayu mbëggeel"},{"style":"q1","text":"te jotul."},{"style":"s1","text":"Daagu, jëm toolu mbëggeel"},{"style":"sp","text":"Ñeneen"},{"style":"q1","text":""},{"style":"v","text":"Ana luy jóge màndiŋ ma nii,"},{"style":"q1","text":"mel ni taxaaru saxaar,"},{"style":"q1","text":"suuroo ndàbb ak xas mu xeeñ"},{"style":"q1","text":"ak mboolem cuuraayi jula?"},{"style":"sp","text":"Ndaw si"},{"style":"q1","text":""},{"style":"v","text":"Laltub Suleymaan a ngoog,"},{"style":"q1","text":"juróom benn fukki jàmbaar dar ko,"},{"style":"q1","text":"di jàmbaari Israayil."},{"style":"q1","text":""},{"style":"v","text":"Ñépp mane saamar,"},{"style":"q1","text":"ku ci nekk mokkal xare,"},{"style":"q1","text":"saamar ci pooj,"},{"style":"q1","text":"ngay fàggu mbettum guddi."},{"style":"q1","text":""},{"style":"v","text":"Ab toogu la Buur Suleymaan defarlu,"},{"style":"q1","text":"ñu defare ko bantu Libaŋ,"},{"style":"q1","text":""},{"style":"v","text":"ponku ya, mu def ko xaalis,"},{"style":"q1","text":"wéeruwaay ba wurus;"},{"style":"q1","text":"tooguwaay ba di ndimo lu xonq curr,"},{"style":"q1","text":"biir ba di liggéeyu ràbb"},{"style":"q1","text":"bu janqi Yerusalem ràbbe cofeel."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yeen janqi \\w Siyoŋ|Siyoŋ:\\w*, génnleen,"},{"style":"q1","text":"xool Buur Suleymaan ak mbaxanam céetal"},{"style":"q1","text":"mi ko yaayam solal ci bésub céetalam,"},{"style":"q1","text":"bésu bànneexam."}]