[{"style":"sp","text":"Waa ji"},{"style":"q1","text":""},{"style":"v","text":"Yaaka rafet, xarit, yaaka taaru!"},{"style":"q1","text":"Sa gët yi niy pitax, làqoo muuraay bi,"},{"style":"q1","text":"njañ liy loy-loyee ngi saf géttu bëy yu ñuul"},{"style":"q1","text":"yu bartaloo kaw tundu Galàdd."},{"style":"q1","text":""},{"style":"v","text":"Sa gëñ yi weex ni xar yu ñu wat"},{"style":"q1","text":"ñuy yéege ca sangukaay ba,"},{"style":"q1","text":"ku nekk ak seexam,"},{"style":"q1","text":"kenn wéetu ca."},{"style":"q1","text":""},{"style":"v","text":"Sa tuñ yi mel ni wëñ gu xonq curr,"},{"style":"q1","text":"ndaw gémmiñ gu jekk!"},{"style":"q1","text":"Sa lex yiy tàkk ni ŋaali gërënaat,"},{"style":"q1","text":"làqoo sa muuraay bi;"},{"style":"q1","text":""},{"style":"v","text":"sa loos wi ne coleet ni tatay Daawuda,"},{"style":"q1","text":"mërgalu ba jekk,"},{"style":"q1","text":"caq bay lang ni junniy kiiraayi jàmbaar."},{"style":"q1","text":""},{"style":"v","text":"Sa ween yi ni ñaari tef,"},{"style":"q1","text":"mbaa seexi kéwél yuy fore digg tóor-tóor yi."},{"style":"q1","text":""},{"style":"v","text":"Balaa jant a fenk,"},{"style":"q1","text":"ker gàddaay,"},{"style":"q1","text":"ma ne coww diggante tund wa ndàbb ne bann,"},{"style":"q1","text":"ak tund wa xas ma neexe."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Xarit, sa lépp a rafet,"},{"style":"q1","text":"sikk nekkul ci yaw."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Séet sama, jógeel Libaŋ, jege ma,"},{"style":"q1","text":"bawool Libaŋ, jegesi ma."},{"style":"q1","text":"Wàcceel colli Amana"},{"style":"q1","text":"ak colli Senir ak Ermon."},{"style":"q1","text":"Génnal xunti gaynde yooyook"},{"style":"q1","text":"tundi segg ya."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Jigéen sama, séet sama, benn yoon nga ma xool,"},{"style":"q1","text":"sama fit ne tërit."},{"style":"q1","text":"Sa benn peru caq rekk,"},{"style":"q1","text":"sama fit ne tërit."},{"style":"q1","text":""},{"style":"v","text":"Jigéen sama, séet sama,"},{"style":"q1","text":"sa mbëggeel a neex!"},{"style":"q1","text":"Sa mbëggeel a dàq biiñ,"},{"style":"q1","text":"sa diw yu xeeñ dàq luy cuuraay."},{"style":"q1","text":""},{"style":"v","text":"Séet sama! Lem ay xellee sa gémmiñ,"},{"style":"q1","text":"lem ak meew lal sa làmmiñ."},{"style":"q1","text":"Sa xetu yére xetu àllub Libaŋ la."},{"style":"q1","text":""},{"style":"v","text":"Jigéen sama, séet sama, yaay tóokër bu tëje,"},{"style":"q1","text":"yaay seyaan bu tëje,"},{"style":"q1","text":"di bëtu ndox bu saañe."},{"style":"q1","text":""},{"style":"v","text":"Yaa di toolub gërënaat bu naat,"},{"style":"q1","text":"meññ ngën-gi-meññeef"},{"style":"q1","text":"ak fuddën ak reen yu xeeñ,"},{"style":"q1","text":""},{"style":"v","text":"muy reen yu xeeñ, safroŋ, barax, kanel,"},{"style":"q1","text":"ak xasoo xas mu xeeñ,"},{"style":"q1","text":"ndàbb ak \\w alowes|alowes:\\w*"},{"style":"q1","text":"ak lépp lu xeeñ a xeeñ."},{"style":"q1","text":""},{"style":"v","text":"Yaa di bëtu ndox ci tool yi,"},{"style":"q1","text":"di teen bu fees ak ndox muy daw,"},{"style":"q1","text":"wale tundi Libaŋ."},{"style":"sp","text":"Ndaw si"},{"style":"q1","text":""},{"style":"v","text":"Yaw, ngelawal bëj-gànnaar, yewwul,"},{"style":"q1","text":"ngelawal bëj-saalum, dikkal,"},{"style":"q1","text":"wolalleen ma sama tool bi,"},{"style":"q1","text":"mu gilli;"},{"style":"q1","text":"sama nijaay dugg toolam,"},{"style":"q1","text":"xéewloo ngën-gi-meññeefam."}]