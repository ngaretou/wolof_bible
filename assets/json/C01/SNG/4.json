[{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Yaaka rafet, xarit, yaaka taaru!"},{"style":"q1","verse":"","text":"Sa gët yi niy pitax, làqoo muuraay bi,"},{"style":"q1","verse":"","text":"njañ liy loy-loyee ngi saf géttu bëy yu ñuul"},{"style":"q1","verse":"","text":"yu bartaloo kaw tundu Galàdd."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Sa gëñ yi weex ni xar yu ñu wat"},{"style":"q1","verse":"","text":"ñuy yéege ca sangukaay ba,"},{"style":"q1","verse":"","text":"ku nekk ak seexam,"},{"style":"q1","verse":"","text":"kenn wéetu ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Sa tuñ yi mel ni wëñ gu xonq curr,"},{"style":"q1","verse":"","text":"ndaw gémmiñ gu jekk!"},{"style":"q1","verse":"","text":"Sa lex yiy tàkk ni ŋaali gërënaat,"},{"style":"q1","verse":"","text":"làqoo sa muuraay bi;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"sa loos wi ne coleet ni tatay Daawuda,"},{"style":"q1","verse":"","text":"mërgalu ba jekk,"},{"style":"q1","verse":"","text":"caq bay lang ni junniy kiiraayi jàmbaar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sa ween yi ni ñaari tef,"},{"style":"q1","verse":"","text":"mbaa seexi kéwél yuy fore digg tóor-tóor yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Balaa jant a fenk,"},{"style":"q1","verse":"","text":"ker gàddaay,"},{"style":"q1","verse":"","text":"ma ne coww diggante tund wa ndàbb ne bann,"},{"style":"q1","verse":"","text":"ak tund wa xas ma neexe."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Xarit, sa lépp a rafet,"},{"style":"q1","verse":"","text":"sikk nekkul ci yaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Séet sama, jógeel Libaŋ, jege ma,"},{"style":"q1","verse":"","text":"bawool Libaŋ, jegesi ma."},{"style":"q1","verse":"","text":"Wàcceel colli Amana"},{"style":"q1","verse":"","text":"ak colli Senir ak Ermon."},{"style":"q1","verse":"","text":"Génnal xunti gaynde yooyook"},{"style":"q1","verse":"","text":"tundi segg ya."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Jigéen sama, séet sama, benn yoon nga ma xool,"},{"style":"q1","verse":"","text":"sama fit ne tërit."},{"style":"q1","verse":"","text":"Sa benn peru caq rekk,"},{"style":"q1","verse":"","text":"sama fit ne tërit."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Jigéen sama, séet sama,"},{"style":"q1","verse":"","text":"sa mbëggeel a neex!"},{"style":"q1","verse":"","text":"Sa mbëggeel a dàq biiñ,"},{"style":"q1","verse":"","text":"sa diw yu xeeñ dàq luy cuuraay."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Séet sama! Lem ay xellee sa gémmiñ,"},{"style":"q1","verse":"","text":"lem ak meew lal sa làmmiñ."},{"style":"q1","verse":"","text":"Sa xetu yére xetu àllub Libaŋ la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Jigéen sama, séet sama, yaay tóokër bu tëje,"},{"style":"q1","verse":"","text":"yaay seyaan bu tëje,"},{"style":"q1","verse":"","text":"di bëtu ndox bu saañe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Yaa di toolub gërënaat bu naat,"},{"style":"q1","verse":"","text":"meññ ngën-gi-meññeef"},{"style":"q1","verse":"","text":"ak fuddën ak reen yu xeeñ,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"muy reen yu xeeñ, safroŋ, barax, kanel,"},{"style":"q1","verse":"","text":"ak xasoo xas mu xeeñ,"},{"style":"q1","verse":"","text":"ndàbb ak \\w alowes|alowes:\\w*"},{"style":"q1","verse":"","text":"ak lépp lu xeeñ a xeeñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Yaa di bëtu ndox ci tool yi,"},{"style":"q1","verse":"","text":"di teen bu fees ak ndox muy daw,"},{"style":"q1","verse":"","text":"wale tundi Libaŋ."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Yaw, ngelawal bëj-gànnaar, yewwul,"},{"style":"q1","verse":"","text":"ngelawal bëj-saalum, dikkal,"},{"style":"q1","verse":"","text":"wolalleen ma sama tool bi,"},{"style":"q1","verse":"","text":"mu gilli;"},{"style":"q1","verse":"","text":"sama nijaay dugg toolam,"},{"style":"q1","verse":"","text":"xéewloo ngën-gi-meññeefam."}]