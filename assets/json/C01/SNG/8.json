[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Éy su meloon ni sama càmmiñ nga,"},{"style":"q1","verse":"","text":"mel ni ku ma bokkal wenn ween,"},{"style":"q1","verse":"","text":"ba ma dajeek yaw biti, sañ laa dar,"},{"style":"q1","verse":"","text":"te du tax ñu sikk ma!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ma jàpp la, yóbbu sama néeg yaay,"},{"style":"q1","verse":"","text":"moom mi ma yar\\f + \\fr 8.2 \\ft Mbaa: \\fqa nga jàngal ma baane,\\f*,"},{"style":"q1","verse":"","text":"ma may la biiñ bu saf,"},{"style":"q1","verse":"","text":"ak sama ndoxum gërënaat, nga naan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Sama càmmoñu waay, ma gegenoo,"},{"style":"q1","verse":"","text":"ndijooram, ma laxasoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yeen janqi Yerusalem, waatalleen ma"},{"style":"q1","verse":"","text":"ne dungeen dakkal te dungeen sooke tëraayu mbëggeel"},{"style":"q1","verse":"","text":"te jotul."},{"style":"s1","verse":"","text":"Joxe naa xol"},{"style":"sp","verse":"","text":"Janqi Yerusalem"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ana ndaw suy jóge màndiŋ ma nii,"},{"style":"q1","verse":"","text":"wéeroo nijaayam?"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":"Ci ron pom gi laa la yee,"},{"style":"q1","verse":"","text":"fa la sa yaay jàppe ëmb,"},{"style":"q1","verse":"","text":"jur la fa."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Teg ma ci sa xol ni torlukaay\\f + \\fr 8.6 \\fk torlukaay \\ft mooy jumtukaay bu mel ni lu ñuy tàmponee.\\f*,"},{"style":"q1","verse":"","text":"mbaa ci sa përëg ni lamu màndarga."},{"style":"q1","verse":"","text":"Mbëggeel a bare doole ni dee,"},{"style":"q1","verse":"","text":"fiiraange xér ni bàmmeel."},{"style":"q1","verse":"","text":"Day jàpp jippét,"},{"style":"q1","verse":"","text":"di jum bu \\w Aji Sax ji|Aji Sax j-:\\w* taal\\f + \\fr 8.6 \\ft Mbaa: \\fqa tàkk di sël-sëli.\\f*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ndox mu ne xéew du fey mbëggeel,"},{"style":"q1","verse":"","text":"ay dex du ko mëdd."},{"style":"q1","verse":"","text":"Ku koy weccee sa alalu kër gépp it,"},{"style":"q1","verse":"","text":"ñu jéppi laa jéppi rekk."},{"style":"sp","verse":"","text":"Càmmiñi ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Sunub jigéen lu ndaw la,"},{"style":"q1","verse":"","text":"ween sax amu ko."},{"style":"q1","verse":"","text":"Lu nuy defal sunub jigéen"},{"style":"q1","verse":"","text":"bés bu ñu koy bëggsi?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Su doon am tata,"},{"style":"q1","verse":"","text":"nu tabaxal ko soorooru xaalis."},{"style":"q1","verse":"","text":"Su doon bunt,"},{"style":"q1","verse":"","text":"nu aare ko xànqi seedar."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Maa dim tata,"},{"style":"q1","verse":"","text":"sama ween di sooroor ya;"},{"style":"q1","verse":"","text":"moo tax waa ji xam ne"},{"style":"q1","verse":"","text":"maa di jàmmam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Suleymaan am na reseñ ca Baal Amon."},{"style":"q1","verse":"","text":"Da koo dénk ay beykat."},{"style":"q1","verse":"","text":"Ku nekk di ko fey meññeef ma"},{"style":"q1","verse":"","text":"junniy dogi xaalis."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Junniy dogi xaalis yi,"},{"style":"q1","verse":"","text":"Suleymaan, yaa ko moom;"},{"style":"q1","verse":"","text":"ñaari téeméer yi, beykat yi."},{"style":"q1","verse":"","text":"Waaye sama toolub reseñ, maa ko moom."},{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Yaw mi tooge digg tool bi,"},{"style":"q1","verse":"","text":"samay xarit a ngi teewlu sa baat,"},{"style":"q1","verse":"","text":"waxal boog, ma dégg."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Nijaay, dawsil,"},{"style":"q1","verse":"","text":"melal ni kéwél,"},{"style":"q1","verse":"","text":"mbaa kooba gu ndaw"},{"style":"q1","verse":"","text":"ci kaw tund wi ci gàncax gu xeeñ gi."}]