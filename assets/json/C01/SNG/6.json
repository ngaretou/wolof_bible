[{"style":"sp","verse":"","text":"Janqi Yerusalem"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Yaw mi dàq ci jigéen ñi,"},{"style":"q1","verse":"","text":"ana fu sa nijaay dem?"},{"style":"q1","verse":"","text":"Wax nu fu sa nijaay jaar,"},{"style":"q1","verse":"","text":"nu seetle la ko."},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Sama nijaay dugg na biir toolam,"},{"style":"q1","verse":"","text":"fa gàncax ga ne bann."},{"style":"q1","verse":"","text":"Mu ngi foral ci tool bi,"},{"style":"q1","verse":"","text":"aka fori tóor-tóor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Man, nijaay a moom;"},{"style":"q1","verse":"","text":"nijaay it, maa moom."},{"style":"q1","verse":"","text":"Mooy fore sama digg tóor-tóor yi."},{"style":"s1","verse":"","text":"Yaaka taaru"},{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Xarit, taaru nga ni Tirca ak Yerusalem,"},{"style":"q1","verse":"","text":"péeyi buur ya,"},{"style":"q1","verse":"","text":"yéeme nga ni gàngoor yi yékkati seeni raaya."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Wuy, geesul fee!"},{"style":"q1","verse":"","text":"Soo ma nee jàkk, ma jànnaxe."},{"style":"q1","verse":"","text":"Sa njañ liy loy-loyee ngi saf géttu bëy yu ñuul,"},{"style":"q1","verse":"","text":"yu bartaloo kaw tundu Galàdd."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Sa gëñ yi ni mbote yu weex,"},{"style":"q1","verse":"","text":"yéege ca sangukaay ba,"},{"style":"q1","verse":"","text":"ku nekk ak seexam,"},{"style":"q1","verse":"","text":"kenn wéetu ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Sa lex yiy tàkk ni ŋaali gërënaat,"},{"style":"q1","verse":"","text":"làqoo sa muuraay bi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Buur day am juróom benn fukki lingeer"},{"style":"q1","verse":"","text":"ak juróom ñett fukki jongama"},{"style":"q1","verse":"","text":"ak jeeg ju dul jeex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Waaye sama nenne kenn la, matal ma sëkk,"},{"style":"q1","verse":"","text":"yaayam kennal ko,"},{"style":"q1","verse":"","text":"muy ngëneelu biiru yaayam."},{"style":"q1","verse":"","text":"Janq ji gis ko, di ko jëwe mbégteem,"},{"style":"q1","verse":"","text":"lingeeri buur aki jongamaam di ko kañ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ña nga naa: «Ndaw sii ku mu?"},{"style":"q1","verse":"","text":"Bu nee xiféet, nga ne fajar a xar,"},{"style":"q1","verse":"","text":"rafet ni leer gu ndaw,"},{"style":"q1","verse":"","text":"yànj ni jant bi,"},{"style":"q1","verse":"","text":"yéeme ni gàngoori biddiiw!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Damaa wàcc ba ca toolub saal ya,"},{"style":"q1","verse":"","text":"xooli cax mu yees mi ci xur wi,"},{"style":"q1","verse":"","text":"bu reseñ ji jebbee,"},{"style":"q1","verse":"","text":"mbaa gërënaat ji tóor, ma gis."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Damaa ràkkaaju bay lox,"},{"style":"q1","verse":"","text":"mel ni ku war watiiru buur\\f + \\fr 6.12 \\ft Li ñu wax fii ci làkku ebrë am na werante.\\f*."}]