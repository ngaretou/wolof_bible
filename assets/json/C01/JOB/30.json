[{"style":"s1","verse":"","text":"Ayóoba jooy na coono"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Tey ñi may diijoo ngii, te ñoo ma féete ndaw,"},{"style":"q1","verse":"","text":"seeni baay a tekkiwuloon tus,"},{"style":"q2","verse":"","text":"ba ma siide leena jël, ñuy sàmmandook samay xaj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ana njariñ lu ñu ma amaloon sax?"},{"style":"q1","verse":"","text":"Yooyu kooñoor yu jeele?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ñii ndóol, xiif, desey yax,"},{"style":"q1","verse":"","text":"di ŋeeñaatoo biir àll,"},{"style":"q1","verse":"","text":"fu lëndëm, gental, maral,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"di witt ñax mu saxe ciy tan"},{"style":"q1","verse":"","text":"aka gasi reen, di dunde."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ñooñu lees dàqe fi biir nit ñi,"},{"style":"q1","verse":"","text":"di leen yuuxoo niy sàcc,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"ñu dëkkey xur,"},{"style":"q1","verse":"","text":"ci biiri pax aki doj,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"di ŋaaxe biir gajj gi,"},{"style":"q1","verse":"","text":"dankaloo ci ron dég yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ñu sew lañu, ñu kenn yégul,"},{"style":"q1","verse":"","text":"xanaa caw leen, génne réew mi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Tey nag ñoo may woyantoo,"},{"style":"q1","verse":"","text":"di ma léeboo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Dañu maa seexlu, ñooru ma,"},{"style":"q1","verse":"","text":"néeg maa serati ci kanam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Yàllaa ma nasaxal, toroxal ma,"},{"style":"q1","verse":"","text":"ba amatuñook man yemukaay."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Mii njurum baadoola mu féete ndijoor, di ma téqtal,"},{"style":"q1","verse":"","text":"di ma xàllal yooni sànkute."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ñu ngi may dogale, nar maa musibaal,"},{"style":"q1","verse":"","text":"te soxlawuñu ci ndimbal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Fu yaa lañu bëtte,"},{"style":"q1","verse":"","text":"buure jali tojit, ne jàyy fi sama kaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Musiba da maa ëlëm,"},{"style":"q1","verse":"","text":"sama daraja naaw ni ngelaw,"},{"style":"q1","verse":"","text":"sama teraanga naaxsaay niw niir."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Tey maa ngii sama bakkan di ŋiis,"},{"style":"q1","verse":"","text":"bési njàqare ne ma taral."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Guddi dikk di bënn samay yax, muy rot,"},{"style":"q1","verse":"","text":"mitit di ŋereñ te du selaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Kii ne nikk sama mbubb,"},{"style":"q1","verse":"","text":"ne ràtt sama baatu mbubb,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"xalab ma ci biir ban bi,"},{"style":"q1","verse":"","text":"may nirook pënd akub dóomu-taal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Woo naa la wall, wuyuwoo ma,"},{"style":"q1","verse":"","text":"ma taxaw, nga niir ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Yaa soppiku, néege ma,"},{"style":"q1","verse":"","text":"di ma soxoree sa diisaayu loxo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Yaa ma ne siféet, waral ma ngelaw,"},{"style":"q1","verse":"","text":"pamti-pamtee ma biir ngëlén."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Xam naa xéll ne ci dee nga ma jëme,"},{"style":"q1","verse":"","text":"te mooy kërug ndajem képp kuy dund."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku sànkule du tàllal loxoo?"},{"style":"q1","verse":"","text":"Ku musiba dab du woote wall?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Moona sama rongooñ a daan tuurul boroom tiis,"},{"style":"q1","verse":"","text":"sama xol di tiisoo aji ñàkk ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Lu baax laa yaakaar, lu bon ne jalañ,"},{"style":"q1","verse":"","text":"leer laa séentu, guddi ne jaas,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"sama xol lëmbaajoo, tëë dal,"},{"style":"q1","verse":"","text":"bési naqar dogale ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Maa ngi wéye ñuulaay bu ma tàngoor tegul,"},{"style":"q1","verse":"","text":"di taxawe biir ndaje, di woote wall."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Damaa mujj bokk ak till,"},{"style":"q1","verse":"","text":"neexook looy,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"ñuul, yaram di xub,"},{"style":"q1","verse":"","text":"tàng, di bax,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"sama xalam tiisal,"},{"style":"q1","verse":"","text":"sama toxoro jooy."}]