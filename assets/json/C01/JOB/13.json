[{"style":"s1","verse":"","text":"Ayóoba nee: Su layoo, yey"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Dama ne, lii lépp maa ko gis,"},{"style":"q1","verse":"","text":"dégg ko, xam ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Li ngeen xam, man it xam naa ko;"},{"style":"q1","verse":"","text":"duma seen cuune fenn."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Waaye man Aji Man ji laay waxal,"},{"style":"q1","verse":"","text":"layook Yàlla laa namm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yeen nag yeenay rafetalkati fen,"},{"style":"q1","verse":"","text":"yeen ñépp di fajkati caaxaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Céy su ngeen noon cell,"},{"style":"q1","verse":"","text":"muy seen rafet xel!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ngalla dégluleen sama tawat,"},{"style":"q1","verse":"","text":"teewluleen sama kàddug layoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ndax Yàlla ngeen di waxal waxi njubadi,"},{"style":"q1","verse":"","text":"di ko duural?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ndax Yàlla ngeen faral,"},{"style":"q1","verse":"","text":"walla Yàlla ngeen di layool?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ndax bu leen Yàlla luqatu woon dina baax?"},{"style":"q1","verse":"","text":"Am ni ñuy naxe nit ngeen koy naxe?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Su ngeen ko faraloon ci kumpa sax,"},{"style":"q1","verse":"","text":"du tee mu sikk leen moos."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Xanaa màggaayu Yàlla tiitalu leen?"},{"style":"q1","verse":"","text":"Am ragluwaayam ëlëmu leen?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Seeni tari, léebi neen,"},{"style":"q1","verse":"","text":"seeni tont, kiiraayu boob."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Xeremluleen, ba ma wax man"},{"style":"q1","verse":"","text":"ak lu ma mana dal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Amul dara lu mu ma jaralul,"},{"style":"q1","verse":"","text":"ba ci jaay sama bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Su ma Yàlla doon rey it, moom laay yaakaar,"},{"style":"q1","verse":"","text":"maay janook moom, layool sama bopp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Loolu it sama mucc a ngi ci,"},{"style":"q1","verse":"","text":"ndax ab yéefar du dikk di janook moom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Dégluleen a déglu sama baat,"},{"style":"q1","verse":"","text":"sama kàddu ci seeni nopp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Dégluleen maa, ma lañal ab layoo,"},{"style":"q1","verse":"","text":"bir na ma ne maay yey."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ana ku may yey cib layoo?"},{"style":"q1","verse":"","text":"Kon de ma wedam te dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Xanaa nga may ma ñaar rekk,"},{"style":"q1","verse":"","text":"ba du jar ma di la làqu:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"ngalla teggi ma sa loxo,"},{"style":"q1","verse":"","text":"te buma dabeeti tiitaange joo sabab."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Wooteel, ma wuyu,"},{"style":"q1","verse":"","text":"mbaa ma wax, nga àddu ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ana luy sama ñaawtéef aki moy?"},{"style":"q1","verse":"","text":"Xamal ma samag tooñ ak sama moy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ana loo may làqe sa leeru kanam?"},{"style":"q1","verse":"","text":"Danga maa jàppe sab noon?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Xob wuy wéy ngay xoqtal?"},{"style":"q1","verse":"","text":"Ana boob buy naaw, loo ciy topp?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Yaa ma bindal dogal yu wex,"},{"style":"q1","verse":"","text":"añale ma sama ñaawtéefi ndaw!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Yaa ma jéng,"},{"style":"q1","verse":"","text":"di sàmm fépp fu ma jaare,"},{"style":"q1","verse":"","text":"topp sama tànk!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Sama bakkan a ngi ruus ni bant bu fënëx,"},{"style":"q1","verse":"","text":"mbaa mbubb mu maxe."}]