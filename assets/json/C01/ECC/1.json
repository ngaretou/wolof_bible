[{"style":"mt1","text":"Kàdduy Waare"},{"style":"s1","verse":"","text":"Ubbite gi"},{"style":"p","verse":"","text":""},{"style":"v","verse":"1","text":"Lii kàdduy waarekat baa, di doomu Daawuda te di buurub Yerusalem."},{"style":"s1","verse":"","text":"Luy solos àddina?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Cóolóoloo cóolóol, waru waarekat ba,"},{"style":"q1","verse":"","text":"cóolóoloo cóolóol,"},{"style":"q1","verse":"","text":"léppi cóolóoli neen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ana sax luy njariñu doñ-doñoo doñ-doñ"},{"style":"q1","verse":"","text":"ju nit di doñ-doñi fi kaw dun bi?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ag maas wéy, ag maas teew;"},{"style":"q1","verse":"","text":"àddina sax ba fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Jant fenk, so,"},{"style":"q1","verse":"","text":"ne coww dellu fa muy fenke."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ngelaw jëm bëj-saalum,"},{"style":"q1","verse":"","text":"wiiri jëm bëj-gànnaar,"},{"style":"q1","verse":"","text":"di wiir aka wiiraat,"},{"style":"q1","verse":"","text":"di delluy wëndeelu, topp yoonam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Dexoo dex wal, wuti géej,"},{"style":"q1","verse":"","text":"te taxul géej fees."},{"style":"q1","verse":"","text":"Fa dex wal jëm,"},{"style":"q1","verse":"","text":"fa lay walati, jëm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Jëfoo jëf ca jëfit,"},{"style":"q1","verse":"","text":"làmmiñ tuddul lépp,"},{"style":"q1","verse":"","text":"bët du suur,"},{"style":"q1","verse":"","text":"nopp du buur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Lu ayoon mooy ayati,"},{"style":"q1","verse":"","text":"lu ñu jëfoon moom lañuy jëfati."},{"style":"q1","verse":"","text":"Dara yeesul fi kaw suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Lëf a ngii, nit naa:"},{"style":"q1","verse":"","text":"«Lii de lu bees la,»"},{"style":"q1","verse":"","text":"te mu ngi fi woon lu yàgg,"},{"style":"q1","verse":"","text":"lu jiitu sunu juddu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Kenn du fàttaliku ñi jiitu,"},{"style":"q1","verse":"","text":"ñiy ñëwit,"},{"style":"q1","verse":"","text":"ñi leen topp duñu leen fàttaliku."},{"style":"ms","verse":"","text":"Ma seet lu ma doy"},{"style":"mr","verse":"","text":"(Saar 1.12---2.23)"},{"style":"s1","verse":"","text":"Rafet xel doyul"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Man waarekat bi,"},{"style":"q1","verse":"","text":"buurub Israayil laa woon ci Yerusalem."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Damaa dogu ne damay gëstu,"},{"style":"q1","verse":"","text":"teg ko ci xel mu rafet, di settantal"},{"style":"q1","verse":"","text":"mboolem lu nit def fu jant bi tiim."},{"style":"q1","verse":"","text":"Ndaw sas wu tiis wu Yàlla sase,"},{"style":"q1","verse":"","text":"ñu war koo sasoo!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Gis naa jépp jëf ju jëfe ci kaw suuf,"},{"style":"q1","verse":"","text":"ndeke lépp cóolóoli neen la ak nappum ngelaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Lu wañaaru, maneesu koo jubbanti;"},{"style":"q1","verse":"","text":"lu teewul, maneesu koo lim."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Damaa wax ci saam xel naa:"},{"style":"q1","verse":"","text":"«Man de, maa ngii di gëna xelu,"},{"style":"q1","verse":"","text":"ba sut ku ma jiitu ci jalub Yerusalem."},{"style":"q1","verse":"","text":"Damaa xuus ci xel mu rafet ak xam-xam.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Noonu dogu naa xam luy xel mu rafet,"},{"style":"q1","verse":"","text":"xam luy nitoodi akug ndof."},{"style":"q1","verse":"","text":"Fekk loolu it nappum ngelaw la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ku géejal xel mu rafet, géejal naqar;"},{"style":"q1","verse":"","text":"yokku xam-xam, yokku tiis."}]