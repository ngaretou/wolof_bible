[{"style":"q1","text":""},{"style":"v","text":"Bul rattax làmmiñ,"},{"style":"q1","text":"bul gaawa dikke Yàlla mbir."},{"style":"q1","text":"Yàllaa ngi kaw asamaan,"},{"style":"q1","text":"yaw yaa ngi fi suuf."},{"style":"q1","text":"Kon na say kàddu néew."},{"style":"q1","text":""},{"style":"v","text":"Bu xalaat baree, indi gént;"},{"style":"q1","text":"bu wax baree, waxi dof jib."},{"style":"q1","text":""},{"style":"v","text":"Boo digook Yàlla,"},{"style":"q1","text":"bu ko yeexe."},{"style":"q1","text":"Daray foontukat neexul,"},{"style":"q1","text":"defal li nga dige."},{"style":"q1","text":""},{"style":"v","text":"Baña digee gën dige loo deful."},{"style":"q1","text":""},{"style":"v","text":"Bu la sa làmmiñ yóbbe bàkkaar,"},{"style":"q1","text":"ba ngay layal ndawal Yàlla la, naan lii njuumte la woon."},{"style":"q1","text":"Ana lu jar Yàlla di la meree sa kàddu,"},{"style":"q1","text":"bay neenal saw ñaq?"},{"style":"q1","text":""},{"style":"v","text":"Wax ju bare, caaxaani neen;"},{"style":"q1","text":"mook gént yu baree yem."},{"style":"q1","text":"Kon, Yàlla, ragal ko."},{"style":"s1","text":"Nguur bare na àq"},{"style":"q1","text":""},{"style":"v","text":"Boo gisee ñuy noot néew-ji-doole,"},{"style":"q1","text":"di jalgati dëgg ak yoon cib gox,"},{"style":"q1","text":"bumu la jaaxal;"},{"style":"q1","text":"kilifa gi, kilifaam a ko yiir;"},{"style":"q1","text":"ñoom it seen kilifaa leen yiir."},{"style":"q1","text":""},{"style":"v","text":"Xanaa ndax buur suqal mbey mi,"},{"style":"q1","text":"ba ñépp jariñoo suuf si."},{"style":"s1","text":"Alal coono la"},{"style":"q1","text":""},{"style":"v","text":"Bëggkatu xaalis du doyal xaalis,"},{"style":"q1","text":"bëggkatu alal du doyal tono,"},{"style":"q1","text":"te loolu it, cóolóoli neen."},{"style":"q1","text":""},{"style":"v","text":"Bu koom yokkoo,"},{"style":"q1","text":"ña cay lekk yokku."},{"style":"q1","text":"Ana lu muy jariñ boroom koom ga,"},{"style":"q1","text":"xanaa di ko duufale bëtam?"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Surga, lekkam néew, bare,"},{"style":"q1","text":"mu nelaw, ba fàttey boram;"},{"style":"q1","text":"boroom alal regg,"},{"style":"q1","text":"du noppalu, du nelaw."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Naqar wu réy laa gis fi kaw suuf:"},{"style":"q1","text":"alal ju boroom dajale ba loru,"},{"style":"q1","text":""},{"style":"v","text":"mbaa pexeem moy, mu sànkule;"},{"style":"q1","text":"ba bu amee doom it, du ko wacce dara."},{"style":"q1","text":""},{"style":"v","text":"Nga juddoo ci sa ndey loxoy neen,"},{"style":"q1","text":"walbatiku dellu na nga dikke woon."},{"style":"q1","text":"Du dara loo ñaqoon ba yóbbaale ko."},{"style":"q1","text":""},{"style":"v","text":"Dikke ni, delloo ni,"},{"style":"q1","text":"loolu it tiis wu réy la."},{"style":"q1","text":"Doñ-doñi, ngelawal neen,"},{"style":"q1","text":"ana jariñ la?"},{"style":"q1","text":""},{"style":"v","text":"Ngay dunde tiis bésoo bés,"},{"style":"q1","text":"ak naqar ak wopp ak xol bu tàng!"},{"style":"s1","text":"Bànneexu bakkan, Yàlla"},{"style":"q1","text":""},{"style":"v","text":"Lii daal laa ci gis, man."},{"style":"q1","text":"Li gën te jaadu ci nit: na lekk, naan"},{"style":"q1","text":"te bànneexoo ñaqam wu mu ñaqe kaw suuf"},{"style":"q1","text":"giiru dund gi ko Yàlla may."},{"style":"q1","text":"Loolu mooy céram."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Alal ak koom, ku ko Yàlla may,"},{"style":"q1","text":"may ko mu tal koo xéewloo,"},{"style":"q1","text":"mu nangu céram, bànneexoo ñaqam,"},{"style":"q1","text":"loolu mayu Yàllaa."},{"style":"q1","text":""},{"style":"v","text":"Yàllaa koy bànneexal,"},{"style":"q1","text":"ba du tiisoo ay fanam."}]