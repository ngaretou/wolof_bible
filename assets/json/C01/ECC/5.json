[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Bul rattax làmmiñ,"},{"style":"q1","verse":"","text":"bul gaawa dikke Yàlla mbir."},{"style":"q1","verse":"","text":"Yàllaa ngi kaw asamaan,"},{"style":"q1","verse":"","text":"yaw yaa ngi fi suuf."},{"style":"q1","verse":"","text":"Kon na say kàddu néew."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Bu xalaat baree, indi gént;"},{"style":"q1","verse":"","text":"bu wax baree, waxi dof jib."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Boo digook Yàlla,"},{"style":"q1","verse":"","text":"bu ko yeexe."},{"style":"q1","verse":"","text":"Daray foontukat neexul,"},{"style":"q1","verse":"","text":"defal li nga dige."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Baña digee gën dige loo deful."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Bu la sa làmmiñ yóbbe bàkkaar,"},{"style":"q1","verse":"","text":"ba ngay layal ndawal Yàlla la, naan lii njuumte la woon."},{"style":"q1","verse":"","text":"Ana lu jar Yàlla di la meree sa kàddu,"},{"style":"q1","verse":"","text":"bay neenal saw ñaq?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Wax ju bare, caaxaani neen;"},{"style":"q1","verse":"","text":"mook gént yu baree yem."},{"style":"q1","verse":"","text":"Kon, Yàlla, ragal ko."},{"style":"s1","verse":"","text":"Nguur bare na àq"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Boo gisee ñuy noot néew-ji-doole,"},{"style":"q1","verse":"","text":"di jalgati dëgg ak yoon cib gox,"},{"style":"q1","verse":"","text":"bumu la jaaxal;"},{"style":"q1","verse":"","text":"kilifa gi, kilifaam a ko yiir;"},{"style":"q1","verse":"","text":"ñoom it seen kilifaa leen yiir."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Xanaa ndax buur suqal mbey mi,"},{"style":"q1","verse":"","text":"ba ñépp jariñoo suuf si."},{"style":"s1","verse":"","text":"Alal coono la"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Bëggkatu xaalis du doyal xaalis,"},{"style":"q1","verse":"","text":"bëggkatu alal du doyal tono,"},{"style":"q1","verse":"","text":"te loolu it, cóolóoli neen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Bu koom yokkoo,"},{"style":"q1","verse":"","text":"ña cay lekk yokku."},{"style":"q1","verse":"","text":"Ana lu muy jariñ boroom koom ga,"},{"style":"q1","verse":"","text":"xanaa di ko duufale bëtam?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Surga, lekkam néew, bare,"},{"style":"q1","verse":"","text":"mu nelaw, ba fàttey boram;"},{"style":"q1","verse":"","text":"boroom alal regg,"},{"style":"q1","verse":"","text":"du noppalu, du nelaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Naqar wu réy laa gis fi kaw suuf:"},{"style":"q1","verse":"","text":"alal ju boroom dajale ba loru,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"mbaa pexeem moy, mu sànkule;"},{"style":"q1","verse":"","text":"ba bu amee doom it, du ko wacce dara."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Nga juddoo ci sa ndey loxoy neen,"},{"style":"q1","verse":"","text":"walbatiku dellu na nga dikke woon."},{"style":"q1","verse":"","text":"Du dara loo ñaqoon ba yóbbaale ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Dikke ni, delloo ni,"},{"style":"q1","verse":"","text":"loolu it tiis wu réy la."},{"style":"q1","verse":"","text":"Doñ-doñi, ngelawal neen,"},{"style":"q1","verse":"","text":"ana jariñ la?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ngay dunde tiis bésoo bés,"},{"style":"q1","verse":"","text":"ak naqar ak wopp ak xol bu tàng!"},{"style":"s1","verse":"","text":"Bànneexu bakkan, Yàlla"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Lii daal laa ci gis, man."},{"style":"q1","verse":"","text":"Li gën te jaadu ci nit: na lekk, naan"},{"style":"q1","verse":"","text":"te bànneexoo ñaqam wu mu ñaqe kaw suuf"},{"style":"q1","verse":"","text":"giiru dund gi ko Yàlla may."},{"style":"q1","verse":"","text":"Loolu mooy céram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Alal ak koom, ku ko Yàlla may,"},{"style":"q1","verse":"","text":"may ko mu tal koo xéewloo,"},{"style":"q1","verse":"","text":"mu nangu céram, bànneexoo ñaqam,"},{"style":"q1","verse":"","text":"loolu mayu Yàllaa."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yàllaa koy bànneexal,"},{"style":"q1","verse":"","text":"ba du tiisoo ay fanam."}]