[{"style":"s1","verse":"","text":"Kenn xamul, waaye beyal sa tool"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Dugalal sa alal, dawloo ko;"},{"style":"q1","verse":"","text":"yàgg-yàgg nga jotaat ci."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Séddal ab cér juróom ñaari nit ba juróom ñett;"},{"style":"q1","verse":"","text":"xamoo safaan buy dikkal àddina."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Xàmbaar bu duyee, ba fees,"},{"style":"q1","verse":"","text":"fi kaw suuf lay soob."},{"style":"q1","verse":"","text":"Garab, bu daanoo ndijoor mbaa càmmoñ,"},{"style":"q1","verse":"","text":"fa mu daanu lay des."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Kuy déglu ngelaw, doo ji,"},{"style":"q1","verse":"","text":"kuy seetluy niir, doo góob."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Xamoo yoonu ngelaw,"},{"style":"q1","verse":"","text":"xamoo nu doom di sosoo ci ndeyam;"},{"style":"q1","verse":"","text":"kon manoo xam lu Yàllay def,"},{"style":"q1","verse":"","text":"moom miy def lépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Xëyal, ji sa jiwu,"},{"style":"q1","verse":"","text":"gont, baña tàyyi;"},{"style":"q1","verse":"","text":"muy lii di lee, xamoo lu ciy nangu,"},{"style":"q1","verse":"","text":"am ñaar ñépp ay bokk baax."},{"style":"s1","verse":"","text":"Bégal te xam ne man saxul"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Leer neex na,"},{"style":"q1","verse":"","text":"gis jant neex na."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ak lu say at baree bare,"},{"style":"q1","verse":"","text":"bégeel at mu ci nekk,"},{"style":"q1","verse":"","text":"waaye bàyyil xel lëndëmu bàmmeel,"},{"style":"q1","verse":"","text":"lu yàgg lay doon,"},{"style":"q1","verse":"","text":"te liy ñëw lépp, cóolóoli neen."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Xalelu góor bi, bànneexul bi ngay gone,"},{"style":"q1","verse":"","text":"bégalal sa xol bi ngay ndaw."},{"style":"q1","verse":"","text":"Toppal loo namm ak loo xemmem,"},{"style":"q1","verse":"","text":"waaye xamal ne loolu lépp,"},{"style":"q1","verse":"","text":"Yàlla da la koy laaj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Félal naqaru xol,"},{"style":"q1","verse":"","text":"dàq woppi yaram;"},{"style":"q1","verse":"","text":"waaye ndaw ak kawar gu ñuul, cóolóoli neen."}]