[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"«Su boobaa déy,»"},{"style":"q2","verse":"","text":"kàddug Aji Sax jee,"},{"style":"q1","verse":"","text":"«noon yeey sulli yaxi buuri Yuda"},{"style":"q1","verse":"","text":"ak yaxi kàngam yi"},{"style":"q1","verse":"","text":"ak yaxi \\w sarxalkat|sarxalkat b-:\\w* yi"},{"style":"q1","verse":"","text":"ak yaxi yonent yi"},{"style":"q1","verse":"","text":"ak yaxi waa Yerusalem,"},{"style":"q1","verse":"","text":"fa seeni bàmmeel,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"weer leen ci naaj week weer wi"},{"style":"q1","verse":"","text":"ak mboolem biddiiw yi ñu soppoon,"},{"style":"q1","verse":"","text":"di leen jaamu, di leen wut aka topp,"},{"style":"q1","verse":"","text":"di leen sukkal."},{"style":"q1","verse":"","text":"Deesu ko dajale, deesu ko suul;"},{"style":"q1","verse":"","text":"day mel niy jonkan fi kaw suuf si."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Dee mooy gënal dund"},{"style":"q1","verse":"","text":"mboolem ku des ci njaboot gu bon gii,"},{"style":"q1","verse":"","text":"ak mboolem fu ñu dese fa ma leen tasaare woon.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee, Boroom gàngoor yi."},{"style":"s1","verse":"","text":"Askan wi të na ticc"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"«Nanga leen wax ne leen:"},{"style":"q1","verse":"","text":"“Aji Sax ji dafa wax ne:"},{"style":"q1","verse":"","text":"Xanaa ku daanu, da dul jógaat?"},{"style":"q1","verse":"","text":"Am ku lajj, du délsi?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Lu tax xeet wii dëddu?"},{"style":"q1","verse":"","text":"Lu tax Yerusalem gii lajj, baña délsi?"},{"style":"q1","verse":"","text":"Ñu ngi langaamoo naxi neen,"},{"style":"q1","verse":"","text":"baña délsi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Teewlu naa, déglu naa;"},{"style":"q1","verse":"","text":"du lu jaadu lañu wax."},{"style":"q1","verse":"","text":"Kenn tuubu ci mbonam,"},{"style":"q1","verse":"","text":"ba ne: ‘Lu ma def nii?’"},{"style":"q1","verse":"","text":"Ku nekk a topp yoonam,"},{"style":"q1","verse":"","text":"ni fas wu màmm cib xare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ag xodd sax xam na bésub demam,"},{"style":"q1","verse":"","text":"pitax, wetwet ak mbelaar it dem,"},{"style":"q1","verse":"","text":"tey sàmm seen bésub délsi."},{"style":"q1","verse":"","text":"Waaye sama ñoñ faalewuñu àtteb Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Nu ngeen mane ne: ‘Am nan xel,"},{"style":"q1","verse":"","text":"nu ngeek yoonu Aji Sax ji,’"},{"style":"q1","verse":"","text":"te xalimay yoonalkat soppi ko fen?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Dees na gàcceel boroom xel yi,"},{"style":"q1","verse":"","text":"ñu jàq, jataŋe."},{"style":"q1","verse":"","text":"Kàddug Aji Sax jaa ngii ñu dëddu ko,"},{"style":"q1","verse":"","text":"ana seenum xel?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"«“Kon nag maay jël seeni jabar, jox ñeneen,"},{"style":"q1","verse":"","text":"jox seeni tool ay foqatikat,"},{"style":"q1","verse":"","text":"ndax ki gëna tuut ba ci ki gëna mag,"},{"style":"q1","verse":"","text":"ñépp a ngi lewal lu lewul."},{"style":"q1","verse":"","text":"Ab yonent akub \\w sarxalkat|sarxalkat b-:\\w*, ñépp a ngi wor!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Sofental nañu sama pajum ñoñ ñi dammatoo,"},{"style":"q1","verse":"","text":"naan: ‘Jàmm, lépp jàmm!’"},{"style":"q1","verse":"","text":"te jàmm amul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ñu wara am gàcce ci seen jëf ju siblu,"},{"style":"q1","verse":"","text":"te gàcce gàcceelu leen."},{"style":"q1","verse":"","text":"Xamuñu sax luy kersa!"},{"style":"q1","verse":"","text":"Moo tax ñuy fëlëñoondook ñiy fëlëñu."},{"style":"q1","verse":"","text":"Bu ma leen dikkalee, dinañu tërëf.”»"},{"style":"q1","verse":"","text":"Aji Sax jee ko wax."},{"style":"s1","verse":"","text":"Yaakaar tas na"},{"style":"sp","verse":"","text":"Yàllaay wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"«Maa leen di witt a witt.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":"«Garabu reseñ du dese doom,"},{"style":"q1","verse":"","text":"figg du dese doom,»"},{"style":"q2","verse":"","text":"kàddug Aji Sax jee,"},{"style":"q1","verse":"","text":"«xob ya sax day wow."},{"style":"q1","verse":"","text":"Lu ma leen joxoon, rëcc na leen.»"},{"style":"sp","verse":"","text":"Askan waay wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ana lu nuy toog?"},{"style":"q1","verse":"","text":"Fabuleen, nu dem ca dëkk ya tata wër,"},{"style":"q1","verse":"","text":"deewi fa."},{"style":"q1","verse":"","text":"Sunu Yàlla Aji Sax ji nar na sunu dee;"},{"style":"q1","verse":"","text":"moo nu nàndal tookey mbugal,"},{"style":"q1","verse":"","text":"nde noo tooñ Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Nuy yaakaar jàmm,"},{"style":"q1","verse":"","text":"dara baaxul;"},{"style":"q1","verse":"","text":"nuy séentum paj,"},{"style":"q1","verse":"","text":"tiitaange ne jaas!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Seen noowi fas yaa dégtoo bëj-gànnaar fa Dan,"},{"style":"q1","verse":"","text":"seen fasi naaru-góor di ŋexal, suuf sépp yëngu."},{"style":"q1","verse":"","text":"Ñu dikk, tas réew mi tasar,"},{"style":"q1","verse":"","text":"booleek li ci biiram,"},{"style":"q1","verse":"","text":"ak dëkk beek ñi ci biiram."},{"style":"sp","verse":"","text":"Yàllaay wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"«Maa ngii déy di leen yónnee"},{"style":"q1","verse":"","text":"jaani ñàngóor yu déggul jat,"},{"style":"q1","verse":"","text":"ñu di leen màtt.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"s1","verse":"","text":"Yeremi am na naqar"},{"style":"sp","verse":"","text":"Yeremeey wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ndaw naqar wu tiis,"},{"style":"q1","verse":"","text":"ndaw xol bu diis!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Sama bokk yaa ngooguy yuuxu"},{"style":"q1","verse":"","text":"yuux yu àkki fu sore,"},{"style":"q1","verse":"","text":"naan: «Moo xanaa Aji Sax ji daa newul \\w Siyoŋ|Siyoŋ:\\w*?"},{"style":"q1","verse":"","text":"Xanaa Buurub Siyoŋ da fee nekkatul?»"},{"style":"sp","verse":"","text":"Yàllaay wax"},{"style":"q1","verse":"","text":"«Lu tax ñuy yékkati sama xol"},{"style":"q1","verse":"","text":"ak seen jëmmi tuur yii,"},{"style":"q1","verse":"","text":"seen yàllay doxandéem yi tekkiwul dara?»"},{"style":"sp","verse":"","text":"Askan waay wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ngóob wéy na, jamonoom wees,"},{"style":"q1","verse":"","text":"te nun de muccagunu."},{"style":"sp","verse":"","text":"Yeremeey wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Sama bokk yi seen xol jeex"},{"style":"q1","verse":"","text":"tax na sama xol jeex,"},{"style":"q1","verse":"","text":"may tiislu, tancoo njàqare!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Moo ndax diwu pajtal amul Galàdd\\f + \\fr 8.22 \\fk Galàdd, \\ft ab gox la bu féete dexu Yurdan penku te nekkoon fu bare gàncaxi pajtal.\\f*?"},{"style":"q1","verse":"","text":"Am fajkat a fi amul?"},{"style":"q1","verse":"","text":"Ana lu tax ba bokk yi taneeguñu?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Aka neexoon sama bopp soppaliku ndox,"},{"style":"q1","verse":"","text":"saay gët di seyaan buy xelliy rongooñ"},{"style":"q1","verse":"","text":"yu ma jooye guddeek bëccëg"},{"style":"q1","verse":"","text":"bokk yi ñu bóom!"}]