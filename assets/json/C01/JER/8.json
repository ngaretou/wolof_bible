[{"style":"q1","text":""},{"style":"v","text":"«Su boobaa déy,»"},{"style":"q2","text":"kàddug Aji Sax jee,"},{"style":"q1","text":"«noon yeey sulli yaxi buuri Yuda"},{"style":"q1","text":"ak yaxi kàngam yi"},{"style":"q1","text":"ak yaxi \\w sarxalkat|sarxalkat b-:\\w* yi"},{"style":"q1","text":"ak yaxi yonent yi"},{"style":"q1","text":"ak yaxi waa Yerusalem,"},{"style":"q1","text":"fa seeni bàmmeel,"},{"style":"q1","text":""},{"style":"v","text":"weer leen ci naaj week weer wi"},{"style":"q1","text":"ak mboolem biddiiw yi ñu soppoon,"},{"style":"q1","text":"di leen jaamu, di leen wut aka topp,"},{"style":"q1","text":"di leen sukkal."},{"style":"q1","text":"Deesu ko dajale, deesu ko suul;"},{"style":"q1","text":"day mel niy jonkan fi kaw suuf si."},{"style":"q1","text":""},{"style":"v","text":"Dee mooy gënal dund"},{"style":"q1","text":"mboolem ku des ci njaboot gu bon gii,"},{"style":"q1","text":"ak mboolem fu ñu dese fa ma leen tasaare woon.»"},{"style":"q2","text":"Kàddug Aji Sax jee, Boroom gàngoor yi."},{"style":"s1","text":"Askan wi të na ticc"},{"style":"q1","text":""},{"style":"v","text":"«Nanga leen wax ne leen:"},{"style":"q1","text":"“Aji Sax ji dafa wax ne:"},{"style":"q1","text":"Xanaa ku daanu, da dul jógaat?"},{"style":"q1","text":"Am ku lajj, du délsi?"},{"style":"q1","text":""},{"style":"v","text":"Lu tax xeet wii dëddu?"},{"style":"q1","text":"Lu tax Yerusalem gii lajj, baña délsi?"},{"style":"q1","text":"Ñu ngi langaamoo naxi neen,"},{"style":"q1","text":"baña délsi."},{"style":"q1","text":""},{"style":"v","text":"Teewlu naa, déglu naa;"},{"style":"q1","text":"du lu jaadu lañu wax."},{"style":"q1","text":"Kenn tuubu ci mbonam,"},{"style":"q1","text":"ba ne: ‘Lu ma def nii?’"},{"style":"q1","text":"Ku nekk a topp yoonam,"},{"style":"q1","text":"ni fas wu màmm cib xare."},{"style":"q1","text":""},{"style":"v","text":"Ag xodd sax xam na bésub demam,"},{"style":"q1","text":"pitax, wetwet ak mbelaar it dem,"},{"style":"q1","text":"tey sàmm seen bésub délsi."},{"style":"q1","text":"Waaye sama ñoñ faalewuñu àtteb Aji Sax ji."},{"style":"q1","text":""},{"style":"v","text":"Nu ngeen mane ne: ‘Am nan xel,"},{"style":"q1","text":"nu ngeek yoonu Aji Sax ji,’"},{"style":"q1","text":"te xalimay yoonalkat soppi ko fen?"},{"style":"q1","text":""},{"style":"v","text":"Dees na gàcceel boroom xel yi,"},{"style":"q1","text":"ñu jàq, jataŋe."},{"style":"q1","text":"Kàddug Aji Sax jaa ngii ñu dëddu ko,"},{"style":"q1","text":"ana seenum xel?"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"«“Kon nag maay jël seeni jabar, jox ñeneen,"},{"style":"q1","text":"jox seeni tool ay foqatikat,"},{"style":"q1","text":"ndax ki gëna tuut ba ci ki gëna mag,"},{"style":"q1","text":"ñépp a ngi lewal lu lewul."},{"style":"q1","text":"Ab yonent akub \\w sarxalkat|sarxalkat b-:\\w*, ñépp a ngi wor!"},{"style":"q1","text":""},{"style":"v","text":"Sofental nañu sama pajum ñoñ ñi dammatoo,"},{"style":"q1","text":"naan: ‘Jàmm, lépp jàmm!’"},{"style":"q1","text":"te jàmm amul."},{"style":"q1","text":""},{"style":"v","text":"Ñu wara am gàcce ci seen jëf ju siblu,"},{"style":"q1","text":"te gàcce gàcceelu leen."},{"style":"q1","text":"Xamuñu sax luy kersa!"},{"style":"q1","text":"Moo tax ñuy fëlëñoondook ñiy fëlëñu."},{"style":"q1","text":"Bu ma leen dikkalee, dinañu tërëf.”»"},{"style":"q1","text":"Aji Sax jee ko wax."},{"style":"s1","text":"Yaakaar tas na"},{"style":"sp","text":"Yàllaay wax"},{"style":"q1","text":""},{"style":"v","text":"«Maa leen di witt a witt.»"},{"style":"q2","text":"Kàddug Aji Sax jee."},{"style":"q1","text":"«Garabu reseñ du dese doom,"},{"style":"q1","text":"figg du dese doom,»"},{"style":"q2","text":"kàddug Aji Sax jee,"},{"style":"q1","text":"«xob ya sax day wow."},{"style":"q1","text":"Lu ma leen joxoon, rëcc na leen.»"},{"style":"sp","text":"Askan waay wax"},{"style":"q1","text":""},{"style":"v","text":"Ana lu nuy toog?"},{"style":"q1","text":"Fabuleen, nu dem ca dëkk ya tata wër,"},{"style":"q1","text":"deewi fa."},{"style":"q1","text":"Sunu Yàlla Aji Sax ji nar na sunu dee;"},{"style":"q1","text":"moo nu nàndal tookey mbugal,"},{"style":"q1","text":"nde noo tooñ Aji Sax ji."},{"style":"q1","text":""},{"style":"v","text":"Nuy yaakaar jàmm,"},{"style":"q1","text":"dara baaxul;"},{"style":"q1","text":"nuy séentum paj,"},{"style":"q1","text":"tiitaange ne jaas!"},{"style":"q1","text":""},{"style":"v","text":"Seen noowi fas yaa dégtoo bëj-gànnaar fa Dan,"},{"style":"q1","text":"seen fasi naaru-góor di ŋexal, suuf sépp yëngu."},{"style":"q1","text":"Ñu dikk, tas réew mi tasar,"},{"style":"q1","text":"booleek li ci biiram,"},{"style":"q1","text":"ak dëkk beek ñi ci biiram."},{"style":"sp","text":"Yàllaay wax"},{"style":"q1","text":""},{"style":"v","text":"«Maa ngii déy di leen yónnee"},{"style":"q1","text":"jaani ñàngóor yu déggul jat,"},{"style":"q1","text":"ñu di leen màtt.»"},{"style":"q2","text":"Kàddug Aji Sax jee."},{"style":"s1","text":"Yeremi am na naqar"},{"style":"sp","text":"Yeremeey wax"},{"style":"q1","text":""},{"style":"v","text":"Ndaw naqar wu tiis,"},{"style":"q1","text":"ndaw xol bu diis!"},{"style":"q1","text":""},{"style":"v","text":"Sama bokk yaa ngooguy yuuxu"},{"style":"q1","text":"yuux yu àkki fu sore,"},{"style":"q1","text":"naan: «Moo xanaa Aji Sax ji daa newul \\w Siyoŋ|Siyoŋ:\\w*?"},{"style":"q1","text":"Xanaa Buurub Siyoŋ da fee nekkatul?»"},{"style":"sp","text":"Yàllaay wax"},{"style":"q1","text":"«Lu tax ñuy yékkati sama xol"},{"style":"q1","text":"ak seen jëmmi tuur yii,"},{"style":"q1","text":"seen yàllay doxandéem yi tekkiwul dara?»"},{"style":"sp","text":"Askan waay wax"},{"style":"q1","text":""},{"style":"v","text":"Ngóob wéy na, jamonoom wees,"},{"style":"q1","text":"te nun de muccagunu."},{"style":"sp","text":"Yeremeey wax"},{"style":"q1","text":""},{"style":"v","text":"Sama bokk yi seen xol jeex"},{"style":"q1","text":"tax na sama xol jeex,"},{"style":"q1","text":"may tiislu, tancoo njàqare!"},{"style":"q1","text":""},{"style":"v","text":"Moo ndax diwu pajtal amul Galàdd\\f + \\fr 8.22 \\fk Galàdd, \\ft ab gox la bu féete dexu Yurdan penku te nekkoon fu bare gàncaxi pajtal.\\f*?"},{"style":"q1","text":"Am fajkat a fi amul?"},{"style":"q1","text":"Ana lu tax ba bokk yi taneeguñu?"},{"style":"q1","text":""},{"style":"v","text":"Aka neexoon sama bopp soppaliku ndox,"},{"style":"q1","text":"saay gët di seyaan buy xelliy rongooñ"},{"style":"q1","text":"yu ma jooye guddeek bëccëg"},{"style":"q1","text":"bokk yi ñu bóom!"}]