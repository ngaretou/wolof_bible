[{"style":"s1","verse":"","text":"Aji Sax ji du moroomu xërëm"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Yeen \\w bànni Israayil|bànni Israayil:\\w*, dégluleen kàddu"},{"style":"q1","verse":"","text":"gi leen Aji Sax ji dégtal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Aji Sax ji dafa wax ne:"},{"style":"q1","verse":"","text":"«Jikkoy xeeti yéefar yi, buleen ko roy;"},{"style":"q1","verse":"","text":"màndargam asamaan, buleen ko tiit;"},{"style":"q1","verse":"","text":"na ko yéefar yi tiit."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aaday xeet yi kat neen la."},{"style":"q1","verse":"","text":"Garab lañuy gore ca gott ba,"},{"style":"q1","verse":"","text":"ab yattkat yatte ko sémmiñ,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"rafetale xaalis ak wurus,"},{"style":"q1","verse":"","text":"daaj koy pont, dëgëral,"},{"style":"q1","verse":"","text":"ba du yëngu;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"muy saf kuñeental ci biir tóokërub xaal."},{"style":"q1","verse":"","text":"Du wax, du séqi jéego,"},{"style":"q1","verse":"","text":"ñu di ko jaŋat-jaŋatee."},{"style":"q1","verse":"","text":"Buleen ko ragal, du lore, du jariñe.»"},{"style":"sp","verse":"","text":"Yeremeey wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax ji, kenn melul ni yaw,"},{"style":"q1","verse":"","text":"yaaka màgg, jàmbaare, màggu tur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yaw Buuru xeet yi, ana ku la dul ragal?"},{"style":"q1","verse":"","text":"Yaw déy yaa yelloo ragal."},{"style":"q1","verse":"","text":"Mboolem ñi xelu ci xeet yi"},{"style":"q1","verse":"","text":"ak mboolem seeni buur,"},{"style":"q1","verse":"","text":"kenn melul ni yaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ñoom ñépp a bokk dofe, ñàkk bopp:"},{"style":"q1","verse":"","text":"xam-xamu kasara, bant kese!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Xaalis lañuy jële Tarsis, xoobe ko bant,"},{"style":"q1","verse":"","text":"wurus wu jóge Ufas, ñu boole ca,"},{"style":"q1","verse":"","text":"ndaanaan liggéey ko,"},{"style":"q1","verse":"","text":"tëggu gànjar yatt ko,"},{"style":"q1","verse":"","text":"ñu solal ko ndimo lu yolet"},{"style":"q1","verse":"","text":"ak lu xonq curr,"},{"style":"q1","verse":"","text":"lépp di liggéeyu ràbb yu xareñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Waaye Aji Sax jeey Yàlla dëgg."},{"style":"q1","verse":"","text":"Mooy Yàlla jiy dund,"},{"style":"q1","verse":"","text":"di buur ba fàww,"},{"style":"q1","verse":"","text":"xadaram gësëm àddina,"},{"style":"q1","verse":"","text":"meram man xeet yi."},{"style":"sp","verse":"","text":"Yàllay wax Yeremi"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"«Lii nag, nangeen ko siiwal:"},{"style":"q1","verse":"","text":"“Seen yàlla yi sàkkul asamaan, sàkkuñu suuf,"},{"style":"q1","verse":"","text":"ñooy sànku, jóge fi suuf ak fi asamaan tiim.”»"},{"style":"sp","verse":"","text":"Yeremeey wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Suuf, Aji Sax jee ko sàkke kàttanam;"},{"style":"q1","verse":"","text":"dun bi, mu sampem xelam;"},{"style":"q1","verse":"","text":"asamaan, mu lale xam-xamam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Bu àddoo, ndoxum asamaan riir;"},{"style":"q1","verse":"","text":"mu yéegey niir fa cati àddina,"},{"style":"q1","verse":"","text":"riddee ngelaw fa dencam ya,"},{"style":"q1","verse":"","text":"daldi melax, taw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Nitoo nit dim sanax, sore xam-xam,"},{"style":"q1","verse":"","text":"te képp kuy tëgg sa jëmmu tuur, yaa añe gàcce."},{"style":"q1","verse":"","text":"Sa jëmmi weñ ji nga xelli, di neenug neen,"},{"style":"q1","verse":"","text":"noowug bakkan, tus!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Cóolóolu neen, liggéeyu caaxaan;"},{"style":"q1","verse":"","text":"bu bésam taxawee, mu sànku."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ki séddoo \\w Yanqóoba|Yanqóoba:\\w* déy wuute naak yii caaxaan."},{"style":"q1","verse":"","text":"Kee bind lépp,"},{"style":"q1","verse":"","text":"te Israayil di giir gi mu séddoo."},{"style":"q1","verse":"","text":"Kookooy Aji Sax ji Boroom gàngoori xare yi!"},{"style":"s1","verse":"","text":"Mbugal dikk na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Yerusalemee, yeen ñi ñu gaw,"},{"style":"q1","verse":"","text":"forleen seen ëmb."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Aji Sax ji déy dafa wax ne:"},{"style":"q1","verse":"","text":"«Maa ngii naa xërr waa réew mi bii yoon,"},{"style":"q1","verse":"","text":"teg leen njàqare, ba ñu yég ko.»"},{"style":"sp","verse":"","text":"Yerusalem ay wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Wóoy man, yàqu naa!"},{"style":"q1","verse":"","text":"Ndaw góom bu metti!"},{"style":"q1","verse":"","text":"Dama noon lii jàngoro doŋŋ a,"},{"style":"q1","verse":"","text":"naa muñ!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Xayma xotatoo,"},{"style":"q1","verse":"","text":"buumi xayma xëttandoo,"},{"style":"q1","verse":"","text":"samay doom wacc ma, ne mes;"},{"style":"q1","verse":"","text":"kenn sampatul sama xayma, firi ko."},{"style":"sp","verse":"","text":"Yeremeey wax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Sàmmi xeet weeka dofe:"},{"style":"q1","verse":"","text":"Aji Sax ji, diisoowuñook moom."},{"style":"q1","verse":"","text":"Moo leen taxa bonle,"},{"style":"q1","verse":"","text":"seen jur gépp tasaaroo\\f + \\fr 10.21 \\ft Seetal ci saar 23.\\f*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Xibaar a ngoog, di ñëw,"},{"style":"q1","verse":"","text":"riir mu réy a yëngoo réewum bëj-gànnaar,"},{"style":"q1","verse":"","text":"nara gental dëkki Yuda yu mag yi,"},{"style":"q1","verse":"","text":"till yi dëkke."},{"style":"s1","verse":"","text":"Yeremi tinu na Aji Sax ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Aji Sax ji, maa xam ne"},{"style":"q1","verse":"","text":"nit moomul yoonam,"},{"style":"q1","verse":"","text":"kuy dox moomuli jéegoom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Aji Sax ji, yar ma lu yem,"},{"style":"q1","verse":"","text":"te bañ maa yare mer,"},{"style":"q1","verse":"","text":"di ma semmal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Xëppal sa xadar yéefar yi la yégul,"},{"style":"q1","verse":"","text":"ak làng yi la tuddul."},{"style":"q1","verse":"","text":"Ñoo mëdd giirug \\w Yanqóoba|Yanqóoba:\\w*,"},{"style":"q1","verse":"","text":"mëdd leen, bóom leen,"},{"style":"q1","verse":"","text":"gental seen dëkkuwaay."}]