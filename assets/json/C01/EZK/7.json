[{"style":"s1","verse":"","text":"Jeexle dikk na"},{"style":"p","verse":"","text":""},{"style":"v","verse":"1","text":"Gannaaw loolu kàddug Aji Sax ji da maa dikkal, ne ma:"},{"style":"v","verse":"2","text":"«Léegi nag, yaw nit ki, Boroom bi Aji Sax ji dafa wax réewum Israayil ne ko:"},{"style":"q1","verse":"","text":"Jeex na tàkk!"},{"style":"q1","verse":"","text":"Jeexal dikkal na ñeenti weti réew mi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Jeexal nga nag léegi!"},{"style":"q1","verse":"","text":"Maay yebal sama mer fi sa kaw."},{"style":"q1","verse":"","text":"Maa lay mbugale say jëfin,"},{"style":"q1","verse":"","text":"yool la sa jépp jëf ju siblu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Duma la xool bëtu ñéeblu,"},{"style":"q1","verse":"","text":"duma ñeewante kenn."},{"style":"q1","verse":"","text":"Say jëfin laa lay yool,"},{"style":"q1","verse":"","text":"sa jëf ju siblu ji ci sa biir."},{"style":"q1","verse":"","text":"Su boobaa ngeen xam ne man maay Aji Sax ji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"«Boroom bi Aji Sax ji dafa wax ne:"},{"style":"q1","verse":"","text":"Céy wii ay, guléet wii ay!"},{"style":"q1","verse":"","text":"Mu ngii di dikk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Jeexlee ngoog, jeexle dikk na,"},{"style":"q1","verse":"","text":"jekki, yewwul la."},{"style":"q1","verse":"","text":"Mu ngii, di dikk!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yaw nitu réew mee, sànkute dikkal na la."},{"style":"q1","verse":"","text":"Àpp mat,"},{"style":"q1","verse":"","text":"bés dëgmal,"},{"style":"q1","verse":"","text":"tiitaange wuutu ca mbéx fa kaw tund ya."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Du yàgg dara ma xëpp la sama xadar,"},{"style":"q1","verse":"","text":"ŋaccal la sama mer,"},{"style":"q1","verse":"","text":"mbugale la say jëfin,"},{"style":"q1","verse":"","text":"yool la sa jépp jëf ju siblu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Duma xoole bëtu ñéeblu,"},{"style":"q1","verse":"","text":"duma ñeewante."},{"style":"q1","verse":"","text":"Say jëfin laa lay yool"},{"style":"q1","verse":"","text":"ak sa jëf ju siblu ji ci sa biir."},{"style":"q1","verse":"","text":"Su boobaa ngeen xam ne maay Aji Sax jiy dumaa."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"«Bés baa ngii, mu ngooguy dikk,"},{"style":"q1","verse":"","text":"sànkute sàqi na,"},{"style":"q1","verse":"","text":"yetu njubadee tóor,"},{"style":"q1","verse":"","text":"reewande jebbi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Fitnaa taxaw, di yet wuy dóoral coxor."},{"style":"q1","verse":"","text":"Seen lenn du des,"},{"style":"q1","verse":"","text":"du seen mbooloo,"},{"style":"q1","verse":"","text":"du seen alal,"},{"style":"q1","verse":"","text":"du seen daraja."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Àpp mat,"},{"style":"q1","verse":"","text":"bés jege."},{"style":"q1","verse":"","text":"Bu ca jëndkat tala bég,"},{"style":"q1","verse":"","text":"bu ca jaaykat tala naqarlu!"},{"style":"q1","verse":"","text":"Am sànj a dikkal askan wépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Bu jaaykat bi muccee it,"},{"style":"q1","verse":"","text":"du jotaat ca njaayam."},{"style":"q1","verse":"","text":"Peeñu mi jëm ci askan wépp, daal, du toxu,"},{"style":"q1","verse":"","text":"ku nekk ñaawtéefam lay dunde,"},{"style":"q1","verse":"","text":"ba du leqaliku."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Liitu xareey jib, ba lépp jekk ngir xare,"},{"style":"q1","verse":"","text":"te kenn du xareji,"},{"style":"q1","verse":"","text":"sama sànj a dikkal askan wépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Saamar biti,"},{"style":"q1","verse":"","text":"mbas akub xiif ci biir,"},{"style":"q1","verse":"","text":"ki ci àll bi deewe xare,"},{"style":"q1","verse":"","text":"ki ci biir dëkk bi, xiif ak mbas rey ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ña rëcc, rawi ca tund ya,"},{"style":"q1","verse":"","text":"waru pitaxi xur ya,"},{"style":"q1","verse":"","text":"ñooy binneendoo ndax seeni tooñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Loxo yépp yoqi,"},{"style":"q1","verse":"","text":"tànk tooy xepp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ñooy sol ay saaku di ko ñaawloo,"},{"style":"q1","verse":"","text":"seen yaram wépp daw,"},{"style":"q1","verse":"","text":"gépp kanam sëlmoo gàcce,"},{"style":"q1","verse":"","text":"bopp ya di nel yu set wecc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Seen xaalis, ñu sànni ci mbedd yi,"},{"style":"q1","verse":"","text":"seen wurus di leen saf sagaru sobe."},{"style":"q1","verse":"","text":"Keroog bésub sànjum Aji Sax ja,"},{"style":"q1","verse":"","text":"du xaalis, du wurus wu leen di xettali."},{"style":"q1","verse":"","text":"Du leen fajal xiif,"},{"style":"q1","verse":"","text":"du leen reggal,"},{"style":"q1","verse":"","text":"xanaa tàbbal leen cig njubadi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ñoo doon sagoo seen gànjar gu yànj,"},{"style":"q1","verse":"","text":"di ci sàkk jëmmi tuur yu jara bañ te jomblu,"},{"style":"q1","verse":"","text":"ba tax ma defal leen ko sagaru sobe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Maa koy teg ci loxoy doxandéem, ñu foqati ko,"},{"style":"q1","verse":"","text":"muy alal ju sagari niti àddina sëxëtoo,"},{"style":"q1","verse":"","text":"ñàkk ko wormaal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Maa leen di dummóoyu,"},{"style":"q1","verse":"","text":"maay bàyyi nit ñi teddadil sama ngëneelu béreb,"},{"style":"q1","verse":"","text":"ay sàcc a cay tàbbi, teddadil ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Waajalleen càllala yi,"},{"style":"q1","verse":"","text":"tuur nañu deret ba réew mi fees,"},{"style":"q1","verse":"","text":"dëkk bi fees ak fitna."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Maay indi xeet yi gëna néeg,"},{"style":"q1","verse":"","text":"ñu nangu seeni kër,"},{"style":"q1","verse":"","text":"ma daldi sàggi sagu boroom doole yi,"},{"style":"q1","verse":"","text":"sobeel seen béreb yu sell."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Tiitaangee ngi dikk,"},{"style":"q1","verse":"","text":"ñuy sàkku jàmm, jàmm réer."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ay ci kaw ay a ngi dikk,"},{"style":"q1","verse":"","text":"xibaar ci kaw xibaar,"},{"style":"q1","verse":"","text":"ñuy sàkku peeñum yonent ci neen,"},{"style":"q1","verse":"","text":"ndigalu yoon réer \\w sarxalkat|sarxalkat b-:\\w*,"},{"style":"q1","verse":"","text":"pexe réer mag."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Buur dina tiislu,"},{"style":"q1","verse":"","text":"garmi làmboo séri ñaawlu,"},{"style":"q1","verse":"","text":"baadoolay réew mi di kat-kati."},{"style":"q1","verse":"","text":"Seeni jëfin laay jëfeek ñoom,"},{"style":"q1","verse":"","text":"seeni àtte laa leen di àttee,"},{"style":"q1","verse":"","text":"ñu xam ne maay Aji Sax ji!»"}]