[{"style":"s1","text":"Yàllaa gën ci àddina, gën ci nit"},{"style":"q1","text":""},{"style":"v","text":"Màggalleen Ki Sax!"},{"style":"q1","text":"Kañ sunu Yàllaa baax,"},{"style":"q1","text":"màggal koo neex te war."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jaa ngi tabaxaat Yerusalem,"},{"style":"q1","text":"di delloosi ña ñu dàqe Israayil,"},{"style":"q1","text":""},{"style":"v","text":"di faj naqaru xol,"},{"style":"q1","text":"fajtal góomi boroom."},{"style":"q1","text":""},{"style":"v","text":"Mooy lim biddiiw yi,"},{"style":"q1","text":"tudd bu ci nekk."},{"style":"q1","text":""},{"style":"v","text":"Sunu Boroom a màgg, bare doole;"},{"style":"q1","text":"am xelam wees kemu."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, néew-ji-doole, mu siggil;"},{"style":"q1","text":"ku bon, mu daane ci suuf."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Woyeleen Aji Sax ji kàddug cant,"},{"style":"q1","text":"kañeleen sunu Yàlla jii ay xalam;"},{"style":"q1","text":""},{"style":"v","text":"mooy sànge asamaan ay niir,"},{"style":"q1","text":"tawal suuf,"},{"style":"q1","text":"saxal tund yi am ñax,"},{"style":"q1","text":""},{"style":"v","text":"leel mala,"},{"style":"q1","text":"xol liiru baaxoñ bu jooy."},{"style":"q1","text":""},{"style":"v","text":"Kii nawul dooley fas,"},{"style":"q1","text":"gërëmul kàttanu jàmbaar."},{"style":"q1","text":""},{"style":"v","text":"Ki Aji Sax ji gërëm mooy ki ko ragal"},{"style":"q1","text":"te di ko yaakaare ngoram."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yerusalem, sargalleen Aji Sax ji!"},{"style":"q1","text":"yeen waa \\w Siyoŋ|Siyoŋ:\\w* laa ne, màggalleen seen Yàlla!"},{"style":"q1","text":""},{"style":"v","text":"Mooy tëj ràpp seen bunti dëkk,"},{"style":"q1","text":"barkeel seen askan wa ca biir,"},{"style":"q1","text":""},{"style":"v","text":"jàmmal seen suufas réew,"},{"style":"q1","text":"reggal leen ngëneelu pepp,"},{"style":"q1","text":""},{"style":"v","text":"yónnee suuf ndigalam,"},{"style":"q1","text":"kàddoom ne coww, daw ca."},{"style":"q1","text":""},{"style":"v","text":"Mooy taw donji yuur yu weexe ni wëttéen."},{"style":"q1","text":"di lay lay bu sedd, mel ni pënd ci suuf."},{"style":"q1","text":""},{"style":"v","text":"Mooy yuri tawub yuuram niy doj."},{"style":"q1","text":"Bu ni seddee, ana kuy taxaw?"},{"style":"q1","text":""},{"style":"v","text":"Mu yebal kàddoom, seeyal;"},{"style":"q1","text":"wal ngelawam, ndox walalaan."},{"style":"q1","text":""},{"style":"v","text":"Mooy wax \\w Yanqóoba|Yanqóoba:\\w* kàddoom,"},{"style":"q1","text":"dogalam yaak ndigali yoonam ñeel Israayil."},{"style":"q1","text":""},{"style":"v","text":"Loolu defalu ko wenn xeet;"},{"style":"q1","text":"kenn xamu ci ndigali yoonam."},{"style":"q1","text":"Màggalleen Ki Sax!"}]