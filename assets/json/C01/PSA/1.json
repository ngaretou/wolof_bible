[{"style":"mt1","text":"Taalifi Cant"},{"style":"s1","verse":"","text":"Ñaari yoon a ngii"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ndokkalee yaw mi toppul tànki ku bon,"},{"style":"q2","verse":"","text":"taxawewuloo yoonu moykat,"},{"style":"q2","verse":"","text":"toogewuloo jataayu ñaawlekat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Xanaa di bége yoonu \\w Aji Sax ji|Aji Sax j-:\\w*,"},{"style":"q1","verse":"","text":"di ko jàngat guddeek bëccëg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Loo def mu àntu:"},{"style":"q1","verse":"","text":"nga mel ni garab gu saxe walum ndox,"},{"style":"q1","verse":"","text":"bu jotee meññ,"},{"style":"q1","verse":"","text":"te xob wa du lax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Loolu dalul moos ku bon"},{"style":"q1","verse":"","text":"kay mel ni xatax bu ngelaw li wal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Moo tax ku bon du siggee àtte ba,"},{"style":"q1","verse":"","text":"moykat du taxawe ndajem aji jub ña."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax ji kay ay sàmm yoonu aji jub;"},{"style":"q1","verse":"","text":"yoonu ku bon di sànk boroom."}]