[{"style":"mt1","text":"Taalifi Cant"},{"style":"s1","text":"Ñaari yoon a ngii"},{"style":"q1","text":""},{"style":"v","text":"Ndokkalee yaw mi toppul tànki ku bon,"},{"style":"q2","text":"taxawewuloo yoonu moykat,"},{"style":"q2","text":"toogewuloo jataayu ñaawlekat."},{"style":"q1","text":""},{"style":"v","text":"Xanaa di bége yoonu \\w Aji Sax ji|Aji Sax j-:\\w*,"},{"style":"q1","text":"di ko jàngat guddeek bëccëg."},{"style":"q1","text":""},{"style":"v","text":"Loo def mu àntu:"},{"style":"q1","text":"nga mel ni garab gu saxe walum ndox,"},{"style":"q1","text":"bu jotee meññ,"},{"style":"q1","text":"te xob wa du lax."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Loolu dalul moos ku bon"},{"style":"q1","text":"kay mel ni xatax bu ngelaw li wal."},{"style":"q1","text":""},{"style":"v","text":"Moo tax ku bon du siggee àtte ba,"},{"style":"q1","text":"moykat du taxawe ndajem aji jub ña."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji kay ay sàmm yoonu aji jub;"},{"style":"q1","text":"yoonu ku bon di sànk boroom."}]