[{"style":"s1","verse":"","text":"Aji Sax ji màgg na"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Asamaan nee Yàlla màgg na,"},{"style":"q1","verse":"","text":"kaw ne màww, biral liggéeyu loxoom!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bésoo bés kàddu seede ko,"},{"style":"q1","verse":"","text":"guddee guddi xamle ca mbir."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Du làkk, duy wax,"},{"style":"q1","verse":"","text":"du baat bu dégtu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"suuf sépp la seen ndéey jibal,"},{"style":"q1","verse":"","text":"seen kàddoo àkki cati àddina."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Kaw la Yàlla sampal jant xayma,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"mu mel ni boroom séet, génne néegam,"},{"style":"q1","verse":"","text":"bég ni dawkat biy xélu cib joŋante."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Asamaan lay fenke cat lii,"},{"style":"q1","verse":"","text":"wëndeelu, sowi cat lee;"},{"style":"q1","verse":"","text":"amul lu rëcc tàngooram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yoonu Aji Sax jee mat,"},{"style":"q1","verse":"","text":"di leqali bakkan."},{"style":"q1","verse":"","text":"Lu Aji Sax ji seede dëgg la,"},{"style":"q1","verse":"","text":"day xelal ku xeluwul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Lu Aji Sax ji tegtale, njub la,"},{"style":"q1","verse":"","text":"di bégal xol."},{"style":"q1","verse":"","text":"Lu Aji Sax ji santaane, leeraange la,"},{"style":"q1","verse":"","text":"di leeral gis-gis."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Santaaneb ragal Aji Sax ji mooy li \\w sell|sell:\\w*"},{"style":"q1","verse":"","text":"te sax ba fàww."},{"style":"q1","verse":"","text":"Lu Aji Sax ji digle, dëgg la,"},{"style":"q1","verse":"","text":"ak njub gu sotti."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Moo dàq wurus,"},{"style":"q1","verse":"","text":"raw wurusu ngalam wu ne xas;"},{"style":"q1","verse":"","text":"dàqati lem,"},{"style":"q1","verse":"","text":"ëpp tem-tem."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Man, Sang bi, ci laay leerloo,"},{"style":"q1","verse":"","text":"ku ko sàmm yokku yool."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Nit juum na, umple ko,"},{"style":"q1","verse":"","text":"baal ma tooñ gu ma umple."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Te Sang bi, musal ma ci bàkkaaru teyeef,"},{"style":"q1","verse":"","text":"mu bañ maa jiital,"},{"style":"q1","verse":"","text":"ma mana màndu,"},{"style":"q1","verse":"","text":"mucc moy gu réy."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Éy Aji Sax ji, lu saaw làmmiñ tudd"},{"style":"q1","verse":"","text":"mbaa sama xol déey ma ko,"},{"style":"q1","verse":"","text":"yal nay loo gërëm,"},{"style":"q1","verse":"","text":"yaa may aar, yaa ma \\w jot|jot:\\w*."}]