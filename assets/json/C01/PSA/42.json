[{"style":"s1","verse":"","text":"Maa namm Yàlla"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, dib taalifu yeete bu ñeel askanu Kore wu góor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yaw Yàlla, ni kéwél di sàkkoo walum ndox,"},{"style":"q1","verse":"","text":"maa la koy sàkkoo xol."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Maa mar Yàlla,"},{"style":"q1","verse":"","text":"Yàlla jiy dund!"},{"style":"q1","verse":"","text":"Kañ laay teewi fa kanam Yàlla?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Rongooñ laay dunde"},{"style":"q1","verse":"","text":"guddeek bëccëg,"},{"style":"q1","verse":"","text":"noon yi di ma dëkke naa:"},{"style":"q1","verse":"","text":"«Ana sa Yàlla ji?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Lii laay fàttaliku, sama xol jeex:"},{"style":"q1","verse":"","text":"dama daan jàll, jiite mbooloo ma,"},{"style":"q1","verse":"","text":"jëm kër Yàlla ga, di sarxolleeka sant,"},{"style":"q1","verse":"","text":"ñu booloo di màggal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Moo man, lu may xàddi, ne yogg?"},{"style":"q1","verse":"","text":"Lu may jàq nii?"},{"style":"q1","verse":"","text":"Tee maa yaakaar Yàlla, ba santati ko,"},{"style":"q1","verse":"","text":"moom sama Yàlla mi may wallu?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Maa ngi ne yogg,"},{"style":"q1","verse":"","text":"ba di la fàttalikoo fa réewum Yurdan"},{"style":"q1","verse":"","text":"ak tundi Ermon ak Miccar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Sa wal ya sotti, xóote awu moroom ma;"},{"style":"q1","verse":"","text":"say wal ak say gannax, lépp a jaare sama kaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Bëccëg Aji Sax ji baaxe ma ngoram,"},{"style":"q1","verse":"","text":"guddi ma fanaane koo woy,"},{"style":"q1","verse":"","text":"di ñaan Yàlla mi may dundal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Naa wax Yàlla sama cëslaay, ne ko:"},{"style":"q1","verse":"","text":"«Lu tax nga fàtte ma,"},{"style":"q1","verse":"","text":"noon di ma fitnaal,"},{"style":"q1","verse":"","text":"may wéye tiis?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Noon yaa ngi may ñaawal,"},{"style":"q1","verse":"","text":"di ma yendoo naa:"},{"style":"q1","verse":"","text":"«Ana sa Yàlla ji?»"},{"style":"q1","verse":"","text":"Ma ne yàcc, ne yasar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Moo man, lu may xàddi, ne yogg?"},{"style":"q1","verse":"","text":"Lu may jàq nii?"},{"style":"q1","verse":"","text":"Tee maa yaakaar Yàlla, ba santati ko,"},{"style":"q1","verse":"","text":"moom sama Yàlla mi may wallu?"}]