[{"style":"s1","verse":"","text":"Ma làqoo Aji Sax ji"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Aji Sax ji laa làqoo."},{"style":"q1","verse":"","text":"Ana lu ngeen ma naa: «Neel fëyy ni picc, toŋi sa kaw tund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Xanaa du ñu soxor ñaa ngii, bank seen xala,"},{"style":"q1","verse":"","text":"nas seenu fitt,"},{"style":"q1","verse":"","text":"nara lëndëmtu, jam ku jub?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bu ponki dëgg màbbee,"},{"style":"q1","verse":"","text":"nu aji jub ñi di def?»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Aji Sax jaa nga ca këram gu sell."},{"style":"q1","verse":"","text":"Aji Sax jaa nga ca asamaan, ca kaw jalam,"},{"style":"q1","verse":"","text":"ne jàkk doom aadama, di ko seetlu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Aji Sax ji day seetlu ku jub;"},{"style":"q1","verse":"","text":"ku soxor, bëgg fitna, mu bañ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Day tawal ku bon ay mbugal;"},{"style":"q1","verse":"","text":"sawaraak tamarax akub jaas dib añam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Aji Sax jee jub, sopp njub;"},{"style":"q1","verse":"","text":"kuy jubal, gis yiwam."}]