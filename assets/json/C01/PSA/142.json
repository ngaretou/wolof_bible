[{"style":"s1","verse":"","text":"Aji Sax ji rekk a ma yég"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Dib taalifu yeete bu ñeel Daawuda, ba mu nekkee ca xunt ma."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ma àddu, ñaan Aji Sax ji;"},{"style":"q1","verse":"","text":"àddu ca kaw, di tinu Aji Sax ji,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"sippi ko sama tawat,"},{"style":"q1","verse":"","text":"diis ko sama njàqare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Bu ma yoqee,"},{"style":"q1","verse":"","text":"ràññee nga samay jéego;"},{"style":"q1","verse":"","text":"yoon wi may jaare"},{"style":"q1","verse":"","text":"lañu ma làqal am yeer."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ma geesu ndijoor, xool;"},{"style":"q1","verse":"","text":"du kenn ku ma faale."},{"style":"q1","verse":"","text":"Rawtu réer ma,"},{"style":"q1","verse":"","text":"du kenn ku sama bakkan saf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax ji, ma woo la wall;"},{"style":"q1","verse":"","text":"ma ne, yaay sama kiiraay,"},{"style":"q1","verse":"","text":"di sama séddoo fi kaw suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Teewlul, ma ne wóoy,"},{"style":"q1","verse":"","text":"damaa manatul,"},{"style":"q1","verse":"","text":"xettali ma ci ñi ma topp,"},{"style":"q1","verse":"","text":"dañu maa ëpp doole."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ngalla génne ma fi ma tëje,"},{"style":"q1","verse":"","text":"ma sant la."},{"style":"q1","verse":"","text":"Man la way-ngëm ñi di yéew ëllëg,"},{"style":"q1","verse":"","text":"yaa may xéewale ab yool."}]