[{"style":"s1","verse":"","text":"Ma sant Yàlla mi ma xettali"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Maa bëgg Aji Sax ji: maa ko woo,"},{"style":"q1","verse":"","text":"dagaan ko, mu dégg ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Teewlu na ma moos,"},{"style":"q1","verse":"","text":"dama koy woo li feek may dund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Buumi dee a ma laawoon,"},{"style":"q1","verse":"","text":"njàqarey njaniiw dab ma,"},{"style":"q1","verse":"","text":"ma dajeek naqar aku tiis."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ma woo Aji Sax ji wall, ne ko:"},{"style":"q1","verse":"","text":"«Ngalla Aji Sax ji, xettali ma!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Aji Sax jeey boroom yiw ak njub,"},{"style":"q1","verse":"","text":"sunu Yàllaay boroom yërmande."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax jeey aar ku xamul àddina;"},{"style":"q1","verse":"","text":"tumurànke woon naa, mu wallu ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Bu ma yeboo sama xel dal,"},{"style":"q1","verse":"","text":"Aji Sax ji baaxe na ma!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Xettali nga ma, ba deewuma,"},{"style":"q1","verse":"","text":"jàppale ma, ba tërëfuma,"},{"style":"q1","verse":"","text":"feral nga it samay rongooñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Damay topp Aji Sax ji"},{"style":"q1","verse":"","text":"ci biir réewum aji dund ñi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ba ma wax: «Torox naa ba toskare,»"},{"style":"q1","verse":"","text":"teewul may ànd ak ngëm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Maa jàqoon ba naan:"},{"style":"q1","verse":"","text":"«Ñépp ay fen-kat lañu!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ana lu may njukkale Aji Sax ji,"},{"style":"q1","verse":"","text":"li mu ma baaxe lépp?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Xanaa ma yékkati kaasu Aji Sax"},{"style":"q1","verse":"","text":"jiy misaal wallam,"},{"style":"q1","verse":"","text":"te may tudd turam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Samay digeek Aji Sax ji laay matal"},{"style":"q1","verse":"","text":"fi kanam ñoñam ñépp."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Wóllërey Aji Sax ji,"},{"style":"q1","verse":"","text":"deewam day doon lu muy naqarlu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Aji Sax ji, maay sab jaam,"},{"style":"q1","verse":"","text":"maay sab jaam, di sa doomu jaam bi ma sukk jur."},{"style":"q1","verse":"","text":"Yaa yiwi samay buum,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"yaw laay sarxalal \\w saraxu cant|Sarax yi:\\w*,"},{"style":"q1","verse":"","text":"woo la ci sa tur, Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Samay digeek Aji Sax ji laay matal"},{"style":"q1","verse":"","text":"fi kanam ñoñam ñépp,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"fi ëtti kër Aji Sax ji,"},{"style":"q1","verse":"","text":"fi digg Yerusalem."},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]