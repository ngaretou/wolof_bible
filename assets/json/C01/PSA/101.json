[{"style":"s1","text":"Buur a ngi jaayanteek Yàlla"},{"style":"d","text":""},{"style":"v","text":"Ñeel Daawuda, dib taalifu cant."},{"style":"b","text":""},{"style":"q1","text":"Ngor ak yoon laay woy."},{"style":"q1","text":"Yaw Aji Sax ji laay joobe."},{"style":"q1","text":""},{"style":"v","text":"Naa teewlu yoonu màndute."},{"style":"q1","text":"Ana kañ nga may dikkal?"},{"style":"q1","text":"Naa doxe màndute"},{"style":"q1","text":"fi sama biir waa kër."},{"style":"q1","text":""},{"style":"v","text":"Caaxaani neen, xooluma;"},{"style":"q1","text":"jëf ju wàcc yoon, bëgguma,"},{"style":"q1","text":"taqu ma fenn."},{"style":"q1","text":""},{"style":"v","text":"Kuy nas njekkar, sore ma;"},{"style":"q1","text":"lu bon, xawma ko."},{"style":"q1","text":""},{"style":"v","text":"Kuy jëw moroomam, ma wedamal;"},{"style":"q1","text":"ku daŋŋiiral ak ku reew, muñaluma la."},{"style":"q1","text":""},{"style":"v","text":"Ku dëggu ci réew mi, ma geesu,"},{"style":"q1","text":"ngir fat ko."},{"style":"q1","text":"Te kuy wéye màndute,"},{"style":"q1","text":"kookoo may liggéeyal."},{"style":"q1","text":""},{"style":"v","text":"Kuy njublaŋ du tooge sama kër,"},{"style":"q1","text":"kuy fen du taxaw sama kanam."},{"style":"q1","text":""},{"style":"v","text":"Maay xëy, sànk képp ku bon ci réew mi,"},{"style":"q1","text":"ngir génne ci dëkkub Aji Sax jii képp kuy def lu ñaaw."}]