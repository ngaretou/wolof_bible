[{"style":"s1","verse":"","text":"Buur a ngi jaayanteek Yàlla"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Ñeel Daawuda, dib taalifu cant."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Ngor ak yoon laay woy."},{"style":"q1","verse":"","text":"Yaw Aji Sax ji laay joobe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Naa teewlu yoonu màndute."},{"style":"q1","verse":"","text":"Ana kañ nga may dikkal?"},{"style":"q1","verse":"","text":"Naa doxe màndute"},{"style":"q1","verse":"","text":"fi sama biir waa kër."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Caaxaani neen, xooluma;"},{"style":"q1","verse":"","text":"jëf ju wàcc yoon, bëgguma,"},{"style":"q1","verse":"","text":"taqu ma fenn."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Kuy nas njekkar, sore ma;"},{"style":"q1","verse":"","text":"lu bon, xawma ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Kuy jëw moroomam, ma wedamal;"},{"style":"q1","verse":"","text":"ku daŋŋiiral ak ku reew, muñaluma la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ku dëggu ci réew mi, ma geesu,"},{"style":"q1","verse":"","text":"ngir fat ko."},{"style":"q1","verse":"","text":"Te kuy wéye màndute,"},{"style":"q1","verse":"","text":"kookoo may liggéeyal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Kuy njublaŋ du tooge sama kër,"},{"style":"q1","verse":"","text":"kuy fen du taxaw sama kanam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Maay xëy, sànk képp ku bon ci réew mi,"},{"style":"q1","verse":"","text":"ngir génne ci dëkkub Aji Sax jii képp kuy def lu ñaaw."}]