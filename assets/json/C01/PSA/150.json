[{"style":"s1","text":"Màggalleen Ki Sax!"},{"style":"q1","text":""},{"style":"v","text":"Màggalleen Ki Sax!"},{"style":"q1","text":"Nangeen màggale Yàlla biir këram gu sell,"},{"style":"q1","text":"màggale ko ca tatay asamaanam."},{"style":"q1","text":""},{"style":"v","text":"Màggaleleen ko ay jalooreem,"},{"style":"q1","text":"màggale ko màggam gu réy."},{"style":"q1","text":""},{"style":"v","text":"Màggaleleen koob bufta,"},{"style":"q1","text":"màggale ko xalam ak moroom ma."},{"style":"q1","text":""},{"style":"v","text":"Màggaleleen ko njiin aki pooj,"},{"style":"q1","text":"màggale ko xalam ak toxoro."},{"style":"q1","text":""},{"style":"v","text":"Màggaleleen ko tëggum weñ mu xumb,"},{"style":"q1","text":"màggale ko tëggum weñ mu kawe."},{"style":"q1","text":""},{"style":"v","text":"Na lépp luy noyyi màggal Ki Sax!"},{"style":"q1","text":"Màggalleen Ki Sax!"}]