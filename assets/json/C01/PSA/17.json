[{"style":"s1","verse":"","text":"Aji Sax ji, jox ma dëgg"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Di ñaanu Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Éy Aji Sax ji, déglul liy dëgg,"},{"style":"q1","verse":"","text":"déglul, ma woote!"},{"style":"q1","verse":"","text":"Teewlul, ma dagaane la"},{"style":"q1","verse":"","text":"làmmiñ wu mucc fen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yal na sama àtte bawoo fi yaw,"},{"style":"q1","verse":"","text":"nga gis sama njubte."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Natt nga saab xol, niir ma guddi,"},{"style":"q1","verse":"","text":"settantal ma, gisoo dara."},{"style":"q1","verse":"","text":"Dogu naa ne duma moye wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Su may jëf it, di sàmm sa kàddu."},{"style":"q1","verse":"","text":"Maay moyu yoonu ku bon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sa tànk, sama tànk,"},{"style":"q1","verse":"","text":"jàdduma fenn."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Éy Yàlla, maa lay woo, nga di ma wuyu;"},{"style":"q1","verse":"","text":"teewlu ma te déglu ma!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Na sa ngor feeñ,"},{"style":"q1","verse":"","text":"yaw miy walloo sa doole"},{"style":"q1","verse":"","text":"ku la làqooy noonam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Sàmm ma ni peru bët;"},{"style":"q1","verse":"","text":"làqe ma sa ker,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"ma rëcc ñu bon ñi ma song,"},{"style":"q1","verse":"","text":"noon ñii ma gaw, nar maa bóom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Seen xol dàq yërmande,"},{"style":"q1","verse":"","text":"seeni wax di reewande."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Tanc nañu nu fépp,"},{"style":"q1","verse":"","text":"ne nu jàkk, nar noo tëral."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Mbete gaynde gu namma fàdde,"},{"style":"q1","verse":"","text":"gaynde gu mat guy tëroo!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Éy Aji Sax ji, jógal dajeek moom, detteel ko."},{"style":"q1","verse":"","text":"Xettalee ma sa saamar, ma rëcc ku bon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Éy Aji Sax ji, walloo ma sa doole,"},{"style":"q1","verse":"","text":"ma rëcc ñi séddoo dundu àddina."},{"style":"q1","verse":"","text":"Say soppe, nga reggal,"},{"style":"q1","verse":"","text":"seeni doom regg,"},{"style":"q1","verse":"","text":"desalal seeni doom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Man nag ma am dëgg, ne la jàkk,"},{"style":"q1","verse":"","text":"yewwu, regge sa jëmm."}]