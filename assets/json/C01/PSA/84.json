[{"style":"s1","verse":"","text":"Ku dal ak Yàlla bég"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, ànd ak xalam gu ñuy wax gitiit\\f + \\fr 84.1 \\fk gitiit: \\ft seetal ci 8.1.\\f*, ñeel askanu Kore wu góor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"yaaka sopplub dëkkuwaay!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Damaa namm ëttub Aji Sax ji,"},{"style":"q1","verse":"","text":"di ko gelu ba jeex tàkk,"},{"style":"q1","verse":"","text":"jëmm jeek xol bi,"},{"style":"q1","verse":"","text":"di xaacul Yàlla jiy dund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Éy Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"sama buur, sama Yàlla,"},{"style":"q1","verse":"","text":"sawoor sax tag, nga fat ko,"},{"style":"q1","verse":"","text":"mbelaar it sàkk tàggam,"},{"style":"q1","verse":"","text":"denc ay cuujam fa sa \\w sarxalukaay|sarxalukaay b-:\\w*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ndokkalee ku dëkke sa kër,"},{"style":"q1","verse":"","text":"di la màggalati. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ndokkalee ku lay doolewoo,"},{"style":"q1","verse":"","text":"te namma topp yoon ya jëm sa kër."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Buy jàll xuru Jooyoo,"},{"style":"q1","verse":"","text":"mu saf ko bëti ndox,"},{"style":"q1","verse":"","text":"céebo sànge xur wa barke,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"muy gëna am doole,"},{"style":"q1","verse":"","text":"ba teewi fa Yàlla ca \\w Siyoŋ|Siyoŋ:\\w*."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Éy Yàlla Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"déglul, ma ñaan la!"},{"style":"q1","verse":"","text":"Yàllay \\w Yanqóoba|Yanqóoba:\\w*, teewlu ma. \\qs Selaw.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Éy Yàlla, geesul Buur, sunu kiiraay,"},{"style":"q1","verse":"","text":"ngalla niiral kii nga fal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Benn fan ci sab ëtt de"},{"style":"q1","verse":"","text":"moo dàq junniy fan feneen."},{"style":"q1","verse":"","text":"Sama taxawaayu bunt kër Yàlla"},{"style":"q1","verse":"","text":"moo ma gënal tëraayu biir xaymab ku bon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Aji Sax ji Yàllaa mat jant, mat kiiraay;"},{"style":"q1","verse":"","text":"yiw ak daraja, Aji Sax ji joxe;"},{"style":"q1","verse":"","text":"te du ngëneel lu muy bañal ku mat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Éy Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"ndokkalee ku la wóolu."}]