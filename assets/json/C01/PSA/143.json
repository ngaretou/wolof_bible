[{"style":"s1","text":"Aji Sax jeey sama yaakaar"},{"style":"d","text":""},{"style":"v","text":"Dib taalifu cant, ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":"Éy Aji Sax ji, déglul, ma ñaan la;"},{"style":"q1","text":"teewlul, ma tinu la;"},{"style":"q1","text":"nangul ma ci sa wormaak sa àtteb dëgg."},{"style":"q1","text":""},{"style":"v","text":"Sang bi, bul layook man;"},{"style":"q1","text":"kenn jubul fi sa kanam, du kuy dund."},{"style":"q1","text":""},{"style":"v","text":"Noon a ma sambal,"},{"style":"q1","text":"daane ma, joggati,"},{"style":"q1","text":"dëël ma ci lëndëm"},{"style":"q1","text":"ni ku dee bu yàgg."},{"style":"q1","text":""},{"style":"v","text":"Sama doole jeex tàkk,"},{"style":"q1","text":"sama xol jeex, ma ne yàcc."},{"style":"q1","text":""},{"style":"v","text":"May fàttaliku bu njëkkoon,"},{"style":"q1","text":"di xalaat sa jaloore yépp,"},{"style":"q1","text":"seetaat li nga def."},{"style":"q1","text":""},{"style":"v","text":"Ma tàllal lay loxo,"},{"style":"q1","text":"soxla la, ni suuf su mar di soxlaa ndox. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, gaawal wuyu ma;"},{"style":"q1","text":"sama xol a jeex, ma ne yàcc,"},{"style":"q1","text":"bu ma xañ sa yiw,"},{"style":"q1","text":"ma yem ak kuy tàbbiji biir pax."},{"style":"q1","text":""},{"style":"v","text":"Xëye ma sa ngor,"},{"style":"q1","text":"yaw laa wóolu;"},{"style":"q1","text":"xamal ma fu may doxe,"},{"style":"q1","text":"yaw laa wékk sama bakkan."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, musal ma ci noon yi,"},{"style":"q1","text":"yaw laa làqoo."},{"style":"q1","text":""},{"style":"v","text":"Jàngal ma, ma jëfe sa coobare;"},{"style":"q1","text":"yaa di sama Yàlla."},{"style":"q1","text":"Yal na ma sam xel jiitee mbaax"},{"style":"q1","text":"ci yoon wu tàlli."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, musal ma ngir sa tur,"},{"style":"q1","text":"jëfeel sa àtte, teggil ma njàqarey bakkan."},{"style":"q1","text":""},{"style":"v","text":"Sànkal ma noon yi ci sa ngor,"},{"style":"q1","text":"bóomal ma bépp mbañ."},{"style":"q1","text":"Man de, maay sab jaam!"}]