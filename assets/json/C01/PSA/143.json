[{"style":"s1","verse":"","text":"Aji Sax jeey sama yaakaar"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Dib taalifu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Éy Aji Sax ji, déglul, ma ñaan la;"},{"style":"q1","verse":"","text":"teewlul, ma tinu la;"},{"style":"q1","verse":"","text":"nangul ma ci sa wormaak sa àtteb dëgg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Sang bi, bul layook man;"},{"style":"q1","verse":"","text":"kenn jubul fi sa kanam, du kuy dund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Noon a ma sambal,"},{"style":"q1","verse":"","text":"daane ma, joggati,"},{"style":"q1","verse":"","text":"dëël ma ci lëndëm"},{"style":"q1","verse":"","text":"ni ku dee bu yàgg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Sama doole jeex tàkk,"},{"style":"q1","verse":"","text":"sama xol jeex, ma ne yàcc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"May fàttaliku bu njëkkoon,"},{"style":"q1","verse":"","text":"di xalaat sa jaloore yépp,"},{"style":"q1","verse":"","text":"seetaat li nga def."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ma tàllal lay loxo,"},{"style":"q1","verse":"","text":"soxla la, ni suuf su mar di soxlaa ndox. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Aji Sax ji, gaawal wuyu ma;"},{"style":"q1","verse":"","text":"sama xol a jeex, ma ne yàcc,"},{"style":"q1","verse":"","text":"bu ma xañ sa yiw,"},{"style":"q1","verse":"","text":"ma yem ak kuy tàbbiji biir pax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Xëye ma sa ngor,"},{"style":"q1","verse":"","text":"yaw laa wóolu;"},{"style":"q1","verse":"","text":"xamal ma fu may doxe,"},{"style":"q1","verse":"","text":"yaw laa wékk sama bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Aji Sax ji, musal ma ci noon yi,"},{"style":"q1","verse":"","text":"yaw laa làqoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Jàngal ma, ma jëfe sa coobare;"},{"style":"q1","verse":"","text":"yaa di sama Yàlla."},{"style":"q1","verse":"","text":"Yal na ma sam xel jiitee mbaax"},{"style":"q1","verse":"","text":"ci yoon wu tàlli."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Aji Sax ji, musal ma ngir sa tur,"},{"style":"q1","verse":"","text":"jëfeel sa àtte, teggil ma njàqarey bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Sànkal ma noon yi ci sa ngor,"},{"style":"q1","verse":"","text":"bóomal ma bépp mbañ."},{"style":"q1","verse":"","text":"Man de, maay sab jaam!"}]