[{"style":"s1","text":"Buur Yàllaay walloo"},{"style":"q1","text":""},{"style":"v","text":"Màggalleen Ki Sax!"},{"style":"q1","text":"Yeen ñiy jaamu Aji Sax ji, màggalleen ko,"},{"style":"q1","text":"màggalleen turu Aji Sax ji!"},{"style":"q1","text":""},{"style":"v","text":"Yal nañu teral turu Aji Sax ji"},{"style":"q1","text":"tey ak ëllëg!"},{"style":"q1","text":""},{"style":"v","text":"Li dale penku ba sowu,"},{"style":"q1","text":"màggalleen Aji Sax ji."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jee kawe, tiim xeetoo xeet,"},{"style":"q1","text":"darajaam sut asamaan!"},{"style":"q1","text":""},{"style":"v","text":"Ana ku mel ni sunu Yàlla Aji Sax ji,"},{"style":"q1","text":"ki kaweel ab jalam, toog,"},{"style":"q1","text":""},{"style":"v","text":"di jéer asamaan ak suuf?"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Mooy yékkatee ku néewle fa suuf,"},{"style":"q1","text":"di seppee ku sonn cib sën,"},{"style":"q1","text":""},{"style":"v","text":"bokkal ko jataayu kilifa"},{"style":"q2","text":"ya jiite xeetam."},{"style":"q1","text":""},{"style":"v","text":"Mooy dëël jigéen ju tële woona jur,"},{"style":"q1","text":"may ko mbégtem njureel."},{"style":"q1","text":"Màggalleen Ki Sax!"}]