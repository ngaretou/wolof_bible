[{"style":"s1","text":"Bu sa xol jóg"},{"style":"d","text":""},{"style":"v","text":"Ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":"Bu sa xol jóg ci ku bon,"},{"style":"q1","text":"bul ñee kuy def njombe."},{"style":"q1","text":""},{"style":"v","text":"Léegi mu wow nim ñax,"},{"style":"q1","text":"lax ni gàncax."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Dénkul ci Aji Sax ji, di def lu baax,"},{"style":"q1","text":"ba des ci réew meek jàmm."},{"style":"q1","text":""},{"style":"v","text":"Deel bànneexoo Aji Sax ji,"},{"style":"q1","text":"mu may la say nammeel."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Jébbalul ci Aji Sax ji,"},{"style":"q1","text":"dénku ci moom, mooy sottal."},{"style":"q1","text":""},{"style":"v","text":"Day setal sab der ni jant bu fenk;"},{"style":"q1","text":"sab àtte ne ràññ ni njolloor."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Neel tekk, xaar Aji Sax ji,"},{"style":"q1","text":"yaakaar ko."},{"style":"q1","text":"Bu sa xol jóg ci kuy baaxle"},{"style":"q1","text":"tey lal ay pexe."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Baal mer, dëddu xadar."},{"style":"q1","text":"Xol bu jóg, loraangey neen."},{"style":"q1","text":""},{"style":"v","text":"Ku bon, ñu dagg, sànni;"},{"style":"q1","text":"ku yaakaar Aji Sax ji, jagoo réew mi."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Nes tuut, ku bon ne mes,"},{"style":"q1","text":"nga seet, seet, mu réer."},{"style":"q1","text":""},{"style":"v","text":"Waaye néew-dooleey jagoo réew mi;"},{"style":"q1","text":"jàmm ne ñoyy, ñu bége."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ku bon fexeel ku jub,"},{"style":"q1","text":"di ko màttu,"},{"style":"q1","text":""},{"style":"v","text":"Buur Yàlla di ko ree,"},{"style":"q1","text":"xam ne bésam a ngi ñëw."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ku bon bocci saamar,"},{"style":"q1","text":"tàwwig fitt,"},{"style":"q1","text":"nara rey ku ñàkk, néewle,"},{"style":"q1","text":"nara bóom kuy jubal;"},{"style":"q1","text":""},{"style":"v","text":"far jam boppam saamaru xol,"},{"style":"q1","text":"fittam damm."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji jub, as tuutam"},{"style":"q1","text":"moo gën koomu ku bon."},{"style":"q1","text":""},{"style":"v","text":"Ku bon kat, sa doole dëddu la;"},{"style":"q1","text":"ku jub, Aji Sax ji wallu la."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ku màndu, Aji Sax ji yég la,"},{"style":"q1","text":"sab cér sax."},{"style":"q1","text":""},{"style":"v","text":"Bu mettee doo rus,"},{"style":"q1","text":"te bésub xiif dangay regg."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ku bon sànku;"},{"style":"q1","text":"noonub Aji Sax ji, taaru tool bu naat la,"},{"style":"q1","text":"bu tàkkee, jeex tàkk."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ku bon leb, du fey;"},{"style":"q1","text":"ku jub yéwén, di joxe."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji barkeel la, nga jagoo réew mi moos;"},{"style":"q1","text":"mu alag la, nga dog."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bu Aji Sax ji rafetloo saw yoon,"},{"style":"q1","text":"mooy jiite say jéego."},{"style":"q1","text":""},{"style":"v","text":"Soo tërëfee it, doo daanu."},{"style":"q1","text":"Aji Sax jee lay walloo loxoom."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ba may ndaw, ba tey may mag,"},{"style":"q1","text":"gisuma ku jub ñàkk ndimbal,"},{"style":"q1","text":"mbaa askanam di dunde yalwaan."},{"style":"q1","text":""},{"style":"v","text":"Day saxoo tabe, di leble,"},{"style":"q1","text":"askanam barkeel."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Dëddul mbon, di jëfe mbaax,"},{"style":"q1","text":"ba dëkke réew mi fàww."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji kat dafa jiital yoon,"},{"style":"q1","text":"te du wacc jaamam bu gore."},{"style":"q1","text":"Day fegu ba fàww;"},{"style":"q1","text":"askanu ku bon dog."},{"style":"q1","text":""},{"style":"v","text":"Aji jub ay jagoo réew mi,"},{"style":"q1","text":"dëkke ko ba fàww."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Kàddug aji jub day xelu,"},{"style":"q1","text":"wax ja jub;"},{"style":"q1","text":""},{"style":"v","text":"yoonu Yàllaam a nga ca xol ba,"},{"style":"q1","text":"du dox ba tërëf."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ku bon a ngi yeeru ku jub,"},{"style":"q1","text":"di ko wuta bóom."},{"style":"q1","text":""},{"style":"v","text":"Waaye Aji Sax ji du ko bàyyeek moom,"},{"style":"q1","text":"te deesu ko daan cib layoo."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yaakaaral Aji Sax ji, sàmm yoonam,"},{"style":"q1","text":"mu teral la, nga jagoo réew mi,"},{"style":"q1","text":"ku bon sànku, ngay gis."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Damaa gis ku bon di noote,"},{"style":"q1","text":"te sëq ni garab gu naat fa mu fekk baax,"},{"style":"q1","text":""},{"style":"v","text":"ma jaareeti fa, ndeke wuute na,"},{"style":"q1","text":"ma seet ko, gisuma ko."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bàyyil xel ku màndu, xool ku jub;"},{"style":"q1","text":"muj, fa nitu jàmm."},{"style":"q1","text":""},{"style":"v","text":"Moykat yeey sànkoondoo,"},{"style":"q1","text":"ku bon amul muj."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jeey musal ku jub,"},{"style":"q1","text":"di ko aar bésub njàqare."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jee koy taxawu, wallu ko,"},{"style":"q1","text":"wallu ko ci ku bon, musal ko,"},{"style":"q1","text":"ndax li mu ko làqoo."}]