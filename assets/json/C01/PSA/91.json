[{"style":"s1","verse":"","text":"Aji Sax jee mat kaaraange"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ku dëkke kiiraayu Aji Kawe ji"},{"style":"q1","verse":"","text":"yaa këroo keppaaru Aji Man ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Dama naa Aji Sax ji:"},{"style":"q1","verse":"","text":"«Yaw laay làqoo, di la fegoo;"},{"style":"q1","verse":"","text":"ngay sama Yàlla ji ma doyloo.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Kii kay moo lay musal cig fiir"},{"style":"q1","verse":"","text":"ak loraangey mbas."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Kiiraayam la lay sànge,"},{"style":"q1","verse":"","text":"nga yiiru, làqu;"},{"style":"q1","verse":"","text":"kóllëreem feg la, di la aar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Doo ragal njàqarey guddi"},{"style":"q1","verse":"","text":"mbaa fittu bëccëg"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"mbaa mbas mu yooteg lëndëm,"},{"style":"q1","verse":"","text":"ak balaa buy faat njolloor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Junni daanu fi sa wet,"},{"style":"q1","verse":"","text":"fukki junni (10~000) daanu fi sa ndijoor,"},{"style":"q1","verse":"","text":"du tax mu jege la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Say gët nga ciy teg,"},{"style":"q1","verse":"","text":"di seetaan ku bon di jot yoolam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Yaw mi màkkaanoo Aji Sax ji,"},{"style":"q1","verse":"","text":"Aji Kawe jii di sama rawtu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"genn loraange du la dab,"},{"style":"q1","verse":"","text":"balaa du jege sa xayma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ay malaakaam la lay booleel,"},{"style":"q1","verse":"","text":"ñu di la sàmm foo jëm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Seeni loxo lañu lay leewoo,"},{"style":"q1","verse":"","text":"ba doo fakkastalu ciw doj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Dangay falaxe jaan ak gaynde,"},{"style":"q1","verse":"","text":"di rajaxe ñàngóor ak sibi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Yàlla da naa: «Kii de man la safoo,"},{"style":"q1","verse":"","text":"ma di ko xettali."},{"style":"q1","verse":"","text":"Man la xam, te maa koy teg fu wóor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Mu woo ma, ma wuyu ko;"},{"style":"q1","verse":"","text":"mook man ci biir njàqare,"},{"style":"q1","verse":"","text":"ma xettali ko, darajaal ko,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"reggal koy fan,"},{"style":"q1","verse":"","text":"baaxe ko samag wall.»"}]