[{"style":"s1","text":"Ma sant Aji Sax ji"},{"style":"d","text":""},{"style":"v","text":"Ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":"Naa sant Aji Sax ji ci saa xol bii,"},{"style":"q1","text":"saa jëmm jépp di sàbbaal sellaayu turam!"},{"style":"q1","text":""},{"style":"v","text":"Naa sant Aji Sax ji ci saa xol bii,"},{"style":"q1","text":"te baña fàtte mboolem xéewalam!"},{"style":"q1","text":""},{"style":"v","text":"Moo lay baal foo ñaawe,"},{"style":"q1","text":"di la wéral foo woppe."},{"style":"q1","text":""},{"style":"v","text":"Mooy jot sa bakkan,"},{"style":"q1","text":"kaalaa la ngor ak yërmande,"},{"style":"q1","text":""},{"style":"v","text":"di reggal sa bakkan bànneex,"},{"style":"q1","text":"yeesalal lag ndaw,"},{"style":"q1","text":"nga màggataale doole nig jaxaay."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jeey àtte njub,"},{"style":"q1","text":"di àtte dëgg jépp néew-ji-doole."},{"style":"q1","text":""},{"style":"v","text":"Moo xamal Musaa ay nammeelam,"},{"style":"q1","text":"xamal \\w bànni Israayil|bànni Israayil:\\w* ay jëfam."},{"style":"q1","text":""},{"style":"v","text":"Yiw ak yërmande, fa Aji Sax ji."},{"style":"q1","text":"Moo muñ mer te bare ngor."},{"style":"q1","text":""},{"style":"v","text":"Saxoowul di toppe,"},{"style":"q1","text":"dencul mer ba fàww."},{"style":"q1","text":""},{"style":"v","text":"Du sunuy bàkkaar la nu mbugal,"},{"style":"q1","text":"du sunuy ñaawtéef la nu fey."},{"style":"q1","text":""},{"style":"v","text":"Ni asamaan tiime suuf,"},{"style":"q1","text":"ni la ngoram yiire ku ko ragal."},{"style":"q1","text":""},{"style":"v","text":"Ni penku soree sowu,"},{"style":"q1","text":"ni la nu soreleek sunu mbugali tooñ."},{"style":"q1","text":""},{"style":"v","text":"Ni baay yërëme doom,"},{"style":"q1","text":"ni la Aji Sax ji yërëme ku ko ragal."},{"style":"q1","text":""},{"style":"v","text":"Moo xam sunu bind,"},{"style":"q1","text":"di bàyyi xel ne pënd lanu."},{"style":"q1","text":""},{"style":"v","text":"Nit aki fanam di saxyaay"},{"style":"q2","text":"suy law ni tóor-tóoru àll;"},{"style":"q1","text":""},{"style":"v","text":"ngelaw li wal, mu ne mes,"},{"style":"q1","text":"mel ni masu faa nekk."},{"style":"q1","text":""},{"style":"v","text":"Waaye ngorug Aji Sax ji naka jekk"},{"style":"q1","text":"ñeel na fàww ku ko ragal;"},{"style":"q1","text":"àtteb dëggam ñeel sëtaati"},{"style":"q2","text":""},{"style":"v","text":"kuy sàmm \\w kóllëreem|kóllëre g-:\\w*,"},{"style":"q2","text":"di saxoo jëfe tegtali yoonam."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jee samp asamaan ab jalam,"},{"style":"q1","text":"nguuram ŋank lépp."},{"style":"q1","text":""},{"style":"v","text":"Yeen malaaka yi, santleen Aji Sax ji,"},{"style":"q1","text":"yeen jàmbaar yu mag yiy jëfe ndigalam,"},{"style":"q1","text":"di sàmm kàddoom."},{"style":"q1","text":""},{"style":"v","text":"Santleen Aji Sax ji, yeen gàngoori xareem yépp,"},{"style":"q1","text":"yeen dagam yiy jëfe coobareem."},{"style":"q1","text":""},{"style":"v","text":"Yeen mbindeef yépp, nangeen sante Aji Sax ji"},{"style":"q1","text":"fépp fu mu tiim."},{"style":"q1","text":"Naa sant Aji Sax ji ci saa xol bii!"}]