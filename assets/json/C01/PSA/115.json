[{"style":"s1","verse":"","text":"Ku wóolu Yàlla barkeel"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Bu muy nun, Aji Sax ji, bu muy nun,"},{"style":"q1","verse":"","text":"saw tur nga wara teral,"},{"style":"q1","verse":"","text":"ngir sa ngor ak sa worma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Bu yéefar yi ne mukk:"},{"style":"q1","verse":"","text":"«Moo ñii, ana seen Yàlla?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Te yaw sunu Yàlla, yaa ngi asamaan,"},{"style":"q1","verse":"","text":"di def lu la soob."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Seeni tuur xaalis laak wurus,"},{"style":"q1","verse":"","text":"loxoo ko sàkk:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"am gémmiñ te du wax,"},{"style":"q1","verse":"","text":"ami gët te du gis,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"ami nopp, du dégg,"},{"style":"q1","verse":"","text":"am bakkan, du xeeñtu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"ami loxo it te du yég,"},{"style":"q1","verse":"","text":"ami tànk it te du dox,"},{"style":"q1","verse":"","text":"du xàddamiku sax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ñi sàkk yii tuur, yal nañu mel ni ñoom,"},{"style":"q1","verse":"","text":"ñook ñi leen wóolu ñépp."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Éy Israayil, wóolul Aji Sax ji,"},{"style":"q1","verse":"","text":"moo lay jàppale, di la aar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Yeen waa kër \\w Aaróona|Aaróona:\\w*, wóoluleen Aji Sax ji,"},{"style":"q1","verse":"","text":"moo leen di jàppale, di leen aar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Yeen ñi ragal Aji Sax ji, wóoluleen Aji Sax ji,"},{"style":"q1","verse":"","text":"moo leen di jàppale, di leen aar."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Aji Sax jee nuy bàyyi xel, di nu barkeel,"},{"style":"q1","verse":"","text":"di barkeel kër Israayil,"},{"style":"q1","verse":"","text":"di barkeel kër Aaróona."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Aji Sax jeey barkeel ku ko ragal,"},{"style":"q1","verse":"","text":"mag ak ndaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Yal na leen Yàlla ful,"},{"style":"q1","verse":"","text":"ful seeni doom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Yal nangeen barkeelu ci Aji Sax ji,"},{"style":"q1","verse":"","text":"Ki sàkk asamaan ak suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Asamaan, asamaanu Aji Sax jee,"},{"style":"q1","verse":"","text":"suuf la dénk doom aadama."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Du ku dee mooy màggal Ki Sax,"},{"style":"q1","verse":"","text":"du kenn ku tàbbi réewum tàbbi-selaw!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Nun, nooy sargal Ki Sax,"},{"style":"q1","verse":"","text":"tey ak ëllëg."},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]