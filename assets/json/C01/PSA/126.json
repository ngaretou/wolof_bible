[{"style":"s1","verse":"","text":"Yàlla, delloo nu"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Di \\w woy yu ñuy yéege|woy yi ñuy yéege\\w* ca tund ya, di ko màggale Yàlla."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Ba Aji Sax ji delloo waa \\w Siyoŋ|Siyoŋ:\\w* seen réew,"},{"style":"q1","verse":"","text":"danoo meloon ni ñuy gént,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"di reeka sarxolle rekk."},{"style":"q1","verse":"","text":"Yéefar yi naa:"},{"style":"q1","verse":"","text":"«Lu réy la Aji Sax ji defal ñii.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Lu réy la nu Aji Sax ji defal moos,"},{"style":"q1","verse":"","text":"nu bég."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Aji Sax ji, rikk delloo nu na woon,"},{"style":"q1","verse":"","text":"ni ngay delloo wal yu ŋiis, mu walaat ca àllub Negew."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Kuy ji di jooy,"},{"style":"q1","verse":"","text":"yal na góob, di sarxolle."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ku doon wéy a wéy, di jooy;"},{"style":"q1","verse":"","text":"yor mbuusum jiwoom,"},{"style":"q1","verse":"","text":"yal na dikk a dikk, di woy,"},{"style":"q1","verse":"","text":"ñibbiwaale sabaaram."}]