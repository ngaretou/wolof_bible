[{"style":"s1","text":"Ma bàkk Aji Sax ji"},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jeey buur, soloo daraja;"},{"style":"q1","text":"Aji Sax jee solu, gañoo doole:"},{"style":"q1","text":"àddinaa ngii, dëju te raful."},{"style":"q1","text":""},{"style":"v","text":"Sab jal a masa sax,"},{"style":"q1","text":"te yaa masa nekk."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, géej a riir,"},{"style":"q1","text":"géej a àddu, mu riir,"},{"style":"q1","text":"gannax ya rëkk, mu riir."},{"style":"q1","text":""},{"style":"v","text":"Waaye riirum wal mu walangaanoo,"},{"style":"q1","text":"ak géej gu sàmbaraaxoo,"},{"style":"q1","text":"Aji Sax ja fa kaw a rawati!"},{"style":"q1","text":""},{"style":"v","text":"Say santaanee wér te wóor;"},{"style":"q1","text":"Aji Sax ji, sellaay la sa kër jagoo ba fàww."}]