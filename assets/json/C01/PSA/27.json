[{"style":"s1","text":"Xare jib, ma ànd ak Yàlla"},{"style":"d","text":""},{"style":"v","text":"Ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":"Aji Sax jee may leeral, di ma wallu."},{"style":"q1","text":"Ana ku may ragal?"},{"style":"q1","text":"Aji Sax jeey aar sama bakkan."},{"style":"q1","text":"Ana ku may tiit?"},{"style":"q1","text":""},{"style":"v","text":"Ab soxor wutsi ma,"},{"style":"q1","text":"nar maa yàpp,"},{"style":"q1","text":"noonoo ma, bañ ma;"},{"style":"q1","text":"mooy tërëf, fëlëñu."},{"style":"q1","text":""},{"style":"v","text":"Bu gàngoor booloo sama kaw,"},{"style":"q1","text":"duma raf."},{"style":"q1","text":"Ab xare jib,"},{"style":"q1","text":"ma ànd ak Yàlla."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Lenn laa ñaan Aji Sax ji,"},{"style":"q1","text":"lii laay sàkku:"},{"style":"q1","text":"dëkk kër Aji Sax ji"},{"style":"q1","text":"sama giiru dund,"},{"style":"q1","text":"di niir taaru Aji Sax ji,"},{"style":"q1","text":"te yéemoo këram."},{"style":"q1","text":""},{"style":"v","text":"Bésub fitna, mu yiire ma mbaaram,"},{"style":"q1","text":"làq ma, ma làqoo xaymaam,"},{"style":"q1","text":"aj ma ciw doj, ma raw."},{"style":"q1","text":""},{"style":"v","text":"Ma doxe fa siggi,"},{"style":"q1","text":"tiim noon yi ma wër,"},{"style":"q1","text":"di sarxali fa xaymaam saraxi sarxolle."},{"style":"q1","text":"Naa woyal Aji Sax ji, joobe ko."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Éy Aji Sax ji, déglul, ma woote!"},{"style":"q1","text":"Ngalla baaxe ma, nangul ma."},{"style":"q1","text":""},{"style":"v","text":"Yaw la sam xel ne ma, ma jaamu la,"},{"style":"q1","text":"te it yaw Aji Sax ji laay jaamu."},{"style":"q1","text":""},{"style":"v","text":"Sang bi, bu ma làqu,"},{"style":"q1","text":"bul mer, jañax ma."},{"style":"q1","text":"Yaa ma daa wallu."},{"style":"q1","text":"Bu ma wacc, bu ma ba,"},{"style":"q1","text":"yaay Yàlla mi may musal."},{"style":"q1","text":""},{"style":"v","text":"Ndey ak baay man nañu maa wacc,"},{"style":"q1","text":"Aji Sax ji fat ma."},{"style":"q1","text":""},{"style":"v","text":"Éy Aji Sax ji, joxoñ ma saw yoon,"},{"style":"q1","text":"jaarale ma joor gu wóor,"},{"style":"q1","text":"ngir ñi may tëru."},{"style":"q1","text":""},{"style":"v","text":"Bu ma bàyyee bànneexu noon."},{"style":"q1","text":"Seedey naaféq a ma jógal,"},{"style":"q1","text":"di sos fitna."},{"style":"q1","text":""},{"style":"v","text":"Maay dajeek ngëneelu Aji Sax ji"},{"style":"q1","text":"fi kaw suuf sii."},{"style":"q1","text":"Lii wóor na ma."},{"style":"q1","text":""},{"style":"v","text":"Yaakaaral Aji Sax ji,"},{"style":"q1","text":"dëgërlul te amu fit,"},{"style":"q1","text":"te yaakaarati Aji Sax ji."}]