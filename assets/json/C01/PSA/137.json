[{"style":"s1","verse":"","text":"Jooye nanu àll"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ca tàkkal dexi Babilon,"},{"style":"q1","verse":"","text":"foofa lanu daa toog,"},{"style":"q1","verse":"","text":"fàttaliku \\w Siyoŋ|Siyoŋ:\\w*, jooy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ca garab ya"},{"style":"q1","verse":"","text":"lanu daa wékk sunuy xalam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Fa la nu sunuy sang daa woyloo,"},{"style":"q1","verse":"","text":"noot nu, bëgg nu bégal leen,"},{"style":"q1","verse":"","text":"naan: «Woyalleen nu ci woyi Siyoŋ.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Moo! Nu nuy woye woyu Aji Sax ji"},{"style":"q1","verse":"","text":"kaw suufas yéefar?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Éy Yerusalem, su ma la naree fàtte,"},{"style":"q1","verse":"","text":"yal na ma doole dëddu!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yerusalem, su ma la fàttalikuwul,"},{"style":"q1","verse":"","text":"fonk la, ba gën laa bége lépp,"},{"style":"q1","verse":"","text":"yal na sama làmmiñ tafoo sama denqleñ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Aji Sax ji, fàttalikul Edomeen ña,"},{"style":"q1","verse":"","text":"keroog jantub Yerusalem,"},{"style":"q1","verse":"","text":"ba ñu naan: «Màbbleen a màbb,"},{"style":"q1","verse":"","text":"ba suuf sa dëkk bi sampe siiñ!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yeen waa Babilon gi ñu nara fàllas,"},{"style":"q1","verse":"","text":"ndokkalee ku leen fey jëf ja ngeen nu def!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ndokkalee ku ne cas seeni fere,"},{"style":"q1","verse":"","text":"tas ciw doj!"}]