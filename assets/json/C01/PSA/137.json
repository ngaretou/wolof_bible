[{"style":"s1","text":"Jooye nanu àll"},{"style":"q1","text":""},{"style":"v","text":"Ca tàkkal dexi Babilon,"},{"style":"q1","text":"foofa lanu daa toog,"},{"style":"q1","text":"fàttaliku \\w Siyoŋ|Siyoŋ:\\w*, jooy."},{"style":"q1","text":""},{"style":"v","text":"Ca garab ya"},{"style":"q1","text":"lanu daa wékk sunuy xalam."},{"style":"q1","text":""},{"style":"v","text":"Fa la nu sunuy sang daa woyloo,"},{"style":"q1","text":"noot nu, bëgg nu bégal leen,"},{"style":"q1","text":"naan: «Woyalleen nu ci woyi Siyoŋ.»"},{"style":"q1","text":""},{"style":"v","text":"Moo! Nu nuy woye woyu Aji Sax ji"},{"style":"q1","text":"kaw suufas yéefar?"},{"style":"q1","text":""},{"style":"v","text":"Éy Yerusalem, su ma la naree fàtte,"},{"style":"q1","text":"yal na ma doole dëddu!"},{"style":"q1","text":""},{"style":"v","text":"Yerusalem, su ma la fàttalikuwul,"},{"style":"q1","text":"fonk la, ba gën laa bége lépp,"},{"style":"q1","text":"yal na sama làmmiñ tafoo sama denqleñ."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, fàttalikul Edomeen ña,"},{"style":"q1","text":"keroog jantub Yerusalem,"},{"style":"q1","text":"ba ñu naan: «Màbbleen a màbb,"},{"style":"q1","text":"ba suuf sa dëkk bi sampe siiñ!»"},{"style":"q1","text":""},{"style":"v","text":"Yeen waa Babilon gi ñu nara fàllas,"},{"style":"q1","text":"ndokkalee ku leen fey jëf ja ngeen nu def!"},{"style":"q1","text":""},{"style":"v","text":"Ndokkalee ku ne cas seeni fere,"},{"style":"q1","text":"tas ciw doj!"}]