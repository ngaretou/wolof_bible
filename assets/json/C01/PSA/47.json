[{"style":"s1","text":"Yàllaay Buur"},{"style":"d","text":""},{"style":"v","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel askanu Kore wu góor."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Na xeetoo xeet tàccu,"},{"style":"q1","text":"xaacu, sarxolleel Yàlla!"},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, Aji Kawe jee jara ragal,"},{"style":"q1","text":"mooy buur bu mag bi tiim àddina sépp."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Moo nu nangulal xeet yi,"},{"style":"q1","text":"nu teg tànk gàngoor yi."},{"style":"q1","text":""},{"style":"v","text":"Moo nu tànnal céru suuf,"},{"style":"q1","text":"muy sagu askanu \\w Yanqóoba|Yanqóoba:\\w* wii mu sopp. \\qs Selaw.\\qs*"},{"style":"q1","text":""},{"style":"v","text":"Yàllaa yéeg, kàddu riir."},{"style":"q1","text":"Aji Sax jee yéeg, bufta\\f + \\fr 47.6 \\fk bufta, \\ft ab béjjénu kuuy la, bu ñu bënn ñaari wet yi, ngir man cee wal.\\f* jolli."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Woyleen Yàlla, woyleen,"},{"style":"q1","text":"woyleen Yàlla sunu buur, woyleen."},{"style":"q1","text":""},{"style":"v","text":"Yàllaay buuru àddina sépp;"},{"style":"q1","text":"taalifleen, woy ko."},{"style":"q1","text":""},{"style":"v","text":"Yàllaa di buuru xeet yi,"},{"style":"q1","text":"Yàllaa tooge jalam bu \\w sell|sell:\\w*."},{"style":"q1","text":""},{"style":"v","text":"Kàngami xeet yeey daje,"},{"style":"q1","text":"ànd ak ñoñi Yàllay Ibraayma;"},{"style":"q1","text":"Yàllaa moom buur yi yiir àddina,"},{"style":"q1","text":"te moo kawee kawe."}]