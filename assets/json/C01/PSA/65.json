[{"style":"s1","text":"Yàllaay nangul suuf"},{"style":"d","text":""},{"style":"v","text":"Mu jëm ci njiital woykat yi, di woyu cant, ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Éy Yàlla, yaa jagoo cant fa \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","text":"yaw lees di jëfeel ab dige."},{"style":"q1","text":""},{"style":"v","text":"Yaw miy nangug ñaan,"},{"style":"q1","text":"yaw la képp di wuyusi."},{"style":"q1","text":""},{"style":"v","text":"Samay ñaawtéef man na ma,"},{"style":"q1","text":"waaye sunuy tooñ, yaa nu koy baal."},{"style":"q1","text":""},{"style":"v","text":"Ndokkalee koo tànn, woo ko,"},{"style":"q1","text":"mu dale say ëtt,"},{"style":"q1","text":"di xéewloo sa ngëneeli kër,"},{"style":"q1","text":"ak sa sellaayu dëkkuwaay."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Defal nga nuy kéemaan,"},{"style":"q1","text":"nangug ñaan ci njekk,"},{"style":"q1","text":"yaw Yàlla mi nuy wallu,"},{"style":"q1","text":"di yaakaaru àddina wërngal këpp,"},{"style":"q1","text":"ba ca géej gu sore ga."},{"style":"q1","text":""},{"style":"v","text":"Yaa dëje tund yu mag yi doole,"},{"style":"q1","text":"gañoo sa kàttan,"},{"style":"q1","text":""},{"style":"v","text":"di dalal riirum géej aki gannaxam,"},{"style":"q1","text":"ak coowal xeet yi,"},{"style":"q1","text":""},{"style":"v","text":"ba waa cati àddina yéemoo say firnde,"},{"style":"q1","text":"waa penkook sowu di sarxolle."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yaa dikke suuf am wal,"},{"style":"q1","text":"nangul koo nangul."},{"style":"q1","text":"Dexu Yàllaa fees ndox,"},{"style":"q1","text":"ngay may nit peppam,"},{"style":"q1","text":"ni nga ko waajale suuf si,"},{"style":"q1","text":""},{"style":"v","text":"di suuxat ngàbb mi,"},{"style":"q1","text":"maasalee ko jal yi,"},{"style":"q1","text":"sottiy waame, mu mokk,"},{"style":"q1","text":"cax ma, nga barkeel."},{"style":"q1","text":""},{"style":"v","text":"Yaay tëje mbeyum at mi say ngëneel,"},{"style":"q1","text":"foo jaare, xéewal walangaan."},{"style":"q1","text":""},{"style":"v","text":"Parluy màndiŋ yaa ngi walangaan,"},{"style":"q1","text":"li wër tund yu ndaw yi di xumbéen."},{"style":"q1","text":""},{"style":"v","text":"Joor gaa ngi, jur gi muur,"},{"style":"q1","text":"xur yi sàngoo pepp,"},{"style":"q1","text":"lépp di xaacooka woy."}]