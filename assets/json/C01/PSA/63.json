[{"style":"s1","verse":"","text":"Maa namm Aji Sax ji"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Dib taalifu cant, ñeel Daawuda. Mooy ba mu nekkee ca màndiŋu Yuda\\f + \\fr 63.1 \\ft Seetal ci \\bk 1.Samiyel\\bk* 23.14; 24.2; \\bk 2.Samiyel\\bk* 15.23.\\f*."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yaw Yàlla, yaay sama Yàlla,"},{"style":"q1","verse":"","text":"ma xëy di la wër,"},{"style":"q1","verse":"","text":"sama bakkan mar la,"},{"style":"q1","verse":"","text":"sama jëmm namm la,"},{"style":"q1","verse":"","text":"fu suuf mare ndox, ne sereŋ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Moo tax kër gu sell ga laa la séentoo,"},{"style":"q1","verse":"","text":"ngir niir sa doole ak sa leer."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Xéewloo sa ngor moo dàq dundu bakkan;"},{"style":"q1","verse":"","text":"ma àddu boog, kañ la!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Dama lay sant, sama giiru dund kay,"},{"style":"q1","verse":"","text":"tàllal la ay loxo, sàbbaale la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Sama xol di sedd, ni ku ñam wu duun suural,"},{"style":"q1","verse":"","text":"ma bég, sarxolle, màggal la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Su ma tëddee, fàttaliku la,"},{"style":"q1","verse":"","text":"waxtuw guddi dama lay xalaat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yaay sama ndimbal,"},{"style":"q1","verse":"","text":"may làqoo sa kiiraay,"},{"style":"q1","verse":"","text":"di sarxolle."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Sama xol taq la,"},{"style":"q1","verse":"","text":"nga téyee ma sa ndijoor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Waaye ñiy wuta faat sama bakkan"},{"style":"q1","verse":"","text":"ñoo jëm xóotey njaniiw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ñu jébbal leen ñawkay saamar,"},{"style":"q1","verse":"","text":"till séddoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Buur moom, di bége Yàlla;"},{"style":"q1","verse":"","text":"képp ku koy giñe di ko kañoo,"},{"style":"q1","verse":"","text":"fen-kat ne xerem."}]