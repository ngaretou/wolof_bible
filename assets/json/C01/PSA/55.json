[{"style":"s1","verse":"","text":"Xarit wor na ma"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, ànd aki xalam, dib taalifu yeete, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Éy Yàlla, déglul, ma ñaan la,"},{"style":"q1","verse":"","text":"bul làqu, dama lay dagaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Teewlu ma, nangul ma."},{"style":"q1","verse":"","text":"Li may tawat a ma xañ jàmm, ub sama bopp;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"coowal noon yee"},{"style":"q1","verse":"","text":"ak coonob ñu bon ñi."},{"style":"q1","verse":"","text":"Dañu maa teg fitna,"},{"style":"q1","verse":"","text":"di ma songe xadar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sama fit a ngi rëcc, di tëf-tëfi;"},{"style":"q1","verse":"","text":"tiitaangey dee ne ma taral."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Njàqaree ma dugg, may kat-kati,"},{"style":"q1","verse":"","text":"musiba mëdd ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Dama ne: «Éy ku may may laafi xati,"},{"style":"q1","verse":"","text":"ma ne fërr, noppaluji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Xanaa ma naaw, ba sore,"},{"style":"q1","verse":"","text":"fanaani màndiŋ ma. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Naa gaawtu, seeluji"},{"style":"q1","verse":"","text":"ngelaw lu riddi ak ngëlén.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Éy, Boroom bi, beenal seeni wax, neenal leen."},{"style":"q1","verse":"","text":"Damaa gis fitnaak xuloo ci dëkk bi,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"guddeek bëccëg ñuy wër,"},{"style":"q1","verse":"","text":"ñaawtéef ak ayib wéttali leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yàq a nga ca biir,"},{"style":"q1","verse":"","text":"te nooteel ak njublaŋ jógul ca pénc ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Su doon noon bu may ñaawal,"},{"style":"q1","verse":"","text":"ma muñ ko,"},{"style":"q1","verse":"","text":"mbaa muy bañ bu may won ab dayo,"},{"style":"q1","verse":"","text":"ma daw ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Waaye yaw la, sama nawle,"},{"style":"q1","verse":"","text":"di sama wóllëre, ma xam la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Nu daan bokk bànneexu diisoo,"},{"style":"q1","verse":"","text":"ànd ak mbooloo, di jaamuji kër Yàlla ga."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Yal na leen dee bett,"},{"style":"q1","verse":"","text":"ñu jekki tàbbi njaniiw,"},{"style":"q1","verse":"","text":"ngir mbon ga ci seen biir ak seen dëkkuwaay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Waaye man, Yàlla laay woo wall,"},{"style":"q1","verse":"","text":"te Aji Sax jee may wallu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Subaak ngoon ak njolloor,"},{"style":"q1","verse":"","text":"may ñaxtooka ñurumtu,"},{"style":"q1","verse":"","text":"mu dégg ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Moo may \\w jot|jot:\\w* ak jàmm,"},{"style":"q1","verse":"","text":"ba ma mucc ci xare ya ma ñu bare di songe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Yàllaay dégg, duma leen,"},{"style":"q1","verse":"","text":"moom mi nguuram yàgga sax. \\qs Selaw.\\qs*"},{"style":"q1","verse":"","text":"Soppikuwuñu fenn te ragaluñu Yàlla."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Workat baa ngi xàcc loxoom jëme ci ñi mu takktool,"},{"style":"q1","verse":"","text":"di fecci \\w kóllëre|kóllëre g-:\\w*,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"làmmiñ wa daa neex ni lem,"},{"style":"q1","verse":"","text":"te xare lal xol ba;"},{"style":"q1","verse":"","text":"kàddu ya gëna nooy ag oliw"},{"style":"q1","verse":"","text":"te di saamar bu génn mbaram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Sippikul Aji Sax ji lu la diis,"},{"style":"q1","verse":"","text":"moom moo lay taxawu."},{"style":"q1","verse":"","text":"Du bàyyi mukk aji jub, muy tërëf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Waaye yaw Yàlla, yaa leen di tàbbal,"},{"style":"q1","verse":"","text":"bóomkat ak naaféq ci pax mu xóota xóot,"},{"style":"q1","verse":"","text":"te seen dund xaajul."},{"style":"q1","verse":"","text":"Man nag, ma di la wóolu."}]