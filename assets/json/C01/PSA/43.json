[{"style":"s1","verse":"","text":"Éy Yàlla, àtte ma"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Éy Yàlla, àtte ma,"},{"style":"q1","verse":"","text":"layool maak xeet wu gorewul,"},{"style":"q1","verse":"","text":"xettali maak ku njublaŋ te dëng."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yaw Yàlla kat yaay sama rawtu."},{"style":"q1","verse":"","text":"Lu tax nga wacc ma,"},{"style":"q1","verse":"","text":"may wéye tiis,"},{"style":"q1","verse":"","text":"ci nooteelu noon?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ngalla yónneel sag leer ak sa dëgg,"},{"style":"q1","verse":"","text":"yal na ma yooyu jiite,"},{"style":"q1","verse":"","text":"yóbbu ma sa tund wu sell,"},{"style":"q1","verse":"","text":"sa màkkaan ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Kon Yàlla, naa àgg ca sa \\w sarxalukaay|sarxalukaay b-:\\w*;"},{"style":"q1","verse":"","text":"yaw Yàlla, yaay sama mbégte, di sama bànneex."},{"style":"q1","verse":"","text":"Yaw Yàlla, sama Yàlla,"},{"style":"q1","verse":"","text":"naa la màggale xalam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Moo man, lu may xàddi, ne yogg?"},{"style":"q1","verse":"","text":"Lu may jàq nii?"},{"style":"q1","verse":"","text":"Tee maa yaakaar Yàlla, ba santati ko,"},{"style":"q1","verse":"","text":"moom sama Yàlla mi may wallu?"}]