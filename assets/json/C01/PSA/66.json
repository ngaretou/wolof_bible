[{"style":"s1","verse":"","text":"Yàllaay aar nitam"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, di woyu cant."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Yeen waa àddina sépp, sarxolleelleen Yàlla,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"woyleen màggaayu turam,"},{"style":"q1","verse":"","text":"darajaalleen cantam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Neleen Yàlla: «Yaaka yéemey jaloore!"},{"style":"q1","verse":"","text":"Sa doolee bare, ba say noon rasu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"àddina sépp di la sujjóotal,"},{"style":"q1","verse":"","text":"di la woy, di joobe sa tur.» \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Dikkleen gis li Yàlla def,"},{"style":"q1","verse":"","text":"jëf la ju yéeme, ñeel doom aadama yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Moo soppi géej ag joor,"},{"style":"q1","verse":"","text":"maam ya dox jàll."},{"style":"q1","verse":"","text":"Nanu ko bége foofa."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Mooy nguuroo njàmbaaram ba fàww,"},{"style":"q1","verse":"","text":"di topp xeet yeek bët."},{"style":"q1","verse":"","text":"Ku koy diiŋat, yàlla boo damu! \\qs Selaw.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yeen xeet yi, santleen sunu Yàlla,"},{"style":"q1","verse":"","text":"te biral cantam!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Moo sàmm sunu bakkan,"},{"style":"q1","verse":"","text":"te waccu nu, nu tërëf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Éy Yàlla, nattu nga nu,"},{"style":"q1","verse":"","text":"xelli nu, ni ñuy xellee xaalis;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"laaw nga nu,"},{"style":"q1","verse":"","text":"teg nu njàqare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"May nga noon noot nu,"},{"style":"q1","verse":"","text":"nu jaare sawara, jaarem ndox;"},{"style":"q1","verse":"","text":"nga génne nu, yaatal nu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Maay duggaale sa kër ay \\w saraxi rendi-dóomal|Sarax yi:\\w*,"},{"style":"q1","verse":"","text":"defal la samay dige"},{"style":"q2","verse":"","text":""},{"style":"v","verse":"14","text":"yi ma waxoon ci sama gémmiñ,"},{"style":"q1","verse":"","text":"sama làmmiñ tudd ko, ba ma jàqee."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Jurug yafal, ma defal la saraxu rendi-dóomal,"},{"style":"q1","verse":"","text":"saxaar sa jollee ca kuuyu sarax ya,"},{"style":"q1","verse":"","text":"ma boole ca saraxu nag ak sikket. \\qs Selaw.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Képp ku ragal Yàlla dikkal, déglu,"},{"style":"q1","verse":"","text":"ma limal la li mu ma defal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Moom laa woo wall,"},{"style":"q1","verse":"","text":"àddu, màggal ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Su ma naroon ñaawtéef,"},{"style":"q1","verse":"","text":"Boroom bi du ma déglu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Waaye Yàlla dégg na ma,"},{"style":"q1","verse":"","text":"teewlu na samag ñaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Cant ñeel na Yàlla,"},{"style":"q1","verse":"","text":"gàntuwul samay ñaan,"},{"style":"q1","verse":"","text":"xañu ma ngoram."}]