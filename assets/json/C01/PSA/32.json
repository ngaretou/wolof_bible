[{"style":"s1","verse":"","text":"Njéggal barke la"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Dib taalifu yeete, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Ndokkalee yaw mi ñu baal ag moy,"},{"style":"q1","verse":"","text":"jéggal la bàkkaar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ndokkalee yaw mi Aji Sax ji toppul say tooñ,"},{"style":"q1","verse":"","text":"te amoo xelum njublaŋ it."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ba ma nee cell,"},{"style":"q1","verse":"","text":"damaa ràgg,"},{"style":"q1","verse":"","text":"dëkke naqar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Guddeek bëccëg"},{"style":"q1","verse":"","text":"sa loxo diis gann ci man."},{"style":"q1","verse":"","text":"Sama doole ŋiis"},{"style":"q1","verse":"","text":"ni ndox mu jaas upp. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ma xamal la ne moy naa,"},{"style":"q1","verse":"","text":"làqatuma la sama ñaawtéef."},{"style":"q1","verse":"","text":"Ma ne: «Naa àgge Aji Sax ji sama bàkkaar.»"},{"style":"q1","verse":"","text":"Yaw nga baal ma peyug tooñ. \\qs Selaw.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Nii la, kuy aji gëm di ñaane,"},{"style":"q1","verse":"","text":"bu ñaan jotee."},{"style":"q1","verse":"","text":"Mbënn mu baawaan, di wal sax,"},{"style":"q1","verse":"","text":"du ko laal fenn."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yaw laay làqoo,"},{"style":"q1","verse":"","text":"nga di ma aar ciw ay,"},{"style":"q1","verse":"","text":"may ma ba ñi ma wër, di ko bége. \\qs Selaw.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Nga ne ma: «Dama laa xelal,"},{"style":"q1","verse":"","text":"xamal la fi ngay jaar,"},{"style":"q1","verse":"","text":"digal la, di la bàyyi xel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Buleen mel ni fas mbaa berkelle"},{"style":"q1","verse":"","text":"mi amul ag dégg,"},{"style":"q1","verse":"","text":"laab ak tarka di ko téye,"},{"style":"q1","verse":"","text":"mu doora yem fi seen wet.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ku bon barew tiis."},{"style":"q1","verse":"","text":"Ku wóolu Aji Sax ji,"},{"style":"q1","verse":"","text":"mu yiire la ngoram."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Yaw aji jub ji, bégal te bànneexoo Aji Sax ji."},{"style":"q1","verse":"","text":"Képp kuy def njub, sarxolleel!"}]