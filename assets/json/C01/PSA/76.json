[{"style":"s1","verse":"","text":"Yàllaa moom xare"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, ànd aki xalam, di woyu cant, ñeel Asaf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yàlla siiw na fa Yuda,"},{"style":"q1","verse":"","text":"aw turam màgg fa Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Xaymaam a nga Salem,"},{"style":"q1","verse":"","text":"di xuntam ma fa \\w Siyoŋ|Siyoŋ:\\w*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Fa la rajaxee fitt yuy boy"},{"style":"q1","verse":"","text":"ak pakk ak saamar ak ngànnaayal xare. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Yàlla yaa ne ràññ te darajawu,"},{"style":"q1","verse":"","text":"ba raw tundi rëbbkat yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Futtees nay boroom fit,"},{"style":"q1","verse":"","text":"ñu ne nërëm, nelaw;"},{"style":"q1","verse":"","text":"kuy ñeyi xare, say loxo nasax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yaw Yàllay \\w Yanqóoba|Yanqóoba:\\w*, yaa gëdd,"},{"style":"q1","verse":"","text":"gawar ak fasam nelaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yaw mii, yaa mata ragal;"},{"style":"q1","verse":"","text":"boo meree, ana kuy taxaw fi sa kanam?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Fa asamaan nga biraleb àtte;"},{"style":"q1","verse":"","text":"suuf tiit, ne cell."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ca la Yàllay jóg, ngir àtte,"},{"style":"q1","verse":"","text":"ba wallu kuy néew dooley àddina. \\qs Selaw.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Doom aadamaak xadaram rafetal sa woy;"},{"style":"q1","verse":"","text":"desu xadaram, nga gañoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Seen Yàlla Aji Sax ji, digooleen ak moom"},{"style":"q1","verse":"","text":"te jëfe ko."},{"style":"q1","verse":"","text":"Kee jara ragal."},{"style":"q1","verse":"","text":"Na ñi ko wër ñépp indi teraanga."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Mooy sàggi sagu kilifa yi,"},{"style":"q1","verse":"","text":"di tiitaangey buuri àddina."}]