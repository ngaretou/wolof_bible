[{"style":"s1","verse":"","text":"Naa woote wall"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, ànd aki xalam, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ngalla Yàlla, déglul, maa ne wallóoy,"},{"style":"q1","verse":"","text":"teewlul, ma dagaan la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Àll laa lay woowe,"},{"style":"q1","verse":"","text":"sama xol jeex;"},{"style":"q1","verse":"","text":"yéege ma doju rawtu wu ma jotul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yaw laa làqoo,"},{"style":"q1","verse":"","text":"ngay tata ju dëgër, fegal ma noon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Naa dëkke sa xayma ba fàww;"},{"style":"q1","verse":"","text":"làqoo sa kiiraay. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yàlla, yaa dégg samay dige,"},{"style":"q1","verse":"","text":"sédd ma céru ñi wormaal sa tur."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ngalla féqal fani Buur,"},{"style":"q1","verse":"","text":"ay atam bare, sët ba sëtaat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Éy Yàlla, yal na Buur toog ba fàww fi sa kanam,"},{"style":"q1","verse":"","text":"nga sédd ko sa ngor ak sa worma, mu fegoo;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"kon ma woy la ba fàww,"},{"style":"q1","verse":"","text":"di jëfe samay dige bésoo bés."}]