[{"style":"s1","text":"Lu Yàlla namm lay def"},{"style":"q1","text":""},{"style":"v","text":"Màggalleen Ki Sax!"},{"style":"q1","text":"Màggalleen turu Aji Sax ji,"},{"style":"q1","text":"éey jaami Aji Sax ji, màggalleen,"},{"style":"q1","text":""},{"style":"v","text":"yeen ñi taxaw ci kër Aji Sax ji,"},{"style":"q1","text":"fi sunu biir ëttu Yàlla bi."},{"style":"q1","text":""},{"style":"v","text":"Màggalleen Ki Sax, Aji Sax jee baax!"},{"style":"q1","text":"Kañleen ko, kee sopplu!"},{"style":"q1","text":""},{"style":"v","text":"Ki Sax a tànn \\w Yanqóoba|Yanqóoba:\\w*,"},{"style":"q1","text":"muy Israayil mi mu séddoo."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Maa xam ne Aji Sax ji màgg na!"},{"style":"q1","text":"Sunu Boroom a sut lépp lu ñuy jaamu."},{"style":"q1","text":""},{"style":"v","text":"Lépp lu Aji Sax ji namm,"},{"style":"q1","text":"moom lay def asamaan ak suuf,"},{"style":"q1","text":"ak biir géej ak xóote yépp."},{"style":"q1","text":""},{"style":"v","text":"Mooy yékkateey niir fa cati àddina,"},{"style":"q1","text":"sàkkal taw ay melax,"},{"style":"q1","text":"yebbee ngelaw cay dencam,"},{"style":"q1","text":""},{"style":"v","text":"faat luy taaw fa Misra,"},{"style":"q1","text":"doom aadama, ba ci mala."},{"style":"q1","text":""},{"style":"v","text":"Yeen waa Misra, moo yónnee ay firndeeki kéemaan fi seen biir,"},{"style":"q1","text":"ñeel Firawnaak jawriñam ñépp."},{"style":"q1","text":""},{"style":"v","text":"Moo duma ay xeeti xeet,"},{"style":"q1","text":"bóom buur yu mag yi,"},{"style":"q1","text":""},{"style":"v","text":"muy Sixon buurub Amoreen ña,"},{"style":"q1","text":"ak Og buuru Basan,"},{"style":"q1","text":"ak buuri Kanaan yépp."},{"style":"q1","text":""},{"style":"v","text":"Moo nangu seen réew, séddale,"},{"style":"q1","text":"\\w bànni Israayil|bànni Israayil:\\w* ñiy ñoñam séddoo."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, sa tur ba fàww;"},{"style":"q1","text":"Aji Sax ji, sa woy a saxal maasoo maas."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji kay ay àtte ñoñam ñi,"},{"style":"q1","text":"di ñeewante jaamam ñi."},{"style":"q1","text":""},{"style":"v","text":"Tuuri yéefar yi, xaalis laak wurus,"},{"style":"q1","text":"loxoo ko sàkk."},{"style":"q1","text":""},{"style":"v","text":"Am gémmiñ te du wax,"},{"style":"q1","text":"ami gët te du gis,"},{"style":"q1","text":""},{"style":"v","text":"ami nopp te du dégg,"},{"style":"q1","text":"du sax noowal bakkan."},{"style":"q1","text":""},{"style":"v","text":"Ñi sàkk yii tuur, yal nañu mel ni ñoom,"},{"style":"q1","text":"ñook ñi leen wóolu ñépp."},{"style":"q1","text":""},{"style":"v","text":"Éey waa kër Israayil, santleen Aji Sax ji!"},{"style":"q1","text":"Éey waa kër \\w Aaróona|Aaróona:\\w*, santleen Aji Sax ji!"},{"style":"q1","text":""},{"style":"v","text":"Éey waa kër \\w Lewi|Leween b-:\\w*, santleen Aji Sax ji!"},{"style":"q1","text":"Yeen ñi ragal Aji Sax ji, santleen Aji Sax ji!"},{"style":"q1","text":""},{"style":"v","text":"Nañuy sant Aji Sax ji ci \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","text":"moom mi dëkke Yerusalem."},{"style":"q1","text":"Màggalleen Ki Sax!"}]