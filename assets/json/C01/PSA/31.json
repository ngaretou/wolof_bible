[{"style":"s1","text":"Naa siggi, noon sëgg"},{"style":"d","text":""},{"style":"v","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, yaw laa làqoo,"},{"style":"q1","text":"yàlla buma rus mukk."},{"style":"q1","text":"Yiwee ma sa àtteb dëgg."},{"style":"q1","text":""},{"style":"v","text":"Teewlu ma,"},{"style":"q1","text":"gaawe ma,"},{"style":"q1","text":"Yal nangay sama cëslaay, sama rawtu."},{"style":"q1","text":"Aar ma, ma mucc."},{"style":"q1","text":""},{"style":"v","text":"Yaay sama cëslaay, sama rawtu."},{"style":"q1","text":"Jiite ma, wommat ma ngir sa tur."},{"style":"q1","text":""},{"style":"v","text":"Génne ma fiir gi ñu may tëroo;"},{"style":"q1","text":"yaw yaa di sama rawtu."},{"style":"q1","text":""},{"style":"v","text":"Yaw laa jébbal samag noo."},{"style":"q1","text":"Aji Sax ji, Yàlla miy dëgg, ngalla jot ma."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Maa bañ kuy topp tuuri neen."},{"style":"q1","text":"Man, Aji Sax ji laa wóolu."},{"style":"q1","text":""},{"style":"v","text":"Damay bég, di bànneexoo sa ngor,"},{"style":"q1","text":"li nga xoole samaw tiis."},{"style":"q1","text":"Ràññee nga sama njàqarey xol."},{"style":"q1","text":""},{"style":"v","text":"Waccoo ma ci loxol noon,"},{"style":"q1","text":"danga maa taxawal fu ma yaatoo."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Éy Aji Sax ji, baaxe ma, jàq naa!"},{"style":"q1","text":"Samay gët a ngi giim ndaxu tiis,"},{"style":"q1","text":"xol jeex, yaram jeex."},{"style":"q1","text":""},{"style":"v","text":"Naqar a ngi may rey,"},{"style":"q1","text":"tiis di wàññi samay fan,"},{"style":"q1","text":"sama doole ŋiis ndax sama njubadi,"},{"style":"q1","text":"samay yax semm."},{"style":"q1","text":""},{"style":"v","text":"Noonoo noon a ma sewal,"},{"style":"q1","text":"dëkkandoo waxi noppi,"},{"style":"q1","text":"xame seexlu ma,"},{"style":"q1","text":"ku ma gis biti, di ma daw."},{"style":"q1","text":""},{"style":"v","text":"Kenn xalaatu ma, xanaa fàtte ma ni ku dee,"},{"style":"q1","text":"ma mel ni ndab lu yàqu."},{"style":"q1","text":""},{"style":"v","text":"Damaa dégg jëw yu bare,"},{"style":"q1","text":"lu raglu dar ma kepp."},{"style":"q1","text":"Ñu ngi mànkoo, di ma fexeel,"},{"style":"q1","text":"di wut sama bakkan."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Man nag Aji Sax ji, ci yaw laa dénku."},{"style":"q1","text":"Ma ne: «Yaay sama Yàlla.»"},{"style":"q1","text":""},{"style":"v","text":"Yaa yor sama àpp."},{"style":"q1","text":"Xettali maak ku ma noonoo"},{"style":"q1","text":"ak ku may sonal."},{"style":"q1","text":""},{"style":"v","text":"Leeralal ma sa kanam, Sang bi,"},{"style":"q1","text":"walloo ma sa ngor."},{"style":"q1","text":""},{"style":"v","text":"Éy Aji Sax ji, woo naa la wall,"},{"style":"q1","text":"yàlla buma rus."},{"style":"q1","text":"Yal na ku bon rus,"},{"style":"q1","text":"ne xerem biir njaniiw."},{"style":"q1","text":""},{"style":"v","text":"Yal nanga tëj gémmiñu fen-kat"},{"style":"q1","text":"buy wax aji jub waxi reewande,"},{"style":"q1","text":"di réy-réylooka xeebaate."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yàlla yaa yaatu ngëneel"},{"style":"q1","text":"loo dencal ku la ragal,"},{"style":"q1","text":"defal ko ku la làqoo,"},{"style":"q1","text":"doom aadama seede."},{"style":"q1","text":""},{"style":"v","text":"Yaa koy làq fa nga làqu,"},{"style":"q1","text":"fa pexey nit àggul."},{"style":"q1","text":"Nga yiir ko, mu yiiru"},{"style":"q1","text":"ci ayu làmmiñ."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Cant ñeel na Aji Sax ji!"},{"style":"q1","text":"Ndaw kéemtaan ci ngor li mu ma jiwe"},{"style":"q1","text":"biir dëkk bu ñu gaw."},{"style":"q1","text":""},{"style":"v","text":"Man nag saalitoon naa ne:"},{"style":"q1","text":"«Yàlla dagg na ma, sànni!»"},{"style":"q1","text":"Ndeke ba ma lay woo,"},{"style":"q1","text":"dégg nga samay dagaan."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Képp kuy aji gëm, soppal Aji Sax ji."},{"style":"q1","text":"Aji Sax jeey sàmm ku ko gëm,"},{"style":"q1","text":"di yool, ba wis kuy réy-réylu."},{"style":"q1","text":""},{"style":"v","text":"Képp ku yaakaar Aji Sax ji,"},{"style":"q1","text":"dëgërlul te amu fit."}]