[{"style":"s1","text":"Ma ñaan, bésub njàqare"},{"style":"d","text":""},{"style":"v","text":"Mu di ñaanu Daawuda."},{"style":"b","text":""},{"style":"q1","text":"Éy Aji Sax ji, teewlu ma, nangul ma,"},{"style":"q1","text":"damaa ñàkk, néew doole."},{"style":"q1","text":""},{"style":"v","text":"Maa gore, musal ma;"},{"style":"q1","text":"maay sa jaam bi la wóolu,"},{"style":"q1","text":"yaw, sama Yàlla, wallu ma."},{"style":"q1","text":""},{"style":"v","text":"Éy Boroom bi, baaxe ma;"},{"style":"q1","text":"yaw laay yendoo woo wall."},{"style":"q1","text":""},{"style":"v","text":"Boroom bi, bànneexalal sab jaam,"},{"style":"q1","text":"yaw laa dékk sama xol."},{"style":"q1","text":""},{"style":"v","text":"Boroom bi, yaa baax, di baale,"},{"style":"q1","text":"di nàddil sa ngor képp ku lay dagaan."},{"style":"q1","text":""},{"style":"v","text":"Éy Aji Sax ji, teewlul, ma ñaan la;"},{"style":"q1","text":"déglul, ma tinu la."},{"style":"q1","text":""},{"style":"v","text":"Bu ma la woowee bésub njàqare,"},{"style":"q1","text":"yaa may nangul."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Boroom bi, doo moroomu tuur yi,"},{"style":"q1","text":"te jëf melul ni sa jos."},{"style":"q1","text":""},{"style":"v","text":"Boroom bi, mboolem xeet yi nga sàkk"},{"style":"q1","text":"ñoo lay sujjóotalsi,"},{"style":"q1","text":"di màggal sa tur."},{"style":"q1","text":""},{"style":"v","text":"Yaa màgg te yéemey jëf,"},{"style":"q1","text":"yaw doŋŋ yaa di Yàlla."},{"style":"q1","text":""},{"style":"v","text":"Éy Aji Sax ji, won ma sa nammeel,"},{"style":"q1","text":"ma doxe sa dëgg."},{"style":"q1","text":"Tënkal sama xol ci ragal la."},{"style":"q1","text":""},{"style":"v","text":"Boroom bi sama Yàlla,"},{"style":"q1","text":"naa la sante xol bu fees,"},{"style":"q1","text":"màggal la ba fàww."},{"style":"q1","text":""},{"style":"v","text":"Ngor lu réy nga ma won,"},{"style":"q1","text":"sorele maak tëraayu bàmmeel."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Éy Yàlla, nit ñu bew a ma jógal,"},{"style":"q1","text":"def gàngoor gu néeg, di rëbb sama bakkan,"},{"style":"q1","text":"te seetuñu la ci."},{"style":"q1","text":""},{"style":"v","text":"Waaye yaw Boroom bi Yàlla, yaa ñeewante, yërëm,"},{"style":"q1","text":"yaa muñ mer, bare ngor ak worma."},{"style":"q1","text":""},{"style":"v","text":"Geesu ma, baaxe ma."},{"style":"q1","text":"Dooleelal sab jaam,"},{"style":"q1","text":"wallul ki sa jaam sukk jur!"},{"style":"q1","text":""},{"style":"v","text":"Ngalla def ma sa firndey mbaax,"},{"style":"q1","text":"ba bañ yi gis ci, rus,"},{"style":"q1","text":"ndax yaw Aji Sax ji ma dimbali, dëfal ma."}]