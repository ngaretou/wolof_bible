[{"style":"s1","verse":"","text":"Ma ñaan, bésub njàqare"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu di ñaanu Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Éy Aji Sax ji, teewlu ma, nangul ma,"},{"style":"q1","verse":"","text":"damaa ñàkk, néew doole."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Maa gore, musal ma;"},{"style":"q1","verse":"","text":"maay sa jaam bi la wóolu,"},{"style":"q1","verse":"","text":"yaw, sama Yàlla, wallu ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Éy Boroom bi, baaxe ma;"},{"style":"q1","verse":"","text":"yaw laay yendoo woo wall."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Boroom bi, bànneexalal sab jaam,"},{"style":"q1","verse":"","text":"yaw laa dékk sama xol."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Boroom bi, yaa baax, di baale,"},{"style":"q1","verse":"","text":"di nàddil sa ngor képp ku lay dagaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Éy Aji Sax ji, teewlul, ma ñaan la;"},{"style":"q1","verse":"","text":"déglul, ma tinu la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Bu ma la woowee bésub njàqare,"},{"style":"q1","verse":"","text":"yaa may nangul."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Boroom bi, doo moroomu tuur yi,"},{"style":"q1","verse":"","text":"te jëf melul ni sa jos."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Boroom bi, mboolem xeet yi nga sàkk"},{"style":"q1","verse":"","text":"ñoo lay sujjóotalsi,"},{"style":"q1","verse":"","text":"di màggal sa tur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Yaa màgg te yéemey jëf,"},{"style":"q1","verse":"","text":"yaw doŋŋ yaa di Yàlla."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Éy Aji Sax ji, won ma sa nammeel,"},{"style":"q1","verse":"","text":"ma doxe sa dëgg."},{"style":"q1","verse":"","text":"Tënkal sama xol ci ragal la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Boroom bi sama Yàlla,"},{"style":"q1","verse":"","text":"naa la sante xol bu fees,"},{"style":"q1","verse":"","text":"màggal la ba fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ngor lu réy nga ma won,"},{"style":"q1","verse":"","text":"sorele maak tëraayu bàmmeel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Éy Yàlla, nit ñu bew a ma jógal,"},{"style":"q1","verse":"","text":"def gàngoor gu néeg, di rëbb sama bakkan,"},{"style":"q1","verse":"","text":"te seetuñu la ci."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Waaye yaw Boroom bi Yàlla, yaa ñeewante, yërëm,"},{"style":"q1","verse":"","text":"yaa muñ mer, bare ngor ak worma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Geesu ma, baaxe ma."},{"style":"q1","verse":"","text":"Dooleelal sab jaam,"},{"style":"q1","verse":"","text":"wallul ki sa jaam sukk jur!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ngalla def ma sa firndey mbaax,"},{"style":"q1","verse":"","text":"ba bañ yi gis ci, rus,"},{"style":"q1","verse":"","text":"ndax yaw Aji Sax ji ma dimbali, dëfal ma."}]