[{"style":"s1","text":"Ma waajal ndajem kër Aji Sax ji"},{"style":"d","text":""},{"style":"v","text":"Ñeel Daawuda."},{"style":"b","text":""},{"style":"q1","text":"Aji Sax ji, àtte ma,"},{"style":"q1","text":"man de màndu laa wéye,"},{"style":"q1","text":"Aji Sax ji laa wóolu, jenguma."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, nattu ma, seetlu ma,"},{"style":"q1","text":"seggal xel ak xol."},{"style":"q1","text":""},{"style":"v","text":"Maa ngi janook sa ngor,"},{"style":"q1","text":"di doxe sa dëgg."},{"style":"q1","text":""},{"style":"v","text":"Toogumaak naxekat,"},{"style":"q1","text":"lëngoowumaak naaféq,"},{"style":"q1","text":""},{"style":"v","text":"sib naa gàngooru saay-saay,"},{"style":"q1","text":"toogumaak ku bon."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, maay jàpp ci biir mucc ayib,"},{"style":"q1","text":"di wër sa \\w sarxalukaay|sarxalukaay b-:\\w*,"},{"style":"q1","text":""},{"style":"v","text":"àddu, sant la,"},{"style":"q1","text":"lim sa jépp jaloore."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, maa sopp sa dëkkuwaay,"},{"style":"q1","text":"bëgg béreb bi sag leer teew."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bu ma buubaaleek bàkkaarkat, sànni,"},{"style":"q1","text":"di ma booleek bóomkat, bóom."},{"style":"q1","text":""},{"style":"v","text":"Seeni loxoo nga sóobu ci mbon,"},{"style":"q1","text":"ndijoor ya fees aki ger."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Man nag màndu laay wéye,"},{"style":"q1","text":"tee nga maa \\w jot|jot:\\w*, baaxe ma?"},{"style":"q1","text":""},{"style":"v","text":"Ndokk bi ma taxawee joor gu wóor."},{"style":"q1","text":"Ma màggal Aji Sax ji fi ndajey way-ngëm ñi."}]