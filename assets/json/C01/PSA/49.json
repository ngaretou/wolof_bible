[{"style":"s1","verse":"","text":"Alal day naxe"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel askanu Kore wu góor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yeen, xeetoo xeet, dégluleen;"},{"style":"q1","verse":"","text":"yeen, waa àddina yépp, teewluleen,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"ngay ku tuut di ku mag,"},{"style":"q1","verse":"","text":"nga bareleek nga néewle."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Li may wax xel la,"},{"style":"q1","verse":"","text":"di xalaat yu déggu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Damay teewlu kàddu yu xelu,"},{"style":"q1","verse":"","text":"xalamal la sama tekkiteb cax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ana lu may tiit bés yu metti,"},{"style":"q1","verse":"","text":"yu ma njublaŋ yéewe seeni ñaawtéef,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"di ñu wóolu seen am-am,"},{"style":"q1","verse":"","text":"di kañoo alal ju bare?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ana ku mana \\w jot|jot:\\w* a jot sa bakkanu moroom,"},{"style":"q1","verse":"","text":"mbaa mu di ko feyal Yàlla \\w njot|jot:\\w* ga?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Njotug bakkan jafe na,"},{"style":"q1","verse":"","text":"bu ci jéem dara."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ana kuy dund ba fàww,"},{"style":"q1","verse":"","text":"ba doo gis bàmmeel?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Xanaa gis ngeen ne xelu, dee;"},{"style":"q1","verse":"","text":"dof, dee; naataxóona it faatu rekk,"},{"style":"q1","verse":"","text":"wacce keneen alalam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Sa bàmmeel di sa kër ba fàww,"},{"style":"q1","verse":"","text":"di sa dëkkuwaay bu sax dàkk,"},{"style":"q1","verse":"","text":"boo tudde woon sa bopp ay suuf sax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Nit ak darajaam fanaanul,"},{"style":"q1","verse":"","text":"mook aw rab a yem, sànku rekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Nii la ku wóolu boppam di mujje,"},{"style":"q1","verse":"","text":"mook ku ko topp, di safooy waxam. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Nara dee rekk niy xar,"},{"style":"q1","verse":"","text":"dee wommat, yóbbu,"},{"style":"q1","verse":"","text":"njub fenk nib jant, ku jub moom leen,"},{"style":"q1","verse":"","text":"njaniiw mëdd seen jëmm,"},{"style":"q1","verse":"","text":"fu sore kërug daraja."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Waaye Yàllaa may musal ci dooley njaniiw,"},{"style":"q1","verse":"","text":"jot sama bakkan, ba jël ma. \\qs Selaw.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Buleen yéemu ci ku barele,"},{"style":"q1","verse":"","text":"teraangay këram di yokku."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Du dee ba yóbbaale dara,"},{"style":"q1","verse":"","text":"teraangaam du ko topp biir bàmmeel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Na bége bànneexu bakkanam,"},{"style":"q1","verse":"","text":"ñu di ko kañe koomam,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"ba tey, day dee, fekkiy maamam,"},{"style":"q1","verse":"","text":"ña dootul gisug leer ba fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Nit ak darajaam te amul dégg,"},{"style":"q1","verse":"","text":"mook aw rab a yem, sànku rekk."}]