[{"style":"s1","text":"Buur Yàllaa jara wóolu"},{"style":"q1","text":""},{"style":"v","text":"Màggalleen Ki Sax!"},{"style":"q1","text":"Naa màggale Aji Sax ji xol,"},{"style":"q1","text":""},{"style":"v","text":"fii ak may dund, di ko kañ,"},{"style":"q1","text":"fii ak may noyyi, di woy sama Yàlla."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Buleen wóolu kilifa,"},{"style":"q1","text":"doom aadama amul wall."},{"style":"q1","text":""},{"style":"v","text":"Noo ga day rëcc, mu dellu suuf;"},{"style":"q1","text":"bu bés baa, pexeem jeex."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ndokkalee kuy dimbalikoo Yàllay \\w Yanqóoba|Yanqóoba:\\w*;"},{"style":"q1","text":"di yaakaar Yàllaam Aji Sax ji,"},{"style":"q1","text":""},{"style":"v","text":"Ki sàkk kaw ak suuf,"},{"style":"q1","text":"géej ak lépp li ci biir,"},{"style":"q1","text":"di sàmm dëgg ba fàww,"},{"style":"q1","text":""},{"style":"v","text":"di àtte néew-ji-doole,"},{"style":"q1","text":"di leel ku xiif."},{"style":"q1","text":"Aji Sax jeey tijji ku ñu tëj."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jeey xippil silmaxa,"},{"style":"q1","text":"di siggil ku sëgg."},{"style":"q1","text":"Aji Sax jee sopp ku jub."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jeey sàmm doxandéem,"},{"style":"q1","text":"taxawu jirim ak jëtun,"},{"style":"q1","text":"waaye mooy lajjal pexem ku bon."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jeey nguuru ba fàww."},{"style":"q1","text":"Yaw \\w Siyoŋ|Siyoŋ:\\w*, sa Yàllaa ñeel maasoo maas."},{"style":"q1","text":"Màggalleen Ki Sax!"}]