[{"style":"s1","verse":"","text":"Buur Yàllaa jara wóolu"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Màggalleen Ki Sax!"},{"style":"q1","verse":"","text":"Naa màggale Aji Sax ji xol,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"fii ak may dund, di ko kañ,"},{"style":"q1","verse":"","text":"fii ak may noyyi, di woy sama Yàlla."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Buleen wóolu kilifa,"},{"style":"q1","verse":"","text":"doom aadama amul wall."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Noo ga day rëcc, mu dellu suuf;"},{"style":"q1","verse":"","text":"bu bés baa, pexeem jeex."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ndokkalee kuy dimbalikoo Yàllay \\w Yanqóoba|Yanqóoba:\\w*;"},{"style":"q1","verse":"","text":"di yaakaar Yàllaam Aji Sax ji,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ki sàkk kaw ak suuf,"},{"style":"q1","verse":"","text":"géej ak lépp li ci biir,"},{"style":"q1","verse":"","text":"di sàmm dëgg ba fàww,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"di àtte néew-ji-doole,"},{"style":"q1","verse":"","text":"di leel ku xiif."},{"style":"q1","verse":"","text":"Aji Sax jeey tijji ku ñu tëj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Aji Sax jeey xippil silmaxa,"},{"style":"q1","verse":"","text":"di siggil ku sëgg."},{"style":"q1","verse":"","text":"Aji Sax jee sopp ku jub."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Aji Sax jeey sàmm doxandéem,"},{"style":"q1","verse":"","text":"taxawu jirim ak jëtun,"},{"style":"q1","verse":"","text":"waaye mooy lajjal pexem ku bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Aji Sax jeey nguuru ba fàww."},{"style":"q1","verse":"","text":"Yaw \\w Siyoŋ|Siyoŋ:\\w*, sa Yàllaa ñeel maasoo maas."},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]