[{"style":"s1","text":"Aji Sax ji dooleel na Buur Sirus"},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji dafa wax ki mu fal,"},{"style":"q1","text":"Buur Sirus mi mu jàpp ci loxol ndijooram,"},{"style":"q1","text":"ngir nangulal ko xeet yi,"},{"style":"q1","text":"yolomal ngañaayal buur yi,"},{"style":"q1","text":"lafal koy bunt,"},{"style":"q1","text":"ba bunti dëkk du tëje,"},{"style":"q1","text":""},{"style":"v","text":"mu ne: «Man maa lay jiitu,"},{"style":"q1","text":"maasale mbartal,"},{"style":"q1","text":"rajaxe buntu xànjar,"},{"style":"q1","text":"damm tëjukaayu weñ."},{"style":"q1","text":""},{"style":"v","text":"Maa lay jagleel denc yu ruqe,"},{"style":"q1","text":"ak alal ju làqe,"},{"style":"q1","text":"ba nga xam ne maay Aji Sax ji lay woowe sa tur,"},{"style":"q1","text":"man Yàllay Israayil."},{"style":"q1","text":""},{"style":"v","text":"Te \\w Yanqóoba|Yanqóoba:\\w* sama jaam bi,"},{"style":"q1","text":"Israayil moomu di sama tànnéef moo tax"},{"style":"q1","text":"ma woowe la saw tur,"},{"style":"q1","text":"takkal la ndombol tànk,"},{"style":"q1","text":"te xamuloo ma."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji man a, du keneen;"},{"style":"q1","text":"kenn du Yàlla ku dul man."},{"style":"q1","text":"Maa la gemb,"},{"style":"q1","text":"te xamuloo ma,"},{"style":"q1","text":""},{"style":"v","text":"ngir xamees penku ba sowu,"},{"style":"q1","text":"ne lu dul man neen la."},{"style":"q1","text":"Aji Sax ji man a, du keneen."},{"style":"q1","text":""},{"style":"v","text":"Maay bind leer,"},{"style":"q1","text":"di def lëndëm."},{"style":"q1","text":"Maay indi jàmm,"},{"style":"q1","text":"di def safaan."},{"style":"q1","text":"Maay Aji Sax jiy def lii lépp!"},{"style":"q1","text":""},{"style":"v","text":"Na asamaan wis-wisal,"},{"style":"q1","text":"niir tawal dëgg ak yoon;"},{"style":"q1","text":"suuf fett, meññal wall,"},{"style":"q1","text":"boole ci jebbil dëgg ak yoon."},{"style":"q1","text":"Man Aji Sax ji, maa def lii!»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Wóoy ngalla kuy diiŋat ka la tabax"},{"style":"q1","text":"te di ndabal xandeer ci biir ndabi xandeer!"},{"style":"q1","text":"Ana ban ak di wax tabaxkatam,"},{"style":"q1","text":"naa ko: «Looy def nii?"},{"style":"q1","text":"Yaw amulooy loxo.»"},{"style":"q1","text":""},{"style":"v","text":"Wóoy ngalla ku naan baayu nit,"},{"style":"q1","text":"«Moo, lu sa geño meññ nii?»"},{"style":"q1","text":"Mbaa ku naan ndeyu nit,"},{"style":"q1","text":"«Li nga wasin lu mu?»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji, \\w Aji Sell|Aji Sell j-:\\w* ju Israayil dafa wax,"},{"style":"q1","text":"moom mi ko bind,"},{"style":"q1","text":"mu ne: «Lu dikkagul, nu ngeen ma ko ñemee laaje;"},{"style":"q1","text":"am dangeen may jox ndigal ci samay doom,"},{"style":"q1","text":"jox ma ko ci li ma liggéeye samay loxo?"},{"style":"q1","text":""},{"style":"v","text":"Man maa amal suuf,"},{"style":"q1","text":"sàkk doom aadama fi kaw suuf."},{"style":"q1","text":"Man mii la samay loxo firi asamaan,"},{"style":"q1","text":"te maay jox gàngoori asamaan lépp ndigal."},{"style":"q1","text":""},{"style":"v","text":"Man maay yékkati Buur Sirus ci àtteb dëgg,"},{"style":"q1","text":"xàllu yoonam."},{"style":"q1","text":"Man maay yékkati Buur Sirus ngir mu doxal yoon,"},{"style":"q1","text":"maa koy xàllal yoon."},{"style":"q1","text":"Moom mooy tabax sama dëkk,"},{"style":"q1","text":"yiwi sama ñoñ ñi ñu toxal,"},{"style":"q1","text":"te du laaj pey mbaa neexal.»"},{"style":"q1","text":"Aji Sax ji Boroom gàngoor yee ko wax."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji dafa wax ne:"},{"style":"q1","text":"«Muy ñaqu Misra, di koomu waa \\w Kuus|Kuus:\\w*,"},{"style":"q1","text":"ak waa Seba ñu njol ñi,"},{"style":"q1","text":"ñoom ñépp ay jàllsi, nga jagoo."},{"style":"q1","text":"Sa gannaaw lañuy topp,"},{"style":"q1","text":"yeewe, jàllsi;"},{"style":"q1","text":"sujjóotal la, di ñaan naan:"},{"style":"q1","text":"“Fi yaw daal la Yàlla ne,"},{"style":"q1","text":"du feneen,"},{"style":"q1","text":"te tuur neen la.”»"},{"style":"q1","text":""},{"style":"v","text":"Yaw déy, yaa di Yàlla ju làqu,"},{"style":"q1","text":"yaw Yàllay Israayil ji koy wallu."},{"style":"q1","text":""},{"style":"v","text":"Yattkati xërëm yaa ngoogu, rus,"},{"style":"q1","text":"torox, riixleendoo."},{"style":"q1","text":""},{"style":"v","text":"Israayil, yaay mucc fa Aji Sax ji"},{"style":"q1","text":"mucc gu sax dàkk!"},{"style":"q1","text":"Doo rus, doo torox"},{"style":"q1","text":"fii ak ëllëg."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji kat dafa wax,"},{"style":"q1","text":"moom mi sàkk asamaan,"},{"style":"q1","text":"moom Yàlla mi bind suuf,"},{"style":"q1","text":"tabax ko, dëj ko,"},{"style":"q1","text":"sàkkewu ko maraas,"},{"style":"q1","text":"xanaa tabaxal ñu ko dëkke."},{"style":"q1","text":"Mu ne: «Aji Sax ji, man a, du keneen."},{"style":"q1","text":""},{"style":"v","text":"Làquwuma, di waxe béreb bu lëndëm."},{"style":"q1","text":"Newuma it askanu Yanqóoba:"},{"style":"q1","text":"“Wërleen ma ci neen.”"},{"style":"q1","text":"Man Aji Sax ji maay wax dëgg,"},{"style":"q1","text":"di biral liy yoon.»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yeen daw-làquy xeet yi,"},{"style":"q1","text":"dajeleen, dikk,"},{"style":"q1","text":"ànd, jegesi."},{"style":"q1","text":"Képp ku gàddu tuuru bantam,"},{"style":"q1","text":"ak kuy ñaan tuur mu ko dul wallu,"},{"style":"q1","text":"kooka xamul dara."},{"style":"q1","text":""},{"style":"v","text":"Àdduleen, layoosi,"},{"style":"q1","text":"diisooleen fi seen biir kay!"},{"style":"q1","text":"Ana ku xamle woon lii bu yàgg,"},{"style":"q1","text":"yégle woon ko booba?"},{"style":"q1","text":"Xanaa du man Aji Sax ji?"},{"style":"q1","text":"Kenn du Yàlla ku dul man!"},{"style":"q1","text":"Yàlla ju jub, di walloo,"},{"style":"q1","text":"du kenn ku dul man!"},{"style":"q1","text":""},{"style":"v","text":"Yeen waa àddina, catoo cat,"},{"style":"q1","text":"walbatikuleen fi man, mucc."},{"style":"q1","text":"Man maay Yàlla, du keneen."},{"style":"q1","text":""},{"style":"v","text":"Giñ naa ko ci man mii,"},{"style":"q1","text":"sama gémmiñ, dëgg a ciy jibe,"},{"style":"q1","text":"di kàddu gu dul randu."},{"style":"q1","text":"Man la ñépp di sukkal seen bëti óom,"},{"style":"q1","text":"man it la bépp boroom làmmiñ di giñe."},{"style":"q1","text":""},{"style":"v","text":"Dees na ma waxal ne:"},{"style":"q1","text":"«Aji Sax ji rekk a jagoo àtte ak doole!»"},{"style":"q1","text":"Mboolem ñi ma mereey ànd,"},{"style":"q1","text":"sëgg, fekksi ma."},{"style":"q1","text":""},{"style":"v","text":"Ci Aji Sax ji la askanu Israayil gépp yeye,"},{"style":"q2","text":"bay damu."}]