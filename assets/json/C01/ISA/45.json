[{"style":"s1","verse":"","text":"Aji Sax ji dooleel na Buur Sirus"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Aji Sax ji dafa wax ki mu fal,"},{"style":"q1","verse":"","text":"Buur Sirus mi mu jàpp ci loxol ndijooram,"},{"style":"q1","verse":"","text":"ngir nangulal ko xeet yi,"},{"style":"q1","verse":"","text":"yolomal ngañaayal buur yi,"},{"style":"q1","verse":"","text":"lafal koy bunt,"},{"style":"q1","verse":"","text":"ba bunti dëkk du tëje,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"mu ne: «Man maa lay jiitu,"},{"style":"q1","verse":"","text":"maasale mbartal,"},{"style":"q1","verse":"","text":"rajaxe buntu xànjar,"},{"style":"q1","verse":"","text":"damm tëjukaayu weñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Maa lay jagleel denc yu ruqe,"},{"style":"q1","verse":"","text":"ak alal ju làqe,"},{"style":"q1","verse":"","text":"ba nga xam ne maay Aji Sax ji lay woowe sa tur,"},{"style":"q1","verse":"","text":"man Yàllay Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Te \\w Yanqóoba|Yanqóoba:\\w* sama jaam bi,"},{"style":"q1","verse":"","text":"Israayil moomu di sama tànnéef moo tax"},{"style":"q1","verse":"","text":"ma woowe la saw tur,"},{"style":"q1","verse":"","text":"takkal la ndombol tànk,"},{"style":"q1","verse":"","text":"te xamuloo ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Aji Sax ji man a, du keneen;"},{"style":"q1","verse":"","text":"kenn du Yàlla ku dul man."},{"style":"q1","verse":"","text":"Maa la gemb,"},{"style":"q1","verse":"","text":"te xamuloo ma,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"ngir xamees penku ba sowu,"},{"style":"q1","verse":"","text":"ne lu dul man neen la."},{"style":"q1","verse":"","text":"Aji Sax ji man a, du keneen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Maay bind leer,"},{"style":"q1","verse":"","text":"di def lëndëm."},{"style":"q1","verse":"","text":"Maay indi jàmm,"},{"style":"q1","verse":"","text":"di def safaan."},{"style":"q1","verse":"","text":"Maay Aji Sax jiy def lii lépp!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Na asamaan wis-wisal,"},{"style":"q1","verse":"","text":"niir tawal dëgg ak yoon;"},{"style":"q1","verse":"","text":"suuf fett, meññal wall,"},{"style":"q1","verse":"","text":"boole ci jebbil dëgg ak yoon."},{"style":"q1","verse":"","text":"Man Aji Sax ji, maa def lii!»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Wóoy ngalla kuy diiŋat ka la tabax"},{"style":"q1","verse":"","text":"te di ndabal xandeer ci biir ndabi xandeer!"},{"style":"q1","verse":"","text":"Ana ban ak di wax tabaxkatam,"},{"style":"q1","verse":"","text":"naa ko: «Looy def nii?"},{"style":"q1","verse":"","text":"Yaw amulooy loxo.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Wóoy ngalla ku naan baayu nit,"},{"style":"q1","verse":"","text":"«Moo, lu sa geño meññ nii?»"},{"style":"q1","verse":"","text":"Mbaa ku naan ndeyu nit,"},{"style":"q1","verse":"","text":"«Li nga wasin lu mu?»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Aji Sax ji, \\w Aji Sell|Aji Sell j-:\\w* ju Israayil dafa wax,"},{"style":"q1","verse":"","text":"moom mi ko bind,"},{"style":"q1","verse":"","text":"mu ne: «Lu dikkagul, nu ngeen ma ko ñemee laaje;"},{"style":"q1","verse":"","text":"am dangeen may jox ndigal ci samay doom,"},{"style":"q1","verse":"","text":"jox ma ko ci li ma liggéeye samay loxo?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Man maa amal suuf,"},{"style":"q1","verse":"","text":"sàkk doom aadama fi kaw suuf."},{"style":"q1","verse":"","text":"Man mii la samay loxo firi asamaan,"},{"style":"q1","verse":"","text":"te maay jox gàngoori asamaan lépp ndigal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Man maay yékkati Buur Sirus ci àtteb dëgg,"},{"style":"q1","verse":"","text":"xàllu yoonam."},{"style":"q1","verse":"","text":"Man maay yékkati Buur Sirus ngir mu doxal yoon,"},{"style":"q1","verse":"","text":"maa koy xàllal yoon."},{"style":"q1","verse":"","text":"Moom mooy tabax sama dëkk,"},{"style":"q1","verse":"","text":"yiwi sama ñoñ ñi ñu toxal,"},{"style":"q1","verse":"","text":"te du laaj pey mbaa neexal.»"},{"style":"q1","verse":"","text":"Aji Sax ji Boroom gàngoor yee ko wax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Aji Sax ji dafa wax ne:"},{"style":"q1","verse":"","text":"«Muy ñaqu Misra, di koomu waa \\w Kuus|Kuus:\\w*,"},{"style":"q1","verse":"","text":"ak waa Seba ñu njol ñi,"},{"style":"q1","verse":"","text":"ñoom ñépp ay jàllsi, nga jagoo."},{"style":"q1","verse":"","text":"Sa gannaaw lañuy topp,"},{"style":"q1","verse":"","text":"yeewe, jàllsi;"},{"style":"q1","verse":"","text":"sujjóotal la, di ñaan naan:"},{"style":"q1","verse":"","text":"“Fi yaw daal la Yàlla ne,"},{"style":"q1","verse":"","text":"du feneen,"},{"style":"q1","verse":"","text":"te tuur neen la.”»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Yaw déy, yaa di Yàlla ju làqu,"},{"style":"q1","verse":"","text":"yaw Yàllay Israayil ji koy wallu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Yattkati xërëm yaa ngoogu, rus,"},{"style":"q1","verse":"","text":"torox, riixleendoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Israayil, yaay mucc fa Aji Sax ji"},{"style":"q1","verse":"","text":"mucc gu sax dàkk!"},{"style":"q1","verse":"","text":"Doo rus, doo torox"},{"style":"q1","verse":"","text":"fii ak ëllëg."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Aji Sax ji kat dafa wax,"},{"style":"q1","verse":"","text":"moom mi sàkk asamaan,"},{"style":"q1","verse":"","text":"moom Yàlla mi bind suuf,"},{"style":"q1","verse":"","text":"tabax ko, dëj ko,"},{"style":"q1","verse":"","text":"sàkkewu ko maraas,"},{"style":"q1","verse":"","text":"xanaa tabaxal ñu ko dëkke."},{"style":"q1","verse":"","text":"Mu ne: «Aji Sax ji, man a, du keneen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Làquwuma, di waxe béreb bu lëndëm."},{"style":"q1","verse":"","text":"Newuma it askanu Yanqóoba:"},{"style":"q1","verse":"","text":"“Wërleen ma ci neen.”"},{"style":"q1","verse":"","text":"Man Aji Sax ji maay wax dëgg,"},{"style":"q1","verse":"","text":"di biral liy yoon.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Yeen daw-làquy xeet yi,"},{"style":"q1","verse":"","text":"dajeleen, dikk,"},{"style":"q1","verse":"","text":"ànd, jegesi."},{"style":"q1","verse":"","text":"Képp ku gàddu tuuru bantam,"},{"style":"q1","verse":"","text":"ak kuy ñaan tuur mu ko dul wallu,"},{"style":"q1","verse":"","text":"kooka xamul dara."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Àdduleen, layoosi,"},{"style":"q1","verse":"","text":"diisooleen fi seen biir kay!"},{"style":"q1","verse":"","text":"Ana ku xamle woon lii bu yàgg,"},{"style":"q1","verse":"","text":"yégle woon ko booba?"},{"style":"q1","verse":"","text":"Xanaa du man Aji Sax ji?"},{"style":"q1","verse":"","text":"Kenn du Yàlla ku dul man!"},{"style":"q1","verse":"","text":"Yàlla ju jub, di walloo,"},{"style":"q1","verse":"","text":"du kenn ku dul man!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Yeen waa àddina, catoo cat,"},{"style":"q1","verse":"","text":"walbatikuleen fi man, mucc."},{"style":"q1","verse":"","text":"Man maay Yàlla, du keneen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Giñ naa ko ci man mii,"},{"style":"q1","verse":"","text":"sama gémmiñ, dëgg a ciy jibe,"},{"style":"q1","verse":"","text":"di kàddu gu dul randu."},{"style":"q1","verse":"","text":"Man la ñépp di sukkal seen bëti óom,"},{"style":"q1","verse":"","text":"man it la bépp boroom làmmiñ di giñe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Dees na ma waxal ne:"},{"style":"q1","verse":"","text":"«Aji Sax ji rekk a jagoo àtte ak doole!»"},{"style":"q1","verse":"","text":"Mboolem ñi ma mereey ànd,"},{"style":"q1","verse":"","text":"sëgg, fekksi ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Ci Aji Sax ji la askanu Israayil gépp yeye,"},{"style":"q2","verse":"","text":"bay damu."}]