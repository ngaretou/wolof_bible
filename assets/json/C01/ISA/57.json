[{"style":"s1","text":"Ab daan ñeel na moykat yi"},{"style":"q1","text":""},{"style":"v","text":"Aji jub ñaa ngi dee,"},{"style":"q1","text":"te kenn yégalu ko yaramam."},{"style":"q1","text":"Gor ñaa ngi faatu,"},{"style":"q1","text":"te kenn ràññeewul"},{"style":"q1","text":"ne aji jub ji, faatoom, musiba la koy moyale."},{"style":"q1","text":""},{"style":"v","text":"Kiy am jàmm ëllëg,"},{"style":"q1","text":"di am noflaay ci biir tëraayam,"},{"style":"q1","text":"mooy kiy wéye njub."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Waaye yeen, dikkleen fii,"},{"style":"q1","text":"yeen doomi xërëmkat yi,"},{"style":"q1","text":"yeen askanu njaalookat yiy gànctu,"},{"style":"q1","text":""},{"style":"v","text":"ana ku ngeen di ñaawal?"},{"style":"q1","text":"Ana ku ngeen di ŋóobi,"},{"style":"q1","text":"ak di ko kókkali?"},{"style":"q1","text":"Xanaa dungeen doomi moykat,"},{"style":"q1","text":"di askanu workat,"},{"style":"q1","text":""},{"style":"v","text":"di xabtaloo bànneex"},{"style":"q1","text":"ci ker garab yu mag yi,"},{"style":"q1","text":"ak ker garaboo garab gu naat,"},{"style":"q1","text":"di rendi seeni doom ci yoonu ndox yi,"},{"style":"q1","text":"ak xarantey doj yi?"},{"style":"q1","text":""},{"style":"v","text":"Yaw Israayil, jëlal doj yu rattax"},{"style":"q2","text":"yi ci yoonu ndox wi, séddoo,"},{"style":"q1","text":"muy sa cér, nga muurloo!"},{"style":"q1","text":"Ñoom ngay tuural,"},{"style":"q1","text":"di leen defal ay \\w saraxi pepp|Sarax yi:\\w*."},{"style":"q1","text":"Waaye Aji Sax ji nee:"},{"style":"q1","text":"«Ndax damay jéllale?»"},{"style":"q1","text":""},{"style":"v","text":"Tund wu mag te kawe,"},{"style":"q1","text":"nga fàkk fa tëraay,"},{"style":"q1","text":"yéeg fa, di fa sarxalal."},{"style":"q1","text":""},{"style":"v","text":"Yaa takk say baaxantal"},{"style":"q1","text":"ci sa lafu bunt ak sa jëni bunt,"},{"style":"q1","text":"yaa ma wacc, di ñorikoo feneen,"},{"style":"q1","text":"yéeg sab lal, yaatal."},{"style":"q1","text":"Yaa takktook ñii nga soppa tëddal"},{"style":"q1","text":"te safoo di niir seeni cér."},{"style":"q1","text":""},{"style":"v","text":"Yaa tukki ba ca buur ba, yóbbaaleg diw,"},{"style":"q1","text":"ak cuuraay lu ne gàññ,"},{"style":"q1","text":"yebal say ndaw fu sore,"},{"style":"q1","text":"ba ñu àkki njaniiw."},{"style":"q1","text":""},{"style":"v","text":"Tukki bu sore bu la sonal sax"},{"style":"q1","text":"taxu la ne: «Fomm naa.»"},{"style":"q1","text":"Bànneex bi ngay séentu ngay leqalikoo,"},{"style":"q1","text":"ba tax sonnuloo."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji nee: «Ana kooy tiit,"},{"style":"q1","text":"ak koo ragal ba di ma wor,"},{"style":"q1","text":"fàttalikootoo ma,"},{"style":"q1","text":"faaleetoo ma?"},{"style":"q1","text":"Damaa noppi lu yàgg daal,"},{"style":"q1","text":"ba tax ragalatoo ma?"},{"style":"q1","text":""},{"style":"v","text":"Defe nga ne yoon nga defee,"},{"style":"q1","text":"waaye dinaa la weer,"},{"style":"q1","text":"te sa jëf du la jariñ."},{"style":"q1","text":""},{"style":"v","text":"Boo wootee wall,"},{"style":"q1","text":"na la sa gàngooru tuur xettali."},{"style":"q1","text":"Ñoom ñépp la ngelaw di buub,"},{"style":"q1","text":"callweer yóbbu leen."},{"style":"q1","text":"Waaye ku ma làqoo,"},{"style":"q1","text":"kookooy moom réew mi,"},{"style":"q1","text":"séddoo sama tund wu sell wi.»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Dees na ne: «Maasaleleenoo, maasaleleen,"},{"style":"q1","text":"yaatalleen yoon wi,"},{"style":"q1","text":"te jële luy pakk ci sama yoonu ñoñ.»"},{"style":"q1","text":""},{"style":"v","text":"Ku màgg, Ku kawe ki,"},{"style":"q1","text":"ki màkkaanam sax dàkk"},{"style":"q1","text":"te turam sell, daa wax ne:"},{"style":"q1","text":"«Fu kawe te sell laa màkkaanoo,"},{"style":"q1","text":"maa dëkk ak boroom xol bu mokk te toroxlu,"},{"style":"q1","text":"ngir leqali ku toroxlu,"},{"style":"q1","text":"leqali boroom xol bu mokk."},{"style":"q1","text":""},{"style":"v","text":"Duma wéye di sikke ba fàww,"},{"style":"q1","text":"duma saxoo di mer,"},{"style":"q1","text":"lu ko moy képp ku ma sàkk"},{"style":"q1","text":"day tàggook bakkanam."},{"style":"q1","text":""},{"style":"v","text":"Ñaawtéefi Israayil ak alalam ju lewul"},{"style":"q2","text":"moo ma merloo,"},{"style":"q1","text":"ma duma ko, xañ ko sama kanam,"},{"style":"q1","text":"waaye mu të ticc, topp xolam."},{"style":"q1","text":""},{"style":"v","text":"Xam naa jikkoom,"},{"style":"q1","text":"waaye maa koy faj,"},{"style":"q1","text":"wommat ko,"},{"style":"q1","text":"dëfale koy xéewal,"},{"style":"q1","text":"mook ñiy ñaawloondook moom."},{"style":"q1","text":""},{"style":"v","text":"Maay teg ci seen làmmiñ kàddug cant."},{"style":"q1","text":"Jàmm, na jàmm ñeel ñépp,"},{"style":"q1","text":"ku soreek ku jege.»"},{"style":"q1","text":"Kàddug Aji Sax jee."},{"style":"q1","text":"Mu ne: «Maa leen di faj.»"},{"style":"q1","text":""},{"style":"v","text":"Waaye ñu bon ñi, mbete géej"},{"style":"q2","text":"gu sàmbaraax, tëë dal,"},{"style":"q1","text":"gannax yay baaje ban ak suuf."},{"style":"q1","text":""},{"style":"v","text":"Sama Yàlla nee:"},{"style":"q1","text":"«Jàmm du ñeel ñu bon ñi.»"}]