[{"style":"s1","verse":"","text":"Ab daan ñeel na moykat yi"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Aji jub ñaa ngi dee,"},{"style":"q1","verse":"","text":"te kenn yégalu ko yaramam."},{"style":"q1","verse":"","text":"Gor ñaa ngi faatu,"},{"style":"q1","verse":"","text":"te kenn ràññeewul"},{"style":"q1","verse":"","text":"ne aji jub ji, faatoom, musiba la koy moyale."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Kiy am jàmm ëllëg,"},{"style":"q1","verse":"","text":"di am noflaay ci biir tëraayam,"},{"style":"q1","verse":"","text":"mooy kiy wéye njub."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Waaye yeen, dikkleen fii,"},{"style":"q1","verse":"","text":"yeen doomi xërëmkat yi,"},{"style":"q1","verse":"","text":"yeen askanu njaalookat yiy gànctu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"ana ku ngeen di ñaawal?"},{"style":"q1","verse":"","text":"Ana ku ngeen di ŋóobi,"},{"style":"q1","verse":"","text":"ak di ko kókkali?"},{"style":"q1","verse":"","text":"Xanaa dungeen doomi moykat,"},{"style":"q1","verse":"","text":"di askanu workat,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"di xabtaloo bànneex"},{"style":"q1","verse":"","text":"ci ker garab yu mag yi,"},{"style":"q1","verse":"","text":"ak ker garaboo garab gu naat,"},{"style":"q1","verse":"","text":"di rendi seeni doom ci yoonu ndox yi,"},{"style":"q1","verse":"","text":"ak xarantey doj yi?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yaw Israayil, jëlal doj yu rattax"},{"style":"q2","verse":"","text":"yi ci yoonu ndox wi, séddoo,"},{"style":"q1","verse":"","text":"muy sa cér, nga muurloo!"},{"style":"q1","verse":"","text":"Ñoom ngay tuural,"},{"style":"q1","verse":"","text":"di leen defal ay \\w saraxi pepp|Sarax yi:\\w*."},{"style":"q1","verse":"","text":"Waaye Aji Sax ji nee:"},{"style":"q1","verse":"","text":"«Ndax damay jéllale?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Tund wu mag te kawe,"},{"style":"q1","verse":"","text":"nga fàkk fa tëraay,"},{"style":"q1","verse":"","text":"yéeg fa, di fa sarxalal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yaa takk say baaxantal"},{"style":"q1","verse":"","text":"ci sa lafu bunt ak sa jëni bunt,"},{"style":"q1","verse":"","text":"yaa ma wacc, di ñorikoo feneen,"},{"style":"q1","verse":"","text":"yéeg sab lal, yaatal."},{"style":"q1","verse":"","text":"Yaa takktook ñii nga soppa tëddal"},{"style":"q1","verse":"","text":"te safoo di niir seeni cér."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Yaa tukki ba ca buur ba, yóbbaaleg diw,"},{"style":"q1","verse":"","text":"ak cuuraay lu ne gàññ,"},{"style":"q1","verse":"","text":"yebal say ndaw fu sore,"},{"style":"q1","verse":"","text":"ba ñu àkki njaniiw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Tukki bu sore bu la sonal sax"},{"style":"q1","verse":"","text":"taxu la ne: «Fomm naa.»"},{"style":"q1","verse":"","text":"Bànneex bi ngay séentu ngay leqalikoo,"},{"style":"q1","verse":"","text":"ba tax sonnuloo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Aji Sax ji nee: «Ana kooy tiit,"},{"style":"q1","verse":"","text":"ak koo ragal ba di ma wor,"},{"style":"q1","verse":"","text":"fàttalikootoo ma,"},{"style":"q1","verse":"","text":"faaleetoo ma?"},{"style":"q1","verse":"","text":"Damaa noppi lu yàgg daal,"},{"style":"q1","verse":"","text":"ba tax ragalatoo ma?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Defe nga ne yoon nga defee,"},{"style":"q1","verse":"","text":"waaye dinaa la weer,"},{"style":"q1","verse":"","text":"te sa jëf du la jariñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Boo wootee wall,"},{"style":"q1","verse":"","text":"na la sa gàngooru tuur xettali."},{"style":"q1","verse":"","text":"Ñoom ñépp la ngelaw di buub,"},{"style":"q1","verse":"","text":"callweer yóbbu leen."},{"style":"q1","verse":"","text":"Waaye ku ma làqoo,"},{"style":"q1","verse":"","text":"kookooy moom réew mi,"},{"style":"q1","verse":"","text":"séddoo sama tund wu sell wi.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Dees na ne: «Maasaleleenoo, maasaleleen,"},{"style":"q1","verse":"","text":"yaatalleen yoon wi,"},{"style":"q1","verse":"","text":"te jële luy pakk ci sama yoonu ñoñ.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Ku màgg, Ku kawe ki,"},{"style":"q1","verse":"","text":"ki màkkaanam sax dàkk"},{"style":"q1","verse":"","text":"te turam sell, daa wax ne:"},{"style":"q1","verse":"","text":"«Fu kawe te sell laa màkkaanoo,"},{"style":"q1","verse":"","text":"maa dëkk ak boroom xol bu mokk te toroxlu,"},{"style":"q1","verse":"","text":"ngir leqali ku toroxlu,"},{"style":"q1","verse":"","text":"leqali boroom xol bu mokk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Duma wéye di sikke ba fàww,"},{"style":"q1","verse":"","text":"duma saxoo di mer,"},{"style":"q1","verse":"","text":"lu ko moy képp ku ma sàkk"},{"style":"q1","verse":"","text":"day tàggook bakkanam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ñaawtéefi Israayil ak alalam ju lewul"},{"style":"q2","verse":"","text":"moo ma merloo,"},{"style":"q1","verse":"","text":"ma duma ko, xañ ko sama kanam,"},{"style":"q1","verse":"","text":"waaye mu të ticc, topp xolam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Xam naa jikkoom,"},{"style":"q1","verse":"","text":"waaye maa koy faj,"},{"style":"q1","verse":"","text":"wommat ko,"},{"style":"q1","verse":"","text":"dëfale koy xéewal,"},{"style":"q1","verse":"","text":"mook ñiy ñaawloondook moom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Maay teg ci seen làmmiñ kàddug cant."},{"style":"q1","verse":"","text":"Jàmm, na jàmm ñeel ñépp,"},{"style":"q1","verse":"","text":"ku soreek ku jege.»"},{"style":"q1","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":"Mu ne: «Maa leen di faj.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Waaye ñu bon ñi, mbete géej"},{"style":"q2","verse":"","text":"gu sàmbaraax, tëë dal,"},{"style":"q1","verse":"","text":"gannax yay baaje ban ak suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Sama Yàlla nee:"},{"style":"q1","verse":"","text":"«Jàmm du ñeel ñu bon ñi.»"}]