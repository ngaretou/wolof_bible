[{"style":"s1","text":"Yoonu ñibbi Siyoŋ taxaw na"},{"style":"q1","text":""},{"style":"v","text":"Màndiŋ ak suuf su ne sereŋ dina bég,"},{"style":"q1","text":"joor gu wow bànneexu, tóor ba ne ràññ."},{"style":"q1","text":""},{"style":"v","text":"Day tóor a tóor,"},{"style":"q1","text":"di bànneexu, bay sarxolle,"},{"style":"q1","text":"ba jagoo darajay gottub Libaŋ"},{"style":"q1","text":"ak taaru àllub Karmel ak Saron."},{"style":"q1","text":"Foofa lees di gise darajay Aji Sax ji,"},{"style":"q1","text":"sunu taaru Yàlla."},{"style":"q1","text":""},{"style":"v","text":"Ku yoqi, dooleelleen ko,"},{"style":"q1","text":"ku óom yay fenqe, jàppaleleen ko."},{"style":"q1","text":""},{"style":"v","text":"Ku fitam rëcc, ne leen ko:"},{"style":"q1","text":"«Dëgërlul te bul tiit."},{"style":"q1","text":"Sa Yàllaa ngii di la feyul,"},{"style":"q1","text":"ba yool say noon."},{"style":"q1","text":"Moom mooy dikk, wallusi la.»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Su boobaa gëti silmaxa muriku,"},{"style":"q1","text":"noppi tëx sikkliku."},{"style":"q1","text":""},{"style":"v","text":"Su boobaa ab làggi ne jàyy ni kéwél,"},{"style":"q1","text":"ab luu sarxolle."},{"style":"q1","text":"Am ndox ay fettaxe màndiŋ ma,"},{"style":"q1","text":"ay wal wale joor gu wow,"},{"style":"q1","text":""},{"style":"v","text":"suuf su wow doonub déeg,"},{"style":"q1","text":"joor gu maral ballal bëti ndox."},{"style":"q1","text":"Fa till fàkkoon, di fa goore,"},{"style":"q1","text":"gattax ak barax a fay sëqe."},{"style":"q1","text":""},{"style":"v","text":"Foofa law yoon di jaare,"},{"style":"q1","text":"ñu di ko woowe Yoon wu sell wi."},{"style":"q1","text":"Ku sobewu du fa jaare,"},{"style":"q1","text":"ñi moom woowu yoon,"},{"style":"q1","text":"ñooy ñi koy topp."},{"style":"q1","text":"Ab dof sax du fa lajje."},{"style":"q1","text":""},{"style":"v","text":"Gaynde du fa am,"},{"style":"q1","text":"rab wu aay du fa dox,"},{"style":"q1","text":"du lenn lees fay gis,"},{"style":"q1","text":"ku ñu jot rekk a fay jaare."},{"style":"q1","text":""},{"style":"v","text":"Ñi Aji Sax ji goreel, nii lañuy dellusee,"},{"style":"q1","text":"agsaaley sarxolle fa Siyoŋ,"},{"style":"q1","text":"kaalawoo mbég mu sax,"},{"style":"q1","text":"mbégteek bànneex wàllsi,"},{"style":"q1","text":"naqar aku tiis daw."}]