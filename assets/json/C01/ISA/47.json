[{"style":"s1","verse":"","text":"Babilon falu, fëlëñu"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Yaw janq Babilon,"},{"style":"q1","verse":"","text":"wàccal, toog ci pënd bi."},{"style":"q1","verse":"","text":"Yaw dëkkub Kaldeen bi ci taar bi laa ne,"},{"style":"q1","verse":"","text":"toogal fi suuf, amatoob jal."},{"style":"q1","verse":"","text":"Dootoo dégg: «Lewet, ndaw su xay.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Fabal wolukaay, wol sunguf."},{"style":"q1","verse":"","text":"Dindil sa muuraay gi,"},{"style":"q1","verse":"","text":"wogos sa mbubb,"},{"style":"q1","verse":"","text":"ëñ sas kumba,"},{"style":"q1","verse":"","text":"xuus, jàll dex yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Na sa yaram feeñ,"},{"style":"q1","verse":"","text":"say cér ne fàŋŋ."},{"style":"q1","verse":"","text":"Damay feyu,"},{"style":"q1","verse":"","text":"te duma baale."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Sunu \\w jotkat|jotkat b-:\\w* ay Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"\\w Aji Sell|Aji Sell j-:\\w* ju Israayil."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Yaw dëkkub Kaldeen bi ci taar bi,"},{"style":"q1","verse":"","text":"toogal ne cell te fatu cig lëndëm,"},{"style":"q1","verse":"","text":"deesatu la woowe lingeeru réew yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Maa mere woon sama ñoñ,"},{"style":"q1","verse":"","text":"sobeel sama séddoo ñii,"},{"style":"q1","verse":"","text":"teg leen ci sa loxo,"},{"style":"q1","verse":"","text":"te wonoo leen yërmande,"},{"style":"q1","verse":"","text":"ba mag sax, nga teg ko lu diisa diis,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"te naa: «Ba fàww maay lingeer!»"},{"style":"q1","verse":"","text":"ba tax bàyyiwoo xel ca la ngay def;"},{"style":"q1","verse":"","text":"xalaatoo la muy jur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Diryànke bee, déglul lii:"},{"style":"q1","verse":"","text":"Yaa ngi ne finaax te naa ci sam xel:"},{"style":"q1","verse":"","text":"«Maa ko yor, man rekk, du keneen."},{"style":"q1","verse":"","text":"Duma jooy jëkkër, duma jooy doom!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Te ñaar yooyoo lay dikkal ci benn bés."},{"style":"q1","verse":"","text":"Jooy doom, jooy jëkkër ay yembandoo,"},{"style":"q1","verse":"","text":"dal fi sa kaw ci sa biir xërëmtu yi ne xas,"},{"style":"q1","verse":"","text":"ak sa jat yu dul jeex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Dangaa yaakaaroon sa pexe yu bon,"},{"style":"q1","verse":"","text":"naan, «Kenn gisu ma!»"},{"style":"q1","verse":"","text":"Sam xel ak sa xam-xam a la wacc,"},{"style":"q1","verse":"","text":"ba tax nga naa: «Maa ko yor, awma moroom.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Musibaa lay dikkal,"},{"style":"q1","verse":"","text":"te doo xam fu mu fenke."},{"style":"q1","verse":"","text":"Njaaxum a lay dal,"},{"style":"q1","verse":"","text":"te du looy mana fanq."},{"style":"q1","verse":"","text":"Sànkuteey jekki dale la foo yégul."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Jàppool say jat rekk"},{"style":"q1","verse":"","text":"ak sa xërëm yu ne xas"},{"style":"q2","verse":"","text":"yoo dale sonn ba ngay ndaw."},{"style":"q1","verse":"","text":"Jombul mu amal la njariñ,"},{"style":"q1","verse":"","text":"mbaa ñu ragale la ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Yaaka sonn ci tegtali maa-man!"},{"style":"q1","verse":"","text":"Nañ taxaw boog, wallu la!"},{"style":"q1","verse":"","text":"Ñii di rëdd nataalu asamaan,"},{"style":"q1","verse":"","text":"di niiri biddiiw,"},{"style":"q1","verse":"","text":"ak di xool weer wu feeñ,"},{"style":"q1","verse":"","text":"xamle ca loo dikkleegul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ñu ngoog bokk demin ak boob bu sawara jafal,"},{"style":"q1","verse":"","text":"duñu musal seen bakkan ca sawara sa,"},{"style":"q1","verse":"","text":"te du doon ab taal bu ñuy lakke mburu"},{"style":"q1","verse":"","text":"mbaa xal wooy uuf di jaaru."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Lii moo di muju ña la taxoon di sonn."},{"style":"q1","verse":"","text":"Ña nga dale jëflanteel ba ngay ndaw,"},{"style":"q1","verse":"","text":"ña ngay tambaambalu tey, ku nekk ak yoonam,"},{"style":"q1","verse":"","text":"kenn walluwu la ci."}]