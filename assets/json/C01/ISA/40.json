[{"style":"ms","text":"Aji Sax jeey dàmp xol yi"},{"style":"mr","text":"(Saar 40---66)"},{"style":"s1","text":"Xibaaru jàmm ñeel na Israayil"},{"style":"q1","text":""},{"style":"v","text":"Dëfalleen a dëfal sama ñoñ,"},{"style":"q1","text":"la seen Yàlla wax!"},{"style":"q1","text":""},{"style":"v","text":"Waxleen lu dalal xelu Yerusalem, yéeneleen"},{"style":"q1","text":"ne leen seen xare jeex na,"},{"style":"q1","text":"seen mbugal mat na."},{"style":"q1","text":"Ñoo nangoo ci loxol Aji Sax ji"},{"style":"q1","text":"lu fey ñaari yoon seen bépp bàkkaar."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Baat a ngi jib, naan:"},{"style":"q2","text":"«Nangeen xàll yoonu Aji Sax ji ci màndiŋ mi,"},{"style":"q1","text":"fàkkalleen sunu Yàlla"},{"style":"q1","text":"xàll wu jub fi joor gi."},{"style":"q1","text":""},{"style":"v","text":"Wépp xur lees di kaweel,"},{"style":"q1","text":"tund wu mag ak wu ndaw lees di suufeel;"},{"style":"q1","text":"ba fu tunde mujj dig joor,"},{"style":"q1","text":"fu ñagasoon maase,"},{"style":"q1","text":""},{"style":"v","text":"darajay Aji Sax ji daldi feeñ,"},{"style":"q1","text":"àddina sépp bokk gis ko."},{"style":"q1","text":"Aji Sax jee ko wax ci làmmiñam.»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Baat a ngi naa: «Yéeneel!»"},{"style":"q1","text":"Ma ne: «Lu may yéene?»"},{"style":"q1","text":"Mu ne: «Neel:"},{"style":"q1","text":"Nit am ñax doŋŋ la;"},{"style":"q1","text":"gépp ngoram mel ni tóor-tóoru àll."},{"style":"q1","text":""},{"style":"v","text":"Ñax ma wow,"},{"style":"q1","text":"tóor-tóor ma lax"},{"style":"q1","text":"fa ko Aji Sax ji yónnee mbóoyo."},{"style":"q1","text":"Askan wi kay, am ñax doŋŋ la."},{"style":"q1","text":""},{"style":"v","text":"Am ñax day wow;"},{"style":"q1","text":"tóor-tóor day lax."},{"style":"q1","text":"Waaye sunu kàddug Yàlla"},{"style":"q2","text":"mooy sax ba fàww.»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Yaw \\w Siyoŋ|Siyoŋ:\\w*, xibaarkatu jàmm bi,"},{"style":"q1","text":"doxal yéegi tund wu kawe,"},{"style":"q1","text":"te xaacook doole."},{"style":"q1","text":"Yaw Yerusalem, xibaarkatu jàmm bi,"},{"style":"q1","text":"xaacul te bul ragal!"},{"style":"q1","text":"Neel dëkki Yuda yi:"},{"style":"q1","text":"«Seen Yàllaa ngii!"},{"style":"q1","text":""},{"style":"v","text":"Boroom bi, Aji Sax jaa ngii,"},{"style":"q1","text":"dikkaale doole"},{"style":"q1","text":"ak kàttanu kilifteefam."},{"style":"q1","text":"Mu ngii ànd ak ñi mu yooloo,"},{"style":"q1","text":"ñu dig peyam, dox jiitu ko."},{"style":"q1","text":""},{"style":"v","text":"Mooy sàmm géttam bii ni kuy sàmmug juram,"},{"style":"q1","text":"boole mbote yi cib loxoom,"},{"style":"q1","text":"taf cib dënnam, yóbbu;"},{"style":"q1","text":"xar yiy nàmpal, mu wommat.»"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ana ku natte géej biir loxoom?"},{"style":"q1","text":"Ku xaymaa asamaan yaatuwaayu loxo?"},{"style":"q1","text":"Ku mana fab suuf sépp, sol cib nattukaay?"},{"style":"q1","text":"Ana ku natte ab màndaxekaay tund wu mag?"},{"style":"q1","text":"Mbaa mu tam-tamal tund wu ndaw?"},{"style":"q1","text":""},{"style":"v","text":"Ana ku mana nànd xelum Aji Sax ji?"},{"style":"q1","text":"Mbaa nga dib xejjam, ba di ko digal?"},{"style":"q1","text":""},{"style":"v","text":"Ana ku mu diisool, ba kooka tegtal ko,"},{"style":"q1","text":"jàngal ko yoonu njub,"},{"style":"q1","text":"jàngal ko xam-xam,"},{"style":"q1","text":"xamal ko yoonu dég-dég?"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Xeet yaa ngii, mel ni aw toq ci biir bagaan,"},{"style":"q1","text":"jàppeesu leen lu moy diisaayu feppu suuf!"},{"style":"q1","text":"Dun yi lay yékkati ni pëndub suuf!"},{"style":"q1","text":""},{"style":"v","text":"Gottub Libaŋ xambul taalu sarxalukaayam,"},{"style":"q1","text":"te mala ya ca ne doyu ko \\w saraxu rendi-dóomal|Sarax yi:\\w*!"},{"style":"q1","text":""},{"style":"v","text":"Xeetoo xeet du tus fi kanamam,"},{"style":"q1","text":"te jàppewu leen lu moy neenug neen."},{"style":"q1","text":""},{"style":"v","text":"Ana kan ngeen di niruleek Yàlla,"},{"style":"q1","text":"ak nataal bu ngeen koy dendaleel?"},{"style":"q1","text":""},{"style":"v","text":"Xanaa du jëmmu tuur ju liggéeykat móol,"},{"style":"q1","text":"ab tëgg xoobe ko wurus,"},{"style":"q1","text":"takk ca caqi xaalis?"},{"style":"q1","text":""},{"style":"v","text":"Ku néewle ba amul may gu ni tollu,"},{"style":"q1","text":"mooy tànn bant bu dul nëb,"},{"style":"q1","text":"seeti liggéeykat bu xareñ"},{"style":"q1","text":"bu ko ci sàkkal jëmmu tuur ju dëgër!"},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Xanaa dangeena xamul lii?"},{"style":"q1","text":"Am dangeena déggul lii?"},{"style":"q1","text":"Dees leena waxul ca njàlbéen,"},{"style":"q1","text":"am seen xel a jàppul nan la àddina sosoo?"},{"style":"q1","text":""},{"style":"v","text":"Moom moo tooge ca kaw wërngalu suuf,"},{"style":"q1","text":"ñi ko dëkke mel niy soccet,"},{"style":"q1","text":"moo tàllal asamaan ni ndimo,"},{"style":"q1","text":"firi ko nib xayma, daldi dëkke."},{"style":"q1","text":""},{"style":"v","text":"Mooy sànk kilifa,"},{"style":"q1","text":"di neenal njiitu àddina."},{"style":"q1","text":""},{"style":"v","text":"Ba ñu sampoogul,"},{"style":"q1","text":"toogaguñu sax,"},{"style":"q1","text":"ba seeni reen di riigu,"},{"style":"q1","text":"ca la leen Aji Sax jiy wal, ñu wow,"},{"style":"q1","text":"ngëlén buub, yóbbu ni boob."},{"style":"q1","text":""},{"style":"v","text":"\\w Aji Sell|Aji Sell j-:\\w* ji ne:"},{"style":"q1","text":"«Ana ku ngeen may nirooleel,"},{"style":"q1","text":"ba may moroomam?»"},{"style":"q1","text":""},{"style":"v","text":"Séentuleen kaw, xoolleen Ki sàkk biddiiw yii!"},{"style":"q1","text":"Xanaa kiy daaguloo leeri asamaan ni gàngoor gu mat,"},{"style":"q1","text":"ñoom ñépp lay woo ci seen tur,"},{"style":"q1","text":"ci kawewaayu dooleem ak màggaayu kàttanam,"},{"style":"q1","text":"te kenn du ci wuute."},{"style":"q1","text":""},{"style":"v","text":"Yeen askanu \\w Yanqóoba|Yanqóoba:\\w*, lu ngeen di jàmbat?"},{"style":"q1","text":"Yeen Israayil laa ne, lu ngeen di jooy,"},{"style":"q1","text":"naa: «Aji Sax ji gisul li nu dal,"},{"style":"q1","text":"sunu Yàlla daal, daa réere sunu àq!»"},{"style":"q1","text":""},{"style":"v","text":"Dangeena xamul,"},{"style":"q1","text":"am dangeena déggul"},{"style":"q1","text":"ne Aji Sax jeey Yàlla ba fàww,"},{"style":"q1","text":"sàkk suuf cat ba cat,"},{"style":"q1","text":"du sonn, du tàyyi,"},{"style":"q1","text":"te am xelam nattuwul?"},{"style":"q1","text":""},{"style":"v","text":"Mooy dooleel ku loof,"},{"style":"q1","text":"di kàttanal ku jeele."},{"style":"q1","text":""},{"style":"v","text":"Waxambaane sonn, loof;"},{"style":"q1","text":"xalelu góor di tërëf a tërëf,"},{"style":"q1","text":""},{"style":"v","text":"kiy yaakaar Aji Sax ji moom, di yeeslu doole;"},{"style":"q1","text":"da naa fëyy ni jaxaay juy naaw,"},{"style":"q1","text":"di xélu te du xiix,"},{"style":"q1","text":"di dox te du xoox."}]