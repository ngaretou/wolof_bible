[{"style":"s1","verse":"","text":"Woyu cofeel rax na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Naa woy sama soppe"},{"style":"q1","verse":"","text":"woyu cofeel wu solowoo tóokëram."},{"style":"q1","verse":"","text":"Sama soppee fi amoon tóokër,"},{"style":"q1","verse":"","text":"kaw tund wu suuf sa nangu lool."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Mu gàbb ko, dindi doj ya,"},{"style":"q1","verse":"","text":"jëmbat ca ngëneeli reseñ,"},{"style":"q1","verse":"","text":"tabax tóojub doj fa digg ba,"},{"style":"q1","verse":"","text":"ba yatt ca mbalkam nalukaayu reseñ,"},{"style":"q1","verse":"","text":"tey yaakaar reseñ ju neex,"},{"style":"q1","verse":"","text":"mu meññ, wex."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Léegi yeen waa Yerusalem"},{"style":"q1","verse":"","text":"ak yeen waa Yuda,"},{"style":"q1","verse":"","text":"ngalla àtteleen maak sama tóokër."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Lu may defalati sama tóokër bi,"},{"style":"q1","verse":"","text":"lu ma ca defuloon?"},{"style":"q1","verse":"","text":"Lu waral ma di ko yaakaare reseñ ju neex,"},{"style":"q1","verse":"","text":"mu meññ, wex xàtt?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ma xamal leen nag"},{"style":"q1","verse":"","text":"li ma naral sama tóokër bi."},{"style":"q1","verse":"","text":"Ñàggib ñagam, mu dim parlu;"},{"style":"q1","verse":"","text":"màbb miiram, ñu dëggaate ko, jàll."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Dama koy gental,"},{"style":"q1","verse":"","text":"mu saxi dég aki dagg,"},{"style":"q1","verse":"","text":"deesu ko tenqi, deesu ko bey;"},{"style":"q1","verse":"","text":"niir yi it, ma sant leen"},{"style":"q1","verse":"","text":"ñu bañ koo tawal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Tóokëru Aji Sax ji Boroom gàngoor yi kat,"},{"style":"q1","verse":"","text":"mooy waa kër Israayil."},{"style":"q1","verse":"","text":"Niti Yuda it"},{"style":"q1","verse":"","text":"ñooy njëmbat li mu safoo."},{"style":"q1","verse":"","text":"Mu di ko yaakaare njub,"},{"style":"q1","verse":"","text":"njubadi ne jaas."},{"style":"q1","verse":"","text":"Mu yaakaar njekk,"},{"style":"q1","verse":"","text":"yuuxi njekkar ne jaas."},{"style":"s1","verse":"","text":"Alkànde ñeel na kàngami Yuda"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Wóoy ngalla ñii di dajale kër ak kër,"},{"style":"q1","verse":"","text":"tool akub tool,"},{"style":"q1","verse":"","text":"aakimoo fépp,"},{"style":"q1","verse":"","text":"ba des ñoomu neen ci réew mi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Maa dégg Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"mu ne: «Giñ naa ne"},{"style":"q1","verse":"","text":"kër yu bareey gental déy,"},{"style":"q1","verse":"","text":"kër yu yaa, rafet, kenn dëkku ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Tóokërub ñetti egtaar déy"},{"style":"q1","verse":"","text":"du meññ juróom fukki liitari biiñ,"},{"style":"q1","verse":"","text":"te jiwum téeméeri kiloy bele it,"},{"style":"q1","verse":"","text":"fukki kilo doŋŋ a cay génne.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Wóoy ngalla ñiy teela xëy,"},{"style":"q1","verse":"","text":"daw wutiw ñoll,"},{"style":"q1","verse":"","text":"gonte biiñ ba tàng jérr."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Xalam yaak tabala yaak toxoro ya jolli,"},{"style":"q1","verse":"","text":"biiñ xelli ca xawaare ja."},{"style":"q1","verse":"","text":"Seetluwuñu sax li Aji Sax ji def,"},{"style":"q1","verse":"","text":"xooluñu li mu jo loxoom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Moo tax ñuy gàddaayloo ñoñi Aji Sax ji"},{"style":"q1","verse":"","text":"ndax li ñu ñàkk xam-xam."},{"style":"q1","verse":"","text":"Seen kàngam ay deek xiif,"},{"style":"q1","verse":"","text":"seen baadoolo deek mar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Moo tax njaniiw di yàkkali ag bóli,"},{"style":"q1","verse":"","text":"gémmiñ ga ne ŋafeet, amul kersa,"},{"style":"q1","verse":"","text":"boroom darajaak baadoolay Yerusalem"},{"style":"q1","verse":"","text":"doon xaxaloo, far tàbbi ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Doom aadama torox na,"},{"style":"q1","verse":"","text":"nit detteelu na,"},{"style":"q1","verse":"","text":"ku doon regeju, detteelu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Aji Sax ji Boroom gàngoor yi, ab àtteem lay fésale màggaayam,"},{"style":"q1","verse":"","text":"Yàlla ju sell ji, ab àtteb yoon lay firndeele sellaayam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Am xar a nara fore biir dëkk bi, mbetem parlu,"},{"style":"q1","verse":"","text":"màngaan lekke gentub boroom alal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Wóoy ngalla ñiy ràngooy buum,"},{"style":"q1","verse":"","text":"di diri ñaawtéefi neen,"},{"style":"q1","verse":"","text":"ràngooy goj di diri seeni tooñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ña nga naa: «Na Aji Sax ji gaaw!"},{"style":"q1","verse":"","text":"Na baral liggéeyam, nu gis."},{"style":"q1","verse":"","text":"Na \\w Aji Sell|Aji Sell j-:\\w* ju Israayil sottal mébétam,"},{"style":"q1","verse":"","text":"mu sotti, nu xam.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Wóoy ngalla ñiy gis lu bon, ne lu baax la;"},{"style":"q1","verse":"","text":"gis lu baax, ne lu bon la;"},{"style":"q1","verse":"","text":"lëndëm, ñu tudde leer,"},{"style":"q1","verse":"","text":"leer, ñu tudde lëndëm;"},{"style":"q1","verse":"","text":"lu wex, ñu ne neex na,"},{"style":"q1","verse":"","text":"lu neex, ñu ne wex na."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Wóoy ngalla ñii defe ne ñoo xam,"},{"style":"q1","verse":"","text":"yaakaar ne ñoo muus."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Wóoy ngalla ñiy jàmbaari naan biiñ,"},{"style":"q1","verse":"","text":"diy ndaanaan ci njafaanu ñoll,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"di dëggal ku sikk ndax ab ger,"},{"style":"q1","verse":"","text":"tey xañ boroom dëgg àqam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Moo tax ni boob di jeexe ci sawara,"},{"style":"q1","verse":"","text":"ngooñ xoyomoo ko cib taal,"},{"style":"q1","verse":"","text":"ni la seenub reen di seeye ni lu maxe,"},{"style":"q1","verse":"","text":"seeni tóor-tóor naaw ni pënd."},{"style":"q1","verse":"","text":"Ñoo gàntal yoonu Aji Sax ji ci gàngoor yi,"},{"style":"q1","verse":"","text":"ñoo sofental kàddug Aji Sell ju Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Moo tax merum Aji Sax ji tàkkal ñoñam,"},{"style":"q1","verse":"","text":"mu xàcc loxoom, duma leen,"},{"style":"q1","verse":"","text":"ba tund yi jaayu,"},{"style":"q1","verse":"","text":"seeni néew wetar ni mbalit fi mbedd yi."},{"style":"q1","verse":"","text":"Teewul Aji Sax ji xàcc loxoom,"},{"style":"q1","verse":"","text":"meddiwul ba tey."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Aji Sax jeey yékkatil raaya xeet wu sore,"},{"style":"q1","verse":"","text":"walug liit, woowe leen fa cati àddina,"},{"style":"q1","verse":"","text":"ñu jekki ne jaas, dikkal leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Kenn sonnu ci, kenn tërëfu ci,"},{"style":"q1","verse":"","text":"kenn dajjantul mbaa muy nelaw;"},{"style":"q1","verse":"","text":"ngañaay yolomul, waruw dàll dogul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Seeni fitt ñaw na,"},{"style":"q1","verse":"","text":"seen xala yépp noppi na,"},{"style":"q1","verse":"","text":"seen wewi tànki fas ni xeer."},{"style":"q1","verse":"","text":"Bu tànki watiir ya dawee,"},{"style":"q1","verse":"","text":"nga defe ne callweer a."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Dañuy ŋar ni gaynde,"},{"style":"q1","verse":"","text":"ŋarati ni gaynde gu mat;"},{"style":"q1","verse":"","text":"yëmmu, ŋank rëbbam,"},{"style":"q1","verse":"","text":"yóbbu te kenn du wallu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Bésub keroog ñu ŋaral Yuda,"},{"style":"q1","verse":"","text":"ŋar yu mel ni géej gu sàmbaraax!"},{"style":"q1","verse":"","text":"Foo geesu ci réew mi, xool,"},{"style":"q1","verse":"","text":"doo gis lu moy lëndëm ak njàqare,"},{"style":"q1","verse":"","text":"xàmbaar maase, ba leer giim."}]