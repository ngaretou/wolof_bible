[{"style":"ms","text":"Kàddug waxyu ñeel na mujug jamono"},{"style":"mr","text":"(Saar 24---27)"},{"style":"s1","text":"Mbugal daj na àddina"},{"style":"q1","text":""},{"style":"v","text":"Aji Sax jaa ngii di tas àddina,"},{"style":"q1","text":"tasaare ko,"},{"style":"q1","text":"salfaañe li fi dénd bi,"},{"style":"q1","text":"wasaare ñi ko dëkke:"},{"style":"q1","text":""},{"style":"v","text":"\\w sarxalkat|sarxalkat b-:\\w* beek baadoolo bi,"},{"style":"q1","text":"sang akub jaam,"},{"style":"q1","text":"njaatige lu jigéen ak ab janqam,"},{"style":"q1","text":"jaaykat ak jëndkat,"},{"style":"q1","text":"ablekat ak àbbkat,"},{"style":"q1","text":"leblekat ak lebkat."},{"style":"q1","text":""},{"style":"v","text":"Àddinaa ne duŋŋ duŋŋaaral,"},{"style":"q1","text":"dees koo futti ba mu ne furaas!"},{"style":"q1","text":"Aji Sax ji kat wax na kàddu gii!"},{"style":"q1","text":""},{"style":"v","text":"Suuf a ngi wowa wow, ne sereŋ,"},{"style":"q1","text":"àddina ne làcc, ne sereŋ,"},{"style":"q1","text":"kàngami xeeti àddina ne làcc."},{"style":"q1","text":""},{"style":"v","text":"Suuf si la ñi ko dëkke sobeel,"},{"style":"q1","text":"ñoo wàcc yoon yi,"},{"style":"q1","text":"xëtt ndigal,"},{"style":"q1","text":"fecci \\w kóllëre|kóllëre g-:\\w* gi sax dàkk."},{"style":"q1","text":""},{"style":"v","text":"Moo tax alkànde yàq suuf,"},{"style":"q1","text":"ñi ko dëkke gàddu àq ji."},{"style":"q1","text":"Moo tax waa àddina raaf,"},{"style":"q1","text":"ba ñi des di lu néew."},{"style":"q1","text":""},{"style":"v","text":"Biiñ ñàkk na,"},{"style":"q1","text":"garabu reseñ lax,"},{"style":"q1","text":"bépp xol bu tooyoon, binni."},{"style":"q1","text":""},{"style":"v","text":"Tabala ju xumb selaw na,"},{"style":"q1","text":"ña daa xaxaloo dal,"},{"style":"q1","text":"mbégtem buumi xalam dakk."},{"style":"q1","text":""},{"style":"v","text":"Kenn naanatul, di woy,"},{"style":"q1","text":"ñoll wex na ka koy naan."},{"style":"q1","text":""},{"style":"v","text":"Dëkk bu gental tasaaroo na,"},{"style":"q1","text":"kër yépp tëje, kenn duggul."},{"style":"q1","text":""},{"style":"v","text":"Mbedd maa ngi soowaloo,"},{"style":"q1","text":"ndax biiñ bu ñàkk,"},{"style":"q1","text":"gépp mbégte fey,"},{"style":"q1","text":"bànneexu àddina dëddu."},{"style":"q1","text":""},{"style":"v","text":"Tojiti neen a des ci dëkk bi,"},{"style":"q1","text":"bunt ba rajaxoo."},{"style":"q1","text":""},{"style":"v","text":"Noonu lay deme ci àddina,"},{"style":"q2","text":"ci biir xeet yi,"},{"style":"q1","text":"mbete benn-benni doomi oliw gu ñu gësëm,"},{"style":"q1","text":"mbaa doomi reseñ yu des ca ndey ja,"},{"style":"q1","text":"gannaaw bu ñu wittee ba sottal."},{"style":"s1","text":"Ndes wi rëcc mbugal a ngi sant"},{"style":"q1","text":""},{"style":"v","text":"Ña ngay xaacu di sarxolle,"},{"style":"q1","text":"waa sowu di sàbbaal ci màggug Aji Sax ji."},{"style":"q1","text":""},{"style":"v","text":"Yeen waa penku, sargalleen Aji Sax ji boog,"},{"style":"q1","text":"yeen waa duni géej, teralleen Aji Sax ji Yàllay Israayil."},{"style":"q1","text":""},{"style":"v","text":"Cati àddina lay woy di jibe, nu dégg ñu naa:"},{"style":"q1","text":"«Màggaay ñeel na Aji Jub ji!»"},{"style":"s1","text":"Coono jeexul"},{"style":"q1","text":"Waaye man dama ne: «Jeexle naa!"},{"style":"q1","text":"Jeexle naa, wóoy man!"},{"style":"q1","text":"Workat yaa ngi wor a wor,"},{"style":"q1","text":"ba far làggi ci wor.»"},{"style":"q1","text":""},{"style":"v","text":"Yeen waa àddina,"},{"style":"q1","text":"kat-kat, pax akub caax dëgmal na leen."},{"style":"q1","text":""},{"style":"v","text":"Nga déggi yuux, daw,"},{"style":"q1","text":"tàbbi biir pax,"},{"style":"q1","text":"génne pax,"},{"style":"q1","text":"laawoob caax."},{"style":"q1","text":"Mbàndi kaw kat ubbi na,"},{"style":"q1","text":"suufu àddina di rëŋ-rëŋi."},{"style":"q1","text":""},{"style":"v","text":"Suuf daa loroo loru,"},{"style":"q1","text":"fett a fett,"},{"style":"q1","text":"salfaañoo salfaañu."},{"style":"q1","text":""},{"style":"v","text":"Suuf a ngi jang a jang ni màndikat,"},{"style":"q1","text":"ba joy ni mbaar,"},{"style":"q1","text":"moyam diis ko gann,"},{"style":"q1","text":"mu daanu te du jógati."},{"style":"s1","text":"Aji Sax jeey nguuru fa Siyoŋ"},{"style":"q1","text":""},{"style":"v","text":"Bu bésub keroogee"},{"style":"q1","text":"Aji Sax ji dikke mbugal"},{"style":"q2","text":"gàngoor ya fa kaw-a-kaw,"},{"style":"q1","text":"ak buuri àddina yi fi suuf."},{"style":"q1","text":""},{"style":"v","text":"Dees leen di boole denc cim pax,"},{"style":"q1","text":"ne leen ràpp tëj,"},{"style":"q1","text":"ba mu am ay fani fan, ñu mbugal leen."},{"style":"q1","text":""},{"style":"v","text":"Kersa la weer wiy am,"},{"style":"q1","text":"gàcce ñeel jant bi;"},{"style":"q1","text":"Aji Sax ji Boroom gàngoor yi daal"},{"style":"q1","text":"mooy nguuru Yerusalem, kaw tundu \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","text":"leeram ga jollee fa kanam magi ñoñam."}]