[{"style":"s2","text":"[6]"},{"style":"q1","text":""},{"style":"v","text":"Soo bokkeek kilifa ndab,"},{"style":"q1","text":"xamal bu baax ni ngay lekke."},{"style":"q1","text":""},{"style":"v","text":"Soo dee ku bëgg lekk,"},{"style":"q1","text":"téyeel sa loxo."},{"style":"q1","text":""},{"style":"v","text":"Bul xemmem daraam lu neex,"},{"style":"q1","text":"ñam woowu la lay naxe."},{"style":"s2","text":"[7]"},{"style":"q1","text":""},{"style":"v","text":"Bul rey sa bopp ci wut alal,"},{"style":"q1","text":"bu ko xalaat sax."},{"style":"q1","text":""},{"style":"v","text":"Ndax xef ak xippi mu wéy,"},{"style":"q1","text":"mel ni lu saxi laaf,"},{"style":"q1","text":"ne fëyy, naaw ni jaxaay."},{"style":"s2","text":"[8]"},{"style":"q1","text":""},{"style":"v","text":"Ab nay, bul lekke këram,"},{"style":"q1","text":"bul xemmem daraam lu neex."},{"style":"q1","text":""},{"style":"v","text":"Ma ngay xool la ngay lekk, nu mu tollu,"},{"style":"q1","text":"naan la: «Lekkal, naanal!»"},{"style":"q1","text":"Te xol ba àndu ca."},{"style":"q1","text":""},{"style":"v","text":"Ndog soo ca lekk, waccu ko,"},{"style":"q1","text":"yiw woo ko wax daldi neen."},{"style":"s2","text":"[9]"},{"style":"q1","text":""},{"style":"v","text":"Bul wax akub dof,"},{"style":"q1","text":"day xeeb sam xel."},{"style":"s2","text":"[10]"},{"style":"q1","text":""},{"style":"v","text":"Bul toxal màndargam cosaan ci làccu suuf,"},{"style":"q1","text":"bul aakimoo suufas jirim."},{"style":"q1","text":""},{"style":"v","text":"Ki koy aar màgg na,"},{"style":"q1","text":"da koy taxawu, dal ci sa kaw."},{"style":"s2","text":"[11]"},{"style":"q1","text":""},{"style":"v","text":"Defal sam xel ci yar,"},{"style":"q1","text":"te teewlu kàdduy xam-xam."},{"style":"s2","text":"[12]"},{"style":"q1","text":""},{"style":"v","text":"Bul ñéebloo yar gone,"},{"style":"q1","text":"bantal ko taxu koo dee."},{"style":"q1","text":""},{"style":"v","text":"Soo ko bantalee,"},{"style":"q1","text":"mu mana mucc."},{"style":"s2","text":"[13]"},{"style":"q1","text":""},{"style":"v","text":"Sama doom, soo xeloo,"},{"style":"q1","text":"sama xol neex."},{"style":"q1","text":""},{"style":"v","text":"Ngay wax njub,"},{"style":"q1","text":"ma bég."},{"style":"s2","text":"[14]"},{"style":"q1","text":""},{"style":"v","text":"Bul ñee bàkkaarkat,"},{"style":"q1","text":"saxool ragal Aji Sax ji,"},{"style":"q1","text":""},{"style":"v","text":"ndax kat moo am muj,"},{"style":"q1","text":"te kon sa yaakaar du tas."},{"style":"s2","text":"[15]"},{"style":"q1","text":""},{"style":"v","text":"Déglul, doom, muusal"},{"style":"q1","text":"te yebu ci yoonu njub."},{"style":"q1","text":""},{"style":"v","text":"Bul bokk ci ñiy lekk lu ëpp"},{"style":"q1","text":"aka màndi,"},{"style":"q1","text":""},{"style":"v","text":"ndax ku bëgg lekk aka màndi, ñàkk dab la,"},{"style":"q1","text":"dajjant it, rafle rekk."},{"style":"s2","text":"[16]"},{"style":"q1","text":""},{"style":"v","text":"Déglul sa baay bi la jur."},{"style":"q1","text":"Bu sa ndey màggatee, bu ko sàggane."},{"style":"q1","text":""},{"style":"v","text":"Jëndal dëgg te bu ko jaay."},{"style":"q1","text":"Sàkkul xel mu rafet ak yar akug dégg."},{"style":"q1","text":""},{"style":"v","text":"Su doom jubee, seral xolu baay."},{"style":"q1","text":"Boo juree ku rafet xel, am nga bànneex."},{"style":"q1","text":""},{"style":"v","text":"Deel bégal ndey ak baay,"},{"style":"q1","text":"rawatina ndey ji la jur."},{"style":"s2","text":"[17]"},{"style":"q1","text":""},{"style":"v","text":"Doom, teewloo ma xel te roy ma."},{"style":"q1","text":""},{"style":"v","text":"Ab gànc yeer mu xóot la,"},{"style":"q1","text":"jabaru jàmbur di teen bu xat."},{"style":"q1","text":""},{"style":"v","text":"Ma ngay tëroo nib sàcc,"},{"style":"q1","text":"góor ñu bare lay fecciloo worma."},{"style":"s2","text":"[18]"},{"style":"q1","text":""},{"style":"v","text":"Ana kuy tiislu, naan: «Wóoy, ngalla man!»"},{"style":"q1","text":"Ana kuy xulooka jàmbat,"},{"style":"q1","text":"di gaañu ci dara,"},{"style":"q1","text":"bët ya xonq curr?"},{"style":"q1","text":""},{"style":"v","text":"Xanaa kiy naan-naane biiñ,"},{"style":"q1","text":"di mosi yeneen njafaan?"},{"style":"q1","text":""},{"style":"v","text":"Bul xool biiñ ak xonqaayam."},{"style":"q1","text":"Aka yànj cib kaas"},{"style":"q1","text":"te neexa jolu!"},{"style":"q1","text":""},{"style":"v","text":"Bu weesoo mu màtt la ni jaan,"},{"style":"q1","text":"ne la càppit ni ñàngóor."},{"style":"q1","text":""},{"style":"v","text":"Dangay mel ni kuy gis lu doy waar,"},{"style":"q1","text":"sam xel di ràbb lu jekkadi."},{"style":"q1","text":""},{"style":"v","text":"Nga mel ni ku tëdd biir géej,"},{"style":"q1","text":"di jaayu ca biir gaal ga,"},{"style":"q1","text":""},{"style":"v","text":"nga naan: «Dañu maa dóor te gaañuwuma,"},{"style":"q1","text":"duma ma te yéguma ko."},{"style":"q1","text":"Moo! Duma yewwu nag,"},{"style":"q1","text":"ba naaneeti?»"}]