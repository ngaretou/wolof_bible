[{"style":"s1","text":"Bul gàddul kenn bor"},{"style":"q1","text":""},{"style":"v","text":"Doom, dangaa gàddul nit bor,"},{"style":"q1","text":"dige feyal jàmbur,"},{"style":"q1","text":""},{"style":"v","text":"saw làmmiñ yeb la cim pax,"},{"style":"q1","text":"nga keppu?"},{"style":"q1","text":""},{"style":"v","text":"Yaa ngi ci sa loxoy moroom,"},{"style":"q1","text":"kon defal nii, doom, ba rëcc:"},{"style":"q1","text":"doxal tinuji ko,"},{"style":"q1","text":"raamali ko."},{"style":"q1","text":""},{"style":"v","text":"Bul gëmm,"},{"style":"q1","text":"bul nelaw!"},{"style":"q1","text":""},{"style":"v","text":"Dawal ni kéwél gu rëccub rëbb,"},{"style":"q1","text":"ne fërr ni picc mu raw ab fiirkat."},{"style":"s1","text":"Bul yaafus"},{"style":"q1","text":""},{"style":"v","text":"Yaafus bee, xoolal xorondom,"},{"style":"q1","text":"seetal ci moom, ba muus."},{"style":"q1","text":""},{"style":"v","text":"Du ku ko yilif mbaa ku koy sas,"},{"style":"q1","text":"te jiiteesu ko."},{"style":"q1","text":""},{"style":"v","text":"Bu jotee mu denc ab dundam,"},{"style":"q1","text":"mu jot, mu for lekkam."},{"style":"q1","text":""},{"style":"v","text":"Moo yaafus bi, foo àppal tëraay bi?"},{"style":"q1","text":"Loo deeti xaar ci yewwu?"},{"style":"q1","text":""},{"style":"v","text":"Ngay dajjant aka for bët,"},{"style":"q1","text":"di tegley loxooka jaaxaan."},{"style":"q1","text":""},{"style":"v","text":"Néewlee ngi lay dikkal nib sàcc,"},{"style":"q1","text":"ñàkk gànnaayul la."},{"style":"s1","text":"Ku dëng, nii lay mel"},{"style":"q1","text":""},{"style":"v","text":"Ku tekkeedi te bon,"},{"style":"q1","text":"day wër di fen,"},{"style":"q1","text":""},{"style":"v","text":"day piis, di wokkeb tànk,"},{"style":"q1","text":"di waxe baaraam,"},{"style":"q1","text":""},{"style":"v","text":"jekkadiy xalaat, di mébét lu bon"},{"style":"q1","text":"aka yokku ay."},{"style":"q1","text":""},{"style":"v","text":"Moo tax musiba di jekki dal ko,"},{"style":"q1","text":"mu jekki yàqu, ba wees paj."},{"style":"s1","text":"Aji Sax ji bañ na lii"},{"style":"q1","text":""},{"style":"v","text":"Juróom benn a ngii yu Aji Sax ji bañ,"},{"style":"q1","text":"ba ci juróom ñaar yu mu seexlu:"},{"style":"q1","text":""},{"style":"v","text":"kuy daŋŋiiral,"},{"style":"q1","text":"kuy fen,"},{"style":"q1","text":"kuy rey nit ku deful dara,"},{"style":"q1","text":""},{"style":"v","text":"kuy fexe lu bon ci xolam,"},{"style":"q1","text":"kuy gaawtuy def lu ñaaw,"},{"style":"q1","text":""},{"style":"v","text":"seede buy fen rekk,"},{"style":"q1","text":"rawatina kuy yokku ay ci biiri bokk."},{"style":"s1","text":"Ku njaaloo gis ko"},{"style":"q1","text":""},{"style":"v","text":"Doom, defal li la baay sant,"},{"style":"q1","text":"te bul wacc ndigalal yaay."},{"style":"q1","text":""},{"style":"v","text":"Takkal foo tollu, taf ci sa xol,"},{"style":"q1","text":"fas ko ci sa baat."},{"style":"q1","text":""},{"style":"v","text":"Booy dox mu di la jiite,"},{"style":"q1","text":"nga tëdd, mu di la sàmm,"},{"style":"q1","text":"nga yewwu, mu di la waxal."},{"style":"q1","text":""},{"style":"v","text":"Santaane da lay niital, ndigal leeralal la,"},{"style":"q1","text":"artookub yar di yoonu dund."},{"style":"q1","text":""},{"style":"v","text":"Da lay aar ci ndaw su bon"},{"style":"q1","text":"ak jabaru jàmbur ak làmmiñam wu neex."},{"style":"q1","text":""},{"style":"v","text":"Bu ko xemmeme taaram,"},{"style":"q1","text":"bumu la fiir aki lamsal."},{"style":"q1","text":""},{"style":"v","text":"Dogu mburu fey nab gànc;"},{"style":"q1","text":"waaye jabaru jàmbur, bakkanu lëmm."},{"style":"q1","text":""},{"style":"v","text":"Te ku ne sawara cas, tafoo,"},{"style":"q1","text":"lakk say yére."},{"style":"q1","text":""},{"style":"v","text":"Ku dox ciy xal it,"},{"style":"q1","text":"say tànk ñor."},{"style":"q1","text":""},{"style":"v","text":"Mooy ànd ak jabaru jàmbur,"},{"style":"q1","text":"ku ko def gis ko."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Deesul sikk ku sàcc aw ñam,"},{"style":"q1","text":"def ci biiram ndax xiif."},{"style":"q1","text":""},{"style":"v","text":"Waaye bu feeñee, fey boroom juróom ñaari yoon,"},{"style":"q1","text":"joxe alalu këram yépp."},{"style":"q1","text":""},{"style":"v","text":"Njaaloo di ñàkk bopp,"},{"style":"q1","text":"kuy sànk bakkanam a koy def."},{"style":"q1","text":""},{"style":"v","text":"Mbugal ak toroxte dib añam,"},{"style":"q1","text":"te gàcceem du deñ."},{"style":"q1","text":""},{"style":"v","text":"Fiiraange day feesal xolu góor,"},{"style":"q1","text":"te du ñéeblu buy feyu."},{"style":"q1","text":""},{"style":"v","text":"Du faale ndàmpaay,"},{"style":"q1","text":"loo ko may, may, du giif."}]