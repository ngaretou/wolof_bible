[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Feppu dugub wu ànd ak dal"},{"style":"q1","verse":"","text":"moo gën lu ne gàññ kërug ayoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Surga, bu rafetee xel, tiim doom juy rusloo,"},{"style":"q1","verse":"","text":"séddu ni doom ci alalu sangam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Xaalis ak wurus sawaraa koy xelli,"},{"style":"q1","verse":"","text":"waaye nit, ab xolam, Aji Sax jee koy nattu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ku bon ay déglu wax ju bon,"},{"style":"q1","verse":"","text":"fen-kat di teewlu ay sos."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Kuy ñaawal ku ñàkk, tooñ nga Ka ko sàkk;"},{"style":"q1","verse":"","text":"kuy reetaan ku jàq, mbugalam du jaas."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Teraangay maam, sët ba;"},{"style":"q1","verse":"","text":"sagu doom, baay ba."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Waxi teggin jekkul cib dof,"},{"style":"q1","verse":"","text":"ay fen ci as gor waxi noppi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Alalu ger njiglaay la ca ka koy joxe,"},{"style":"q1","verse":"","text":"ba fu mu jublu, mu nooy."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Baale tooñ, yokk cofeel;"},{"style":"q1","verse":"","text":"sulliw ay, tas xarit."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ku amug dégg ngay yedd benn yoon,"},{"style":"q1","verse":"","text":"ab dof téeméeri yar du ko waññi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ku bon fippu doŋŋ lay jéem,"},{"style":"q1","verse":"","text":"te musibaa koy dikkal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Taseek gaynde gu ñàkki doomam"},{"style":"q1","verse":"","text":"moo gën taseek dof ak yëfi dofam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Kuy feye mbon jëf ju baax,"},{"style":"q1","verse":"","text":"lu bon du jóge sa kër."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ndoortel ay di wal mu tàmbali,"},{"style":"q1","verse":"","text":"luy indib xuloo, bàyyil."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Dëggal ku sikk, daan ku jub,"},{"style":"q1","verse":"","text":"Aji Sax ji sib na yooyu ñaar."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ab dof du am xaalis, jënde xel mu rafet;"},{"style":"q1","verse":"","text":"bëggu ca dara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Xarit du bëgg, di bañ;"},{"style":"q1","verse":"","text":"mbokk day bokk ak yaw say coono."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ku ñàkk bopp ay dige feyal jàmbur,"},{"style":"q1","verse":"","text":"di ko gàddul boram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ku bëggu ay bëggi tooñ;"},{"style":"q1","verse":"","text":"damu, yàqule."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku njublaŋ du baaxle,"},{"style":"q1","verse":"","text":"kuy wax njekkar añe musiba."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ku jur ab dof, am naqar;"},{"style":"q1","verse":"","text":"ab dof waajuram du bég."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Xol bu sedd day garabal,"},{"style":"q1","verse":"","text":"xol bu tiis day semmal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ku bon day nangu alalu ger ca suuf,"},{"style":"q1","verse":"","text":"nara jalgati yoon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku amug dégg ne jàkk ci xel mu rafet,"},{"style":"q1","verse":"","text":"ab dof ne xóll, xel ma sore lool."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Doom ju dof di tiisu baay ba"},{"style":"q1","verse":"","text":"ak naqaru ndey ja."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ku deful dara, feyloo kob daan, dug njub;"},{"style":"q1","verse":"","text":"dóor as gor du yoon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ku moom sa làmmiñ am nga xam-xam,"},{"style":"q1","verse":"","text":"te ku teey am ngag dégg."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Ab dof sax bu noppee, nirook boroom xel;"},{"style":"q1","verse":"","text":"ku ne cell, ñu fooge lag muus."}]