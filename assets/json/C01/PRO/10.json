[{"style":"ms","verse":"","text":"Kàddu yu xelu yu Suleymaan"},{"style":"mr","verse":"","text":"(Saar 10---24)"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Kàddu yu xelu yu Suleymaan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Doom rafet xel, baay ba bég;"},{"style":"q1","verse":"","text":"doom dof, ndey ja tiis."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Alal ju lewul du jariñ;"},{"style":"q1","verse":"","text":"jub, mucc ci gàtt fan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aji Sax ji du seetaan kiy def njekk, di xiif,"},{"style":"q1","verse":"","text":"waaye day xañ ab soxor la mu xemmem."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yaafus, walaakaana;"},{"style":"q1","verse":"","text":"farlu, woomle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ku ngóob jot, nga góob, xelu nga;"},{"style":"q1","verse":"","text":"ngóob taxaw, ngay nelaw, gàcce la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Jub, barkeelu;"},{"style":"q1","verse":"","text":"ku soxor, wax ja làq fitna."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ku jëfe njekk barkeel, saw tur du fey;"},{"style":"q1","verse":"","text":"ab soxor, turam seey."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ku rafet xel day jëfe santaane,"},{"style":"q1","verse":"","text":"ab dof wax a wax, ba loru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Màndu, fegu;"},{"style":"q1","verse":"","text":"dëng, weeru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Piiseek làq-làqal, sabab njàqare;"},{"style":"q1","verse":"","text":"ab dof wax a wax, ba loru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Kàdduy ku jub bëtu ndox la, day naatal;"},{"style":"q1","verse":"","text":"waaye ku soxor, wax ja làq fitna."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Mbañeel ak ayoo;"},{"style":"q1","verse":"","text":"cofeel ak jéggale bépp tooñ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Kuy dégg, say wax rafet;"},{"style":"q1","verse":"","text":"te ku ñàkk bopp, yelloo yetu gannaaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku rafet xel day xam, ne cell;"},{"style":"q1","verse":"","text":"bu dof noppiwul, yàqule teew."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Alal day aar boroom ni ab tata,"},{"style":"q1","verse":"","text":"ñàkk di lor baadoolo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Kiy jëfe njekk jot peyam, dunde;"},{"style":"q1","verse":"","text":"ab soxor di bàkkaare alalam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Yaru, gudd fan;"},{"style":"q1","verse":"","text":"sàgganey artu, sànku."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ku la nëbbal mbañeel, da lay fen;"},{"style":"q1","verse":"","text":"kuy wër di sos, ab dof la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Wax ju bare, moy ñàkku ca;"},{"style":"q1","verse":"","text":"ku moom sa làmmiñ, xelu nga."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Kàddug boroom njekk di ngën-gi-xaalis,"},{"style":"q1","verse":"","text":"xelum coxor amul solo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Kàdduy boroom njekk jariñ na ñu bare,"},{"style":"q1","verse":"","text":"dof ñàkk na bopp, ba far dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Barkeb Aji Sax jeey maye alal,"},{"style":"q1","verse":"","text":"kër-këri taxula barele."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Jëf ju bon la dof di fowe;"},{"style":"q1","verse":"","text":"ku am dég-dég safoo lu xelu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ab soxor, li mu ragal moo koy dab;"},{"style":"q1","verse":"","text":"nammeelu kiy def njekk nag day sotti."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Bu ngëlén walee, ab soxor ne mes;"},{"style":"q1","verse":"","text":"kiy def njekk, sab reen sax ba fàww."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ndaw lu tayel ca ka yónnee"},{"style":"q1","verse":"","text":"daa mel ni lu forox ciy gëñ mbaa saxaar ciy gët."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ragal Aji Sax ji, gudd fan;"},{"style":"q1","verse":"","text":"soxor, gàtt fan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Kiy def njekk day séentu mbégte, gis ko;"},{"style":"q1","verse":"","text":"ab soxor yaakaar, yàkki."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Aji Sax jeeki nammeelam rawtu la ci ku mat,"},{"style":"q1","verse":"","text":"di sànkute ci kuy def lu bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Kiy def njekk deesu ko daaneel mukk,"},{"style":"q1","verse":"","text":"ku soxor du yàgg cim réew."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Kàddug boroom njekk day jur xel mu rafet,"},{"style":"q1","verse":"","text":"ku wax njekkar ñu tëj sa gémmiñ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Boroom njekk day xam wax ju yiw;"},{"style":"q1","verse":"","text":"ab soxor sàlloo waxi njekkar."}]