[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ab siis, bànneexu boppam rekk,"},{"style":"q1","verse":"","text":"loo ko digal mu xalab."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ab dof wutul ag dégg,"},{"style":"q1","verse":"","text":"li muy def kay mooy jaay ag muus."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bu mbon nuyoo, xeebeel topp ca;"},{"style":"q1","verse":"","text":"jëf ju ñaaw, gàcce rekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Waxi nit ndox mu xóot la,"},{"style":"q1","verse":"","text":"bu bënnee, xelli, ñu di ca xelu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Faral ku tooñ baaxul,"},{"style":"q1","verse":"","text":"bul xañ dëgg ku deful dara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ab dof, làmmiñam sooke naw ay,"},{"style":"q1","verse":"","text":"ay waxam di sàkku yeti gannaaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ab dof, waxam a koy sànk,"},{"style":"q1","verse":"","text":"làmmiñu boppam a koy dugal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Waxi jëwkat di ñam wu neex"},{"style":"q1","verse":"","text":"wuy seey, jàll ca biir-a-biir."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Kuy sàggane sa liggéey,"},{"style":"q1","verse":"","text":"yaak kuy yàq a bokk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Turu Aji Sax ji rawtu bu mag la."},{"style":"q1","verse":"","text":"Ku jub daw làqu ca, raw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Alal day aar boroom ni ab tata,"},{"style":"q1","verse":"","text":"mu foog ne maneesu koo bëtt."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Bew, yàqule;"},{"style":"q1","verse":"","text":"jëkke woyof, mujje tedd."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Tontu te déggagoo,"},{"style":"q1","verse":"","text":"ndof la, ak ñàkk kersa."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku bëgga dund, dékku woppi yaram,"},{"style":"q1","verse":"","text":"waaye xol bu jeex maneesu koo wéye."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Ku amug dégg wut xam-xam,"},{"style":"q1","verse":"","text":"ku rafet xel sàkku xam-xam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Deel maye, da lay ubbil bunt,"},{"style":"q1","verse":"","text":"di la àggale ci boroom daraja."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ku njëkk a layoo, ñu ne yaa yey,"},{"style":"q1","verse":"","text":"ba keroog ka nga joteel weddi la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Tegoo bant day feyu ay,"},{"style":"q1","verse":"","text":"di àtte boroom doole yu jote."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Jubook mbokk moo tooñ, jéggi tataa ko gëna yomb,"},{"style":"q1","verse":"","text":"mbaa ubbi bunt yu tëje ràpp."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Làmmiñ feesal na biiru boroom,"},{"style":"q1","verse":"","text":"yoolu kàddu suur na boroom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Dund ak dee a ngi ci làmmiñ,"},{"style":"q1","verse":"","text":"te wax garab la, ku ko bëgg, lekk ca doom ya."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Ku am jabar, am nga ngëneel,"},{"style":"q1","verse":"","text":"am nga yiwu Aji Sax ji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ku ñàkk a ngi leewaayu,"},{"style":"q1","verse":"","text":"boroom alal di ko jànni."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Barey àndandoo lor a ngi ci,"},{"style":"q1","verse":"","text":"waaye xarit a ngi gëna taq nit mbokkam."}]