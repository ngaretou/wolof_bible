[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ku bëgga xam, bëgg ku la yedd;"},{"style":"q1","verse":"","text":"ku bañ ku la artu, bàyyima nga."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ku baax, Aji Sax ji bége la;"},{"style":"q1","verse":"","text":"kuy fexe lu bon, mu daan la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ab soxor, taxawaayam du yàgg;"},{"style":"q1","verse":"","text":"kuy jëfe njekk garab la, reen ba ne kekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Jeeg bu jàmbaare day teral jëkkër ja,"},{"style":"q1","verse":"","text":"jeeg buy rusloo day semmal jëkkër ja."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Boroom njekk, lu jekk lay mébét;"},{"style":"q1","verse":"","text":"ab soxor di lal pexey wor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ab soxor, waxam am tëru la, day reye;"},{"style":"q1","verse":"","text":"kàdduy kiy jubal di musle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ab soxor bu daanoo, jeex na;"},{"style":"q1","verse":"","text":"ku jub, sa kër taxaw, ne këcc."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Xel mu ñaw, jëw bu rafet;"},{"style":"q1","verse":"","text":"sam xel jekkadi, ñu xeeb la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Woyof mbubb, am benn surga doŋŋ,"},{"style":"q1","verse":"","text":"moo gën réyal turki tey dee ak xiif."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ku jub day toppatoo ag juram,"},{"style":"q1","verse":"","text":"ab soxor néeg cig juram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Beyal sab tool, sab dund doy;"},{"style":"q1","verse":"","text":"topp ay caaxaan ñàkk bopp la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ab soxor ay xemmem alal ju lewul,"},{"style":"q1","verse":"","text":"kiy jëfe njekk garab la, reen baa ko meññal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Bàkkaaru làmmiñ dugal na boroom,"},{"style":"q1","verse":"","text":"ku jub mucc ci njàqare."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Làmmiñ reggal na boroom;"},{"style":"q1","verse":"","text":"ñaq, jariñu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Na dof di jëfe, njub la ci moom;"},{"style":"q1","verse":"","text":"dégg ndigal rafetum xel la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ab dof bu meree, mu gaawa feeñ;"},{"style":"q1","verse":"","text":"ku teey, tanqamlu saaga."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ku dëggu, seede dëgg;"},{"style":"q1","verse":"","text":"seede bu bon, fen rekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Làmmiñu waxkat, jam-jami jaasi;"},{"style":"q1","verse":"","text":"kàddu gu xelu, garab la ci."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Kàdduy dëgg, day sax dàkk;"},{"style":"q1","verse":"","text":"fen, xef-xippi, mu wéy."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Kuy ràbb lu bon lal pexey wor,"},{"style":"q1","verse":"","text":"kuy digle jàmm, am mbégte."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ku jub du ami ay,"},{"style":"q1","verse":"","text":"ab soxor du tàggook musiba."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Aji Sax ji sib na fen-kat,"},{"style":"q1","verse":"","text":"safoo ku dëggu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Nit ku ñaw day xam, ba ca;"},{"style":"q1","verse":"","text":"ab dof siiwal waxi dofam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Njaxlaf, jiitu;"},{"style":"q1","verse":"","text":"yaafus, des gannaaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Njàqare day jeexal xolu boroom,"},{"style":"q1","verse":"","text":"wax ju neex di tooyal xolam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ku jub day xool ku muy xaritool\\f + \\fr 12.26 \\ft Mbaa: \\fqa Ku jub day teg moroomam ciw yoon;\\f*;"},{"style":"q1","verse":"","text":"soxor bi, jikkoom a koy sànk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ab yaafus bu rëbbee, sooy;"},{"style":"q1","verse":"","text":"cawarte koom la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Jaaral ci njub, ba gudd fan;"},{"style":"q1","verse":"","text":"yoon wu baax deesu fa deewe."}]