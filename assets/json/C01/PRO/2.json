[{"style":"s1","verse":"","text":"Muus, mucc"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Sama doom, déggal samay wax,"},{"style":"q1","verse":"","text":"fonk samay santaane."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Teewlul xel mu rafet,"},{"style":"q1","verse":"","text":"sam xel di dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ngalla wutala dégg,"},{"style":"q1","verse":"","text":"wool dég-dég wall."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Sàkkul xel mu rafet ni xaalis,"},{"style":"q1","verse":"","text":"wut ko ni alal ju làqu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Kon nga xam luy ragal Aji Sax ji,"},{"style":"q1","verse":"","text":"xam Yàllaa di kan;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"ndax Aji Sax jeey maye xel mu rafet,"},{"style":"q1","verse":"","text":"kàddoom di taxa xam, di dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Day dencalal ndam kiy jubal,"},{"style":"q1","verse":"","text":"di yiir ku mat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Day wattu ku jub fu mu jaare,"},{"style":"q1","verse":"","text":"di sàmm wóllëreem ciw yoon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Kon nga xam njub ak yoon,"},{"style":"q1","verse":"","text":"xam jubal ak mboolem yoonu mbaax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ndax xel mu rafet miy tàbbi sa xol,"},{"style":"q1","verse":"","text":"nga xam, sa xol tooy,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"nga foog, fegu,"},{"style":"q1","verse":"","text":"amug dégg, raw,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"mucc ci yoonu ku bon"},{"style":"q1","verse":"","text":"ak kuy wax lu jekkadi,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"mucc ci kuy wacc yoonu njub,"},{"style":"q1","verse":"","text":"di jaare mbedd yu lëndëm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Kon nga mucc ci kuy def lu bon,"},{"style":"q1","verse":"","text":"tey bànneexoo lu jekkadi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Yoonu ku ni mel day lunk,"},{"style":"q1","verse":"","text":"jaaruwaayam dëng."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Xel mu rafet da lay musal ci jigéenu njaalookat,"},{"style":"q1","verse":"","text":"bokk feneen, di wax lu neex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Day dëddu wóllëreem, ba muy ndaw,"},{"style":"q1","verse":"","text":"fàtte \\w kóllëreem|kóllëre g-:\\w* ak Yàllaam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Këram day joy wuti dee,"},{"style":"q1","verse":"","text":"ay jaaruwaayam jëm njaniiw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ku dem ca moom dootoo délsi,"},{"style":"q1","verse":"","text":"doo gisati yoonu dund mukk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Kon nag aweel mbeddum waa ju baax,"},{"style":"q1","verse":"","text":"toppal yoonu defkatu njekk,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"ndax kuy jubal ay dëkke réew mi,"},{"style":"q1","verse":"","text":"te ku mat a fiy des."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Ku bon, ñu génne réew mi;"},{"style":"q1","verse":"","text":"kuy fecci worma, ñu buddi, sànni."}]