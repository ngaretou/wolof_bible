[{"style":"s1","text":"Muus, mucc"},{"style":"q1","text":""},{"style":"v","text":"Sama doom, déggal samay wax,"},{"style":"q1","text":"fonk samay santaane."},{"style":"q1","text":""},{"style":"v","text":"Teewlul xel mu rafet,"},{"style":"q1","text":"sam xel di dégg."},{"style":"q1","text":""},{"style":"v","text":"Ngalla wutala dégg,"},{"style":"q1","text":"wool dég-dég wall."},{"style":"q1","text":""},{"style":"v","text":"Sàkkul xel mu rafet ni xaalis,"},{"style":"q1","text":"wut ko ni alal ju làqu."},{"style":"q1","text":""},{"style":"v","text":"Kon nga xam luy ragal Aji Sax ji,"},{"style":"q1","text":"xam Yàllaa di kan;"},{"style":"q1","text":""},{"style":"v","text":"ndax Aji Sax jeey maye xel mu rafet,"},{"style":"q1","text":"kàddoom di taxa xam, di dégg."},{"style":"q1","text":""},{"style":"v","text":"Day dencalal ndam kiy jubal,"},{"style":"q1","text":"di yiir ku mat."},{"style":"q1","text":""},{"style":"v","text":"Day wattu ku jub fu mu jaare,"},{"style":"q1","text":"di sàmm wóllëreem ciw yoon."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Kon nga xam njub ak yoon,"},{"style":"q1","text":"xam jubal ak mboolem yoonu mbaax."},{"style":"q1","text":""},{"style":"v","text":"Ndax xel mu rafet miy tàbbi sa xol,"},{"style":"q1","text":"nga xam, sa xol tooy,"},{"style":"q1","text":""},{"style":"v","text":"nga foog, fegu,"},{"style":"q1","text":"amug dégg, raw,"},{"style":"q1","text":""},{"style":"v","text":"mucc ci yoonu ku bon"},{"style":"q1","text":"ak kuy wax lu jekkadi,"},{"style":"q1","text":""},{"style":"v","text":"mucc ci kuy wacc yoonu njub,"},{"style":"q1","text":"di jaare mbedd yu lëndëm."},{"style":"q1","text":""},{"style":"v","text":"Kon nga mucc ci kuy def lu bon,"},{"style":"q1","text":"tey bànneexoo lu jekkadi."},{"style":"q1","text":""},{"style":"v","text":"Yoonu ku ni mel day lunk,"},{"style":"q1","text":"jaaruwaayam dëng."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Xel mu rafet da lay musal ci jigéenu njaalookat,"},{"style":"q1","text":"bokk feneen, di wax lu neex."},{"style":"q1","text":""},{"style":"v","text":"Day dëddu wóllëreem, ba muy ndaw,"},{"style":"q1","text":"fàtte \\w kóllëreem|kóllëre g-:\\w* ak Yàllaam."},{"style":"q1","text":""},{"style":"v","text":"Këram day joy wuti dee,"},{"style":"q1","text":"ay jaaruwaayam jëm njaniiw."},{"style":"q1","text":""},{"style":"v","text":"Ku dem ca moom dootoo délsi,"},{"style":"q1","text":"doo gisati yoonu dund mukk."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Kon nag aweel mbeddum waa ju baax,"},{"style":"q1","text":"toppal yoonu defkatu njekk,"},{"style":"q1","text":""},{"style":"v","text":"ndax kuy jubal ay dëkke réew mi,"},{"style":"q1","text":"te ku mat a fiy des."},{"style":"q1","text":""},{"style":"v","text":"Ku bon, ñu génne réew mi;"},{"style":"q1","text":"kuy fecci worma, ñu buddi, sànni."}]