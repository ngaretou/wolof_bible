[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Tont lu neex day giifal,"},{"style":"q1","verse":"","text":"baat bu ñagas di jógal xol."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ku xelu àddu, xam-xam rafet;"},{"style":"q1","verse":"","text":"ab dof di talalali."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aji Sax ji moo gis lépp,"},{"style":"q1","verse":"","text":"di xool ku baax ak ku bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Wax ju neex mooy garab guy taxa dund,"},{"style":"q1","verse":"","text":"kàddu yu jekkadi di jeexal xol."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ab dof day xeeb yaru baay ba,"},{"style":"q1","verse":"","text":"kuy dégg waxi artu, xelu nga."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Kër ku jub, koom gu yaa;"},{"style":"q1","verse":"","text":"ku jubadi, alalam jur fitna."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ku xelu àddu, xam-xam law;"},{"style":"q1","verse":"","text":"ab dof amul xelam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Saraxub ku soxor, Aji Sax ji bañ na ko;"},{"style":"q1","verse":"","text":"ñaanu kuy jubal da koy bége."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Jëfi ku bon, Aji Sax ji seexlu na ko;"},{"style":"q1","verse":"","text":"ku sàlloo jëf ju baax, safoo na la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Mbugal tar na, ñeel ku wacc yoon;"},{"style":"q1","verse":"","text":"ku bañ waxi artu, dangay dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Njaniiw ak biir suuf, Aji Sax ji di gis,"},{"style":"q1","verse":"","text":"xolu doom aadama waxi noppi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Kuy ñaawle bëggul ku ko artu,"},{"style":"q1","verse":"","text":"te du laaji ku rafet xel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Xol bu sedd, kanam gu leer;"},{"style":"q1","verse":"","text":"xol bu tiis, boroom ne yogg."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku amug dégg sàkku xam-xam,"},{"style":"q1","verse":"","text":"ab dof di toppi caaxaan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Xol bu tiis, naqar wu sax;"},{"style":"q1","verse":"","text":"xol bu neex, bànneex bu sax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Néewle te ragal Aji Sax ji"},{"style":"q1","verse":"","text":"moo gën barele, sa bopp ubu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Njëlu ñetti xob fa ñu la soppe"},{"style":"q1","verse":"","text":"moo dàq yàpp wu duuf fu ñu la bañe."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Naqari deret, taalu ay."},{"style":"q1","verse":"","text":"Teey, fey fitna."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yoonu yaafus, dégi neen;"},{"style":"q1","verse":"","text":"kuy jubal, saw xàll yaa."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Doom rafet xel, baay ba bég;"},{"style":"q1","verse":"","text":"te ab dof ay xeeb ndey ja."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Jëfi dof a neex ku ñàkk bopp,"},{"style":"q1","verse":"","text":"ku amug dégg def njub."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Diisoo ñàkk, pexe sànku;"},{"style":"q1","verse":"","text":"digle yu takku, pexe àntu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Nit bége na tontam lu dal,"},{"style":"q1","verse":"","text":"kàddu gu jib fa mu ñoree neex!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku rafet xel, jubal nga yoonu gudd fan,"},{"style":"q1","verse":"","text":"moyu teggi, ba jëm njaniiw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Ku bew, Aji Sax ji màbb sa kër;"},{"style":"q1","verse":"","text":"ab jëtun, Aji Sax ji ñoŋal pàkkam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Aji Sax ji bañ na mébét mu bon,"},{"style":"q1","verse":"","text":"te wax ju yiw daa \\w sell|sell:\\w* fa moom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Wutin wu lewul, sonal sa waa kër;"},{"style":"q1","verse":"","text":"ku bañ alalu ger gudd fan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Ku jub day teeylu tontam;"},{"style":"q1","verse":"","text":"ab soxor, wax ju ñaaw rekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Aji Sax ji day dëddu ab soxor,"},{"style":"q1","verse":"","text":"di nangu ñaanu ku jub."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Kanam gu leer day seral xol;"},{"style":"q1","verse":"","text":"xibaaru jàmm, jàmmu yaram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Kuy dégg artu yu koy musal,"},{"style":"q1","verse":"","text":"ku rafet xel a ngoog."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Xalab waxi yar, fowe sa bopp la;"},{"style":"q1","verse":"","text":"deel déggi artu, yokk sam xel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Ragal Aji Sax ji day rafetal xel;"},{"style":"q1","verse":"","text":"jëkke woyof, mujje tedd."}]