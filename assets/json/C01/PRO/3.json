[{"style":"s1","text":"Wóolul Yàlla"},{"style":"q1","text":""},{"style":"v","text":"Doom, bul fàtte sama njàngale,"},{"style":"q1","text":"defal sa xel ci saay santaane,"},{"style":"q1","text":""},{"style":"v","text":"ngir fan wi gudd lool,"},{"style":"q1","text":"nga gëna am jàmm."},{"style":"q1","text":""},{"style":"v","text":"Bu la ngor ak worma dëddu,"},{"style":"q1","text":"booleel takk ci sa baat,"},{"style":"q1","text":"bind ko ci sa àlluway xol."},{"style":"q1","text":""},{"style":"v","text":"Kon nga xam lu jaadu,"},{"style":"q1","text":"neex Yàllaak doom aadama."},{"style":"q1","text":""},{"style":"v","text":"Nanga wóoloo Aji Sax ji sa léppi xol,"},{"style":"q1","text":"te bul wéeru ci saw déggin."},{"style":"q1","text":""},{"style":"v","text":"Loo mébét, bàyyil Yàlla xel,"},{"style":"q1","text":"mu xàllal la yoon."},{"style":"q1","text":""},{"style":"v","text":"Bul doyloo sa xel mu rafet,"},{"style":"q1","text":"ragalal Aji Sax ji, dëddu mbon;"},{"style":"q1","text":""},{"style":"v","text":"sa yaram wér,"},{"style":"q1","text":"say cér naat."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Teralal Aji Sax ji ci sa alal"},{"style":"q1","text":"ak loo njëkka meññle,"},{"style":"q1","text":""},{"style":"v","text":"say sàq day fees, ne xéew,"},{"style":"q1","text":"say mbàndi ndoxi reseñ di wal."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Doom, bul xeeb yaru Aji Sax ji,"},{"style":"q1","text":"bul bañ mu waññi la,"},{"style":"q1","text":""},{"style":"v","text":"ndax Aji Sax ji, ku mu sopp, yar la,"},{"style":"q1","text":"ni baay, ak doom ju ko neex."},{"style":"s1","text":"Li xel mu rafet di jariñ"},{"style":"q1","text":""},{"style":"v","text":"Mbégtee ñeel ku daj xel mu rafet"},{"style":"q1","text":"ak ku amug dégg."},{"style":"q1","text":""},{"style":"v","text":"Am koo gën am xaalis,"},{"style":"q1","text":"te njariñ laa raw wurus."},{"style":"q1","text":""},{"style":"v","text":"Xel mu rafet a gën gànjar;"},{"style":"q1","text":"loo mana bëgg moo ko raw."},{"style":"q1","text":""},{"style":"v","text":"Gudd fan, mu joxee ndijoor;"},{"style":"q1","text":"càmmoñ ba, alal ak teraanga."},{"style":"q1","text":""},{"style":"v","text":"Fa muy jaare yiw la,"},{"style":"q1","text":"fu mu awe jàmm la."},{"style":"q1","text":""},{"style":"v","text":"Mooy garab giy taxa dund"},{"style":"q1","text":"ci ku ko jàpp,"},{"style":"q1","text":"di mbégtey ku ca ŋoy."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji xel mu rafet la sose suuf,"},{"style":"q1","text":"lale asamaan ag dégg."},{"style":"q1","text":""},{"style":"v","text":"Ci xam-xamam la ndoxi xóote ya ne jàyy,"},{"style":"q1","text":"ay niir di lay."},{"style":"s1","text":"Rafet xel barke la"},{"style":"q1","text":""},{"style":"v","text":"Sama doom, saxool xelu akug foog,"},{"style":"q1","text":"dee ci xool foo tollu."},{"style":"q1","text":""},{"style":"v","text":"Day guddal saw fan,"},{"style":"q1","text":"di rafetal sa jikko."},{"style":"q1","text":""},{"style":"v","text":"Kon sab dox wóor,"},{"style":"q1","text":"doo fakkastalu."},{"style":"q1","text":""},{"style":"v","text":"Soo tëddee doo tiit;"},{"style":"q1","text":"say nelaw day neex."},{"style":"q1","text":""},{"style":"v","text":"Kon doo ragal njàqare lu bette"},{"style":"q1","text":"ak saay-saay buy songe,"},{"style":"q1","text":""},{"style":"v","text":"ndax Aji Sax ji da lay yiir,"},{"style":"q1","text":"di la musal cig fiir."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bul xañ njekk ku ko yelloo,"},{"style":"q1","text":"ndegam man nga ko."},{"style":"q1","text":""},{"style":"v","text":"Bul ne say dëkk: «Demal ba beneen, ma jox la,»"},{"style":"q1","text":"te fekk la yor."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bul fexeel dëkkandoo"},{"style":"q1","text":"bu dëkk ak yaw ci kóolute."},{"style":"q1","text":""},{"style":"v","text":"Bul joteek kenn ci neen,"},{"style":"q1","text":"te tooñu la."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bul ñee ku néeg,"},{"style":"q1","text":"te bu ko roy fenn."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji daa sib ku dëng,"},{"style":"q1","text":"xejjoo kuy jubal."},{"style":"q1","text":""},{"style":"v","text":"Aji Sax ji day alag kër ku bon,"},{"style":"q1","text":"di barkeel waa kër ku jub."},{"style":"q1","text":""},{"style":"v","text":"Kuy ñaawle, moo lay ñaawal,"},{"style":"q1","text":"ku toroxlu, mu may law yiw."},{"style":"q1","text":""},{"style":"v","text":"Teraanga di céru ku rafet xel,"},{"style":"q1","text":"ab dof gàccoo gàcce."}]