[{"style":"s2","verse":"","text":"[19]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Bul ñee ku bon,"},{"style":"q1","verse":"","text":"bul wuta ànd ak moom,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"ndax coxor lay nas ci xolam,"},{"style":"q1","verse":"","text":"te fitna la ay waxam di yee."},{"style":"s2","verse":"","text":"[20]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Xel mu rafet ay tabax kër,"},{"style":"q1","verse":"","text":"rafet déggin a koy samp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Xam-xam ay feesal néeg ya"},{"style":"q1","verse":"","text":"ak mboolem alal ju jafe te yànj."},{"style":"s2","verse":"","text":"[21]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ku rafet xel di boroom doole,"},{"style":"q1","verse":"","text":"ku xam, gëna man;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"tegtal yu xelu lañuy xaree,"},{"style":"q1","verse":"","text":"diisoo bu yaatooy maye ndam."},{"style":"s2","verse":"","text":"[22]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Wax ju xelu sut nab dof,"},{"style":"q1","verse":"","text":"jataayu pénc du fa àddoo."},{"style":"s2","verse":"","text":"[23]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Kuy mébét lu bon,"},{"style":"q1","verse":"","text":"ñu ne yaay rambaaj bi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Pexem dof bàkkaar la,"},{"style":"q1","verse":"","text":"te kuy ñaawle, ñu sib la."},{"style":"s2","verse":"","text":"[24]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Bu mettee, nga yoqi,"},{"style":"q1","verse":"","text":"sa doolee néew."},{"style":"s2","verse":"","text":"[25]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Wallul ku ñuy reyi te tooñul,"},{"style":"q1","verse":"","text":"xettalil kuy tërëf, ñu di ko reyi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Soo nee sa yoon nekku ci it,"},{"style":"q1","verse":"","text":"kiy natt xol yi, xam na,"},{"style":"q1","verse":"","text":"moom miy wattu sa bakkan, xam na,"},{"style":"q1","verse":"","text":"te kat mooy fey nit jëfam."},{"style":"s2","verse":"","text":"[26]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Doom, deel lekk lem, baax na,"},{"style":"q1","verse":"","text":"lem ju xelli, boo sexee, mu tem-temi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Xamal ne xel mu rafet ni la neexe;"},{"style":"q1","verse":"","text":"soo xeloo, sa muj rafet,"},{"style":"q1","verse":"","text":"te sa yaakaar du tas."},{"style":"s2","verse":"","text":"[27]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Soxor bi, bul tëru boroom njekk ci këram,"},{"style":"q1","verse":"","text":"bul yàq dëkkuwaayam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Boroom njekk day daanu juróom ñaari yoon, jógaat,"},{"style":"q1","verse":"","text":"ab soxor sërëx ci njekkar."},{"style":"s2","verse":"","text":"[28]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Bu sab noon daanoo, bu ko bànneexoo,"},{"style":"q1","verse":"","text":"bu tërëfee, bu ko bége."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Lu ko moy Aji Sax ji gis la, ñaawlu ko,"},{"style":"q1","verse":"","text":"daldi giif, dootu ko mbugal."},{"style":"s2","verse":"","text":"[29]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Bu sa xol tàng ci ku bon,"},{"style":"q1","verse":"","text":"bul ñee ku soxor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku bon amul muj,"},{"style":"q1","verse":"","text":"ku soxor day mel ni taal bu fey."},{"style":"s2","verse":"","text":"[30]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Doom, ragalal Aji Sax ji, wormaal buur."},{"style":"q1","verse":"","text":"Bul lëngook kuy fippu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Yàllaak buur a koy bette mbugal,"},{"style":"q1","verse":"","text":"te kenn xamul musiba mu muy doon."},{"style":"s1","verse":"","text":"Yeneen tegtal a ngii"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Lii it ñi rafet xel a ko wax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Par-parloo cib àtte baaxul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku ne defkatu lu bon: «Yaw, tooñoo,»"},{"style":"q1","verse":"","text":"aw nit móolu la, mbooloo ŋàññ la;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"ku ko ne: «Yaa tooñ,» yaay baaxle,"},{"style":"q1","verse":"","text":"taasoo barke bu yaa."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ki lay wax dëgg"},{"style":"q1","verse":"","text":"sa soppe la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ruujal sab tool, ji ko,"},{"style":"q1","verse":"","text":"doora tabax sa kër."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Bul tuumaal nit ci neen,"},{"style":"q1","verse":"","text":"te bu ko seedeel ay fen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Bul ne: «Li mu ma def laa koy def,"},{"style":"q1","verse":"","text":"damay feyu li mu ma def.»"},{"style":"s1","verse":"","text":"Yaafus amul njariñ"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Toolub ku yaafus laa jaare,"},{"style":"q1","verse":"","text":"ak tóokërub nit ku ñàkk bopp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Ndeke lépp a nga saxi dég,"},{"style":"q1","verse":"","text":"fépp fees akum ñax,"},{"style":"q1","verse":"","text":"tabaxu doj ba ko wër màbb."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Maa ko gis, di xalaat,"},{"style":"q1","verse":"","text":"xoolaat ko, jànge ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Booy dajjant aka for bët,"},{"style":"q1","verse":"","text":"di tegley loxooka jaaxaan,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"néewlee ngi lay dikkal nib sàcc,"},{"style":"q1","verse":"","text":"ñàkk gànnaayul la."}]