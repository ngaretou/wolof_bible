[{"style":"s2","text":"[19]"},{"style":"q1","text":""},{"style":"v","text":"Bul ñee ku bon,"},{"style":"q1","text":"bul wuta ànd ak moom,"},{"style":"q1","text":""},{"style":"v","text":"ndax coxor lay nas ci xolam,"},{"style":"q1","text":"te fitna la ay waxam di yee."},{"style":"s2","text":"[20]"},{"style":"q1","text":""},{"style":"v","text":"Xel mu rafet ay tabax kër,"},{"style":"q1","text":"rafet déggin a koy samp."},{"style":"q1","text":""},{"style":"v","text":"Xam-xam ay feesal néeg ya"},{"style":"q1","text":"ak mboolem alal ju jafe te yànj."},{"style":"s2","text":"[21]"},{"style":"q1","text":""},{"style":"v","text":"Ku rafet xel di boroom doole,"},{"style":"q1","text":"ku xam, gëna man;"},{"style":"q1","text":""},{"style":"v","text":"tegtal yu xelu lañuy xaree,"},{"style":"q1","text":"diisoo bu yaatooy maye ndam."},{"style":"s2","text":"[22]"},{"style":"q1","text":""},{"style":"v","text":"Wax ju xelu sut nab dof,"},{"style":"q1","text":"jataayu pénc du fa àddoo."},{"style":"s2","text":"[23]"},{"style":"q1","text":""},{"style":"v","text":"Kuy mébét lu bon,"},{"style":"q1","text":"ñu ne yaay rambaaj bi."},{"style":"q1","text":""},{"style":"v","text":"Pexem dof bàkkaar la,"},{"style":"q1","text":"te kuy ñaawle, ñu sib la."},{"style":"s2","text":"[24]"},{"style":"q1","text":""},{"style":"v","text":"Bu mettee, nga yoqi,"},{"style":"q1","text":"sa doolee néew."},{"style":"s2","text":"[25]"},{"style":"q1","text":""},{"style":"v","text":"Wallul ku ñuy reyi te tooñul,"},{"style":"q1","text":"xettalil kuy tërëf, ñu di ko reyi."},{"style":"q1","text":""},{"style":"v","text":"Soo nee sa yoon nekku ci it,"},{"style":"q1","text":"kiy natt xol yi, xam na,"},{"style":"q1","text":"moom miy wattu sa bakkan, xam na,"},{"style":"q1","text":"te kat mooy fey nit jëfam."},{"style":"s2","text":"[26]"},{"style":"q1","text":""},{"style":"v","text":"Doom, deel lekk lem, baax na,"},{"style":"q1","text":"lem ju xelli, boo sexee, mu tem-temi."},{"style":"q1","text":""},{"style":"v","text":"Xamal ne xel mu rafet ni la neexe;"},{"style":"q1","text":"soo xeloo, sa muj rafet,"},{"style":"q1","text":"te sa yaakaar du tas."},{"style":"s2","text":"[27]"},{"style":"q1","text":""},{"style":"v","text":"Soxor bi, bul tëru boroom njekk ci këram,"},{"style":"q1","text":"bul yàq dëkkuwaayam."},{"style":"q1","text":""},{"style":"v","text":"Boroom njekk day daanu juróom ñaari yoon, jógaat,"},{"style":"q1","text":"ab soxor sërëx ci njekkar."},{"style":"s2","text":"[28]"},{"style":"q1","text":""},{"style":"v","text":"Bu sab noon daanoo, bu ko bànneexoo,"},{"style":"q1","text":"bu tërëfee, bu ko bége."},{"style":"q1","text":""},{"style":"v","text":"Lu ko moy Aji Sax ji gis la, ñaawlu ko,"},{"style":"q1","text":"daldi giif, dootu ko mbugal."},{"style":"s2","text":"[29]"},{"style":"q1","text":""},{"style":"v","text":"Bu sa xol tàng ci ku bon,"},{"style":"q1","text":"bul ñee ku soxor."},{"style":"q1","text":""},{"style":"v","text":"Ku bon amul muj,"},{"style":"q1","text":"ku soxor day mel ni taal bu fey."},{"style":"s2","text":"[30]"},{"style":"q1","text":""},{"style":"v","text":"Doom, ragalal Aji Sax ji, wormaal buur."},{"style":"q1","text":"Bul lëngook kuy fippu."},{"style":"q1","text":""},{"style":"v","text":"Yàllaak buur a koy bette mbugal,"},{"style":"q1","text":"te kenn xamul musiba mu muy doon."},{"style":"s1","text":"Yeneen tegtal a ngii"},{"style":"q1","text":""},{"style":"v","text":"Lii it ñi rafet xel a ko wax."},{"style":"b","text":""},{"style":"q1","text":"Par-parloo cib àtte baaxul."},{"style":"q1","text":""},{"style":"v","text":"Ku ne defkatu lu bon: «Yaw, tooñoo,»"},{"style":"q1","text":"aw nit móolu la, mbooloo ŋàññ la;"},{"style":"q1","text":""},{"style":"v","text":"ku ko ne: «Yaa tooñ,» yaay baaxle,"},{"style":"q1","text":"taasoo barke bu yaa."},{"style":"q1","text":""},{"style":"v","text":"Ki lay wax dëgg"},{"style":"q1","text":"sa soppe la."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Ruujal sab tool, ji ko,"},{"style":"q1","text":"doora tabax sa kër."},{"style":"b","text":""},{"style":"q1","text":""},{"style":"v","text":"Bul tuumaal nit ci neen,"},{"style":"q1","text":"te bu ko seedeel ay fen."},{"style":"q1","text":""},{"style":"v","text":"Bul ne: «Li mu ma def laa koy def,"},{"style":"q1","text":"damay feyu li mu ma def.»"},{"style":"s1","text":"Yaafus amul njariñ"},{"style":"q1","text":""},{"style":"v","text":"Toolub ku yaafus laa jaare,"},{"style":"q1","text":"ak tóokërub nit ku ñàkk bopp."},{"style":"q1","text":""},{"style":"v","text":"Ndeke lépp a nga saxi dég,"},{"style":"q1","text":"fépp fees akum ñax,"},{"style":"q1","text":"tabaxu doj ba ko wër màbb."},{"style":"q1","text":""},{"style":"v","text":"Maa ko gis, di xalaat,"},{"style":"q1","text":"xoolaat ko, jànge ca."},{"style":"q1","text":""},{"style":"v","text":"Booy dajjant aka for bët,"},{"style":"q1","text":"di tegley loxooka jaaxaan,"},{"style":"q1","text":""},{"style":"v","text":"néewlee ngi lay dikkal nib sàcc,"},{"style":"q1","text":"ñàkk gànnaayul la."}]