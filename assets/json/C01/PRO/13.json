[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Doom ju xelu dégg yaru baay,"},{"style":"q1","verse":"","text":"kuy ñaawle faalewuli artu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Wax ju rafet yool na boroom,"},{"style":"q1","verse":"","text":"workat fitna la namm."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Moom sa làmmiñ, sàmm sa bakkan;"},{"style":"q1","verse":"","text":"rattaxle, yàqule."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ab yaafus day yaakaar, du am;"},{"style":"q1","verse":"","text":"ab njaxlaf sàkku, woomle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ku jub bañ na ay fen,"},{"style":"q1","verse":"","text":"ku soxor di indi gàcceek yeraange."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ku mat day jub, ba fegu;"},{"style":"q1","verse":"","text":"moykat soxor, ba sànku."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Nit a ngi am-amlu, amul dara;"},{"style":"q1","verse":"","text":"nit di dee-deelu te fees dell."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ku am ay feye alalam, ba mucc;"},{"style":"q1","verse":"","text":"ku amul deesu ko tëkku."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ku jub day leer nàññ,"},{"style":"q1","verse":"","text":"ab soxor mel ni taal bu fey."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Réy-réylu jote rekk lay jur,"},{"style":"q1","verse":"","text":"ku dégg ndigal a xelu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Alal ju lewul day naaw;"},{"style":"q1","verse":"","text":"foral benn-benn, ba barele."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yaakaar ju tas day jeexal xol,"},{"style":"q1","verse":"","text":"aajo ju faju di garab guy taxa dund."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ku sofental ndigalu Yàlla, yàqule;"},{"style":"q1","verse":"","text":"ku jëfe santaane Yàlla, yoolu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Njàngale mu xelu day suuxat bakkan,"},{"style":"q1","verse":"","text":"ba boroom du tàbbi ci fiiri dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Dangay xam lu jaadu, ñu naw la;"},{"style":"q1","verse":"","text":"yoonu workat day metti."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Képp ku teey jëfe xam-xam;"},{"style":"q1","verse":"","text":"ab dof ay siiwal ndofam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ndaw lu bon day loru."},{"style":"q1","verse":"","text":"Ndaw lu wóor garab la ci nit."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ku sàggane yoonu yar, séddoo ñàkk ak gàcce;"},{"style":"q1","verse":"","text":"kuy déggi artu, am teraanga."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Aajo ju faju tooyal na xol,"},{"style":"q1","verse":"","text":"te ab dof jomb naa dëddu mbon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Àndal ak ku xelu, sam xel rafet;"},{"style":"q1","verse":"","text":"ku lëngookub dof, loru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ay topp na moykat,"},{"style":"q1","verse":"","text":"ku jub juble."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Ku baax, donale ba cay sëtam;"},{"style":"q1","verse":"","text":"alalu moykat, muuru ku jub."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ku néewle bey na, meññeef ne gàññ,"},{"style":"q1","verse":"","text":"ñu àtte ko ñàkkal, mu ñàkk ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku dul bantal sa doom, bëggoo ko."},{"style":"q1","verse":"","text":"Ku bëgg sa doom teel koo yar."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Ku jub day lekk ba regg,"},{"style":"q1","verse":"","text":"soxor ba dëkke xiif."}]