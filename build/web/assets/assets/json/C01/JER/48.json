[{"style":"s1","verse":"","text":"Lu jëm ci réewum Mowab"},{"style":"p","verse":"","text":""},{"style":"v","verse":"1","text":"Lu jëm ci Mowab:"},{"style":"p","verse":"","text":"Aji Sax ji Yàllay Israayil, Boroom gàngoori xare yi dafa wax ne:"},{"style":"q1","verse":"","text":"«Ngalla waa Nebo, seen dëkk ba yàqu na!"},{"style":"q1","verse":"","text":"Kiryaatayim, ñu toroxal, nangu ko;"},{"style":"q1","verse":"","text":"tata ja daanu, tasaaroo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Mowab amatul woy,"},{"style":"q1","verse":"","text":"fa Esbon la leen noon ya fexeel lu bon,"},{"style":"q1","verse":"","text":"ne: “Aycaleen nu raafal wii xeet!”"},{"style":"q1","verse":"","text":"Yeen waa Madmen it, ñu wedamal;"},{"style":"q1","verse":"","text":"saamar topp leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Yuux jibe na Oronayim,"},{"style":"q1","verse":"","text":"ndaw salfaañook rajaxoo ju réy!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Mowab tas, ay fereem serandoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ñay yéeg jëm Luxit, di jooyoo."},{"style":"q1","verse":"","text":"Ña ngay yéeg, di jooyoo."},{"style":"q1","verse":"","text":"Ñay wàcci Oronayim di dégg aji jàq jiy yuuxoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ñu naan: “Dawleen, rawale seen bakkan,"},{"style":"q1","verse":"","text":"ba tumurànkeji ni gajju màndiŋ!”"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Mowab, yeena yaakaar dooleek alal,"},{"style":"q1","verse":"","text":"yeen it, dees na leen jàpp;"},{"style":"q1","verse":"","text":"Kemos seen tuur mi duggi ngàllo,"},{"style":"q1","verse":"","text":"mooki \\w sarxalkatam|sarxalkat b-:\\w* aki kàngamam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Dëkkoo dëkk ay yàqtoo,"},{"style":"q1","verse":"","text":"benn dëkk du mucc!"},{"style":"q1","verse":"","text":"Waa xur wii sànku,"},{"style":"q1","verse":"","text":"waa joor gee yàqtoo.»"},{"style":"q1","verse":"","text":"Muy la Aji Sax ji wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"«Gasalleen Mowab,"},{"style":"q1","verse":"","text":"day sànku moos,"},{"style":"q1","verse":"","text":"ay dëkkam gental,"},{"style":"q1","verse":"","text":"kenn du fa des!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Alkànde ñeel na kuy sàggane liggéeyu Aji Sax ji."},{"style":"q1","verse":"","text":"Alkànde ñeel na kuy xañ deret saamaru Aji Sax ji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"«Mowab a dale féex cag ndawam,"},{"style":"q1","verse":"","text":"kenn jàppu ko njaam, yóbbu,"},{"style":"q1","verse":"","text":"sotteesu ko ndab ak ndab,"},{"style":"q1","verse":"","text":"xanaa mu ne degg ni biiñ bu taaje,"},{"style":"q1","verse":"","text":"di saf, di xeeñ ba tey."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Moo tax ay bés a ngii di ñëw,»"},{"style":"q2","verse":"","text":"kàddug Aji Sax jee,"},{"style":"q1","verse":"","text":"«maa leen di yónnee ay nit ñu sotti diw gi,"},{"style":"q1","verse":"","text":"ndab, soteet; njaq, rajax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Yaakaaru Mowab ci Kemos tuur mi tas,"},{"style":"q1","verse":"","text":"ni yaakaaru waa kër Israayil ci Betel ma ñu wóolu tase woon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"«Yaw réewu Mowab, du yaay kañu,"},{"style":"q1","verse":"","text":"naan jàmbaar nga,"},{"style":"q1","verse":"","text":"diy ñeyi xare?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Song nañu Mowab, yàqate ay dëkkam;"},{"style":"q1","verse":"","text":"la man ci ay ndawam daanu dee.»"},{"style":"q1","verse":"","text":"Kàddug Buur Aji Sax jee, Boroom gàngoori xare yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"«Musibay Mowab a ngi jegesi,"},{"style":"q1","verse":"","text":"safaanam làbbaleeti, wuti ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Mboolem yeen ñi wër xeet wii, jooyleen ko;"},{"style":"q1","verse":"","text":"yeen ñi dégg turam ñépp, jooyleen ko."},{"style":"q1","verse":"","text":"Neleen: “Moo, nu gii nguur mana foqe,"},{"style":"q1","verse":"","text":"ba foqaale jii daraja?”"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"«Yeen waa Dibon, dëkk bu taaru bi,"},{"style":"q1","verse":"","text":"wàccleen seen teraanga ju kawe"},{"style":"q1","verse":"","text":"te toog ci digg maral;"},{"style":"q1","verse":"","text":"kiy yàqatesi Mowab kat song na leen,"},{"style":"q1","verse":"","text":"di màbbsi seeni tata."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yeen waa Arower,"},{"style":"q1","verse":"","text":"taxawleen fi yoon wi te teewlu;"},{"style":"q1","verse":"","text":"góor gu fi jaare mbaa ndaw su rëcc, di daw,"},{"style":"q1","verse":"","text":"ngeen laaj ko ne ko: “Lu xew?”"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ñu ne: “Gàcce ñeel na Mowab gii tas!”"},{"style":"q1","verse":"","text":"Yuuxooleen, ne wóoy!"},{"style":"q1","verse":"","text":"Yéeneleen fa tàkkal dexu Arnon,"},{"style":"q1","verse":"","text":"ne Mowab tas na!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Mbugal dikkal joor gi,"},{"style":"q1","verse":"","text":"dikkal dëkk yii di Olon, Yaxacc ak Mefaat"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"ak Dibon ak Nebo ak Bet Diblatayim"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"ak Kiryaatayim ak Bet Gamul ak Bet Mewon"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"ak Keryot ak Boccara,"},{"style":"q1","verse":"","text":"mboolem dëkki Mowab daal,"},{"style":"q1","verse":"","text":"fu jegeek fu sore."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Sidditu Mowab day dog,"},{"style":"q1","verse":"","text":"loxoom damm.»"},{"style":"q1","verse":"","text":"Kàddug Aji Sax jee!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"«Du Mowab a doon diir Aji Sax ji mbagg?"},{"style":"q1","verse":"","text":"Nañu leen màndal sànjum Aji Sax ji,"},{"style":"q1","verse":"","text":"ba ñu waccu, di ca xuus,"},{"style":"q1","verse":"","text":"ba ñoom it, ñu di leen ree."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Waa Mowabee, du yeena doon ree Israayil?"},{"style":"q1","verse":"","text":"Dangeen leena fekk ci biiri sàcc,"},{"style":"q1","verse":"","text":"ba fu ngeen leen tudde, wëcc bopp?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Yeen waa Mowab,"},{"style":"q1","verse":"","text":"baleen dëkk yi, dëkke paxi doj."},{"style":"q1","verse":"","text":"Defleen niy xati,"},{"style":"q1","verse":"","text":"tàgge peggi xunt."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"«Noo dégg ni Mowab bewe,"},{"style":"q1","verse":"","text":"bew lool,"},{"style":"q1","verse":"","text":"réy, reew, di damu;"},{"style":"q1","verse":"","text":"ne daŋŋ-daŋŋiiral."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Maa xam seen reewande,»"},{"style":"q2","verse":"","text":"kàddug Aji Sax jee,"},{"style":"q1","verse":"","text":"«seeni kañu neen la,"},{"style":"q1","verse":"","text":"seeni jëf di neen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Moo tax may jooy Mowab,"},{"style":"q1","verse":"","text":"di wooteel wall Mowab gépp,"},{"style":"q1","verse":"","text":"di yuuxul waa Kir Eres."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Yaw reseñu Sibma gi,"},{"style":"q1","verse":"","text":"ma jooyal la lu raw la ma jooyal Yaser."},{"style":"q1","verse":"","text":"Yaw la say car jàlloon géej,"},{"style":"q1","verse":"","text":"àkki géeju Yaser,"},{"style":"q1","verse":"","text":"sa figg jeek sa reseñ, noon yàqate."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Tukkal nañu mbégteek bànneex"},{"style":"q1","verse":"","text":"fi tóokëri réewum Mowab."},{"style":"q1","verse":"","text":"Maa ŋacc mbàndi biiñ ya,"},{"style":"q1","verse":"","text":"kenn nalatu fa reseñ, di soowoo"},{"style":"q1","verse":"","text":"coowal mbégte, xanaa ay yuux."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Yuux jibe na Esbon, ba Elale, àkki ba Yaxacc"},{"style":"q1","verse":"","text":"ak Cowar ba Oronayim ak Eglat Selisiya."},{"style":"q1","verse":"","text":"Ndoxi Nimrim sax ŋiis na."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"Maay bóome biir Mowab.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":"«Maay bóom kuy joxe sarax ci \\w bérebi jaamookaay|bérebi jaamookaay y-:\\w* yi,"},{"style":"q1","verse":"","text":"di taalal am tuuram \\w cuuraay|cuuraay l-:\\w*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"36","text":"Naa jooy Mowab jooyi xol"},{"style":"q1","verse":"","text":"yu toxoro di jooye ab dëj,"},{"style":"q1","verse":"","text":"na toxoro di jooye dëju waa Kir Eres."},{"style":"q1","verse":"","text":"Lu ñu dajale woon sànku na!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"37","text":"Ñoom ñépp a nelu,"},{"style":"q1","verse":"","text":"wat seeni sikkim,"},{"style":"q1","verse":"","text":"daggat seeni loxo,"},{"style":"q1","verse":"","text":"sol ay saaku, di ko ñaawloo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"38","text":"Kaw taaxi Mowab yépp aki péncam,"},{"style":"q1","verse":"","text":"fépp ay yuuxoo di jib."},{"style":"q1","verse":"","text":"Maa toj Mowab"},{"style":"q1","verse":"","text":"ni ndab lu kenn soxlaatul.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"39","text":"«Yuuxooleen, ne leen: “Céy gii yàqtoo!"},{"style":"q1","verse":"","text":"Mowab gii wone gannaaw! Mowab rus na.”"},{"style":"q1","verse":"","text":"Ñi ko wër ñépp a koy reetaan, di ci raf.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"40","text":"Aji Sax ji déy dafa wax ne:"},{"style":"q1","verse":"","text":"«Lu mel ni jaxaay a ngii naa jàyy,"},{"style":"q1","verse":"","text":"firiy laafam, këfsi Mowab."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"41","text":"Dees na teg Keryot ga ca Mowab loxo,"},{"style":"q1","verse":"","text":"ne tata ya taral;"},{"style":"q1","verse":"","text":"su keroogee fitu jàmbaar ne tërit,"},{"style":"q1","verse":"","text":"mu mel ni kuy matu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"42","text":"Mowab day sànku, ba xeetam fey."},{"style":"q1","verse":"","text":"Ñoo doon diir Aji Sax ji mbagg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"43","text":"Yeen waa Mowab,"},{"style":"q1","verse":"","text":"pat-pati rekk, di tàbbiy pax aki yeer.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"44","text":"«Pat-patiy daw, tàbbim pax;"},{"style":"q1","verse":"","text":"génnum pax, tàbbim yeer."},{"style":"q1","verse":"","text":"Maay yen Mowab, maa leen di yen moos"},{"style":"q1","verse":"","text":"seen atum mbugal.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"45","text":"«Fa keppaaru Esbon lañuy daw taxawi,"},{"style":"q1","verse":"","text":"doole dëddu."},{"style":"q1","verse":"","text":"Booba sawaraa tàkke Esbon, di law;"},{"style":"q1","verse":"","text":"sël-sëlee foofa ca péeyu Buur Sixon,"},{"style":"q1","verse":"","text":"ba dajal réewum Mowab, nitu coow ña,"},{"style":"q1","verse":"","text":"boole jë akum kaaŋ xoyom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"46","text":"Wóoy, waa Mowab, ngalla yeen!"},{"style":"q1","verse":"","text":"Yeen ñiy jaamu Kemos, sànku ngeen!"},{"style":"q1","verse":"","text":"Dees na jàpp seen doom yu góor njaam;"},{"style":"q1","verse":"","text":"jigéen ñi it, njaam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"47","text":"«Du tee fan yu mujj ya,"},{"style":"q1","verse":"","text":"ma tijji wërsëgu Mowab.»"},{"style":"q2","verse":"","text":"Kàddug Aji Sax jee."},{"style":"q1","verse":"","text":"Fii la àtteb Mowab yem."}]