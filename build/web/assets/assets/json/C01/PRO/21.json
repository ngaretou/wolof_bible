[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Xolu buur walum ndox la ci loxol Aji Sax ji,"},{"style":"q1","verse":"","text":"fu ko soob mu jëme ko fa."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Lu waay def, daa jub ca moom,"},{"style":"q1","verse":"","text":"waaye Aji Sax jeey natt xol yi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Deel def dëgg ak lu jub,"},{"style":"q1","verse":"","text":"moo gënal Aji Sax ji ab sarax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Xeebaateek réy-réylu"},{"style":"q1","verse":"","text":"mooy bàkkaar yi ñuy ràññee ku bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Farlu, woomle;"},{"style":"q1","verse":"","text":"ku gaawtu, mujj néewle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Alal ju la fen may,"},{"style":"q1","verse":"","text":"cóolóol la, day naaw, wut ko xaru la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Coxor day sànk boroom,"},{"style":"q1","verse":"","text":"ndax day baña def njub."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ab saay-saay day dëngal,"},{"style":"q1","verse":"","text":"nit ku dëggu di jubal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Dëkkeb ruq cim sàq"},{"style":"q1","verse":"","text":"moo gën jabar ju pànk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ab soxor day namma lore,"},{"style":"q1","verse":"","text":"te du yërëm moroomam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Boo mbugalee kuy ñaawle, ab téxét jànge ca;"},{"style":"q1","verse":"","text":"nga jàngal ku rafet xel, mu yokku."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Aji Jub ji Yàlla xam na la ne ca biir kër ku bon,"},{"style":"q1","verse":"","text":"te mooy sànk ku bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ku tanqamlu jooyi ku ñàkk"},{"style":"q1","verse":"","text":"dina woote wall, wall ñàkk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku mer, may ko ci sutura, mu giif;"},{"style":"q1","verse":"","text":"boroom xadar, boqal ko neexal, mu dal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Bu yoon amee, ku jub bég;"},{"style":"q1","verse":"","text":"kuy def lu bon jàq."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ku noppee jëfe xel,"},{"style":"q1","verse":"","text":"noppaluji njaniiw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ku topp sa bànneex, mujje ñàkk;"},{"style":"q1","verse":"","text":"ku sopp biiñ ak lu niin du woomle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ku bon këppoo ayu ku baax,"},{"style":"q1","verse":"","text":"workat gàddu musibam kuy jubal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Dëkke ndànd-foyfoy"},{"style":"q1","verse":"","text":"moo gën jabar ju tàng, bare ay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku xelu denc këram ngëneeli alal aku diw,"},{"style":"q1","verse":"","text":"ab dof saax-saaxee josam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ku saxoo njekk ak ngor,"},{"style":"q1","verse":"","text":"am fan wu gudd, naataangeek daraja."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Ku ñaw mana daan jàmbaari dëkk bu mag,"},{"style":"q1","verse":"","text":"ba màbb tata ja ñu yaakaaroon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ub sa gémmiñ, moom sa làmmiñ,"},{"style":"q1","verse":"","text":"mucc ci njàqare."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku réy te bew, mooy ñaawle,"},{"style":"q1","verse":"","text":"day reew, ba jéggi dayo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Ab yaafus day bëgg lu mu amul ba dee,"},{"style":"q1","verse":"","text":"ndax du nangoo liggéey,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"day yendoo xemmem, te du am;"},{"style":"q1","verse":"","text":"kay jëfe njekk di joxeeka joxewaat."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Saraxu nit ku bon ñaawtéef la,"},{"style":"q1","verse":"","text":"rawatina bu ci jubloo lu bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Seede buy fen day sànku,"},{"style":"q1","verse":"","text":"kuy dégg, sa kàddu sax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Ku bon day ñeme-ñemelu,"},{"style":"q1","verse":"","text":"ku jub la muy def da koy wóor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Nit xeluwul, amulug dégg ak pexe,"},{"style":"q1","verse":"","text":"ba manal Aji Sax ji dara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Nit ay takk, bésub xare,"},{"style":"q1","verse":"","text":"Aji Sax ji di maye ndam."}]