[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Nit ay dogu ci xolam,"},{"style":"q1","verse":"","text":"Aji Sax ji dogal, mu sotti."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Lu waay def, daa jekk ca moom,"},{"style":"q1","verse":"","text":"waaye Aji Sax jeey natt xol yi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Loo bëgga def, diis ko Aji Sax ji,"},{"style":"q1","verse":"","text":"say mébét sotti."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Lu Aji Sax ji sàkk, mbir la ca namm,"},{"style":"q1","verse":"","text":"ba ci ku soxor ak bésu mbugalam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Aji Sax ji sib na ku réy,"},{"style":"q1","verse":"","text":"da koy mbugal ci lu wér."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ku jiital ngor ak worma, Yàlla jéggal la;"},{"style":"q1","verse":"","text":"ku ragal Aji Sax ji, dëddu lu bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ku Aji Sax ji rafetlu say jëf,"},{"style":"q1","verse":"","text":"say bañ sax, mu jubaleek yaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Néewle te jub"},{"style":"q1","verse":"","text":"moo gën barele te jubadi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Nit ay sumb yoonam,"},{"style":"q1","verse":"","text":"Aji Sax ji sottal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Su buur àddoo cib àtte,"},{"style":"q1","verse":"","text":"muy kàddu gu Yàlla dogal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Nattub diisaay aki ndabam, na jub ngir Aji Sax ji."},{"style":"q1","verse":"","text":"Moo sàkk mboolem nattukaay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Buur daa sib kuy def lu bon,"},{"style":"q1","verse":"","text":"ndax njub mooy dëgëral nguuram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Wax ju dëggu, buur safoo boroom;"},{"style":"q1","verse":"","text":"ku jub, buur bëgg sa kàddu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Merum buur ndaw la, dee a ko yónni;"},{"style":"q1","verse":"","text":"ku rafetum xel, giifal ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Buur, na kanam ga leer, sa bakkan mucc,"},{"style":"q1","verse":"","text":"su la baaxee, mu mel ni taw bu toppum nji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Wutal xel mu rafet, bàyyi wurus;"},{"style":"q1","verse":"","text":"taamul ag dégg, wacc xaalis."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Yoonu kuy jubal day moyu lu bon,"},{"style":"q1","verse":"","text":"ku teeylu sa jëfin, sàmm sa bakkan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Réy, yàqule;"},{"style":"q1","verse":"","text":"xeebaate, jóoru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Woyof, ànd ak ku néewle"},{"style":"q1","verse":"","text":"moo gën séddook ku bew la mu lewal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku teewlu mbir, baaxle;"},{"style":"q1","verse":"","text":"ku wóolu Aji Sax ji, bég."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ki rafet xel mooy kiy ràññee,"},{"style":"q1","verse":"","text":"te su wax rafetee, dég-dég yomb."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Xel mu ñaw day suuxat bakkan,"},{"style":"q1","verse":"","text":"te ab dof jëfi dofam a koy yar."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ku rafet xel, say kàddu xelu;"},{"style":"q1","verse":"","text":"soo waxee, yey."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Wax ju yiw di lem juy xelli,"},{"style":"q1","verse":"","text":"neexa ñam, di jàmmi yaram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Yoon a ngii, nit defe ne jub na,"},{"style":"q1","verse":"","text":"te mu jëme ko ci dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ku bëgg lu neex, liggéey;"},{"style":"q1","verse":"","text":"ku bëgga faj sab xiif, njaxlaf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Sagaru nit mooy sulli lu bon,"},{"style":"q1","verse":"","text":"ay waxam sax sawara la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Nitu njekkar day yokk réeroo,"},{"style":"q1","verse":"","text":"ab soskat di féewaley xarit."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Ab soxor day yóbbaale dëkkandoom,"},{"style":"q1","verse":"","text":"jëme ko ci lu bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Piise, ku nar njekkar;"},{"style":"q1","verse":"","text":"màttu, ku def lu bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Bijjaawu bopp kaala gu yànj la,"},{"style":"q1","verse":"","text":"topp yoonu njub a koy joxe."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Muñ mer, moo gën njàmbaar;"},{"style":"q1","verse":"","text":"tënk sa bakkan moo raw nangub dëkk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Tegoo bant nit a koy def,"},{"style":"q1","verse":"","text":"dogal ba Aji Sax ji la."}]