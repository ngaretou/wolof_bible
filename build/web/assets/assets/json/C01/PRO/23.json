[{"style":"s2","verse":"","text":"[6]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Soo bokkeek kilifa ndab,"},{"style":"q1","verse":"","text":"xamal bu baax ni ngay lekke."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Soo dee ku bëgg lekk,"},{"style":"q1","verse":"","text":"téyeel sa loxo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bul xemmem daraam lu neex,"},{"style":"q1","verse":"","text":"ñam woowu la lay naxe."},{"style":"s2","verse":"","text":"[7]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Bul rey sa bopp ci wut alal,"},{"style":"q1","verse":"","text":"bu ko xalaat sax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ndax xef ak xippi mu wéy,"},{"style":"q1","verse":"","text":"mel ni lu saxi laaf,"},{"style":"q1","verse":"","text":"ne fëyy, naaw ni jaxaay."},{"style":"s2","verse":"","text":"[8]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ab nay, bul lekke këram,"},{"style":"q1","verse":"","text":"bul xemmem daraam lu neex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ma ngay xool la ngay lekk, nu mu tollu,"},{"style":"q1","verse":"","text":"naan la: «Lekkal, naanal!»"},{"style":"q1","verse":"","text":"Te xol ba àndu ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ndog soo ca lekk, waccu ko,"},{"style":"q1","verse":"","text":"yiw woo ko wax daldi neen."},{"style":"s2","verse":"","text":"[9]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Bul wax akub dof,"},{"style":"q1","verse":"","text":"day xeeb sam xel."},{"style":"s2","verse":"","text":"[10]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Bul toxal màndargam cosaan ci làccu suuf,"},{"style":"q1","verse":"","text":"bul aakimoo suufas jirim."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ki koy aar màgg na,"},{"style":"q1","verse":"","text":"da koy taxawu, dal ci sa kaw."},{"style":"s2","verse":"","text":"[11]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Defal sam xel ci yar,"},{"style":"q1","verse":"","text":"te teewlu kàdduy xam-xam."},{"style":"s2","verse":"","text":"[12]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Bul ñéebloo yar gone,"},{"style":"q1","verse":"","text":"bantal ko taxu koo dee."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Soo ko bantalee,"},{"style":"q1","verse":"","text":"mu mana mucc."},{"style":"s2","verse":"","text":"[13]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Sama doom, soo xeloo,"},{"style":"q1","verse":"","text":"sama xol neex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ngay wax njub,"},{"style":"q1","verse":"","text":"ma bég."},{"style":"s2","verse":"","text":"[14]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Bul ñee bàkkaarkat,"},{"style":"q1","verse":"","text":"saxool ragal Aji Sax ji,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"ndax kat moo am muj,"},{"style":"q1","verse":"","text":"te kon sa yaakaar du tas."},{"style":"s2","verse":"","text":"[15]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Déglul, doom, muusal"},{"style":"q1","verse":"","text":"te yebu ci yoonu njub."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Bul bokk ci ñiy lekk lu ëpp"},{"style":"q1","verse":"","text":"aka màndi,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"ndax ku bëgg lekk aka màndi, ñàkk dab la,"},{"style":"q1","verse":"","text":"dajjant it, rafle rekk."},{"style":"s2","verse":"","text":"[16]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Déglul sa baay bi la jur."},{"style":"q1","verse":"","text":"Bu sa ndey màggatee, bu ko sàggane."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Jëndal dëgg te bu ko jaay."},{"style":"q1","verse":"","text":"Sàkkul xel mu rafet ak yar akug dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Su doom jubee, seral xolu baay."},{"style":"q1","verse":"","text":"Boo juree ku rafet xel, am nga bànneex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Deel bégal ndey ak baay,"},{"style":"q1","verse":"","text":"rawatina ndey ji la jur."},{"style":"s2","verse":"","text":"[17]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Doom, teewloo ma xel te roy ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ab gànc yeer mu xóot la,"},{"style":"q1","verse":"","text":"jabaru jàmbur di teen bu xat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Ma ngay tëroo nib sàcc,"},{"style":"q1","verse":"","text":"góor ñu bare lay fecciloo worma."},{"style":"s2","verse":"","text":"[18]"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Ana kuy tiislu, naan: «Wóoy, ngalla man!»"},{"style":"q1","verse":"","text":"Ana kuy xulooka jàmbat,"},{"style":"q1","verse":"","text":"di gaañu ci dara,"},{"style":"q1","verse":"","text":"bët ya xonq curr?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Xanaa kiy naan-naane biiñ,"},{"style":"q1","verse":"","text":"di mosi yeneen njafaan?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Bul xool biiñ ak xonqaayam."},{"style":"q1","verse":"","text":"Aka yànj cib kaas"},{"style":"q1","verse":"","text":"te neexa jolu!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Bu weesoo mu màtt la ni jaan,"},{"style":"q1","verse":"","text":"ne la càppit ni ñàngóor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Dangay mel ni kuy gis lu doy waar,"},{"style":"q1","verse":"","text":"sam xel di ràbb lu jekkadi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Nga mel ni ku tëdd biir géej,"},{"style":"q1","verse":"","text":"di jaayu ca biir gaal ga,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"nga naan: «Dañu maa dóor te gaañuwuma,"},{"style":"q1","verse":"","text":"duma ma te yéguma ko."},{"style":"q1","verse":"","text":"Moo! Duma yewwu nag,"},{"style":"q1","verse":"","text":"ba naaneeti?»"}]