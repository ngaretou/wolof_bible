[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Naan biiñ ak ñaawle, naan ñoll, tanqale,"},{"style":"q1","verse":"","text":"ku mu nax xeluwoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Buur mer lool, mel ni gayndee ŋar;"},{"style":"q1","verse":"","text":"boo ko merloo, dangay xaru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Moytuw ay, ndam la;"},{"style":"q1","verse":"","text":"dof boo gis day bëgg xuloo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ab yaafus du gàbb ba mu jotee,"},{"style":"q1","verse":"","text":"day wut am ngóob, du am dara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Mébétu jàmbur day teen bu xóot,"},{"style":"q1","verse":"","text":"ku amug dégg a cay root."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ñu baree ngi naa: «Gore naa,»"},{"style":"q1","verse":"","text":"waaye kuy boroom kóllëre dëgg?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ku jub, di jëfeg mat,"},{"style":"q1","verse":"","text":"doom ju la wuutu bég na."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Su buur toogee di àtte,"},{"style":"q1","verse":"","text":"day gis, di ràññee mboolem ayib."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ana ku man ne fóot na xolam,"},{"style":"q1","verse":"","text":"ba tàggook bàkkaar?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Nattu diisaay yu yemul ak lu ni mel,"},{"style":"q1","verse":"","text":"Aji Sax ji bañ na ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Gone sax day jëf, nga gis jikkoom;"},{"style":"q1","verse":"","text":"bu naree dëggu te jub, nga xam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Nopp buy dégg ak bët buy gis,"},{"style":"q1","verse":"","text":"Aji Sax jee sàkk lu ci nekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Bul bëggi nelaw, ba ñàkk dab la;"},{"style":"q1","verse":"","text":"boo njaxlafee, lekk ba desal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Kuy waxaalee ngi naan: «Baaxul de!»"},{"style":"q1","verse":"","text":"Bu nee wërëñ, di damu naan: «Aka jar!»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Wurus am na ak gànjar yu bare,"},{"style":"q1","verse":"","text":"waaye kàdduy xam-xam a gën per yu jafe."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ku gàddul jàmbur bor, jëlal mbubbam;"},{"style":"q1","verse":"","text":"tayle ko, moo dige feyal jàmbur."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Njublaŋ, lekk, jëkke neex,"},{"style":"q1","verse":"","text":"mujj mel ni lancub suuf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Pexe, ndigal a koy lal;"},{"style":"q1","verse":"","text":"xare, ay tegtal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Kuy wër di jëw, wuññi sutura;"},{"style":"q1","verse":"","text":"ku réy làmmiñ, bul déeyook moom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku saaga sa ndey mbaa sa baay"},{"style":"q1","verse":"","text":"añe lëndëmu bàmmeel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Alal ju gaaw"},{"style":"q1","verse":"","text":"du mujje barkeel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Bul feyantook ku la tooñ;"},{"style":"q1","verse":"","text":"dénkul ci Aji Sax ji, mu wallu la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Aji Sax ji bañ na nattu diisaay yu wuute,"},{"style":"q1","verse":"","text":"njublaŋ ci natti diisaay baaxul."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Jéegoy jaam Aji Sax jee ko yor,"},{"style":"q1","verse":"","text":"kenn xamul foo jëm."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Bul gaawtuy digeek Yàlla, di dugal sa bopp;"},{"style":"q1","verse":"","text":"bul giñ, di réccu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Buur, bu xeloo, ràññee ku bon,"},{"style":"q1","verse":"","text":"mbugal ko, te du ko ñéeblu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Xelum nit làmp la bu Aji Sax ji taal,"},{"style":"q1","verse":"","text":"da koy niital ba ca biir xolam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Ngor ak worma day aar buur,"},{"style":"q1","verse":"","text":"ngor ay saxal ab jalam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Sagu ndaw, doole ja;"},{"style":"q1","verse":"","text":"gànjaru mag, bijjaaw ba."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Dóor yu la gaañ faj na sag mbon,"},{"style":"q1","verse":"","text":"ay yar fóot na xolu boroom."}]