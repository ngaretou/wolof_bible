[{"style":"s1","verse":"","text":"Bul gàddul kenn bor"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Doom, dangaa gàddul nit bor,"},{"style":"q1","verse":"","text":"dige feyal jàmbur,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"saw làmmiñ yeb la cim pax,"},{"style":"q1","verse":"","text":"nga keppu?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Yaa ngi ci sa loxoy moroom,"},{"style":"q1","verse":"","text":"kon defal nii, doom, ba rëcc:"},{"style":"q1","verse":"","text":"doxal tinuji ko,"},{"style":"q1","verse":"","text":"raamali ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Bul gëmm,"},{"style":"q1","verse":"","text":"bul nelaw!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Dawal ni kéwél gu rëccub rëbb,"},{"style":"q1","verse":"","text":"ne fërr ni picc mu raw ab fiirkat."},{"style":"s1","verse":"","text":"Bul yaafus"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yaafus bee, xoolal xorondom,"},{"style":"q1","verse":"","text":"seetal ci moom, ba muus."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Du ku ko yilif mbaa ku koy sas,"},{"style":"q1","verse":"","text":"te jiiteesu ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Bu jotee mu denc ab dundam,"},{"style":"q1","verse":"","text":"mu jot, mu for lekkam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Moo yaafus bi, foo àppal tëraay bi?"},{"style":"q1","verse":"","text":"Loo deeti xaar ci yewwu?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ngay dajjant aka for bët,"},{"style":"q1","verse":"","text":"di tegley loxooka jaaxaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Néewlee ngi lay dikkal nib sàcc,"},{"style":"q1","verse":"","text":"ñàkk gànnaayul la."},{"style":"s1","verse":"","text":"Ku dëng, nii lay mel"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ku tekkeedi te bon,"},{"style":"q1","verse":"","text":"day wër di fen,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"day piis, di wokkeb tànk,"},{"style":"q1","verse":"","text":"di waxe baaraam,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"jekkadiy xalaat, di mébét lu bon"},{"style":"q1","verse":"","text":"aka yokku ay."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Moo tax musiba di jekki dal ko,"},{"style":"q1","verse":"","text":"mu jekki yàqu, ba wees paj."},{"style":"s1","verse":"","text":"Aji Sax ji bañ na lii"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Juróom benn a ngii yu Aji Sax ji bañ,"},{"style":"q1","verse":"","text":"ba ci juróom ñaar yu mu seexlu:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"kuy daŋŋiiral,"},{"style":"q1","verse":"","text":"kuy fen,"},{"style":"q1","verse":"","text":"kuy rey nit ku deful dara,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"kuy fexe lu bon ci xolam,"},{"style":"q1","verse":"","text":"kuy gaawtuy def lu ñaaw,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"seede buy fen rekk,"},{"style":"q1","verse":"","text":"rawatina kuy yokku ay ci biiri bokk."},{"style":"s1","verse":"","text":"Ku njaaloo gis ko"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Doom, defal li la baay sant,"},{"style":"q1","verse":"","text":"te bul wacc ndigalal yaay."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Takkal foo tollu, taf ci sa xol,"},{"style":"q1","verse":"","text":"fas ko ci sa baat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Booy dox mu di la jiite,"},{"style":"q1","verse":"","text":"nga tëdd, mu di la sàmm,"},{"style":"q1","verse":"","text":"nga yewwu, mu di la waxal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Santaane da lay niital, ndigal leeralal la,"},{"style":"q1","verse":"","text":"artookub yar di yoonu dund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Da lay aar ci ndaw su bon"},{"style":"q1","verse":"","text":"ak jabaru jàmbur ak làmmiñam wu neex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Bu ko xemmeme taaram,"},{"style":"q1","verse":"","text":"bumu la fiir aki lamsal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Dogu mburu fey nab gànc;"},{"style":"q1","verse":"","text":"waaye jabaru jàmbur, bakkanu lëmm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Te ku ne sawara cas, tafoo,"},{"style":"q1","verse":"","text":"lakk say yére."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Ku dox ciy xal it,"},{"style":"q1","verse":"","text":"say tànk ñor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Mooy ànd ak jabaru jàmbur,"},{"style":"q1","verse":"","text":"ku ko def gis ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Deesul sikk ku sàcc aw ñam,"},{"style":"q1","verse":"","text":"def ci biiram ndax xiif."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Waaye bu feeñee, fey boroom juróom ñaari yoon,"},{"style":"q1","verse":"","text":"joxe alalu këram yépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Njaaloo di ñàkk bopp,"},{"style":"q1","verse":"","text":"kuy sànk bakkanam a koy def."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Mbugal ak toroxte dib añam,"},{"style":"q1","verse":"","text":"te gàcceem du deñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Fiiraange day feesal xolu góor,"},{"style":"q1","verse":"","text":"te du ñéeblu buy feyu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"Du faale ndàmpaay,"},{"style":"q1","verse":"","text":"loo ko may, may, du giif."}]