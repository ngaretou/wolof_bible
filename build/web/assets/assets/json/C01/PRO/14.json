[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Jigéen, rafet xel, dimbali waa këram,"},{"style":"q1","verse":"","text":"su deeb dof, tas këram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Kuy jubal day ragal Aji Sax ji,"},{"style":"q1","verse":"","text":"ku dëng teddadil nga ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ab dof a ngi réy-réylu, làmmiñam yar ko;"},{"style":"q1","verse":"","text":"ku rafet xel, sa kàddu aar la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Su yëkk takkul, bey, sàq wéet;"},{"style":"q1","verse":"","text":"mu beye dooleem, meññeef ne gàññ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Seede bu dëggu, dëgg rekk,"},{"style":"q1","verse":"","text":"seede bu dëgguwul, fen rekk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ab ñaawlekat day wut xel mu rafet, mu réer ko;"},{"style":"q1","verse":"","text":"ku amug dégg, xam yomb la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Dàndal ab dof,"},{"style":"q1","verse":"","text":"dof du la yokk xam-xam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ku ñaw day xelu, xam li muy def;"},{"style":"q1","verse":"","text":"ab dof di nax boppam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ab dof, su bàkkaaree, yoonam!"},{"style":"q1","verse":"","text":"Te ñay jubal a séq yiwu Yàlla."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Tiisu xol, boroom a ko xam;"},{"style":"q1","verse":"","text":"mbégte ma it jàmbur bokku ca."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ab soxor, këram day tas,"},{"style":"q1","verse":"","text":"mbaarum ku jub di naat."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yoon a ngii, nit defe ne jub na,"},{"style":"q1","verse":"","text":"te mu jëme ko ci dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Nit a ngi ree, xol ba tiis,"},{"style":"q1","verse":"","text":"te mbégte mujje na njàqare."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku def njekkar sab añ mat,"},{"style":"q1","verse":"","text":"ku baax gën law demin."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Ab téxét, loo wax mu gëm;"},{"style":"q1","verse":"","text":"ku ñaw xam fi ngay jaare."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ku xelu day ragal, di dëddu lu bon;"},{"style":"q1","verse":"","text":"ab dof day ràkkaaju, du tiit."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ku gaawa mer, def jëfi dof,"},{"style":"q1","verse":"","text":"te kuy fexeel nit, ñu bañ la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ab téxét ndof ay céram,"},{"style":"q1","verse":"","text":"ku ñaw jagoo xam-xam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ku bon sujjóotal ku baax;"},{"style":"q1","verse":"","text":"ku soxor ay yaakaar ku jub."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku ñàkk, say dëkk sax bañ la;"},{"style":"q1","verse":"","text":"ku barele, barey xarit."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ku xeeb sa dëkkandoo, bàkkaar nga;"},{"style":"q1","verse":"","text":"ku baaxe ku ñàkk, mbégte ñeel la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Kuy mébét lu bon daa réer,"},{"style":"q1","verse":"","text":"kuy mébét lu baax am nga ngor ak worma."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Kër-këri, jariñu;"},{"style":"q1","verse":"","text":"waxi neen, loxoy neen."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ku rafet xel jagoo alal,"},{"style":"q1","verse":"","text":"ab dof ràngoo ndofam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Seede bu dëggu day jot bakkan,"},{"style":"q1","verse":"","text":"kuy fen day wore."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Ragal Aji Sax ji kaaraange gu wér la ci nit,"},{"style":"q1","verse":"","text":"di rawtu ciy doomam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ragal Aji Sax ji day suuxat bakkan,"},{"style":"q1","verse":"","text":"ba boroom du tàbbi ci fiiri dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Su mbooloo baree, buur am ndam;"},{"style":"q1","verse":"","text":"fu nit ña néew, kilifa du fa dara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Muñ mer, déggin wu yaa;"},{"style":"q1","verse":"","text":"gaawa tàng, gënatee dof."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Xel mu dal, yaram wu naat;"},{"style":"q1","verse":"","text":"fiiraange semmal boroom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Ku sonal ku ñàkk, tooñ nga Ka ko sàkk;"},{"style":"q1","verse":"","text":"ku baaxe ku néewle, teral nga Ka ko sàkk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Coxor detteel boroom;"},{"style":"q1","verse":"","text":"ku jub fegoo mànduteem."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Kuy dégg, sam xel saxoo rafet;"},{"style":"q1","verse":"","text":"ab dof sax xam na lu xelu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Àtteb dëgg day teral aw xeet,"},{"style":"q1","verse":"","text":"moy di gàcceel aw askan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"Buur day bége jawriñ ju xelu,"},{"style":"q1","verse":"","text":"di mbugal nitu gàcce."}]