[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ñàkk te mat,"},{"style":"q1","verse":"","text":"moo gën dof, di wax njekkar."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Teeyadi baaxul,"},{"style":"q1","verse":"","text":"ku yàkkamti moy saw yoon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Nit def na jëfi dof, ba sànku,"},{"style":"q1","verse":"","text":"walbatiku mere Aji Sax ji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ku barele, barey xarit;"},{"style":"q1","verse":"","text":"ku ñàkk, sab xarit dagg la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Seede buy fen muccul mbugalam,"},{"style":"q1","verse":"","text":"kuy noyyee ay fen du rëcc àtteem."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ñu baree ngi wuta neex boroom daraja,"},{"style":"q1","verse":"","text":"ku nekk a bëgga xaritook kuy joxe."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ku ñàkk, sa bokk yépp dëddu la,"},{"style":"q1","verse":"","text":"sab xarit gën laa soreeti,"},{"style":"q1","verse":"","text":"ngay wax ak ñoom, dara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Kuy sàkku xel, bëgg nga sa bopp;"},{"style":"q1","verse":"","text":"kuy wut ag dégg day baaxle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Seede buy fen muccul mbugalam,"},{"style":"q1","verse":"","text":"kuy noyyee ay fen, mujje sànku."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Dund gu neex jekkul cib dof,"},{"style":"q1","verse":"","text":"surga buy jiite kilifa it moo yées."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ku xam lu jaadu, muñ mer,"},{"style":"q1","verse":"","text":"tanqamlu ku la tooñ ngay damoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Merum buur ni gaynde gu ŋar;"},{"style":"q1","verse":"","text":"yërmandey buur ni lay cig mbooy."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Doom ju dof naqaru baay baa,"},{"style":"q1","verse":"","text":"te jabar ju pànk mooy senn bu dakkul."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Kër ak alal ngay donne ci baay;"},{"style":"q1","verse":"","text":"jabar ju xelu, Aji Sax jee koy maye."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Ab yaafus, nelaw yu xóot,"},{"style":"q1","verse":"","text":"te ku yàccaaral xiif."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ku jëfe ndigal, sàmm sa bakkan;"},{"style":"q1","verse":"","text":"ku moytuwul sa bopp, dangay dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ku baaxe ku ñàkk, lebal nga Aji Sax ji,"},{"style":"q1","verse":"","text":"te moo lay yool."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Yaral sa doom ba muy teel,"},{"style":"q1","verse":"","text":"waaye bul topp sa xol, ba rey ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ku naqari deret, sonal nga sa bopp;"},{"style":"q1","verse":"","text":"ku la xettali, tóllanti ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Déggal ndigal te yaru,"},{"style":"q1","verse":"","text":"ndax ëllëg nga xelu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Bare na lu xol di mébét,"},{"style":"q1","verse":"","text":"waaye Aji Sax jeey dogal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Li ñu sopp ci nit mooy ngor,"},{"style":"q1","verse":"","text":"néewle moo gën fen."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ragalal Aji Sax ji ndax nga gudd fan,"},{"style":"q1","verse":"","text":"noppalu te baña loru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Ab yaafus day yeb loxoom ci ndab,"},{"style":"q1","verse":"","text":"tàyyi ko yékkati, sex."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Boo ñefee ab ñaawlekat, téxét ba jànge ca;"},{"style":"q1","verse":"","text":"nga yedd kuy dégg, mu xame ca."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Kuy yàqal sa baay, di dàq sa ndey,"},{"style":"q1","verse":"","text":"yaay indi gàcceek yeraange."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Doom, soo noppee déglu ku lay yar,"},{"style":"q1","verse":"","text":"sore nga kàdduy xam-xam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Seedeb naaféq faalewul yoon,"},{"style":"q1","verse":"","text":"ab soxor lu bon lay tàqamtikoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Na ñaawlekat bi séentu mbugal;"},{"style":"q1","verse":"","text":"ab dof, yeti gannaaw."}]