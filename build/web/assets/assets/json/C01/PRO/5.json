[{"style":"s1","verse":"","text":"Moytul njaaloo"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Sama doom, déglul sama xel mu rafet,"},{"style":"q1","verse":"","text":"teewlul sama déggin,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"kon nga saxoo foog,"},{"style":"q1","verse":"","text":"say wax tegu ci xam-xam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Lu mel ni lem la jigéenu njaalookat di wax,"},{"style":"q1","verse":"","text":"làmmiñ wa ni diw gu ñu lay raay."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Muj ga mu wex xàtt,"},{"style":"q1","verse":"","text":"di gaañe ni ñawkay saamar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Tànk ya day bartalu wuti dee,"},{"style":"q1","verse":"","text":"ay jéegoom àkki njaniiw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Rëddul yoonu dund,"},{"style":"q1","verse":"","text":"daa lajj, te xamu ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Kon, samay doom, dégluleen ma,"},{"style":"q1","verse":"","text":"buleen wacc samay kàddu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Soreel ndaw soosu,"},{"style":"q1","verse":"","text":"bul jege bunt këram."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Kon nga ñàkk daraja, keneen jagoo;"},{"style":"q1","verse":"","text":"ku néeg bóom la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Jàmbur jariñoo sa doole,"},{"style":"q1","verse":"","text":"doxandéem jagoo saw ñaq."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ngay mujje onk,"},{"style":"q1","verse":"","text":"desey yax, jeex tàkk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Nga naan: «Su ma yégoon, sopp ab yar"},{"style":"q1","verse":"","text":"te baña sofental waxi artu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Lu ma tee woona dégg ku may digal,"},{"style":"q1","verse":"","text":"tey teewlu sama waxi sëriñ ya?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Tuuti ma yàqu yaxeet"},{"style":"q1","verse":"","text":"fi kanam ñépp!»"},{"style":"s1","verse":"","text":"Yemal ci sa jabar"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Naanal ci sa mbalkam bopp,"},{"style":"q1","verse":"","text":"ndox ma balle sa teenu bopp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Xanaa doo wasaare sa bëti ndox ca biti,"},{"style":"q1","verse":"","text":"mu def ay wal ca pénc ya?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Nay sa alal, yaw doŋŋ,"},{"style":"q1","verse":"","text":"ku bokk feneen bokkul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Yal na sa naanukaay barkeel,"},{"style":"q1","verse":"","text":"nga bànneexoo kiy sa jabar ba ngay ndaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Aka sopplu te jekk!"},{"style":"q1","verse":"","text":"Na lay céram doy foo tollu,"},{"style":"q1","verse":"","text":"na la xañ sago saa su ne."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Doom, ana looy xemmeme keneen,"},{"style":"q1","verse":"","text":"bay fowe céri jàmbur?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Lu waay jëf, Aji Sax ji gis,"},{"style":"q1","verse":"","text":"di xool mboolem fu mu jaare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Jëf ju bon day fiir ka ko sàlloo,"},{"style":"q1","verse":"","text":"bàkkaaram di mbaal, jàpp ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ñàkk yar, dee;"},{"style":"q1","verse":"","text":"ndof gu réy, sànku."}]