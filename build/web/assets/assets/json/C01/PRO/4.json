[{"style":"s1","verse":"","text":"Ku dégg ndigal, jariñu"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Gone yi, tee ngeena dégg yaru baay,"},{"style":"q1","verse":"","text":"te déglu, ba amug dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Maa ngi leen di jàngal lu baax,"},{"style":"q1","verse":"","text":"buleen fàtte sama njàngale."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Man it amoon naa baay,"},{"style":"q1","verse":"","text":"di benn bàjjo ci saa ndey."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Baay digal ma, ne ma:"},{"style":"q1","verse":"","text":"«Jàppal samay wax ci sam xel,"},{"style":"q1","verse":"","text":"di jëfe samay santaane, ba dund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Amal xel mu rafet akug dégg."},{"style":"q1","verse":"","text":"Bul fàtte te bul moy samay wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Bul dëddu xel mu rafet, da lay aar;"},{"style":"q1","verse":"","text":"sopp ko, mu sàmm la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Njàlbéenu xel mu rafet mooy sàkku ko;"},{"style":"q1","verse":"","text":"ak loo mana am, amalug dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Nawal xel mu rafet, mu darajaal la;"},{"style":"q1","verse":"","text":"tafoo ko, mu teral la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Da lay takkal kaala gu jekk,"},{"style":"q1","verse":"","text":"solal la mbaxanam buur.»"},{"style":"s1","verse":"","text":"Moytul yoonu ku bon"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Déglul, doom, te dégg saay wax,"},{"style":"q1","verse":"","text":"ndax nga gudd fan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Yoon wu xelu laa la teg,"},{"style":"q1","verse":"","text":"jaarloo la yoonu njub."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Doo dox ba tërëf,"},{"style":"q1","verse":"","text":"doo daw, téqtalu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ŋoyal cib yar, bul yoqi,"},{"style":"q1","verse":"","text":"sàmm ko, moo yor sa bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Yoonu ku bon, bu ci tegu;"},{"style":"q1","verse":"","text":"fu ab soxor di jaar, bu fa dox."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Moyul, bu fa jaare;"},{"style":"q1","verse":"","text":"mbasal te wéy!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ku bon du nelaw te lorewul,"},{"style":"q1","verse":"","text":"du dajjant ba kera muy téqe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Daa bon, def ko lekk,"},{"style":"q1","verse":"","text":"di màndee coxoram."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Yoonu ku jub day leer,"},{"style":"q1","verse":"","text":"ba ne ràññ ni bëccëg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yoonu ku bon ne këruus,"},{"style":"q1","verse":"","text":"du xam lu muy téqtaloo."},{"style":"s1","verse":"","text":"Saxal ci lu baax"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Doom, déglul samay kàddu,"},{"style":"q1","verse":"","text":"teewlul samay wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Bu ci noppee ja jàkk,"},{"style":"q1","verse":"","text":"te jàpp ko ci sa biir xel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Mooy dundloo ku ci jàpp,"},{"style":"q1","verse":"","text":"di wéral jëmmam jépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Sàmmal a sàmm sab xol,"},{"style":"q1","verse":"","text":"nde xalaati xol ngay jëfe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Dàqal wor, mu sore la;"},{"style":"q1","verse":"","text":"kàdduy naxe, na la dànd."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Xooleel, sa bët ne jàkk,"},{"style":"q1","verse":"","text":"ngay xool sa kanam màkk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Seetlul fi ngay teg tànk,"},{"style":"q1","verse":"","text":"ba foo awe, mu wóor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Bul jàdd, ndijoor mbaa càmmoñ,"},{"style":"q1","verse":"","text":"na sa tànk moyu lu bon."}]