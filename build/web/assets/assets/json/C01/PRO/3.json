[{"style":"s1","verse":"","text":"Wóolul Yàlla"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Doom, bul fàtte sama njàngale,"},{"style":"q1","verse":"","text":"defal sa xel ci saay santaane,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"ngir fan wi gudd lool,"},{"style":"q1","verse":"","text":"nga gëna am jàmm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bu la ngor ak worma dëddu,"},{"style":"q1","verse":"","text":"booleel takk ci sa baat,"},{"style":"q1","verse":"","text":"bind ko ci sa àlluway xol."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Kon nga xam lu jaadu,"},{"style":"q1","verse":"","text":"neex Yàllaak doom aadama."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Nanga wóoloo Aji Sax ji sa léppi xol,"},{"style":"q1","verse":"","text":"te bul wéeru ci saw déggin."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Loo mébét, bàyyil Yàlla xel,"},{"style":"q1","verse":"","text":"mu xàllal la yoon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Bul doyloo sa xel mu rafet,"},{"style":"q1","verse":"","text":"ragalal Aji Sax ji, dëddu mbon;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"sa yaram wér,"},{"style":"q1","verse":"","text":"say cér naat."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Teralal Aji Sax ji ci sa alal"},{"style":"q1","verse":"","text":"ak loo njëkka meññle,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"say sàq day fees, ne xéew,"},{"style":"q1","verse":"","text":"say mbàndi ndoxi reseñ di wal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Doom, bul xeeb yaru Aji Sax ji,"},{"style":"q1","verse":"","text":"bul bañ mu waññi la,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"ndax Aji Sax ji, ku mu sopp, yar la,"},{"style":"q1","verse":"","text":"ni baay, ak doom ju ko neex."},{"style":"s1","verse":"","text":"Li xel mu rafet di jariñ"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Mbégtee ñeel ku daj xel mu rafet"},{"style":"q1","verse":"","text":"ak ku amug dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Am koo gën am xaalis,"},{"style":"q1","verse":"","text":"te njariñ laa raw wurus."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Xel mu rafet a gën gànjar;"},{"style":"q1","verse":"","text":"loo mana bëgg moo ko raw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Gudd fan, mu joxee ndijoor;"},{"style":"q1","verse":"","text":"càmmoñ ba, alal ak teraanga."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Fa muy jaare yiw la,"},{"style":"q1","verse":"","text":"fu mu awe jàmm la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Mooy garab giy taxa dund"},{"style":"q1","verse":"","text":"ci ku ko jàpp,"},{"style":"q1","verse":"","text":"di mbégtey ku ca ŋoy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Aji Sax ji xel mu rafet la sose suuf,"},{"style":"q1","verse":"","text":"lale asamaan ag dégg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ci xam-xamam la ndoxi xóote ya ne jàyy,"},{"style":"q1","verse":"","text":"ay niir di lay."},{"style":"s1","verse":"","text":"Rafet xel barke la"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Sama doom, saxool xelu akug foog,"},{"style":"q1","verse":"","text":"dee ci xool foo tollu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Day guddal saw fan,"},{"style":"q1","verse":"","text":"di rafetal sa jikko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Kon sab dox wóor,"},{"style":"q1","verse":"","text":"doo fakkastalu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Soo tëddee doo tiit;"},{"style":"q1","verse":"","text":"say nelaw day neex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Kon doo ragal njàqare lu bette"},{"style":"q1","verse":"","text":"ak saay-saay buy songe,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"ndax Aji Sax ji da lay yiir,"},{"style":"q1","verse":"","text":"di la musal cig fiir."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Bul xañ njekk ku ko yelloo,"},{"style":"q1","verse":"","text":"ndegam man nga ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Bul ne say dëkk: «Demal ba beneen, ma jox la,»"},{"style":"q1","verse":"","text":"te fekk la yor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Bul fexeel dëkkandoo"},{"style":"q1","verse":"","text":"bu dëkk ak yaw ci kóolute."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Bul joteek kenn ci neen,"},{"style":"q1","verse":"","text":"te tooñu la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Bul ñee ku néeg,"},{"style":"q1","verse":"","text":"te bu ko roy fenn."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Aji Sax ji daa sib ku dëng,"},{"style":"q1","verse":"","text":"xejjoo kuy jubal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Aji Sax ji day alag kër ku bon,"},{"style":"q1","verse":"","text":"di barkeel waa kër ku jub."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Kuy ñaawle, moo lay ñaawal,"},{"style":"q1","verse":"","text":"ku toroxlu, mu may law yiw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"Teraanga di céru ku rafet xel,"},{"style":"q1","verse":"","text":"ab dof gàccoo gàcce."}]