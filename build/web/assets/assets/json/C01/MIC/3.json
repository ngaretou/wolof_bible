[{"style":"s1","verse":"","text":"Aji Sax ji tiiñal na njiiti Israayil"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Dama ne:"},{"style":"q1","verse":"","text":"«Ngalla dégluleen yeen njiiti \\w Yanqóoba|Yanqóoba:\\w*,"},{"style":"q1","verse":"","text":"yeen kilifay kër Israayil!"},{"style":"q1","verse":"","text":"Xanaa du yeena wara xam yoon?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Te yeenay bañ-kati lu baax,"},{"style":"q1","verse":"","text":"di bëggkati lu bon."},{"style":"q1","verse":"","text":"Yeenay fees sama ñoñ te ñuy dund,"},{"style":"q1","verse":"","text":"yeenay rocci suux wi sàng seeni yax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Yeenay yàpp samay bokk,"},{"style":"q1","verse":"","text":"fulli seen der,"},{"style":"q1","verse":"","text":"damm seeni yax, dogat,"},{"style":"q1","verse":"","text":"def ko ni toggug kawdir,"},{"style":"q1","verse":"","text":"mel ni ndawal ci cin.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Lee tax noggatukat yii di woo Aji Sax ji wall,"},{"style":"q1","verse":"","text":"te du leen wuyu,"},{"style":"q1","verse":"","text":"kanamam la leen di làq, bésub keroog,"},{"style":"q1","verse":"","text":"ndax ñoo bonal seeni jëf."},{"style":"s1","verse":"","text":"Aji Sax ji artu na yonent yu bëgge yi"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Lii la Aji Sax ji wax ci yonent yiy lajjal samay bokk:"},{"style":"q1","verse":"","text":"«Bu ñu sàqamee ab lanc, yéene jàmm;"},{"style":"q1","verse":"","text":"ku leen leelul, ñu waajal xare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Moo tax yeen, guddi moo leen di ñeel, ba peeñu réer,"},{"style":"q1","verse":"","text":"lëndëm a leen di dikkal, ba am ngis tukkil leen,"},{"style":"q1","verse":"","text":"jant mooy sowal yonent yi,"},{"style":"q1","verse":"","text":"bëccëg lëndëmal leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Seetkat yi dañuy rus,"},{"style":"q1","verse":"","text":"boroom peeñu yi torox,"},{"style":"q1","verse":"","text":"ñoom ñépp jàpp seen gémmiñ;"},{"style":"q1","verse":"","text":"ndax Yàllaa leen dul faale.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Teewul man, may fees ak doole,"},{"style":"q1","verse":"","text":"fees ak noowug Aji Sax ji,"},{"style":"q1","verse":"","text":"ak dëgg ak njàmbaarte,"},{"style":"q1","verse":"","text":"ngir biralal askanu \\w Yanqóoba|Yanqóoba:\\w* bàkkaaram,"},{"style":"q1","verse":"","text":"ngir tuddal \\w bànni Israayil|bànni Israayil:\\w* googu moyam."},{"style":"s1","verse":"","text":"Aji Sax ji tiiñalaat na njiiti Israayil"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ngalla dégluleen lii, yeen njiiti kër \\w Yanqóoba|Yanqóoba:\\w*;"},{"style":"q1","verse":"","text":"yeen kilifay kër Israayil."},{"style":"q1","verse":"","text":"Yeen ñi seexlu dëgg,"},{"style":"q1","verse":"","text":"lunkal wépp yoonu njub."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Deret ngeen tabaxe \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","verse":"","text":"njubadi ngeen tabaxe Yerusalem googu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Seeni njiit, alalu ger lañuy àttee,"},{"style":"q1","verse":"","text":"seeni \\w sarxalkat|sarxalkat b-:\\w* di feyeeku am njàngale,"},{"style":"q1","verse":"","text":"seeni yonent di weccikoo xaalis seenum ngisaane,"},{"style":"q1","verse":"","text":"te Aji Sax ji lañuy dëgërloo, naan:"},{"style":"q1","verse":"","text":"«Xanaa du Aji Sax jaa ngi ci sunu biir?"},{"style":"q1","verse":"","text":"Musiba du nu dab!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Kon nag yeena tax dees na gàbb Siyoŋ nib tool,"},{"style":"q1","verse":"","text":"Yerusalem googu diy tojit, jale,"},{"style":"q1","verse":"","text":"tundu kërug Yàlla gaay doon gajj bu kawe."}]