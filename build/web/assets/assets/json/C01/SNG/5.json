[{"style":"sp","verse":"","text":"Waa ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Jigéen sama, séet sama, dikk naa sama tool,"},{"style":"q1","verse":"","text":"sama ndàbb ak sama cuuraay, ma for,"},{"style":"q1","verse":"","text":"sama lem xelli, ma lekk,"},{"style":"q1","verse":"","text":"sama biiñ ak sama meew, ma naan."},{"style":"sp","verse":"","text":"Awu"},{"style":"q1","verse":"","text":"Yeen soppe yi, lekkleen;"},{"style":"q1","verse":"","text":"yeen ñi bëggante, naanleen ba màndi."},{"style":"s1","verse":"","text":"Xol jeex na, mujje neex"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Maa ngi nelaw, te xol yewwu;"},{"style":"q1","verse":"","text":"dégluleen, sama nijaay ay fëgg."},{"style":"q1","verse":"","text":"Mu ne: «Xaritoo, jigéen sama, ubbil ma,"},{"style":"q1","verse":"","text":"sama nenne, alal sama!"},{"style":"q1","verse":"","text":"Sama bopp a ngi laye ba tooy,"},{"style":"q1","verse":"","text":"njañ li fanaanee guus.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ma ne ko: «Summiku naa de,"},{"style":"q1","verse":"","text":"xanaa duma soluwaat?"},{"style":"q1","verse":"","text":"Jàngu naa jeeg,"},{"style":"q1","verse":"","text":"xanaa duma taqati?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Sama nijaay yoor loxoom ci xarante bi,"},{"style":"q1","verse":"","text":"sama yaram ne sàyy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ma ne bërét, di ubbil nijaay,"},{"style":"q1","verse":"","text":"sama loxo yi, diwu ndàbb di ca siit,"},{"style":"q1","verse":"","text":"sama waaraam yi, ndàbb rogalaat"},{"style":"q1","verse":"","text":"ba ci njàppul bunt bi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Maa ubbil nijaay,"},{"style":"q1","verse":"","text":"waaye nijaay waññiku na, ba wéy."},{"style":"q1","verse":"","text":"Ba mu waññikoo, tuuti ma dee."},{"style":"q1","verse":"","text":"Seet naa ko, gisuma ko;"},{"style":"q1","verse":"","text":"ma woo ko, wuyuwul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Wattukat yiy wër dëkk bi gis ma;"},{"style":"q1","verse":"","text":"ñu dóor ma, gaañ ma,"},{"style":"q1","verse":"","text":"ñori sama malaan."},{"style":"q1","verse":"","text":"Wattukati tata jaa!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ngalla yeen, janqi Yerusalem, waatalleen ma"},{"style":"q1","verse":"","text":"ne su ngeen gisee sama nijaay,"},{"style":"q1","verse":"","text":"ne ko damaa wopp ndax mbëggeel."},{"style":"sp","verse":"","text":"Janqi Yerusalem"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Yaw mi dàq ci jigéen ñi,"},{"style":"q1","verse":"","text":"ana lu sa nijaay gëne keneen?"},{"style":"q1","verse":"","text":"Ana lu mu ëpplee keneen,"},{"style":"q1","verse":"","text":"ba nga di nu waatloo lii?"},{"style":"sp","verse":"","text":"Ndaw si"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Sama nijaay daa xees, leer;"},{"style":"q1","verse":"","text":"kenn la ci fukki junniy góor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Kanam gi ni wurus, di wurusu ngalam;"},{"style":"q1","verse":"","text":"njañ li lëmësu,"},{"style":"q1","verse":"","text":"ñuul ni ndobin."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Gët yi ni pitax yu feggook wali ndox,"},{"style":"q1","verse":"","text":"sangoo meew,"},{"style":"q1","verse":"","text":"feggook ndox mu ne xéew\\f + \\fr 5.12 \\ft Li ñu wax fii ci làkku ebrë am na werante.\\f*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Lex ya di tool,"},{"style":"q1","verse":"","text":"gàncax ga ne ca bann,"},{"style":"q1","verse":"","text":"tuñ ya diy tóor-tóor,"},{"style":"q1","verse":"","text":"ndàbb wale ca, rogalaat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Loxo yi banti wurus la,"},{"style":"q1","verse":"","text":"ngën-gi-per tappe ca;"},{"style":"q1","verse":"","text":"jëmm ja rattax ni bëñu ñey"},{"style":"q1","verse":"","text":"laloo peri safiir;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"yeel yi kenuy doj wu jafe la,"},{"style":"q1","verse":"","text":"sampe ci tànki wurusu ngalam."},{"style":"q1","verse":"","text":"Meloom ni tundi Libaŋ,"},{"style":"q1","verse":"","text":"di tànnéef ni garabi seedar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Gémmiñam, bànneexi neen,"},{"style":"q1","verse":"","text":"jëmmam jépp jara xédd!"},{"style":"q1","verse":"","text":"Yeen janqi Yerusalem,"},{"style":"q1","verse":"","text":"sama nijaay a ngoog, samab xarit."}]