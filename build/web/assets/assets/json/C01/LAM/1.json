[{"style":"mt1","text":"Jooytu"},{"style":"sp","verse":"","text":"Yonent Yàlla Yeremi àddu na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ndeysaan! Dëkk beeka wéet tey!"},{"style":"q1","verse":"","text":"Te ñoñam noon gàññ démb."},{"style":"q1","verse":"","text":"Ndeysaan! Mu ngi mel nib jëtun tey,"},{"style":"q1","verse":"","text":"te noon ràññ fi biir xeet yi!"},{"style":"q1","verse":"","text":"Moo doon lingeeru diiwaan yi,"},{"style":"q1","verse":"","text":"tey, mu sasoo liggéeyu sañul-bañ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Day fanaanee jooy a jooy,"},{"style":"q1","verse":"","text":"rongooñ yay bas-basi,"},{"style":"q1","verse":"","text":"te du kenn ku koy dëfal"},{"style":"q1","verse":"","text":"ci mboolemi soppeem."},{"style":"q1","verse":"","text":"Xaritoo xarit wor ko,"},{"style":"q1","verse":"","text":"walbatiku diy noonam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ngàllo lañu yóbbu Yuda,"},{"style":"q1","verse":"","text":"ci biir naqar ak njaam gu metti."},{"style":"q1","verse":"","text":"Ci biiri jàmbur lañu dëkk,"},{"style":"q1","verse":"","text":"amuñu fu ñu noppaloo,"},{"style":"q1","verse":"","text":"mboolem ña leen dàq a leen dab,"},{"style":"q1","verse":"","text":"ba tance leen ca ruq ya."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yoon yi jëm \\w Siyoŋ|Siyoŋ:\\w*\\f + \\fr 1.4 \\fk Siyoŋ \\ft mooy Yerusalem itam.\\f* jooy na wéet,"},{"style":"q1","verse":"","text":"kenn jaareetu ca màggalsi."},{"style":"q1","verse":"","text":"Pénc yépp a gental,"},{"style":"q1","verse":"","text":"\\w sarxalkat|sarxalkat b-:\\w* yay binni,"},{"style":"q1","verse":"","text":"janq ja jàq,"},{"style":"q1","verse":"","text":"Siyoŋ gépp di naqarlu!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Seen noon yaa jiite tey,"},{"style":"q1","verse":"","text":"seen bañ yooyoo ne finaax,"},{"style":"q1","verse":"","text":"\\w Aji Sax jee|Aji Sax j-:\\w* mbugale waa Siyoŋ"},{"style":"q1","verse":"","text":"seeni tooñ yu bare,"},{"style":"q1","verse":"","text":"seeni doom duggig njaam,"},{"style":"q1","verse":"","text":"di moomeelub noon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Lu doon sagub Siyoŋ, dëkk bu taaru bi,"},{"style":"q1","verse":"","text":"tey, sàggiku na."},{"style":"q1","verse":"","text":"Seeni garmee ngi mel ni kéwél"},{"style":"q1","verse":"","text":"yu amul parlu,"},{"style":"q1","verse":"","text":"di koyoŋ-koyoŋiy daw,"},{"style":"q1","verse":"","text":"te rëbb ne leen dann."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Tey ci janti toroxte ak tumurànke,"},{"style":"q1","verse":"","text":"la Yerusalem di fàttalikoo"},{"style":"q1","verse":"","text":"mboolem ngëneeli démbam."},{"style":"q1","verse":"","text":"Aw askanam a tàbbi ci loxol noon,"},{"style":"q1","verse":"","text":"te wall amul,"},{"style":"q1","verse":"","text":"noon ba gis leen,"},{"style":"q1","verse":"","text":"di tex-texi ci seenug yàqule."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yerusalem a moy a moy,"},{"style":"q1","verse":"","text":"ba tax koo mujj di sobe;"},{"style":"q1","verse":"","text":"ku ko teraloon, sofental ko,"},{"style":"q1","verse":"","text":"ndax gis ko, mu ne duŋŋ."},{"style":"q1","verse":"","text":"Moom ci boppam mu ngi binni,"},{"style":"q1","verse":"","text":"fuuylu."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Sobey Siyoŋ ca pendalam,"},{"style":"q1","verse":"","text":"te bàyyiwul xel ag mujam."},{"style":"q1","verse":"","text":"Jéllam ba yéeme na,"},{"style":"q1","verse":"","text":"te amul ku ko dëfal."},{"style":"q1","verse":"","text":"Siyoŋ nee: «Éy Aji Sax ji, xoolal sama njàqare;"},{"style":"q1","verse":"","text":"noon bi déy, am na ndam!»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Loxo la noon bi teg ci"},{"style":"q1","verse":"","text":"ngëneeli Siyoŋ gépp."},{"style":"q1","verse":"","text":"Siyoŋ kay a gis yéefar yi songsi"},{"style":"q1","verse":"","text":"këram gu sell gi,"},{"style":"q1","verse":"","text":"te ci ñoom nga santaane woon, ne"},{"style":"q1","verse":"","text":"buñu dugg sam ndaje\\f + \\fr 1.10 \\ft Seetal ci \\bk Baamtug yoon wi \\bk* 23.4.\\f*."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Waa Siyoŋ gépp a ngi binni,"},{"style":"q1","verse":"","text":"di wut ab dund\\f + \\fr 1.11 \\fk wut ab dund: \\ft li leen xañ ab dund mooy gaw bu yàgg bu ñu leen gaw, ba ñu mujj xiif lool. Seetal ci \\bk 2.Buur ya\\bk* 25.1-3.\\f*,"},{"style":"q1","verse":"","text":"seeni ngëneel lañuy weccikoo aw ñam"},{"style":"q1","verse":"","text":"wu ñu leqalikoo."},{"style":"sp","verse":"","text":"Yerusalem àddu na"},{"style":"q1","verse":"","text":"Gisal Aji Sax ji, seetal"},{"style":"q1","verse":"","text":"ni ma mujj tekkeedee!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yeen mboolem ñiy jaare yoon wi,"},{"style":"q1","verse":"","text":"lii safu leen,"},{"style":"q1","verse":"","text":"waaye seetleen rekk ba gis"},{"style":"q1","verse":"","text":"ndax wenn mitit dab na"},{"style":"q1","verse":"","text":"mitit wi ñu ma teg,"},{"style":"q1","verse":"","text":"te Aji Sax ji naqarale ma ko"},{"style":"q1","verse":"","text":"ci jantub tàkktel sànjam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Fa kaw la Aji Sax ji sànnee sawara"},{"style":"q1","verse":"","text":"ci sama biiri yax,"},{"style":"q1","verse":"","text":"moo laaw ab caax ngir fakkastal ma,"},{"style":"q1","verse":"","text":"waññi ma, jànnaxe ma,"},{"style":"q1","verse":"","text":"ma dëkke yoggooral."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Samay tooñ la Aji Sax ji ëmb,"},{"style":"q1","verse":"","text":"fastee ko loxoom,"},{"style":"q1","verse":"","text":"mu tege sama kaw ndodd,"},{"style":"q1","verse":"","text":"ba jeexal sama doole."},{"style":"q1","verse":"","text":"Boroom bee ma tàbbal"},{"style":"q1","verse":"","text":"ci loxo yu ma manula të."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Ku doon jàmbaar fi sama biir,"},{"style":"q1","verse":"","text":"la Boroom bi xalab."},{"style":"q1","verse":"","text":"Am ndaje la ma wooteel,"},{"style":"q1","verse":"","text":"ngir falaxe sama ndaw yu góor."},{"style":"q1","verse":"","text":"Boroom bee dëggaate Yuda, janq ju taaru ji,"},{"style":"q1","verse":"","text":"ni reseñ ci biir nalukaay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Looloo ma tax di jooy,"},{"style":"q1","verse":"","text":"rongooñ di wal-wali,"},{"style":"q1","verse":"","text":"te ku may dëfal sore ma,"},{"style":"q1","verse":"","text":"mook kuy seral sama xol."},{"style":"q1","verse":"","text":"Samay doom a ngi jaaxle,"},{"style":"q1","verse":"","text":"nde noon bi daan na."},{"style":"sp","verse":"","text":"Yeremi àddu na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"\\w Siyoŋ|Siyoŋ:\\w* tàllal na loxoom,"},{"style":"q1","verse":"","text":"waaye amul ku ko dëfal."},{"style":"q1","verse":"","text":"Aji Sax jee wool askanu \\w Yanqóoba|Yanqóoba:\\w*"},{"style":"q1","verse":"","text":"seeni noon yi leen wër,"},{"style":"q1","verse":"","text":"ba Yerusalem mujj di sobe fi seen biir."},{"style":"sp","verse":"","text":"Yerusalem àddu na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Aji Sax ji moo yey,"},{"style":"q1","verse":"","text":"kàddoom laa gàntal."},{"style":"q1","verse":"","text":"Yeen, xeetoo xeet, ngalla dégluleen"},{"style":"q1","verse":"","text":"te xool samaw tiis."},{"style":"q1","verse":"","text":"Samay ndaw yu góor ak yu jigéen,"},{"style":"q1","verse":"","text":"ñoo duggi ngàllo!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Samay soppe laa woo wall,"},{"style":"q1","verse":"","text":"ñoom ñu wor ma."},{"style":"q1","verse":"","text":"Samay \\w sarxalkat|sarxalkat b-:\\w* ak samay mag di wuti ab dund"},{"style":"q1","verse":"","text":"bu ñu leqalikoo,"},{"style":"q1","verse":"","text":"far faatu ci biir dëkk bi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Éy Aji Sax ji, xoolal sama njàqare,"},{"style":"q1","verse":"","text":"samay butit di fuddu,"},{"style":"q1","verse":"","text":"sama biir xol salfaañoo ndaxu tiis."},{"style":"q1","verse":"","text":"Man de maa gàntal ba gàntaaral,"},{"style":"q1","verse":"","text":"ba fa biti, saamar di fa bóome,"},{"style":"q1","verse":"","text":"te biir kër it di dee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Dégg nañu may binni,"},{"style":"q1","verse":"","text":"te awma ku ma dëfal."},{"style":"q1","verse":"","text":"Noon yépp a dégg samaw ay"},{"style":"q1","verse":"","text":"te di ko bége, nde yaa ko def."},{"style":"q1","verse":"","text":"Ngalla indil bés bi nga dige,"},{"style":"q1","verse":"","text":"ba ñu daj li ma daj."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Yal na seen gépp mbon teew fi sa kanam,"},{"style":"q1","verse":"","text":"ba nga mbugal leen"},{"style":"q1","verse":"","text":"ni nga ma mbugale"},{"style":"q1","verse":"","text":"ndax mboolem samay tooñ."},{"style":"q1","verse":"","text":"Samay onk bare na,"},{"style":"q1","verse":"","text":"sama xol jeex tàkk."}]