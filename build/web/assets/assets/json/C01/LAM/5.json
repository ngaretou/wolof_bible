[{"style":"sp","verse":"","text":"Waa Yerusalem a ngi ñaan"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Éy Aji Sax ji, bàyyil xel li nu dal,"},{"style":"q1","verse":"","text":"xoolal, gisal sunu toroxte!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Sunu suuf toxu ciy jàmbur,"},{"style":"q1","verse":"","text":"sunuy kër ñeel ay doxandéem."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Baay dëddu, nu diy jirim;"},{"style":"q1","verse":"","text":"ndey dib jëtun."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Sunum ndox, xaalis lanu koy feye, naan,"},{"style":"q1","verse":"","text":"sunu matt ak njëgam la mu nuy dikke."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sunuw loos lañu ràng,"},{"style":"q1","verse":"","text":"nu loof, te noppaleesu nu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Nu nangul Misra,"},{"style":"q1","verse":"","text":"nangul Asiri ngir ab dund."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Sunuy maam a moyoon, nekkatuñu tey;"},{"style":"q1","verse":"","text":"nun, noo gàddu seeni mbugal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ay surgaa nu yilif, te amunu"},{"style":"q1","verse":"","text":"ku nu wallu ci seeni loxo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Bu nu lekkee, sunu bakkan lanu jaay,"},{"style":"q1","verse":"","text":"nde saamar a ngi ci màndiŋ mi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Sunu yaram nib taal la tànge"},{"style":"q1","verse":"","text":"ndax tàngoorub xiif."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Jigéeni \\w Siyoŋ|Siyoŋ:\\w* lañuy tëdde,"},{"style":"q1","verse":"","text":"ak janqi dëkki Yuda."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Kàngam yi, ñu jàpp, wékk;"},{"style":"q1","verse":"","text":"te mag ñi, teraleesu leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Waxambaane yee yenu doji wolukaay,"},{"style":"q1","verse":"","text":"gone yu góor yi sëfoo sayi matt, di tërëf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Mag dakkal na jataayu pénc,"},{"style":"q1","verse":"","text":"te waxambaane talatula xalam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Xol nux-nuxeetul,"},{"style":"q1","verse":"","text":"te ay rongooñ a wuutuy pooj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Sunu mbaxana foq na!"},{"style":"q1","verse":"","text":"Wóoy ngalla nun ñi moy!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Lee jeexal sunu xol,"},{"style":"q1","verse":"","text":"lii moo nu jooyloo, ba gët giim:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"tundu Siyoŋ wi gental,"},{"style":"q1","verse":"","text":"ba till yi di fi daagoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yaw Aji Sax ji, yaay toog ba fàww,"},{"style":"q1","verse":"","text":"sab jal, sët ba sëtaat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ana lu waral nga fàtte nu fàww,"},{"style":"q1","verse":"","text":"bàyyi nu diir bu yàgge nii?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Éy Aji Sax ji, delloosi nu fi yaw, nu dellusi,"},{"style":"q1","verse":"","text":"yeesalal sunuy jant, mu mel na woon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Xanaa doo nu wacc a wacc,"},{"style":"q1","verse":"","text":"mere nu mer mu amul kemu?"}]