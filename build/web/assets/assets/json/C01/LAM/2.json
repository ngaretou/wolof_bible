[{"style":"sp","verse":"","text":"Yeremi àddu na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Ndeysaan Boroom bee mer ba xiinal \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","verse":"","text":"dëkk bu taaru bi."},{"style":"q1","verse":"","text":"Fa asamaan la rattaxe sagu Israayil fa suuf."},{"style":"q1","verse":"","text":"Keroog bésu meram ma, bàyyiwul xel,"},{"style":"q1","verse":"","text":"ne këram googoo di ndëggastalam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Boroom bee modd te yërëmul"},{"style":"q1","verse":"","text":"mboolem sanci \\w Yanqóoba|Yanqóoba:\\w*."},{"style":"q1","verse":"","text":"Ca meram ma la màbbe"},{"style":"q1","verse":"","text":"tatay Yuda réew mu taaru ma."},{"style":"q1","verse":"","text":"Moo detteel nguur ga aki garmeem,"},{"style":"q1","verse":"","text":"sobeel leen."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Tàngooru sànjam la daane"},{"style":"q1","verse":"","text":"gépp kàttanu Israayil."},{"style":"q1","verse":"","text":"Fa kanam noon ba la roccee"},{"style":"q1","verse":"","text":"dooley ndijooram."},{"style":"q1","verse":"","text":"Moo tàkkal Yanqóoba"},{"style":"q1","verse":"","text":"ni sawara wuy sël-sëli,"},{"style":"q1","verse":"","text":"ba xoyom mboolem la ko wër."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Boroom bee tàwwi ag fittam"},{"style":"q1","verse":"","text":"mel ni ab noon,"},{"style":"q1","verse":"","text":"moo diire ndijooram, mel ni bañ,"},{"style":"q1","verse":"","text":"moo bóom mboolem lu neexa xool."},{"style":"q1","verse":"","text":"Ci biir xaymab Siyoŋ dëkk bu taaru ba"},{"style":"q1","verse":"","text":"la yuri am sànjam ni sawara."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Boroom bee mel nib noon,"},{"style":"q1","verse":"","text":"modd Israayil,"},{"style":"q1","verse":"","text":"moddaale këram yu taaru yépp,"},{"style":"q1","verse":"","text":"tas ay tataam,"},{"style":"q1","verse":"","text":"ba fulal Yuda mu taaru ma,"},{"style":"q1","verse":"","text":"ay yuux aki jooy."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Moo tas mbaaram ni bu doon mbaarum tool,"},{"style":"q1","verse":"","text":"moo rajaxe bérebu ndajeem."},{"style":"q1","verse":"","text":"Aji Sax ji moo fàtteloo waa Siyoŋ"},{"style":"q1","verse":"","text":"màggal ak \\w bésub Noflaay|bésub Noflaay b-:\\w*,"},{"style":"q1","verse":"","text":"te tàngooru meram la xalabe buur ak \\w sarxalkat|sarxalkat b-:\\w*."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Aji Sax jee jañax \\w sarxalukaayam|sarxalukaay b-:\\w*,"},{"style":"q1","verse":"","text":"gedd."},{"style":"q1","verse":"","text":"Moo teg ci loxol noon tatay këram yi."},{"style":"q1","verse":"","text":"Biir kër Aji Sax ji la baat xaacoo,"},{"style":"q1","verse":"","text":"ni bu doon bésu màggal."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Aji Sax jee dogoo rajaxe"},{"style":"q1","verse":"","text":"tatam Siyoŋ mu taaru ma."},{"style":"q1","verse":"","text":"Moo tàllal buumu nattukaay, ngir tas ko,"},{"style":"q1","verse":"","text":"bankul loxoom it te màbbu ko."},{"style":"q1","verse":"","text":"Moo jooyloo tata ak tabaxub fegukaay,"},{"style":"q1","verse":"","text":"ñu ànd tojatoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Bunti dëkk ba suulu na,"},{"style":"q1","verse":"","text":"te moo yàqate, dammate weñi tëjukaay ya."},{"style":"q1","verse":"","text":"Buuram aki garmeem a tumurànkeji"},{"style":"q1","verse":"","text":"ca yéefar ya,"},{"style":"q1","verse":"","text":"te téereb yoon amul,"},{"style":"q1","verse":"","text":"yonent it gisatul peeñu"},{"style":"q1","verse":"","text":"mu bawoo fa Aji Sax ji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Magi Siyoŋ dëkk bu taaru ba ña nga toog, xeremlu,"},{"style":"q1","verse":"","text":"xëppoo suuf, sol ay saaku, di ñaawlu."},{"style":"q1","verse":"","text":"Janqi Yerusalem sëgg, seen bopp jëm suuf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Samay gët a giim ndaxi rongooñ,"},{"style":"q1","verse":"","text":"samay butit fuddu,"},{"style":"q1","verse":"","text":"xol ŋiis, jeex tàkk:"},{"style":"q1","verse":"","text":"ci tasteg samaw askan;"},{"style":"q1","verse":"","text":"xale yeek gone yiy nàmp"},{"style":"q1","verse":"","text":"ne lasar fi mbedd yi!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Seen yaay lañu naan:"},{"style":"q1","verse":"","text":"«Ana fu nuy lekke, ba naan?»"},{"style":"q1","verse":"","text":"te ne lasiim ci mbeddi dëkk bi,"},{"style":"q1","verse":"","text":"ni ñu gaañoo xare,"},{"style":"q1","verse":"","text":"bakkan di tàggoo fa ñu uufoo"},{"style":"q1","verse":"","text":"ca weenu yaay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Yaw Yerusalem, dëkk bu taaru bi,"},{"style":"q1","verse":"","text":"lu ma lay wax? Ku ma lay méngaleel?"},{"style":"q1","verse":"","text":"Siyoŋ, janq ju taaru ji laa ne,"},{"style":"q1","verse":"","text":"ana ku ma lay dendaleel, ba seral sa xol?"},{"style":"q1","verse":"","text":"Yaw mi sab góom yaa ni géej,"},{"style":"q1","verse":"","text":"ana ku lay faj?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ag neen aki caaxaan"},{"style":"q1","verse":"","text":"la la say yonent gisal,"},{"style":"q1","verse":"","text":"te biraluñu sag ñaawtéef,"},{"style":"q1","verse":"","text":"ngir tijji sa wërsëg,"},{"style":"q1","verse":"","text":"xanaa di la yéeneel \\w waxyuy|waxyu:\\w* caaxaan aki nax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Mboolem ñi fi jaareey tacc-taccali loxo,"},{"style":"q1","verse":"","text":"ciipu, wëcc bopp ci gentub Yerusalem mu taaru ma,"},{"style":"q1","verse":"","text":"te naan: «Xanaa du biib dëkk lañu doon tudd"},{"style":"q1","verse":"","text":"“Taar bu sotti,”"},{"style":"q1","verse":"","text":"te muy mbégtem àddina sépp?»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Sa noon yépp a lay ñaawal,"},{"style":"q1","verse":"","text":"di la kókkaleeka kekku,"},{"style":"q1","verse":"","text":"te naan: «Sànk nanu leen nag!"},{"style":"q1","verse":"","text":"Bii bés lanu yàggoona séentu,"},{"style":"q1","verse":"","text":"te noo ko fekke tey.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Aji Sax jee def la mu nammoon,"},{"style":"q1","verse":"","text":"moo sottal kàddoom"},{"style":"q1","verse":"","text":"ga mu santaane woon janti démb."},{"style":"q1","verse":"","text":"Moo faat te yërëmul,"},{"style":"q1","verse":"","text":"ba may noon bége leen,"},{"style":"q1","verse":"","text":"te moo kaweel sa kàttanu bañ yi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Askan waa ngi jooy jooyi xol"},{"style":"q1","verse":"","text":"fi kanam Boroom bi."},{"style":"q1","verse":"","text":"Éy yaw tatay Siyoŋ, dëkk bu taaru bi,"},{"style":"q1","verse":"","text":"sottil say rongooñ ni walum dex guddeek bëccëg;"},{"style":"q1","verse":"","text":"bul noppalu, bul feral say rongooñ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Jógal guddi, xaacu:"},{"style":"q1","verse":"","text":"ndoortel wattub guddi, ngay yuri sab xol"},{"style":"q1","verse":"","text":"nim ndox, jàkkaarlook Boroom bi."},{"style":"q1","verse":"","text":"Tàllal koy loxo, ngir sa bakkani doom"},{"style":"q1","verse":"","text":"yi xiif tax ne lasiim fi boppu mbeddoo mbedd."},{"style":"sp","verse":"","text":"Waa Yerusalem àddu na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Éy Aji Sax ji, xoolal, seetal rekk"},{"style":"q1","verse":"","text":"ki nga def nii!"},{"style":"q1","verse":"","text":"Xanaa du ndey mooy dem ba yàpp doomu boppam,"},{"style":"q1","verse":"","text":"doom ju mu sukk jur, di ko toppatoo?"},{"style":"q1","verse":"","text":"Xanaa du \\w sarxalkat|sarxalkat b-:\\w* akub yonent lañuy bóom"},{"style":"q1","verse":"","text":"fi kër gu sell gi ñeel Aji Sax ji?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Mag ak ndaw a ngi ne lareet fi suuf ci mbedd yi;"},{"style":"q1","verse":"","text":"sama janq jeek sama xale yu góor yi tëdd,"},{"style":"q1","verse":"","text":"saamar faat leen."},{"style":"q1","verse":"","text":"Yaa rey ci sa bésub mer,"},{"style":"q1","verse":"","text":"yaa rendi te yërëmoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Yaa woote ni su doon bésu màggal,"},{"style":"q1","verse":"","text":"tiitaange dar kepp ma."},{"style":"q1","verse":"","text":"Bésub merum Aji Sax ji, kenn rëccul,"},{"style":"q1","verse":"","text":"kenn muccul."},{"style":"q1","verse":"","text":"Doom yi ma jur, yar leen,"},{"style":"q1","verse":"","text":"noon bi rey leen!"}]