[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Bàyyil xel Ki la sàkk,"},{"style":"q1","verse":"","text":"bi ngay dese ndaw,"},{"style":"q1","verse":"","text":"bala bési safaan a taxaw,"},{"style":"q1","verse":"","text":"ba jant ñëw, nga naa:"},{"style":"q1","verse":"","text":"«Amatuma bànneex.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Fa la jant di lëndëme,"},{"style":"q1","verse":"","text":"weer giim, biddiiw giim,"},{"style":"q1","verse":"","text":"taw sewet, mu niru la lu xiinati."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Su bés baa wattukati kër gi di lox,"},{"style":"q1","verse":"","text":"jàmbaari xare yi sëgg,"},{"style":"q1","verse":"","text":"wolkat yi néew, ba wolatuñu,"},{"style":"q1","verse":"","text":"ñiy séentu ci caax yi giimle,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"bunt yi féeteek mbedd mi tëje,"},{"style":"q1","verse":"","text":"coowal wol dal,"},{"style":"q1","verse":"","text":"as picc sab, nelaw naaw,"},{"style":"q1","verse":"","text":"janq ju doon woy, selaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Nga dem, ba ragal fu kawe,"},{"style":"q1","verse":"","text":"am tiitaange ciw yoon,"},{"style":"q1","verse":"","text":"garab tóor, ba weex furr,"},{"style":"q1","verse":"","text":"soccet di yokokoki,"},{"style":"q1","verse":"","text":"doomu garab tëë yékkati siddit,"},{"style":"q1","verse":"","text":"nga jëm kër gu mujj,"},{"style":"q1","verse":"","text":"waa dëj bi fees mbedd mi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Bàyyil xel Ki la sàkk,"},{"style":"q1","verse":"","text":"balaa buumu xaalis bi ne tipp,"},{"style":"q1","verse":"","text":"ndabal wurus wi, tasar,"},{"style":"q1","verse":"","text":"njaq la ñuy roote, rajax,"},{"style":"q1","verse":"","text":"poli ba ca teen ba, tàllit."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Su boobaa pënd dellu suuf, na woon;"},{"style":"q1","verse":"","text":"bakkan dellu fa Yàlla ma able woon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Cóolóoloo cóolóol, waru waarekat ba."},{"style":"q1","verse":"","text":"Léppi cóolóoli neen."},{"style":"s1","verse":"","text":"Ma daaneel"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Waarekat ba daa xelu,"},{"style":"q1","verse":"","text":"di jàngalewaale xam-xam,"},{"style":"q1","verse":"","text":"di natt aka gëstu,"},{"style":"q1","verse":"","text":"di jekk-jekkal kàdduy xel yu takku."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Waarekat ba daa tànn baat yu saf,"},{"style":"q1","verse":"","text":"bind ko, mu jub te dëggu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Waxi ku xelu day gindee, bantub sàmm la;"},{"style":"q1","verse":"","text":"tënk ba diy pont, sampe ca."},{"style":"q1","verse":"","text":"Loolu ag may la, jóge ci sàmm biy kenn."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Doom, leneen lu ci tegu, moytu ko:"},{"style":"q1","verse":"","text":"yokki téere amul kemu,"},{"style":"q1","verse":"","text":"njàng mu bare, coonoy boroom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ma daaneele fii, ndax déggees na lii lépp:"},{"style":"q1","verse":"","text":"Yàlla, ragal ko;"},{"style":"q1","verse":"","text":"ay santaaneem, jëfe ko;"},{"style":"q1","verse":"","text":"loolu rekk mooy nit."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Jépp jëf déy, la Yàlla di àtte,"},{"style":"q1","verse":"","text":"ba ci lépp lu làqu,"},{"style":"q1","verse":"","text":"muy lu baax, di lu bon."}]