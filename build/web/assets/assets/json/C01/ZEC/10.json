[{"style":"s1","verse":"","text":"Xërëm, neen la"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Bu suuf maree, ñaanal Aji Sax ji ab taw,"},{"style":"q1","verse":"","text":"Aji Sax ji mooy sàkk ay xàmbaar,"},{"style":"q1","verse":"","text":"sóobal nit ab taw,"},{"style":"q1","verse":"","text":"saxalal ku ne am ñax ci àll bi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Xërëm, waxi neen,"},{"style":"q1","verse":"","text":"gisaanekat gisi fen,"},{"style":"q1","verse":"","text":"seeni gént, nari neen,"},{"style":"q1","verse":"","text":"te caaxaani neen lañuy dalalee."},{"style":"q1","verse":"","text":"Moo tax mbooloo mii di taxawaalu niy xar,"},{"style":"q1","verse":"","text":"tumurànke ndax ñàkkub sàmm."},{"style":"s1","verse":"","text":"Yiwiku ñeelati na Israayil"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"«Sàmm yi la sama mer tàkkal,"},{"style":"q1","verse":"","text":"ñoom sikket yooyu laay dikke mbugal."},{"style":"q1","verse":"","text":"Aji Sax ji Boroom gàngoor yi kat mooy taxawu kërug Yuda, géttam,"},{"style":"q1","verse":"","text":"te moo leen di def aw fasam wu am daraja, ngir xare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Fa Yuda la doju koñu tabax di bawoo,"},{"style":"q1","verse":"","text":"jënu xayma, fa moom,"},{"style":"q1","verse":"","text":"fittu xare, fa moom,"},{"style":"q1","verse":"","text":"mboolem njiit yi ba ñépp daj, fa moom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ni ñeyi xare lañuy mel,"},{"style":"q1","verse":"","text":"di joggati banu mbedd mi ci biir xeex bi."},{"style":"q1","verse":"","text":"Noonu lañuy xaree, nde ñoom la Aji Sax jiy àndal,"},{"style":"q1","verse":"","text":"ñuy toroxal gawar yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Maay jàmbaaral waa kër Yuda,"},{"style":"q1","verse":"","text":"maay wallu waa kër Yuusufa,"},{"style":"q1","verse":"","text":"suqali leen, ñeewante leen,"},{"style":"q1","verse":"","text":"ñu mel ni masuma leena xarab,"},{"style":"q1","verse":"","text":"nde man maay seen Yàlla Aji Sax ji,"},{"style":"q1","verse":"","text":"te maa leen di nangul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Waa Efrayim ni ñeyi xare lañuy mel,"},{"style":"q1","verse":"","text":"seen xol neexe ni bànneexu biiñ,"},{"style":"q1","verse":"","text":"seeni doom gis ko, bég,"},{"style":"q1","verse":"","text":"seenub xol di bànneexoo Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Maay waliis, dajale leen,"},{"style":"q1","verse":"","text":"nde maa leen di jot,"},{"style":"q1","verse":"","text":"ñu bareeti na ñu baree woon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Fi biir xeet yi laa leen saawi,"},{"style":"q1","verse":"","text":"waaye fa béreb yu sore ya lañu may fàttalikoo,"},{"style":"q1","verse":"","text":"di fa dund ñook seeni doom,"},{"style":"q1","verse":"","text":"ba keroog ñuy délsi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Fa réewum Misra laa leen di waññee,"},{"style":"q1","verse":"","text":"te fa Asiri laa leen di dajalee,"},{"style":"q1","verse":"","text":"yóbbu leen réewum Galàdd ak Libaŋ,"},{"style":"q1","verse":"","text":"te deesu fa gis fu ñu xaj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Maay jàll géeju njàqare ga,"},{"style":"q1","verse":"","text":"dal ca gannax ya,"},{"style":"q1","verse":"","text":"mboolem xóotey dexu Niil wow,"},{"style":"q1","verse":"","text":"sagub Asiri sàggiku,"},{"style":"q1","verse":"","text":"nguurug Misra foq."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ci Aji Sax ji laa leen di jàmbaarale,"},{"style":"q1","verse":"","text":"ciw turam lañuy daagoo.»"},{"style":"q1","verse":"","text":"Kàddug Aji Sax jee!"}]