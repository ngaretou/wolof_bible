[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"«Dees na topp waa Samari,"},{"style":"q1","verse":"","text":"nde ñoo fippu ci seen kaw Yàlla."},{"style":"q1","verse":"","text":"Saamar lañuy faate,"},{"style":"q1","verse":"","text":"seeni doom, ñu rajaxe leen,"},{"style":"q1","verse":"","text":"seen jigéeni wérul, ñu butti.»"},{"style":"s1","verse":"","text":"Dëpp, am njéggal"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Israayiloo, dellul ci sa Yàlla Aji Sax ji,"},{"style":"q1","verse":"","text":"fakkastaloo nga sag ñaawtéef."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Waajalleen ay kàddu te dellu ci Aji Sax ji,"},{"style":"q1","verse":"","text":"ne ko: «Baal nu mboolem ñaawtéef"},{"style":"q1","verse":"","text":"te nangul nu,"},{"style":"q1","verse":"","text":"nu yékkati kàdduy màggal"},{"style":"q1","verse":"","text":"yu wuutu saraxi yëkk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Asiri du nu xettali,"},{"style":"q1","verse":"","text":"ay fasi xare it, dunu ko war,"},{"style":"q1","verse":"","text":"te dootunu wax lu nu sàkke sunuy loxo, ne ko:"},{"style":"q1","verse":"","text":"“Yaw yaa di sunu Yàlla.”"},{"style":"q1","verse":"","text":"Li nuy wax kay mooy yaw yaay kiy yërëm jirim.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Aji Sax ji nee: «Maay garabal seenug déggadi,"},{"style":"q1","verse":"","text":"maa leen di bëggal sama bopp,"},{"style":"q1","verse":"","text":"nde mereetuma leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Maay mel nib lay buy guusal Israayil,"},{"style":"q1","verse":"","text":"ñu focci ni tóor-tóor,"},{"style":"q1","verse":"","text":"seeni reen sampu ni garabi Libaŋ,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"car ya law, yànj nig oliw,"},{"style":"q1","verse":"","text":"xeeñ ni gottub Libaŋ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ñooy délsi, keppaaroo ko,"},{"style":"q1","verse":"","text":"dellu ji, pepp dundaat."},{"style":"q1","verse":"","text":"Dañuy giir nig reseñ,"},{"style":"q1","verse":"","text":"am bayre ni biiñu Libaŋ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"«Yeen Efrayim, lu ma masa séq ak jëmmi tuur?"},{"style":"q1","verse":"","text":"Man maa leen di nangul, di leen wattu,"},{"style":"q1","verse":"","text":"di leen keral ni garab gu naat gu dul ruus,"},{"style":"q1","verse":"","text":"sa meññeef bawoo fi man.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ku rafet xel, na dégg lii,"},{"style":"q1","verse":"","text":"kuy ràññee na ko xam."},{"style":"q1","verse":"","text":"Yooni Aji Sax ji kat day jub,"},{"style":"q1","verse":"","text":"te kuy jubal jaare ca,"},{"style":"q1","verse":"","text":"ab jàddkat moo cay tërëf."}]