[{"style":"s1","verse":"","text":"Aji Sax ji layoo na ak Israayil ak Yuda"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"«Efrayim, ay fen la ma gawe,"},{"style":"q1","verse":"","text":"waa kër Israayil laa ne,"},{"style":"q1","verse":"","text":"ñoo ma yéewe ag njublaŋ,"},{"style":"q1","verse":"","text":"te Yudaa ngi fippu ci kaw Yàlla ba tey,"},{"style":"q1","verse":"","text":"di fecci kóllëreem ak Ku sell ki.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Efrayim mooy sàmmkatub ngelaw,"},{"style":"q1","verse":"","text":"yendoo topp ngelawal penku,"},{"style":"q1","verse":"","text":"di yokki fen ak fitna,"},{"style":"q1","verse":"","text":"fasoo \\w kóllëre|kóllëre g-:\\w* ak Asiri,"},{"style":"q1","verse":"","text":"yónnee yóbbalu diw ba Misra."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aji Sax ji moo séq ak Yuda ab layoo,"},{"style":"q1","verse":"","text":"mooy mbugale \\w Yanqóoba|Yanqóoba:\\w* ay jëfinam,"},{"style":"q1","verse":"","text":"añale ko ay jëfam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ci biiru ndeyam la kii suufoo ab seexam;"},{"style":"q1","verse":"","text":"kii moo tollu ci dooleem, bëreek Yàlla."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Kee bëreek malaaka, ba génn ca,"},{"style":"q1","verse":"","text":"kee jooy di sàkku aw yiwam."},{"style":"q1","verse":"","text":"Fa Betel la ko fekk,"},{"style":"q1","verse":"","text":"te foofa la Yàlla waxeek nun."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax ji Yàlla Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"Aji Sax jee di turu bàkkam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yaw nag, ca sa Yàlla nga wara dellu,"},{"style":"q1","verse":"","text":"ngor ak njub, sàmm ko,"},{"style":"q1","verse":"","text":"te nga saxoo yaakaar sa Yàlla."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ni Kanaaneen biy jula, ni la Efrayim"},{"style":"q1","verse":"","text":"yore màndaxekaayu njublaŋ cib loxoom,"},{"style":"q1","verse":"","text":"bëgga naxe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Efrayim a ngi naan: «Woomle naa,"},{"style":"q1","verse":"","text":"maa amal sama bopp alal,"},{"style":"q1","verse":"","text":"te mboolem samaw ñaq, giseesu ci"},{"style":"q1","verse":"","text":"tooñaange ju di bàkkaar.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"«Man Aji Sax ji dale di sa Yàlla ca Misra,"},{"style":"q1","verse":"","text":"maa lay dëëlaat ciy xayma,"},{"style":"q1","verse":"","text":"na woon ca bésub ndaje."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Maa wax ak yonent yi,"},{"style":"q1","verse":"","text":"yokk peeñu yi,"},{"style":"q1","verse":"","text":"te yonent yi, maa leen léeb ay misaal.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Su Galàdd dee dëkkub defkati ñaawtéef,"},{"style":"q1","verse":"","text":"dañoo tekkiwul dara."},{"style":"q1","verse":"","text":"Fa Gilgal lees di rendee ay saraxi yëkk,"},{"style":"q1","verse":"","text":"te seeni \\w sarxalukaay|sarxalukaay b-:\\w* ni jali xeer lay mel"},{"style":"q1","verse":"","text":"ci ay tooboy tool."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Yanqóobaa dawoon ba réewum Siri,"},{"style":"q1","verse":"","text":"Israayil laa ne, moo liggéey ngir am jabar,"},{"style":"q1","verse":"","text":"wut jabar tax koo sàmm ag jur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ci loxol yonent la Aji Sax ji génnee Israayil Misra,"},{"style":"q1","verse":"","text":"te ab yonent la sàmmloo Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Waaye Efrayim moomoo ko merloo,"},{"style":"q1","verse":"","text":"naqaral ko."},{"style":"q1","verse":"","text":"Deret ji mu tuur la Boroomam di këpp ci kawam,"},{"style":"q1","verse":"","text":"añub tooñam la koy fey."}]