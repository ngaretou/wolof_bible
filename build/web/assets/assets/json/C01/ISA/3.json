[{"style":"s1","verse":"","text":"Yuda salfaañoo na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Aji Sax ji Boroom bi, Aji Sax ju gàngoor yi kat,"},{"style":"q1","verse":"","text":"mu ngi nii di jële Yerusalem ak Yuda,"},{"style":"q1","verse":"","text":"lu doon wéeruwaay, di cëslaay,"},{"style":"q1","verse":"","text":"muy dugub ju ñu yaakaaroon,"},{"style":"q1","verse":"","text":"di ndox mu ñu yaakaaroon"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"ak jàmbaar ak ñeyu xare"},{"style":"q1","verse":"","text":"akub àttekat akub yonent"},{"style":"q1","verse":"","text":"ak gisaanekat ak magum pénc"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"ak njiitu juróom fukki dag akub kàngam"},{"style":"q1","verse":"","text":"ak kiy xelal buur ak ndaanaanu liggéeykat"},{"style":"q1","verse":"","text":"ak jatkat bu xareñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"«Maay def ay gone jiite leen,"},{"style":"q1","verse":"","text":"di ñu yaroo seen sago, yilif leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Nit ñiy soxorante,"},{"style":"q1","verse":"","text":"ku nekk ak moroomam, ku nekk ak dëkkandoom,"},{"style":"q1","verse":"","text":"ndaw di ñàkke mag kersa,"},{"style":"q1","verse":"","text":"sagaru nit sofental boroom daraja."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Nit dina ne nikk mbokk ma,"},{"style":"q1","verse":"","text":"ca biir kër baayam, ne ko:"},{"style":"q1","verse":"","text":"“Yaw mi dese mbubb, jiite nu,"},{"style":"q1","verse":"","text":"ba lu bokk ci sunu gent bii, di sa kilifteef.”"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Keroog kooka àddu ne ko:"},{"style":"q1","verse":"","text":"“Duma leen mana fajal dara!"},{"style":"q1","verse":"","text":"Du dugub, du mbubb fa sama kër,"},{"style":"q1","verse":"","text":"dungeen ma def njiit!”»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yerusalem day téqtalu,"},{"style":"q1","verse":"","text":"Yuda fëlëñu."},{"style":"q1","verse":"","text":"Ñooy wax aka jëf lu neexul Aji Sax ji,"},{"style":"q1","verse":"","text":"di gàntal leeram gi teew."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Seen dëgër bët tuumaal na leen,"},{"style":"q1","verse":"","text":"ñuy bàkkaar ni waa \\w Sodom|Sodom ak Gomor:\\w*,"},{"style":"q1","verse":"","text":"làquwuñu sax."},{"style":"q1","verse":"","text":"Wóoy, ngalla ñoom!"},{"style":"q1","verse":"","text":"nde ñoo lor seen bopp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Neleen aji jub ji mooy baaxle,"},{"style":"q1","verse":"","text":"mooy yooloo añam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Waaye wóoy ku bon, nde mooy bonle;"},{"style":"q1","verse":"","text":"la mu def rekk lees koy def."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Bokk yi, ñu xalele ñoo leen di noot,"},{"style":"q1","verse":"","text":"ay jigéen yilif leen."},{"style":"q1","verse":"","text":"Bokk yi, seeni njiit a ngi leen di sànk,"},{"style":"q1","verse":"","text":"lëndëmal nañu seeni yoon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Aji Sax jaa ngi ci jataayu layoo,"},{"style":"q1","verse":"","text":"moo jóg di àtte ay xeet."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Aji Sax jeey yékkati kàddug sikkaange,"},{"style":"q1","verse":"","text":"jëme ci magi ñoñam ak seeni kàngam."},{"style":"q1","verse":"","text":"«Yeena sëxëtoo tóokëru reseñ bi,"},{"style":"q1","verse":"","text":"la ñu foqatee ci néew-ji-doolee nga seeni kër."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Lu waral ngeen di dëggaate sama ñoñ,"},{"style":"q1","verse":"","text":"nappaaje néew-doole yi?»"},{"style":"q1","verse":"","text":"Kàddug Boroom bee, Aji Sax ji ci gàngoor yi."},{"style":"s1","verse":"","text":"Puukare sànk na jigéeni Siyoŋ"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Aji Sax ji da ne:"},{"style":"q1","verse":"","text":"«Jigéeni \\w Siyoŋ|Siyoŋ:\\w* a ngi daŋŋiiral,"},{"style":"q1","verse":"","text":"di goŋeet-goŋeeti,"},{"style":"q1","verse":"","text":"di regejooka rékk-rékkal,"},{"style":"q1","verse":"","text":"sampum ndaag,"},{"style":"q1","verse":"","text":"keleŋal lami wëq.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Moo tax Boroom beey wat jigéeni Siyoŋ nel,"},{"style":"q1","verse":"","text":"Aji Sax jeey muri seen jë."},{"style":"p","verse":"","text":""},{"style":"v","verse":"18","text":"Bésub keroog Boroom bi xañ leen gànjari tànk ak takkaay yu bindoo ni jant ak yu bindoo ni weer wu feeñ"},{"style":"v","verse":"19","text":"aki loŋ-loŋ aki caq aki muuraay."},{"style":"v","verse":"20","text":"Moo leen di xañ ay tañlaay ak lami përëg aki ngañaay ak téere yu xeeñ aki gàllaaj,"},{"style":"v","verse":"21","text":"xañ leen jaaroy torlu ak jaaroy bakkan,"},{"style":"v","verse":"22","text":"ak mbubbi xew aki mëlfë ak séri càngaay ak saagi loxo,"},{"style":"v","verse":"23","text":"ba ci yérey sooy ak yérey lẽe aki musóor aki kol."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Su boobaa lu xasaw wuutu lu xeeñ,"},{"style":"q1","verse":"","text":"buum wuutu ngañaay,"},{"style":"q1","verse":"","text":"nel bu set wuutuy létt,"},{"style":"q1","verse":"","text":"yérey saaku wuutu ndoket bu rafet,"},{"style":"q1","verse":"","text":"toroxte wuutub taar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Yerusalem, say góor ay saamar lañu leen faate,"},{"style":"q1","verse":"","text":"say jàmbaar des xare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Su boobaa sa bunti dëkk sax di ñaawlooka jooy,"},{"style":"q1","verse":"","text":"mel ni ndaw su ñu futti,"},{"style":"q1","verse":"","text":"mu ne dett fi suuf."}]