[{"style":"ms","verse":"","text":"Kàddug waxyu ñeel na mujug jamono"},{"style":"mr","verse":"","text":"(Saar 24---27)"},{"style":"s1","verse":"","text":"Mbugal daj na àddina"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Aji Sax jaa ngii di tas àddina,"},{"style":"q1","verse":"","text":"tasaare ko,"},{"style":"q1","verse":"","text":"salfaañe li fi dénd bi,"},{"style":"q1","verse":"","text":"wasaare ñi ko dëkke:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"\\w sarxalkat|sarxalkat b-:\\w* beek baadoolo bi,"},{"style":"q1","verse":"","text":"sang akub jaam,"},{"style":"q1","verse":"","text":"njaatige lu jigéen ak ab janqam,"},{"style":"q1","verse":"","text":"jaaykat ak jëndkat,"},{"style":"q1","verse":"","text":"ablekat ak àbbkat,"},{"style":"q1","verse":"","text":"leblekat ak lebkat."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Àddinaa ne duŋŋ duŋŋaaral,"},{"style":"q1","verse":"","text":"dees koo futti ba mu ne furaas!"},{"style":"q1","verse":"","text":"Aji Sax ji kat wax na kàddu gii!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Suuf a ngi wowa wow, ne sereŋ,"},{"style":"q1","verse":"","text":"àddina ne làcc, ne sereŋ,"},{"style":"q1","verse":"","text":"kàngami xeeti àddina ne làcc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Suuf si la ñi ko dëkke sobeel,"},{"style":"q1","verse":"","text":"ñoo wàcc yoon yi,"},{"style":"q1","verse":"","text":"xëtt ndigal,"},{"style":"q1","verse":"","text":"fecci \\w kóllëre|kóllëre g-:\\w* gi sax dàkk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Moo tax alkànde yàq suuf,"},{"style":"q1","verse":"","text":"ñi ko dëkke gàddu àq ji."},{"style":"q1","verse":"","text":"Moo tax waa àddina raaf,"},{"style":"q1","verse":"","text":"ba ñi des di lu néew."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Biiñ ñàkk na,"},{"style":"q1","verse":"","text":"garabu reseñ lax,"},{"style":"q1","verse":"","text":"bépp xol bu tooyoon, binni."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Tabala ju xumb selaw na,"},{"style":"q1","verse":"","text":"ña daa xaxaloo dal,"},{"style":"q1","verse":"","text":"mbégtem buumi xalam dakk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Kenn naanatul, di woy,"},{"style":"q1","verse":"","text":"ñoll wex na ka koy naan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Dëkk bu gental tasaaroo na,"},{"style":"q1","verse":"","text":"kër yépp tëje, kenn duggul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Mbedd maa ngi soowaloo,"},{"style":"q1","verse":"","text":"ndax biiñ bu ñàkk,"},{"style":"q1","verse":"","text":"gépp mbégte fey,"},{"style":"q1","verse":"","text":"bànneexu àddina dëddu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Tojiti neen a des ci dëkk bi,"},{"style":"q1","verse":"","text":"bunt ba rajaxoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Noonu lay deme ci àddina,"},{"style":"q2","verse":"","text":"ci biir xeet yi,"},{"style":"q1","verse":"","text":"mbete benn-benni doomi oliw gu ñu gësëm,"},{"style":"q1","verse":"","text":"mbaa doomi reseñ yu des ca ndey ja,"},{"style":"q1","verse":"","text":"gannaaw bu ñu wittee ba sottal."},{"style":"s1","verse":"","text":"Ndes wi rëcc mbugal a ngi sant"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ña ngay xaacu di sarxolle,"},{"style":"q1","verse":"","text":"waa sowu di sàbbaal ci màggug Aji Sax ji."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Yeen waa penku, sargalleen Aji Sax ji boog,"},{"style":"q1","verse":"","text":"yeen waa duni géej, teralleen Aji Sax ji Yàllay Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Cati àddina lay woy di jibe, nu dégg ñu naa:"},{"style":"q1","verse":"","text":"«Màggaay ñeel na Aji Jub ji!»"},{"style":"s1","verse":"","text":"Coono jeexul"},{"style":"q1","verse":"","text":"Waaye man dama ne: «Jeexle naa!"},{"style":"q1","verse":"","text":"Jeexle naa, wóoy man!"},{"style":"q1","verse":"","text":"Workat yaa ngi wor a wor,"},{"style":"q1","verse":"","text":"ba far làggi ci wor.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Yeen waa àddina,"},{"style":"q1","verse":"","text":"kat-kat, pax akub caax dëgmal na leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Nga déggi yuux, daw,"},{"style":"q1","verse":"","text":"tàbbi biir pax,"},{"style":"q1","verse":"","text":"génne pax,"},{"style":"q1","verse":"","text":"laawoob caax."},{"style":"q1","verse":"","text":"Mbàndi kaw kat ubbi na,"},{"style":"q1","verse":"","text":"suufu àddina di rëŋ-rëŋi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Suuf daa loroo loru,"},{"style":"q1","verse":"","text":"fett a fett,"},{"style":"q1","verse":"","text":"salfaañoo salfaañu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Suuf a ngi jang a jang ni màndikat,"},{"style":"q1","verse":"","text":"ba joy ni mbaar,"},{"style":"q1","verse":"","text":"moyam diis ko gann,"},{"style":"q1","verse":"","text":"mu daanu te du jógati."},{"style":"s1","verse":"","text":"Aji Sax jeey nguuru fa Siyoŋ"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Bu bésub keroogee"},{"style":"q1","verse":"","text":"Aji Sax ji dikke mbugal"},{"style":"q2","verse":"","text":"gàngoor ya fa kaw-a-kaw,"},{"style":"q1","verse":"","text":"ak buuri àddina yi fi suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Dees leen di boole denc cim pax,"},{"style":"q1","verse":"","text":"ne leen ràpp tëj,"},{"style":"q1","verse":"","text":"ba mu am ay fani fan, ñu mbugal leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Kersa la weer wiy am,"},{"style":"q1","verse":"","text":"gàcce ñeel jant bi;"},{"style":"q1","verse":"","text":"Aji Sax ji Boroom gàngoor yi daal"},{"style":"q1","verse":"","text":"mooy nguuru Yerusalem, kaw tundu \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","verse":"","text":"leeram ga jollee fa kanam magi ñoñam."}]