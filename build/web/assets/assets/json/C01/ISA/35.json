[{"style":"s1","verse":"","text":"Yoonu ñibbi Siyoŋ taxaw na"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Màndiŋ ak suuf su ne sereŋ dina bég,"},{"style":"q1","verse":"","text":"joor gu wow bànneexu, tóor ba ne ràññ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Day tóor a tóor,"},{"style":"q1","verse":"","text":"di bànneexu, bay sarxolle,"},{"style":"q1","verse":"","text":"ba jagoo darajay gottub Libaŋ"},{"style":"q1","verse":"","text":"ak taaru àllub Karmel ak Saron."},{"style":"q1","verse":"","text":"Foofa lees di gise darajay Aji Sax ji,"},{"style":"q1","verse":"","text":"sunu taaru Yàlla."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ku yoqi, dooleelleen ko,"},{"style":"q1","verse":"","text":"ku óom yay fenqe, jàppaleleen ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ku fitam rëcc, ne leen ko:"},{"style":"q1","verse":"","text":"«Dëgërlul te bul tiit."},{"style":"q1","verse":"","text":"Sa Yàllaa ngii di la feyul,"},{"style":"q1","verse":"","text":"ba yool say noon."},{"style":"q1","verse":"","text":"Moom mooy dikk, wallusi la.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Su boobaa gëti silmaxa muriku,"},{"style":"q1","verse":"","text":"noppi tëx sikkliku."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Su boobaa ab làggi ne jàyy ni kéwél,"},{"style":"q1","verse":"","text":"ab luu sarxolle."},{"style":"q1","verse":"","text":"Am ndox ay fettaxe màndiŋ ma,"},{"style":"q1","verse":"","text":"ay wal wale joor gu wow,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"suuf su wow doonub déeg,"},{"style":"q1","verse":"","text":"joor gu maral ballal bëti ndox."},{"style":"q1","verse":"","text":"Fa till fàkkoon, di fa goore,"},{"style":"q1","verse":"","text":"gattax ak barax a fay sëqe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Foofa law yoon di jaare,"},{"style":"q1","verse":"","text":"ñu di ko woowe Yoon wu sell wi."},{"style":"q1","verse":"","text":"Ku sobewu du fa jaare,"},{"style":"q1","verse":"","text":"ñi moom woowu yoon,"},{"style":"q1","verse":"","text":"ñooy ñi koy topp."},{"style":"q1","verse":"","text":"Ab dof sax du fa lajje."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Gaynde du fa am,"},{"style":"q1","verse":"","text":"rab wu aay du fa dox,"},{"style":"q1","verse":"","text":"du lenn lees fay gis,"},{"style":"q1","verse":"","text":"ku ñu jot rekk a fay jaare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ñi Aji Sax ji goreel, nii lañuy dellusee,"},{"style":"q1","verse":"","text":"agsaaley sarxolle fa Siyoŋ,"},{"style":"q1","verse":"","text":"kaalawoo mbég mu sax,"},{"style":"q1","verse":"","text":"mbégteek bànneex wàllsi,"},{"style":"q1","verse":"","text":"naqar aku tiis daw."}]