[{"style":"s1","verse":"","text":"Barke ñeel na ñoñi Aji Sax ji"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Dégluleen maa, yeen ñiy wuta doxee yoon,"},{"style":"q1","verse":"","text":"wuta déggal Aji Sax ji."},{"style":"q1","verse":"","text":"Xoolleen doj wi ñu leen yatte,"},{"style":"q1","verse":"","text":"xool xóotey kàmb gi ñu leen seppee:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"seen maam Ibraayma laa ne, moom ngeen di xool,"},{"style":"q1","verse":"","text":"xool ci Saarata mi leen meññ."},{"style":"q1","verse":"","text":"Ba muy kenn laa ko woo,"},{"style":"q1","verse":"","text":"barkeel ko, ful ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aji Sax jeey dëfal \\w Siyoŋ|Siyoŋ:\\w* moos,"},{"style":"q1","verse":"","text":"dëfalaale fépp fu fa dib gent,"},{"style":"q1","verse":"","text":"ba def màndiŋam ni toolub Àjjana,"},{"style":"q1","verse":"","text":"jooram gu wow mel ni toolub Aji Sax ji,"},{"style":"q1","verse":"","text":"mbégteek bànneex teew fa,"},{"style":"q1","verse":"","text":"ak cantug woy feelu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yeen sama ñoñ, teewluleen ma,"},{"style":"q1","verse":"","text":"yeen sama ñoñ, dégluleen ma."},{"style":"q1","verse":"","text":"Fi man la téereb yoon di bawoo,"},{"style":"q1","verse":"","text":"te maay fettaxal sama àtte,"},{"style":"q1","verse":"","text":"muy leeru xeet yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sama wall jege na,"},{"style":"q1","verse":"","text":"sama àtte dëgmal na,"},{"style":"q1","verse":"","text":"ma nara àttee xeet yi sama dooley përëg."},{"style":"q1","verse":"","text":"Man la waa dun yiy xaar,"},{"style":"q1","verse":"","text":"sama dooley përëg lañu yaakaar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Séentuleen asamaan"},{"style":"q1","verse":"","text":"te xool suuf si mu tiim."},{"style":"q1","verse":"","text":"Asamaan ay tasaaroo ni saxaar,"},{"style":"q1","verse":"","text":"suuf ràpp ni mbubb,"},{"style":"q1","verse":"","text":"ñi ko dëkke dee niy weñ,"},{"style":"q1","verse":"","text":"te du tee sama wall di sax ba fàww,"},{"style":"q1","verse":"","text":"sama doxalinu yoon it du dagg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yeen ñi xam liy yoon, dégluleen ma,"},{"style":"q1","verse":"","text":"yeen, askan wi sama yoon yebu ci seen xol."},{"style":"q1","verse":"","text":"Buleen ragal nit ku leen sewal,"},{"style":"q1","verse":"","text":"buleen tiit seeni xaste."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Sax a leen di lekk ni mbubb,"},{"style":"q1","verse":"","text":"max ŋeeñ leen ni ndimol kawaru gàtt;"},{"style":"q1","verse":"","text":"te du tee sama doxalinu yoon sax dàkk,"},{"style":"q1","verse":"","text":"sama wall saxal maasoo maas."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Aji Sax ji, yewwul, ngalla yewwul;"},{"style":"q1","verse":"","text":"wone sa doole!"},{"style":"q1","verse":"","text":"Yewwul na bu njëkkoon,"},{"style":"q1","verse":"","text":"bu yàgga yàgg."},{"style":"q1","verse":"","text":"Xanaa du yaa falaxe Raxab?"},{"style":"q1","verse":"","text":"Du yaa jam ninki-nanka jooja?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Xanaa du yaa wowal géej"},{"style":"q1","verse":"","text":"ak ndoxum xuram wu ne màww,"},{"style":"q1","verse":"","text":"def xóotey géej aw yoon,"},{"style":"q1","verse":"","text":"ña nga jot jàlle ca?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ñi Aji Sax ji goreel, nii lañuy dellusee,"},{"style":"q1","verse":"","text":"agsaaley sarxolle fa Siyoŋ,"},{"style":"q1","verse":"","text":"kaalawoo mbég mu sax,"},{"style":"q1","verse":"","text":"mbégteek bànneex wàllsi,"},{"style":"q1","verse":"","text":"naqar aku tiis daw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Man ci sama bopp maa leen di dëfal."},{"style":"q1","verse":"","text":"Ana lu ngeen di ragal ci nit kuy dee,"},{"style":"q1","verse":"","text":"doom aadama ju bokk aw demin akum ñax?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Dangeena fàtte Aji Sax ji leen sàkk,"},{"style":"q1","verse":"","text":"ki firi asamaan, lal suuf?"},{"style":"q1","verse":"","text":"Dangeen di dëkke bésoo bés,"},{"style":"q1","verse":"","text":"di tiit xadaru boroom doole buy waaja sànke?"},{"style":"q1","verse":"","text":"Ana fu xadaru boroom doole mujje?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku diisoon ba waaf léegi nga féex;"},{"style":"q1","verse":"","text":"doo deewe pax ma nga tëje,"},{"style":"q1","verse":"","text":"te doo ñàkk loo lekk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Man maay seen Yàlla Aji Sax ji,"},{"style":"q1","verse":"","text":"kiy yëngal géej, riiral gannaxam,"},{"style":"q1","verse":"","text":"Aji Sax ji Boroom gàngoor yi di sama tur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Maa la sexal samay kàddu,"},{"style":"q1","verse":"","text":"yiire la sama keru loxo,"},{"style":"q1","verse":"","text":"aj asamaan, lal suuf,"},{"style":"q1","verse":"","text":"ne Siyoŋ: «Yeenay sama ñoñ.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Yewwulee, yewwul, Yerusalemee jógal,"},{"style":"q1","verse":"","text":"yaw mi Aji Sax ji nàndal kaasu mbugalam."},{"style":"q1","verse":"","text":"Kaas bu wex bay miirloo,"},{"style":"q1","verse":"","text":"taat wa, yaa ko siitaatu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Amul ci ñi nga jur, kenn koo dese"},{"style":"q1","verse":"","text":"ku lay wommat."},{"style":"q1","verse":"","text":"Amul ci doom yi nga yar, kenn"},{"style":"q1","verse":"","text":"ku lay jàpp ci loxo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yii yaar de dikkal na la:"},{"style":"q1","verse":"","text":"yàquleek sànkule."},{"style":"q1","verse":"","text":"Ana kuy bokk ak yaw saw naqar?"},{"style":"q1","verse":"","text":"Xiif ak loraangey saamar!"},{"style":"q1","verse":"","text":"Ana nu ma lay dëfale?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Sa doom yi ne lasar,"},{"style":"q1","verse":"","text":"mbete kooba yu fiir jàpp,"},{"style":"q1","verse":"","text":"ñu ne lareet fépp fu mbedd daje,"},{"style":"q1","verse":"","text":"këppoo sànjum Aji Sax ji,"},{"style":"q1","verse":"","text":"sa Yàlla ji leen rëbb."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Rikk déglul nag lii, yaw mi musiba dal,"},{"style":"q1","verse":"","text":"nga màndi te du biiñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Sa boroom Aji Sax ji,"},{"style":"q1","verse":"","text":"sa Yàlla jiy layool ñoñam"},{"style":"q1","verse":"","text":"dafa wax ne: «Mu ngoog, maa jële ci sa loxo,"},{"style":"q1","verse":"","text":"kaas bu wex biy miirloo;"},{"style":"q1","verse":"","text":"taatu kaas ba def sama mbugal,"},{"style":"q1","verse":"","text":"dootoo ko naan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Maa koy teg ci loxol ñi la doon naqaral,"},{"style":"q1","verse":"","text":"te naan la: “Sëggal, nu joggi, jàll,”"},{"style":"q1","verse":"","text":"nga def sa gannaaw gi aw yoon,"},{"style":"q1","verse":"","text":"ñu joggi, wéy.»"}]