[{"style":"s1","verse":"","text":"Bildàdd buurati na"},{"style":"p","verse":"","text":""},{"style":"v","verse":"1","text":"Bildàdd mu Suwa àddu, ne:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Xanaa dungeen noppi mukk?"},{"style":"q1","verse":"","text":"Àndleen ak xel, nu wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ndax ay mala lañu nu teg daal,"},{"style":"q1","verse":"","text":"def nu ay naataxóona?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yaw nag yaa topp sa mer ba loru!"},{"style":"q1","verse":"","text":"Ayóobaa, àddina day gental ngir yaw a?"},{"style":"q1","verse":"","text":"Am aw doj ay toxal boppam ngir yaw?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ku bon kay lag leeram di fey,"},{"style":"q1","verse":"","text":"te taalam du jolli."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Xaymaam day lëndëm,"},{"style":"q1","verse":"","text":"làmpam ba ko tiim ne kamaj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Bu daa waaxu démb, di temp tey,"},{"style":"q1","verse":"","text":"ay mébétam di ko mbas."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Moo dox ba tàbbi cib caax,"},{"style":"q1","verse":"","text":"wëndeeloo biir mbaal,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"tànk ba téqtaloog fiir,"},{"style":"q1","verse":"","text":"ab racctal ne ko ràtt."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Buum ga koy lonk a nga suulu,"},{"style":"q1","verse":"","text":"am yeer di ko xaare aw ñall."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Lu ragloo koy darey mbetteel,"},{"style":"q1","verse":"","text":"ne ko dann."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Mooy xiif ba doole réer ko,"},{"style":"q1","verse":"","text":"musiba taxaw, xejjoo ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Jàngoro di ko ŋacc,"},{"style":"q1","verse":"","text":"woppi dee di lekk cér ya."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku bon lees di ñoddee fa jàmmi xaymaam,"},{"style":"q1","verse":"","text":"diri ko ba fa kanam buuru musiba yi,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"manees naa dëkke xaymaam, moomatu ko,"},{"style":"q1","verse":"","text":"tamarax lees fay suy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Ku bon mooy garab gu dee kaw ak suuf,"},{"style":"q1","verse":"","text":"reen wow, xob lax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Turam tukkee kaw suuf,"},{"style":"q1","verse":"","text":"ba deesatu ko tuddem pénc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Dees koy bëmëxeg leer, tàbbal lëndëm,"},{"style":"q1","verse":"","text":"dàqe ko àddina."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Du doom, du askan wu muy waccey bokkam,"},{"style":"q1","verse":"","text":"te kenn du desey dalam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Waa sowu jommi cig mujam,"},{"style":"q1","verse":"","text":"waa penku di rasu naa:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"«Dalub kàccoor déy, lee di mujam,"},{"style":"q1","verse":"","text":"ku faalewul Yàlla, nii la màkkaanam!»"}]