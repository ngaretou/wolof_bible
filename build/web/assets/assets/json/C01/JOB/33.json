[{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Léegi Baay Ayóoba, ngalla déglul samay wax"},{"style":"q1","verse":"","text":"te teewlu sama kàddu yépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Xaaral, ma àddu boog,"},{"style":"q1","verse":"","text":"ba sama làmmiñ tudd li may bëgga wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Xel mu dëggu laay waxe,"},{"style":"q1","verse":"","text":"samay kàddu di xam-xam bu wóor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ngelawal Yàllaa ma sos,"},{"style":"q1","verse":"","text":"noowal Aji Man ji daal a ma may bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Soo manee, tontu ma;"},{"style":"q1","verse":"","text":"defarul, janook man."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Maa ngii, yem ak yaw fa Yàlla;"},{"style":"q1","verse":"","text":"ci ban lañu ma mooñe man itam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Du sama ñàngaay bu lay tiital,"},{"style":"q1","verse":"","text":"du sama daraja ju la diis."},{"style":"s1","verse":"","text":"Yàlla du moroom"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yaa wax déy, ma dégg,"},{"style":"q1","verse":"","text":"te dégg naa bu baax say kàddu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Nga ne: «Set laa def, tooñuma;"},{"style":"q1","verse":"","text":"mucc ayib laa def, ñaawuma fenn."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Waaye Yàllaa ngii di ma wuta taqal,"},{"style":"q1","verse":"","text":"def ma ab noonam,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"jéng ma,"},{"style":"q1","verse":"","text":"di xool fépp fu ma jaare.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Foofu déy, moy nga dëgg, ma wax la ko,"},{"style":"q1","verse":"","text":"Yàllaa sut doom aadama."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ana loo koy sikke,"},{"style":"q1","verse":"","text":"naan du layoo lenn ci mboolemi jëfam?"},{"style":"s1","verse":"","text":"Yàlla dina wax ciy gént"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Yàlla déy, dees koo teewluwul,"},{"style":"q1","verse":"","text":"waaye day wax benn yoon ba ñaar"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"ci biir peeñum géntu guddi,"},{"style":"q1","verse":"","text":"fa nelaw dabe nit,"},{"style":"q1","verse":"","text":"ba mu yàndoore kaw lalam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Mu ne ko rukk ndéey,"},{"style":"q1","verse":"","text":"boole kook màndarga, artoo ko ko,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"ngir dëdduloo doom aadama ji ay jëfam,"},{"style":"q1","verse":"","text":"musal jàmbaar cig réy,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"sorale bakkanam pax,"},{"style":"q1","verse":"","text":"ba ab xeej du ko bóom\\f + \\fr 33.18 \\ft Manees na koo jànge nii itam: \\fqa ba du jàllagum dexu dee.\\f*."},{"style":"s1","verse":"","text":"Eliyu nee: Mitit ndéey la"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Nit dina mbugaloo mitit wu ko tëral,"},{"style":"q1","verse":"","text":"muy kat-kati rekk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Day bañ dugub,"},{"style":"q1","verse":"","text":"seexlu ñam wu ko gënoona neex,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"jeex tàkk bay réer aka feeñ,"},{"style":"q1","verse":"","text":"yaxam ya ne fàŋŋ te sàngu woon,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"dundam dëgmal dee,"},{"style":"q1","verse":"","text":"bakkanam di jegeji pax."},{"style":"s1","verse":"","text":"Eliyu nee: Njotlaay du ñàkk"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Waaye su nit ki amee malaakam àttekat,"},{"style":"q1","verse":"","text":"nay kenn sax ci junniy malaaka,"},{"style":"q1","verse":"","text":"ngir xamal nit ki li ko war,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"da koy baaxe, daldi ne Yàlla:"},{"style":"q1","verse":"","text":"«Musal ko ba du wàccagum biir pax."},{"style":"q1","verse":"","text":"Maa ngeek njotlaayam.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Yaram wa jotaat tooyaayu ndaw,"},{"style":"q1","verse":"","text":"mu amaat dooley ngoneem;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"bu ñaanee Yàlla, mu nangul ko,"},{"style":"q1","verse":"","text":"geesu ko, mu sarxolle,"},{"style":"q1","verse":"","text":"nde Yàllaay fey nit njekkam,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"nit ki seedeeli ko ca nit ña, ne:"},{"style":"q1","verse":"","text":"«Maa bàkkaar, maa xëtt njub,"},{"style":"q1","verse":"","text":"te feyeesu ma ko sax!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Yàllaa ma jot ba jàlluma, jëm pax,"},{"style":"q1","verse":"","text":"xanaa dellu di gisug leer.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Lii lépp déy, la Yàlla di def,"},{"style":"q1","verse":"","text":"mook nit ñaari yoon ba ñett,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"ngir sorale bakkanam pax,"},{"style":"q1","verse":"","text":"bàyyee ko leeraangey àddina."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"Baay Ayóoba, teewlul, déglu ma,"},{"style":"q1","verse":"","text":"neel cell, ma wax la, man."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Soo amee lenn loo wax, àddul;"},{"style":"q1","verse":"","text":"waxal, ngir bëgg naa laa dëggal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Lu ko moy nag, yaa may déglu,"},{"style":"q1","verse":"","text":"ne cell, ma jàngal la xam-xam."}]