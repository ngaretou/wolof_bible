[{"style":"s1","verse":"","text":"Buur Yàllaay walloo"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Màggalleen Ki Sax!"},{"style":"q1","verse":"","text":"Yeen ñiy jaamu Aji Sax ji, màggalleen ko,"},{"style":"q1","verse":"","text":"màggalleen turu Aji Sax ji!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yal nañu teral turu Aji Sax ji"},{"style":"q1","verse":"","text":"tey ak ëllëg!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Li dale penku ba sowu,"},{"style":"q1","verse":"","text":"màggalleen Aji Sax ji."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Aji Sax jee kawe, tiim xeetoo xeet,"},{"style":"q1","verse":"","text":"darajaam sut asamaan!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ana ku mel ni sunu Yàlla Aji Sax ji,"},{"style":"q1","verse":"","text":"ki kaweel ab jalam, toog,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"di jéer asamaan ak suuf?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Mooy yékkatee ku néewle fa suuf,"},{"style":"q1","verse":"","text":"di seppee ku sonn cib sën,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"bokkal ko jataayu kilifa"},{"style":"q2","verse":"","text":"ya jiite xeetam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Mooy dëël jigéen ju tële woona jur,"},{"style":"q1","verse":"","text":"may ko mbégtem njureel."},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]