[{"style":"s1","verse":"","text":"Bu sa xol jóg"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Bu sa xol jóg ci ku bon,"},{"style":"q1","verse":"","text":"bul ñee kuy def njombe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Léegi mu wow nim ñax,"},{"style":"q1","verse":"","text":"lax ni gàncax."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Dénkul ci Aji Sax ji, di def lu baax,"},{"style":"q1","verse":"","text":"ba des ci réew meek jàmm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Deel bànneexoo Aji Sax ji,"},{"style":"q1","verse":"","text":"mu may la say nammeel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Jébbalul ci Aji Sax ji,"},{"style":"q1","verse":"","text":"dénku ci moom, mooy sottal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Day setal sab der ni jant bu fenk;"},{"style":"q1","verse":"","text":"sab àtte ne ràññ ni njolloor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Neel tekk, xaar Aji Sax ji,"},{"style":"q1","verse":"","text":"yaakaar ko."},{"style":"q1","verse":"","text":"Bu sa xol jóg ci kuy baaxle"},{"style":"q1","verse":"","text":"tey lal ay pexe."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Baal mer, dëddu xadar."},{"style":"q1","verse":"","text":"Xol bu jóg, loraangey neen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Ku bon, ñu dagg, sànni;"},{"style":"q1","verse":"","text":"ku yaakaar Aji Sax ji, jagoo réew mi."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Nes tuut, ku bon ne mes,"},{"style":"q1","verse":"","text":"nga seet, seet, mu réer."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Waaye néew-dooleey jagoo réew mi;"},{"style":"q1","verse":"","text":"jàmm ne ñoyy, ñu bége."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ku bon fexeel ku jub,"},{"style":"q1","verse":"","text":"di ko màttu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Buur Yàlla di ko ree,"},{"style":"q1","verse":"","text":"xam ne bésam a ngi ñëw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Ku bon bocci saamar,"},{"style":"q1","verse":"","text":"tàwwig fitt,"},{"style":"q1","verse":"","text":"nara rey ku ñàkk, néewle,"},{"style":"q1","verse":"","text":"nara bóom kuy jubal;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"far jam boppam saamaru xol,"},{"style":"q1","verse":"","text":"fittam damm."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Aji jub, as tuutam"},{"style":"q1","verse":"","text":"moo gën koomu ku bon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ku bon kat, sa doole dëddu la;"},{"style":"q1","verse":"","text":"ku jub, Aji Sax ji wallu la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ku màndu, Aji Sax ji yég la,"},{"style":"q1","verse":"","text":"sab cér sax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Bu mettee doo rus,"},{"style":"q1","verse":"","text":"te bésub xiif dangay regg."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Ku bon sànku;"},{"style":"q1","verse":"","text":"noonub Aji Sax ji, taaru tool bu naat la,"},{"style":"q1","verse":"","text":"bu tàkkee, jeex tàkk."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ku bon leb, du fey;"},{"style":"q1","verse":"","text":"ku jub yéwén, di joxe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Aji Sax ji barkeel la, nga jagoo réew mi moos;"},{"style":"q1","verse":"","text":"mu alag la, nga dog."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Bu Aji Sax ji rafetloo saw yoon,"},{"style":"q1","verse":"","text":"mooy jiite say jéego."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Soo tërëfee it, doo daanu."},{"style":"q1","verse":"","text":"Aji Sax jee lay walloo loxoom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Ba may ndaw, ba tey may mag,"},{"style":"q1","verse":"","text":"gisuma ku jub ñàkk ndimbal,"},{"style":"q1","verse":"","text":"mbaa askanam di dunde yalwaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Day saxoo tabe, di leble,"},{"style":"q1","verse":"","text":"askanam barkeel."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Dëddul mbon, di jëfe mbaax,"},{"style":"q1","verse":"","text":"ba dëkke réew mi fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Aji Sax ji kat dafa jiital yoon,"},{"style":"q1","verse":"","text":"te du wacc jaamam bu gore."},{"style":"q1","verse":"","text":"Day fegu ba fàww;"},{"style":"q1","verse":"","text":"askanu ku bon dog."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"29","text":"Aji jub ay jagoo réew mi,"},{"style":"q1","verse":"","text":"dëkke ko ba fàww."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"30","text":"Kàddug aji jub day xelu,"},{"style":"q1","verse":"","text":"wax ja jub;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"31","text":"yoonu Yàllaam a nga ca xol ba,"},{"style":"q1","verse":"","text":"du dox ba tërëf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"32","text":"Ku bon a ngi yeeru ku jub,"},{"style":"q1","verse":"","text":"di ko wuta bóom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"33","text":"Waaye Aji Sax ji du ko bàyyeek moom,"},{"style":"q1","verse":"","text":"te deesu ko daan cib layoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"34","text":"Yaakaaral Aji Sax ji, sàmm yoonam,"},{"style":"q1","verse":"","text":"mu teral la, nga jagoo réew mi,"},{"style":"q1","verse":"","text":"ku bon sànku, ngay gis."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"35","text":"Damaa gis ku bon di noote,"},{"style":"q1","verse":"","text":"te sëq ni garab gu naat fa mu fekk baax,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"36","text":"ma jaareeti fa, ndeke wuute na,"},{"style":"q1","verse":"","text":"ma seet ko, gisuma ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"37","text":"Bàyyil xel ku màndu, xool ku jub;"},{"style":"q1","verse":"","text":"muj, fa nitu jàmm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"38","text":"Moykat yeey sànkoondoo,"},{"style":"q1","verse":"","text":"ku bon amul muj."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"39","text":"Aji Sax jeey musal ku jub,"},{"style":"q1","verse":"","text":"di ko aar bésub njàqare."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"40","text":"Aji Sax jee koy taxawu, wallu ko,"},{"style":"q1","verse":"","text":"wallu ko ci ku bon, musal ko,"},{"style":"q1","verse":"","text":"ndax li mu ko làqoo."}]