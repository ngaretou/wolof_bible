[{"style":"s1","verse":"","text":"Aji Sax ji waccu ma"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Ma sante la sama léppi xol,"},{"style":"q1","verse":"","text":"woy la, gàngooru Yàlla ga ca kaw seede."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ma sujjóot jublook sa kër gu sell gi,"},{"style":"q1","verse":"","text":"di wormaal saw tur,"},{"style":"q1","verse":"","text":"ngir sa ngor ak sa worma."},{"style":"q1","verse":"","text":"Màggal nga sa tur ak sa kàddu,"},{"style":"q1","verse":"","text":"ba mu tiim lépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bés ba ma wootee, yaa ma wuyu,"},{"style":"q1","verse":"","text":"ñemeel ma, dooleel ma."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Aji Sax ji, na la buuri àddina yépp màggal,"},{"style":"q1","verse":"","text":"ñoo dégg kàddu yi nga wax."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Nañu woy say jaloore, naan:"},{"style":"q1","verse":"","text":"«Aji Sax jeeka màgg ndam!»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax jee kawe!"},{"style":"q1","verse":"","text":"Ku toroxlu, mu niir;"},{"style":"q1","verse":"","text":"ku réy, mu ñooru."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Ma wéye njàqare,"},{"style":"q1","verse":"","text":"nga musal ma,"},{"style":"q1","verse":"","text":"dogalee sa loxo merum noon,"},{"style":"q1","verse":"","text":"walloo ma sa ndijoor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Aji Sax jee may feyul."},{"style":"q1","verse":"","text":"Éy Aji Sax ji, yaa saxoo ngor!"},{"style":"q1","verse":"","text":"Bul wacc ñi nga binde sa loxo."}]