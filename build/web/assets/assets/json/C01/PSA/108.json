[{"style":"s1","verse":"","text":"Yàlla, may nu ndam"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Di woyu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yàlla, dogu naa,"},{"style":"q1","verse":"","text":"damay woy, di la kañ;"},{"style":"q1","verse":"","text":"sama jëmm jépp di la woy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Na xalam ak moroom ma yewwu,"},{"style":"q1","verse":"","text":"ma yee leeru njël."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Aji Sax ji, dama lay gërëm ci biir xeet yi,"},{"style":"q1","verse":"","text":"kañ la ci biir waaso yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sa ngor a màgg ba sut asamaan,"},{"style":"q1","verse":"","text":"sa worma àkki fa kaw-a-kaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Éy Yàlla, yaa màgg ba sut asamaan,"},{"style":"q1","verse":"","text":"sag leer tiim suuf sépp."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Walloo nu sa ndijoor,"},{"style":"q1","verse":"","text":"nangul ma,"},{"style":"q1","verse":"","text":"ba say soppe xettaliku."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yàllaa àddoo fa këram gu sell,"},{"style":"q1","verse":"","text":"ne: «Maay damu, dogat suufas Sikem,"},{"style":"q1","verse":"","text":"séddale xuru Sukkót."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Maa moom diiwaanu Galàdd, moom Manase,"},{"style":"q1","verse":"","text":"Efrayim di sama mbaxanam xare,"},{"style":"q1","verse":"","text":"Yuda di sama yetu nguur,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"réewum Mowab di sama ndabal raxasukaay,"},{"style":"q1","verse":"","text":"Edom, ma teg tànk;"},{"style":"q1","verse":"","text":"Filisti, ma xaacoo fa.»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Éy Yàlla, ana ku may yóbbu dëkk ba tata wër?"},{"style":"q1","verse":"","text":"Ku may jiite ba Edom?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yàlla, xanaa du yaa nu wacc?"},{"style":"q1","verse":"","text":"Éy Yàlla, yaa baña ànd ak sunu mboolooy xare?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ngalla dimbali nu ci bañaale;"},{"style":"q1","verse":"","text":"doom aadama, wallam neen la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Nook Yàlla, nu def njàmbaar;"},{"style":"q1","verse":"","text":"moom mooy noot sunuy noon."}]