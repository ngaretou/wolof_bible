[{"style":"s1","verse":"","text":"Ma sant Aji Sax ji"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Naa sant Aji Sax ji ci saa xol bii,"},{"style":"q1","verse":"","text":"saa jëmm jépp di sàbbaal sellaayu turam!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Naa sant Aji Sax ji ci saa xol bii,"},{"style":"q1","verse":"","text":"te baña fàtte mboolem xéewalam!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Moo lay baal foo ñaawe,"},{"style":"q1","verse":"","text":"di la wéral foo woppe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Mooy jot sa bakkan,"},{"style":"q1","verse":"","text":"kaalaa la ngor ak yërmande,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"di reggal sa bakkan bànneex,"},{"style":"q1","verse":"","text":"yeesalal lag ndaw,"},{"style":"q1","verse":"","text":"nga màggataale doole nig jaxaay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax jeey àtte njub,"},{"style":"q1","verse":"","text":"di àtte dëgg jépp néew-ji-doole."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Moo xamal Musaa ay nammeelam,"},{"style":"q1","verse":"","text":"xamal \\w bànni Israayil|bànni Israayil:\\w* ay jëfam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yiw ak yërmande, fa Aji Sax ji."},{"style":"q1","verse":"","text":"Moo muñ mer te bare ngor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Saxoowul di toppe,"},{"style":"q1","verse":"","text":"dencul mer ba fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Du sunuy bàkkaar la nu mbugal,"},{"style":"q1","verse":"","text":"du sunuy ñaawtéef la nu fey."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ni asamaan tiime suuf,"},{"style":"q1","verse":"","text":"ni la ngoram yiire ku ko ragal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ni penku soree sowu,"},{"style":"q1","verse":"","text":"ni la nu soreleek sunu mbugali tooñ."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ni baay yërëme doom,"},{"style":"q1","verse":"","text":"ni la Aji Sax ji yërëme ku ko ragal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Moo xam sunu bind,"},{"style":"q1","verse":"","text":"di bàyyi xel ne pënd lanu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Nit aki fanam di saxyaay"},{"style":"q2","verse":"","text":"suy law ni tóor-tóoru àll;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"ngelaw li wal, mu ne mes,"},{"style":"q1","verse":"","text":"mel ni masu faa nekk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Waaye ngorug Aji Sax ji naka jekk"},{"style":"q1","verse":"","text":"ñeel na fàww ku ko ragal;"},{"style":"q1","verse":"","text":"àtteb dëggam ñeel sëtaati"},{"style":"q2","verse":"","text":""},{"style":"v","verse":"18","text":"kuy sàmm \\w kóllëreem|kóllëre g-:\\w*,"},{"style":"q2","verse":"","text":"di saxoo jëfe tegtali yoonam."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Aji Sax jee samp asamaan ab jalam,"},{"style":"q1","verse":"","text":"nguuram ŋank lépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Yeen malaaka yi, santleen Aji Sax ji,"},{"style":"q1","verse":"","text":"yeen jàmbaar yu mag yiy jëfe ndigalam,"},{"style":"q1","verse":"","text":"di sàmm kàddoom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Santleen Aji Sax ji, yeen gàngoori xareem yépp,"},{"style":"q1","verse":"","text":"yeen dagam yiy jëfe coobareem."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Yeen mbindeef yépp, nangeen sante Aji Sax ji"},{"style":"q1","verse":"","text":"fépp fu mu tiim."},{"style":"q1","verse":"","text":"Naa sant Aji Sax ji ci saa xol bii!"}]