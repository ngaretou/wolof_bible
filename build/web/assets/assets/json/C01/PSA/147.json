[{"style":"s1","verse":"","text":"Yàllaa gën ci àddina, gën ci nit"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Màggalleen Ki Sax!"},{"style":"q1","verse":"","text":"Kañ sunu Yàllaa baax,"},{"style":"q1","verse":"","text":"màggal koo neex te war."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Aji Sax jaa ngi tabaxaat Yerusalem,"},{"style":"q1","verse":"","text":"di delloosi ña ñu dàqe Israayil,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"di faj naqaru xol,"},{"style":"q1","verse":"","text":"fajtal góomi boroom."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Mooy lim biddiiw yi,"},{"style":"q1","verse":"","text":"tudd bu ci nekk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Sunu Boroom a màgg, bare doole;"},{"style":"q1","verse":"","text":"am xelam wees kemu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Aji Sax ji, néew-ji-doole, mu siggil;"},{"style":"q1","verse":"","text":"ku bon, mu daane ci suuf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Woyeleen Aji Sax ji kàddug cant,"},{"style":"q1","verse":"","text":"kañeleen sunu Yàlla jii ay xalam;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"mooy sànge asamaan ay niir,"},{"style":"q1","verse":"","text":"tawal suuf,"},{"style":"q1","verse":"","text":"saxal tund yi am ñax,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"leel mala,"},{"style":"q1","verse":"","text":"xol liiru baaxoñ bu jooy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Kii nawul dooley fas,"},{"style":"q1","verse":"","text":"gërëmul kàttanu jàmbaar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ki Aji Sax ji gërëm mooy ki ko ragal"},{"style":"q1","verse":"","text":"te di ko yaakaare ngoram."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yerusalem, sargalleen Aji Sax ji!"},{"style":"q1","verse":"","text":"yeen waa \\w Siyoŋ|Siyoŋ:\\w* laa ne, màggalleen seen Yàlla!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Mooy tëj ràpp seen bunti dëkk,"},{"style":"q1","verse":"","text":"barkeel seen askan wa ca biir,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"jàmmal seen suufas réew,"},{"style":"q1","verse":"","text":"reggal leen ngëneelu pepp,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"yónnee suuf ndigalam,"},{"style":"q1","verse":"","text":"kàddoom ne coww, daw ca."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Mooy taw donji yuur yu weexe ni wëttéen."},{"style":"q1","verse":"","text":"di lay lay bu sedd, mel ni pënd ci suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Mooy yuri tawub yuuram niy doj."},{"style":"q1","verse":"","text":"Bu ni seddee, ana kuy taxaw?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Mu yebal kàddoom, seeyal;"},{"style":"q1","verse":"","text":"wal ngelawam, ndox walalaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Mooy wax \\w Yanqóoba|Yanqóoba:\\w* kàddoom,"},{"style":"q1","verse":"","text":"dogalam yaak ndigali yoonam ñeel Israayil."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Loolu defalu ko wenn xeet;"},{"style":"q1","verse":"","text":"kenn xamu ci ndigali yoonam."},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]