[{"style":"s1","verse":"","text":"Yàlla, gaawe ma"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, ñeel Daawuda, di woy wuy fàttalee."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yàlla, wallu ma!"},{"style":"q1","verse":"","text":"Éy Aji Sax ji, gaawe ma!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Kuy wut sama bakkan,"},{"style":"q1","verse":"","text":"yal na rus, torox ne tott."},{"style":"q1","verse":"","text":"Kuy bége sama loraange,"},{"style":"q1","verse":"","text":"yal nañu ko duma, waññi ko, mu ne yàcc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Kuy ñaawalaate,"},{"style":"q1","verse":"","text":"yal nañu ko toroxal, mu daw, dellu gannaaw."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Képp ku la wër,"},{"style":"q1","verse":"","text":"yal na bég, di la bànneexoo."},{"style":"q1","verse":"","text":"Képp ku safoo sag wall,"},{"style":"q1","verse":"","text":"yal na jàppoo: «Yàlla màgg na!»"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Man de, néew naa doole, ñàkk naa."},{"style":"q1","verse":"","text":"Éy Yàlla, gaawe ma."},{"style":"q1","verse":"","text":"Yaw yaa may wallu, xettali ma."},{"style":"q1","verse":"","text":"Éy Aji Sax ji, bu ma yeexe."}]