[{"style":"s1","verse":"","text":"Maa wopp, tuub, di jooy"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Dib taalifu cant buy fàttalee, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Éy Aji Sax ji, bul mer, di ma rëbb;"},{"style":"q1","verse":"","text":"bul xadaru, di ma mbugal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aji Sax ji, fitt nga maa fitt,"},{"style":"q1","verse":"","text":"dóor nga ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Danga maa mere, ma wéradi;"},{"style":"q1","verse":"","text":"maa bàkkaar, ba jagadi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Samay ñaawtéef mëdd ma,"},{"style":"q1","verse":"","text":"diisa diis, ba àttanuma ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Maa defug ndof,"},{"style":"q1","verse":"","text":"ba am dana yu bédd, ba xasaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Maa ngi sëgg, ne yàcc, ne yasar,"},{"style":"q1","verse":"","text":"di wéye tiis,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"yaram tàng, di bax,"},{"style":"q1","verse":"","text":"ma wéradi, ne làcc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Sama yaram dee goyy, ma ne lasar,"},{"style":"q1","verse":"","text":"jàq, di binni."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Boroom bi, yaa ngi ne jàkk ci samay nammeel,"},{"style":"q1","verse":"","text":"samay onk umpu la."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Sama fit a ngi tëf-tëfi, doole ŋiis,"},{"style":"q1","verse":"","text":"gët giim."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Samay dana tax xarit ak dëkkandoo di ma daw,"},{"style":"q1","verse":"","text":"mbokk dànd ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Ñi may wuta rey a ngi may fiir,"},{"style":"q1","verse":"","text":"di ma jéema lor, di ma waxal musiba,"},{"style":"q1","verse":"","text":"yendu maa lalal pexe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Man, ma mel nib tëx, dégguma,"},{"style":"q1","verse":"","text":"mel ni luu, àdduwuma,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"di saf ku dul dégg,"},{"style":"q1","verse":"","text":"bay layoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Aji Sax ji, yaw laa yaakaar;"},{"style":"q1","verse":"","text":"Boroom bi, sama Yàlla, yaa may nangul."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ma ne, ngalla buñu ma ree,"},{"style":"q1","verse":"","text":"di damoo samab jéll."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Maa ngii di waaja daanu,"},{"style":"q1","verse":"","text":"dëkke mitit."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Ma ne, aylayéwén def naa lu ñaaw,"},{"style":"q1","verse":"","text":"sama bàkkaar tiital na ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Noon yaa ngi ne jonn ak seen doole,"},{"style":"q1","verse":"","text":"bare na ñu ma bañ ci neen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Ma ji leen njekk, ñu fey ma njekkar;"},{"style":"q1","verse":"","text":"may sàkku lu baax, ñu di ma sikk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Éy Aji Sax ji sama Yàlla, bu ma wacc,"},{"style":"q1","verse":"","text":"bu ma sore."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Ngalla gaawe ma;"},{"style":"q1","verse":"","text":"Boroom bi, yaa may musal."}]