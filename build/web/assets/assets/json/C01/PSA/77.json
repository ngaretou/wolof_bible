[{"style":"s1","verse":"","text":"Yàllaa ngi seetaan"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci Yedutun, njiital woykat yi, dib taalifu cant, ñeel Asaf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Naa xaacu, woo Yàlla,"},{"style":"q1","verse":"","text":"sama baat fa Yàlla, ngir mu déglu ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bëccëg, ma jàq, sàkku ndimbalal Boroom bi;"},{"style":"q1","verse":"","text":"guddi, ma tàllal loxo, toqiwuma."},{"style":"q1","verse":"","text":"Sama xol tëë sedd."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ma bàyyi Yàlla xel, binni,"},{"style":"q1","verse":"","text":"xalaat, xol jeex. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Éy Yàlla, tere nga maa gëmm bët,"},{"style":"q1","verse":"","text":"ma jaaxle ba waxatuma,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"xanaa xalaat janti démb,"},{"style":"q1","verse":"","text":"at ya woon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Guddi may fàttaliku sama woy,"},{"style":"q1","verse":"","text":"di waxtaan ak sama ngegenaay,"},{"style":"q1","verse":"","text":"sama xel di jéex te naa:"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"«Boroom bi da may gàntal ba fàww,"},{"style":"q1","verse":"","text":"ba du ma baaxeeti mukk a?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Xanaa daa jeexal ngor tàkk?"},{"style":"q1","verse":"","text":"Am kàddoom a deñ ba fàww?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Moo, mbaa du Yàlla daa fàtte ñeewante?"},{"style":"q1","verse":"","text":"Am meram a tëj buntu yërmandeem?» \\qs Selaw.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Ma ne, sama naqar daal"},{"style":"q1","verse":"","text":"mooy Aji Kawe ji daa soppi loxoom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Waaye naa fàttaliku jaloorey Ki Sax rekk,"},{"style":"q1","verse":"","text":"fàttaliku kéemaanam ya woon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Naa xalaat sa bépp liggéey,"},{"style":"q1","verse":"","text":"di jàngat say jëf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Céy Yàlla, yaa \\w sell|sell:\\w* jikko!"},{"style":"q1","verse":"","text":"Jan yàllaa màgg ni Yàlla?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Yaa di Yàlla juy jëfi kéemaan,"},{"style":"q1","verse":"","text":"yaa siiwal sa doole ci biir xeet yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Kàttan nga jote sa mbooloo ma,"},{"style":"q1","verse":"","text":"sëti \\w Yanqóobaak|Yanqóoba:\\w* Yuusufa. \\qs Selaw.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Yàlla, ndox maa la gis,"},{"style":"q1","verse":"","text":"ndox maa la gis, di lox;"},{"style":"q1","verse":"","text":"xóote yaa say,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"niir ya yuri,"},{"style":"q1","verse":"","text":"kàddu riire asamaan,"},{"style":"q1","verse":"","text":"say fitt, fettaxandoo,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"nga dënoo biir callweer,"},{"style":"q1","verse":"","text":"melax yi taal àddina,"},{"style":"q1","verse":"","text":"suuf di lox, di reg-regi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Biir géej di saw yoon;"},{"style":"q1","verse":"","text":"saw xàll ci digg ndox mu ne xéew,"},{"style":"q1","verse":"","text":"te kenn gisul foo teg tànk!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Wommat nga sa mbooloo ni ag jur,"},{"style":"q1","verse":"","text":"Musaak \\w Aaróona|Aaróona:\\w* jiite."}]