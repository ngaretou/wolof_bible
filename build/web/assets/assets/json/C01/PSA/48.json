[{"style":"s1","verse":"","text":"Aji Sax ji aar na Siyoŋ"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Di woyu cant, ñeel askanu Kore wu góor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Aji Sax jee màgg, jara màggal a màggal"},{"style":"q1","verse":"","text":"ca sunu biir dëkku Yàlla, kaw tundam wu sell."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Jekk taxawaayee tundu \\w Siyoŋ|Siyoŋ:\\w*, catu bëj-gànnaar,"},{"style":"q1","verse":"","text":"dëkku Buur bu mag bi,"},{"style":"q1","verse":"","text":"di mbégtem àddina sépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yàllaa wone kaaraangeem"},{"style":"q1","verse":"","text":"ca biir tatay dëkk ba."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Buur yaa ngoog daje,"},{"style":"q1","verse":"","text":"àndandoo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Nun noo gis, jommi,"},{"style":"q1","verse":"","text":"tiit, fëx,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"ne fay pat-pati,"},{"style":"q1","verse":"","text":"jàq ni kuy matu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Ngelawal penku, moom ngay tase"},{"style":"q1","verse":"","text":"gaal gu mag."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"La nu déggoon de lanu gis"},{"style":"q1","verse":"","text":"ca dëkkub Aji Sax ji Boroom gàngoor yi,"},{"style":"q1","verse":"","text":"sunu dëkku Yàlla ba."},{"style":"q1","verse":"","text":"Yàllaa koy dëgëral ba fàww. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Céy Yàlla, nu ngi sa biir kër,"},{"style":"q1","verse":"","text":"di xalaat sa ngor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Céy Yàlla, sa tur ay sa woy"},{"style":"q1","verse":"","text":"ba ca cati àddina."},{"style":"q1","verse":"","text":"Njubte la sa loxo fees."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yal na waa tundu Siyoŋ bég,"},{"style":"q1","verse":"","text":"waa Yuda bànneexoo say àtte."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Doxleen biir Siyoŋ, wër ko,"},{"style":"q1","verse":"","text":"waññleen ay sooroori tataam,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"nemmikuy tataam,"},{"style":"q1","verse":"","text":"doxantoo këri buur,"},{"style":"q1","verse":"","text":"ba man koo yegge maasug ëllëg,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"ne leen kii ay Yàlla, sunu Yàlla,"},{"style":"q1","verse":"","text":"tey ak ëllëg."},{"style":"q1","verse":"","text":"Moo nuy jiite ba saa taxaw."}]