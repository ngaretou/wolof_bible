[{"style":"s1","verse":"","text":"Naa siggi, noon sëgg"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Aji Sax ji, yaw laa làqoo,"},{"style":"q1","verse":"","text":"yàlla buma rus mukk."},{"style":"q1","verse":"","text":"Yiwee ma sa àtteb dëgg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Teewlu ma,"},{"style":"q1","verse":"","text":"gaawe ma,"},{"style":"q1","verse":"","text":"Yal nangay sama cëslaay, sama rawtu."},{"style":"q1","verse":"","text":"Aar ma, ma mucc."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Yaay sama cëslaay, sama rawtu."},{"style":"q1","verse":"","text":"Jiite ma, wommat ma ngir sa tur."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Génne ma fiir gi ñu may tëroo;"},{"style":"q1","verse":"","text":"yaw yaa di sama rawtu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yaw laa jébbal samag noo."},{"style":"q1","verse":"","text":"Aji Sax ji, Yàlla miy dëgg, ngalla jot ma."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Maa bañ kuy topp tuuri neen."},{"style":"q1","verse":"","text":"Man, Aji Sax ji laa wóolu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Damay bég, di bànneexoo sa ngor,"},{"style":"q1","verse":"","text":"li nga xoole samaw tiis."},{"style":"q1","verse":"","text":"Ràññee nga sama njàqarey xol."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Waccoo ma ci loxol noon,"},{"style":"q1","verse":"","text":"danga maa taxawal fu ma yaatoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Éy Aji Sax ji, baaxe ma, jàq naa!"},{"style":"q1","verse":"","text":"Samay gët a ngi giim ndaxu tiis,"},{"style":"q1","verse":"","text":"xol jeex, yaram jeex."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Naqar a ngi may rey,"},{"style":"q1","verse":"","text":"tiis di wàññi samay fan,"},{"style":"q1","verse":"","text":"sama doole ŋiis ndax sama njubadi,"},{"style":"q1","verse":"","text":"samay yax semm."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Noonoo noon a ma sewal,"},{"style":"q1","verse":"","text":"dëkkandoo waxi noppi,"},{"style":"q1","verse":"","text":"xame seexlu ma,"},{"style":"q1","verse":"","text":"ku ma gis biti, di ma daw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Kenn xalaatu ma, xanaa fàtte ma ni ku dee,"},{"style":"q1","verse":"","text":"ma mel ni ndab lu yàqu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Damaa dégg jëw yu bare,"},{"style":"q1","verse":"","text":"lu raglu dar ma kepp."},{"style":"q1","verse":"","text":"Ñu ngi mànkoo, di ma fexeel,"},{"style":"q1","verse":"","text":"di wut sama bakkan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Man nag Aji Sax ji, ci yaw laa dénku."},{"style":"q1","verse":"","text":"Ma ne: «Yaay sama Yàlla.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Yaa yor sama àpp."},{"style":"q1","verse":"","text":"Xettali maak ku ma noonoo"},{"style":"q1","verse":"","text":"ak ku may sonal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Leeralal ma sa kanam, Sang bi,"},{"style":"q1","verse":"","text":"walloo ma sa ngor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Éy Aji Sax ji, woo naa la wall,"},{"style":"q1","verse":"","text":"yàlla buma rus."},{"style":"q1","verse":"","text":"Yal na ku bon rus,"},{"style":"q1","verse":"","text":"ne xerem biir njaniiw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yal nanga tëj gémmiñu fen-kat"},{"style":"q1","verse":"","text":"buy wax aji jub waxi reewande,"},{"style":"q1","verse":"","text":"di réy-réylooka xeebaate."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Yàlla yaa yaatu ngëneel"},{"style":"q1","verse":"","text":"loo dencal ku la ragal,"},{"style":"q1","verse":"","text":"defal ko ku la làqoo,"},{"style":"q1","verse":"","text":"doom aadama seede."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Yaa koy làq fa nga làqu,"},{"style":"q1","verse":"","text":"fa pexey nit àggul."},{"style":"q1","verse":"","text":"Nga yiir ko, mu yiiru"},{"style":"q1","verse":"","text":"ci ayu làmmiñ."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Cant ñeel na Aji Sax ji!"},{"style":"q1","verse":"","text":"Ndaw kéemtaan ci ngor li mu ma jiwe"},{"style":"q1","verse":"","text":"biir dëkk bu ñu gaw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Man nag saalitoon naa ne:"},{"style":"q1","verse":"","text":"«Yàlla dagg na ma, sànni!»"},{"style":"q1","verse":"","text":"Ndeke ba ma lay woo,"},{"style":"q1","verse":"","text":"dégg nga samay dagaan."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Képp kuy aji gëm, soppal Aji Sax ji."},{"style":"q1","verse":"","text":"Aji Sax jeey sàmm ku ko gëm,"},{"style":"q1","verse":"","text":"di yool, ba wis kuy réy-réylu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Képp ku yaakaar Aji Sax ji,"},{"style":"q1","verse":"","text":"dëgërlul te amu fit."}]