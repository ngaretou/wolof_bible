[{"style":"s1","verse":"","text":"Am Yàllaa gën"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Dib taalifu cant, ñeel Asaf."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Yàlla kay baax na ci Israayil,"},{"style":"q1","verse":"","text":"baax ci ku rafet xol."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Waaye man de tuuti kon ma lajj,"},{"style":"q1","verse":"","text":"tuuti ma tarxiis,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"ngir maa ñee ku bew,"},{"style":"q1","verse":"","text":"ndax gis mu baaxle te di ku bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Amuñu mitit,"},{"style":"q1","verse":"","text":"xanaa ne faaj."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Doñ-doñu nit dabu leen,"},{"style":"q1","verse":"","text":"coonoy doom aadama dalu leen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Moo leen taxa ràngoo reewande,"},{"style":"q1","verse":"","text":"làmboo coxor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Dañoo suur ba gët suulu,"},{"style":"q1","verse":"","text":"seen xalaati xel xëtt yoon."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Dañuy ñaawle, di wax lu bon,"},{"style":"q1","verse":"","text":"di réy-réylu, boole ci kàdduy jaay-doole."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Seen ŋalŋal àkki asamaan,"},{"style":"q1","verse":"","text":"làmmiñ dajal suuf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Moo tax ñoñi Yàlla walbatiku ci ñoom,"},{"style":"q1","verse":"","text":"di jolu seen wax nim ndox"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"te naan: «Lu ci Yàlla xam?"},{"style":"q1","verse":"","text":"Ana xam-xam fa Aji Kawe ji?»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Ñu bon ñaa ngoog!"},{"style":"q1","verse":"","text":"Ne finaax, di gëna woomle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Man kay maa sellal ci neen,"},{"style":"q1","verse":"","text":"di sàmm sama der."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Bés bu ne yar dal ma,"},{"style":"q1","verse":"","text":"saa yu ma xëyee, jot mbugal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Su ma noon: «Naa waxe ni ñoom,»"},{"style":"q1","verse":"","text":"kon de, ma wor sa askanu mbooloo."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Naka laay jéema ràññee lii,"},{"style":"q1","verse":"","text":"mbir mi ëlëm ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"Ba ma àggee sa kër gu sell ga,"},{"style":"q1","verse":"","text":"laa doora gis muju ku bon."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Yoonu tarxiis kay, nga leen teg,"},{"style":"q1","verse":"","text":"daane leen, ñu sànku,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Yàquleeka leena bett!"},{"style":"q1","verse":"","text":"Musibaa leen ne fuuf, ñu ne mes!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Day mel ni gént goo yewwoo."},{"style":"q1","verse":"","text":"Céy Boroom bi, boo xippee, jëmmu gént ga soof la."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Naqar laa amoon,"},{"style":"q1","verse":"","text":"sama xol di dagg."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"22","text":"Booba ngaaka laa, xawma dara;"},{"style":"q1","verse":"","text":"rab laa woon fi sa kanam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"23","text":"Teewul ma des ak yaw,"},{"style":"q1","verse":"","text":"nga jàpp sama loxol ndijoor."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"24","text":"Danga maa gindee sam xel,"},{"style":"q1","verse":"","text":"teg ca dalale ma sa teraanga."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"25","text":"Ana ku ma am asamaan ku la moy?"},{"style":"q1","verse":"","text":"Ku ma safoo kaw suuf ku la moy?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"26","text":"Kàttan ak pexe jeex,"},{"style":"q1","verse":"","text":"Yàlla di ma dooleel,"},{"style":"q1","verse":"","text":"ma séddoo ko fàww."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"27","text":"Ku la sore kat, sànku;"},{"style":"q1","verse":"","text":"képp ku la wor, nga bóom."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"28","text":"Man de, jege Yàllaa ma gënal."},{"style":"q1","verse":"","text":"Boroom bi Aji Sax ji, yaw laa def kiiraay,"},{"style":"q1","verse":"","text":"ngir siiwal sa jépp jëf."}]