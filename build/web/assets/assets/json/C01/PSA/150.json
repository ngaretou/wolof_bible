[{"style":"s1","verse":"","text":"Màggalleen Ki Sax!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Màggalleen Ki Sax!"},{"style":"q1","verse":"","text":"Nangeen màggale Yàlla biir këram gu sell,"},{"style":"q1","verse":"","text":"màggale ko ca tatay asamaanam."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Màggaleleen ko ay jalooreem,"},{"style":"q1","verse":"","text":"màggale ko màggam gu réy."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Màggaleleen koob bufta,"},{"style":"q1","verse":"","text":"màggale ko xalam ak moroom ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Màggaleleen ko njiin aki pooj,"},{"style":"q1","verse":"","text":"màggale ko xalam ak toxoro."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Màggaleleen ko tëggum weñ mu xumb,"},{"style":"q1","verse":"","text":"màggale ko tëggum weñ mu kawe."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Na lépp luy noyyi màggal Ki Sax!"},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]