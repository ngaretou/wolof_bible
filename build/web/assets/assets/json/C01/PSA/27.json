[{"style":"s1","verse":"","text":"Xare jib, ma ànd ak Yàlla"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Aji Sax jee may leeral, di ma wallu."},{"style":"q1","verse":"","text":"Ana ku may ragal?"},{"style":"q1","verse":"","text":"Aji Sax jeey aar sama bakkan."},{"style":"q1","verse":"","text":"Ana ku may tiit?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Ab soxor wutsi ma,"},{"style":"q1","verse":"","text":"nar maa yàpp,"},{"style":"q1","verse":"","text":"noonoo ma, bañ ma;"},{"style":"q1","verse":"","text":"mooy tërëf, fëlëñu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Bu gàngoor booloo sama kaw,"},{"style":"q1","verse":"","text":"duma raf."},{"style":"q1","verse":"","text":"Ab xare jib,"},{"style":"q1","verse":"","text":"ma ànd ak Yàlla."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Lenn laa ñaan Aji Sax ji,"},{"style":"q1","verse":"","text":"lii laay sàkku:"},{"style":"q1","verse":"","text":"dëkk kër Aji Sax ji"},{"style":"q1","verse":"","text":"sama giiru dund,"},{"style":"q1","verse":"","text":"di niir taaru Aji Sax ji,"},{"style":"q1","verse":"","text":"te yéemoo këram."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Bésub fitna, mu yiire ma mbaaram,"},{"style":"q1","verse":"","text":"làq ma, ma làqoo xaymaam,"},{"style":"q1","verse":"","text":"aj ma ciw doj, ma raw."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Ma doxe fa siggi,"},{"style":"q1","verse":"","text":"tiim noon yi ma wër,"},{"style":"q1","verse":"","text":"di sarxali fa xaymaam saraxi sarxolle."},{"style":"q1","verse":"","text":"Naa woyal Aji Sax ji, joobe ko."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Éy Aji Sax ji, déglul, ma woote!"},{"style":"q1","verse":"","text":"Ngalla baaxe ma, nangul ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yaw la sam xel ne ma, ma jaamu la,"},{"style":"q1","verse":"","text":"te it yaw Aji Sax ji laay jaamu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Sang bi, bu ma làqu,"},{"style":"q1","verse":"","text":"bul mer, jañax ma."},{"style":"q1","verse":"","text":"Yaa ma daa wallu."},{"style":"q1","verse":"","text":"Bu ma wacc, bu ma ba,"},{"style":"q1","verse":"","text":"yaay Yàlla mi may musal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Ndey ak baay man nañu maa wacc,"},{"style":"q1","verse":"","text":"Aji Sax ji fat ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Éy Aji Sax ji, joxoñ ma saw yoon,"},{"style":"q1","verse":"","text":"jaarale ma joor gu wóor,"},{"style":"q1","verse":"","text":"ngir ñi may tëru."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Bu ma bàyyee bànneexu noon."},{"style":"q1","verse":"","text":"Seedey naaféq a ma jógal,"},{"style":"q1","verse":"","text":"di sos fitna."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Maay dajeek ngëneelu Aji Sax ji"},{"style":"q1","verse":"","text":"fi kaw suuf sii."},{"style":"q1","verse":"","text":"Lii wóor na ma."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Yaakaaral Aji Sax ji,"},{"style":"q1","verse":"","text":"dëgërlul te amu fit,"},{"style":"q1","verse":"","text":"te yaakaarati Aji Sax ji."}]