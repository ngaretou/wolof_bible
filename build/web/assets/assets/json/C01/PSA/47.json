[{"style":"s1","verse":"","text":"Yàllaay Buur"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel askanu Kore wu góor."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Na xeetoo xeet tàccu,"},{"style":"q1","verse":"","text":"xaacu, sarxolleel Yàlla!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Aji Sax ji, Aji Kawe jee jara ragal,"},{"style":"q1","verse":"","text":"mooy buur bu mag bi tiim àddina sépp."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Moo nu nangulal xeet yi,"},{"style":"q1","verse":"","text":"nu teg tànk gàngoor yi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Moo nu tànnal céru suuf,"},{"style":"q1","verse":"","text":"muy sagu askanu \\w Yanqóoba|Yanqóoba:\\w* wii mu sopp. \\qs Selaw.\\qs*"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yàllaa yéeg, kàddu riir."},{"style":"q1","verse":"","text":"Aji Sax jee yéeg, bufta\\f + \\fr 47.6 \\fk bufta, \\ft ab béjjénu kuuy la, bu ñu bënn ñaari wet yi, ngir man cee wal.\\f* jolli."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Woyleen Yàlla, woyleen,"},{"style":"q1","verse":"","text":"woyleen Yàlla sunu buur, woyleen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yàllaay buuru àddina sépp;"},{"style":"q1","verse":"","text":"taalifleen, woy ko."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Yàllaa di buuru xeet yi,"},{"style":"q1","verse":"","text":"Yàllaa tooge jalam bu \\w sell|sell:\\w*."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Kàngami xeet yeey daje,"},{"style":"q1","verse":"","text":"ànd ak ñoñi Yàllay Ibraayma;"},{"style":"q1","verse":"","text":"Yàllaa moom buur yi yiir àddina,"},{"style":"q1","verse":"","text":"te moo kawee kawe."}]