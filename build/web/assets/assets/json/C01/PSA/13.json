[{"style":"s1","verse":"","text":"Aji Sax ji, loo deeti xaar?"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, dib taalifu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Céy Aji Sax ji, loo deeti xaar?"},{"style":"q1","verse":"","text":"Xanaa doo ma tanqamlu ba fàww?"},{"style":"q1","verse":"","text":"Loo deeti xaar, bañ maa geesu,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"ba ma tàggook bopp bu ubu"},{"style":"q1","verse":"","text":"ak njàqarey xol bu sax,"},{"style":"q1","verse":"","text":"ba noon dootu ma man?"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Céy Aji Sax ji sama Yàlla, geesu ma, wuyu ma,"},{"style":"q1","verse":"","text":"leqali ma, bala ma ne sërëx ci nelawi dee,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"noon ba naan: «Daan naa ko,»"},{"style":"q1","verse":"","text":"bañ yay bége saab jéll."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Man de maa ngi dénkoo sa ngor,"},{"style":"q1","verse":"","text":"di bànneexoo sag wall."},{"style":"q1","verse":"","text":"Naa woy Aji Sax ji,"},{"style":"q1","verse":"","text":"ki may baaxe xéewalam."}]