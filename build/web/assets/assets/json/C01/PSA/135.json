[{"style":"s1","verse":"","text":"Lu Yàlla namm lay def"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Màggalleen Ki Sax!"},{"style":"q1","verse":"","text":"Màggalleen turu Aji Sax ji,"},{"style":"q1","verse":"","text":"éey jaami Aji Sax ji, màggalleen,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"yeen ñi taxaw ci kër Aji Sax ji,"},{"style":"q1","verse":"","text":"fi sunu biir ëttu Yàlla bi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Màggalleen Ki Sax, Aji Sax jee baax!"},{"style":"q1","verse":"","text":"Kañleen ko, kee sopplu!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Ki Sax a tànn \\w Yanqóoba|Yanqóoba:\\w*,"},{"style":"q1","verse":"","text":"muy Israayil mi mu séddoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Maa xam ne Aji Sax ji màgg na!"},{"style":"q1","verse":"","text":"Sunu Boroom a sut lépp lu ñuy jaamu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Lépp lu Aji Sax ji namm,"},{"style":"q1","verse":"","text":"moom lay def asamaan ak suuf,"},{"style":"q1","verse":"","text":"ak biir géej ak xóote yépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Mooy yékkateey niir fa cati àddina,"},{"style":"q1","verse":"","text":"sàkkal taw ay melax,"},{"style":"q1","verse":"","text":"yebbee ngelaw cay dencam,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"faat luy taaw fa Misra,"},{"style":"q1","verse":"","text":"doom aadama, ba ci mala."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Yeen waa Misra, moo yónnee ay firndeeki kéemaan fi seen biir,"},{"style":"q1","verse":"","text":"ñeel Firawnaak jawriñam ñépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Moo duma ay xeeti xeet,"},{"style":"q1","verse":"","text":"bóom buur yu mag yi,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"muy Sixon buurub Amoreen ña,"},{"style":"q1","verse":"","text":"ak Og buuru Basan,"},{"style":"q1","verse":"","text":"ak buuri Kanaan yépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Moo nangu seen réew, séddale,"},{"style":"q1","verse":"","text":"\\w bànni Israayil|bànni Israayil:\\w* ñiy ñoñam séddoo."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Aji Sax ji, sa tur ba fàww;"},{"style":"q1","verse":"","text":"Aji Sax ji, sa woy a saxal maasoo maas."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Aji Sax ji kay ay àtte ñoñam ñi,"},{"style":"q1","verse":"","text":"di ñeewante jaamam ñi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"15","text":"Tuuri yéefar yi, xaalis laak wurus,"},{"style":"q1","verse":"","text":"loxoo ko sàkk."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"16","text":"Am gémmiñ te du wax,"},{"style":"q1","verse":"","text":"ami gët te du gis,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"17","text":"ami nopp te du dégg,"},{"style":"q1","verse":"","text":"du sax noowal bakkan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"18","text":"Ñi sàkk yii tuur, yal nañu mel ni ñoom,"},{"style":"q1","verse":"","text":"ñook ñi leen wóolu ñépp."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"19","text":"Éey waa kër Israayil, santleen Aji Sax ji!"},{"style":"q1","verse":"","text":"Éey waa kër \\w Aaróona|Aaróona:\\w*, santleen Aji Sax ji!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"20","text":"Éey waa kër \\w Lewi|Leween b-:\\w*, santleen Aji Sax ji!"},{"style":"q1","verse":"","text":"Yeen ñi ragal Aji Sax ji, santleen Aji Sax ji!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"21","text":"Nañuy sant Aji Sax ji ci \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","verse":"","text":"moom mi dëkke Yerusalem."},{"style":"q1","verse":"","text":"Màggalleen Ki Sax!"}]