[{"style":"s1","verse":"","text":"Yaakaar, fa Yàlla"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci Yedutun, njiital woykat yi, dib taalifu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Yàlla doŋŋ ay dalal sama xel,"},{"style":"q1","verse":"","text":"moo may wallu."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Moom doŋŋ mooy sama wéeru-mucc,"},{"style":"q1","verse":"","text":"di ma rawale, ba duma sànku!"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Xanaa dungeen bàyyee songandoo nit,"},{"style":"q1","verse":"","text":"nar koo sànk,"},{"style":"q1","verse":"","text":"ni kuy màbb tabax bu joy,"},{"style":"q1","verse":"","text":"mbaa ngay bàddi per mu ràpp?"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Seeni mébét, wàcce boroom daraja doŋŋ;"},{"style":"q1","verse":"","text":"yeena ngi bége ay fen,"},{"style":"q1","verse":"","text":"seeni kàddu diw yiw,"},{"style":"q1","verse":"","text":"seen xalaat diy saaga. \\qs \\+w Selaw|selaw:\\+w*.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Yal na Yàlla doŋŋ di dalal sama xel,"},{"style":"q1","verse":"","text":"moom mi ma yaakaar."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Moom doŋŋ a di sama wéeru-mucc,"},{"style":"q1","verse":"","text":"di sama rawtu, duma tërëf."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"Yàllaa may musal, di ma sagal,"},{"style":"q1","verse":"","text":"ma di ko doolewoo."},{"style":"q1","verse":"","text":"Sama kiiraay, Yàllaa!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"Bokk yi, wóoluleen ko fu ngeen tollu,"},{"style":"q1","verse":"","text":"diisleen ko seen xol,"},{"style":"q1","verse":"","text":"Yàllaay sunu kiiraay. \\qs Selaw.\\qs*"},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Doom aadama, ngelawal neen;"},{"style":"q1","verse":"","text":"nit, naxi neen."},{"style":"q1","verse":"","text":"Booleel teg ci lu ñuy natte diisaay,"},{"style":"q1","verse":"","text":"kaw la jëm."},{"style":"q1","verse":"","text":"Ñooy gëna woyof ngelaw!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"Bul wóolu ribaa,"},{"style":"q1","verse":"","text":"bul yaakaar alalu càcc, neen la."},{"style":"q1","verse":"","text":"Bu sa alal yokku, nga taf ci sa xol."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Benn yoon la Yàlla wax,"},{"style":"q1","verse":"","text":"ñaari yoon laa dégg, ñu ne;"},{"style":"q1","verse":"","text":"«Doole, fa Yàlla.»"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Boroom bi, ngor, yaay boroom,"},{"style":"q1","verse":"","text":"yaw daal, yaay yool nit jëfam."}]