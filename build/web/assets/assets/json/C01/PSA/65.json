[{"style":"s1","verse":"","text":"Yàllaay nangul suuf"},{"style":"d","verse":"","text":""},{"style":"v","verse":"1","text":"Mu jëm ci njiital woykat yi, di woyu cant, ñeel Daawuda."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Éy Yàlla, yaa jagoo cant fa \\w Siyoŋ|Siyoŋ:\\w*,"},{"style":"q1","verse":"","text":"yaw lees di jëfeel ab dige."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Yaw miy nangug ñaan,"},{"style":"q1","verse":"","text":"yaw la képp di wuyusi."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"Samay ñaawtéef man na ma,"},{"style":"q1","verse":"","text":"waaye sunuy tooñ, yaa nu koy baal."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"Ndokkalee koo tànn, woo ko,"},{"style":"q1","verse":"","text":"mu dale say ëtt,"},{"style":"q1","verse":"","text":"di xéewloo sa ngëneeli kër,"},{"style":"q1","verse":"","text":"ak sa sellaayu dëkkuwaay."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Defal nga nuy kéemaan,"},{"style":"q1","verse":"","text":"nangug ñaan ci njekk,"},{"style":"q1","verse":"","text":"yaw Yàlla mi nuy wallu,"},{"style":"q1","verse":"","text":"di yaakaaru àddina wërngal këpp,"},{"style":"q1","verse":"","text":"ba ca géej gu sore ga."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"7","text":"Yaa dëje tund yu mag yi doole,"},{"style":"q1","verse":"","text":"gañoo sa kàttan,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"8","text":"di dalal riirum géej aki gannaxam,"},{"style":"q1","verse":"","text":"ak coowal xeet yi,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"9","text":"ba waa cati àddina yéemoo say firnde,"},{"style":"q1","verse":"","text":"waa penkook sowu di sarxolle."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":""},{"style":"v","verse":"10","text":"Yaa dikke suuf am wal,"},{"style":"q1","verse":"","text":"nangul koo nangul."},{"style":"q1","verse":"","text":"Dexu Yàllaa fees ndox,"},{"style":"q1","verse":"","text":"ngay may nit peppam,"},{"style":"q1","verse":"","text":"ni nga ko waajale suuf si,"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"11","text":"di suuxat ngàbb mi,"},{"style":"q1","verse":"","text":"maasalee ko jal yi,"},{"style":"q1","verse":"","text":"sottiy waame, mu mokk,"},{"style":"q1","verse":"","text":"cax ma, nga barkeel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"12","text":"Yaay tëje mbeyum at mi say ngëneel,"},{"style":"q1","verse":"","text":"foo jaare, xéewal walangaan."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"13","text":"Parluy màndiŋ yaa ngi walangaan,"},{"style":"q1","verse":"","text":"li wër tund yu ndaw yi di xumbéen."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"14","text":"Joor gaa ngi, jur gi muur,"},{"style":"q1","verse":"","text":"xur yi sàngoo pepp,"},{"style":"q1","verse":"","text":"lépp di xaacooka woy."}]