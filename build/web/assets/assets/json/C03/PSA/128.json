[{"style":"r","verse":"","text":"Ps 112; 127:3, 4."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"1","text":"Cantique des degrés."},{"style":"b","verse":"","text":""},{"style":"q1","verse":"","text":"Heureux tout homme qui craint l’Éternel,"},{"style":"q1","verse":"","text":"Qui marche dans ses voies!"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"2","text":"Tu jouis alors du travail de tes mains,"},{"style":"q1","verse":"","text":"Tu es heureux, tu prospères."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"3","text":"Ta femme est comme une vigne féconde"},{"style":"q1","verse":"","text":"Dans l’intérieur de ta maison;"},{"style":"q1","verse":"","text":"Tes fils sont comme des plants d’olivier,"},{"style":"q1","verse":"","text":"Autour de ta table."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"4","text":"C’est ainsi qu’est béni"},{"style":"q1","verse":"","text":"L’homme qui craint l’Éternel."},{"style":"q1","verse":"","text":""},{"style":"v","verse":"5","text":"L’Éternel te bénira de Sion,"},{"style":"q1","verse":"","text":"Et tu verras le bonheur de Jérusalem"},{"style":"q1","verse":"","text":"Tous les jours de ta vie;"},{"style":"q1","verse":"","text":""},{"style":"v","verse":"6","text":"Tu verras les fils de tes fils."},{"style":"q1","verse":"","text":"Que la paix soit sur Israël!"}]